  <script>
    $(window).on('load resize', function() {
      $('#mainnav .sf-menu').superfish({
        delay: 400,// the delay before sub closes
        speed: 'fast',// speed of the fade-in
        autoArrows: false
      });

      switch (screenMode) {
        case 'standard':
          $('#mainnav .sf-menu').removeClass('sf-js-disabled').addClass('sf-js-enabled');
          break;

        case 'mobile':
          $('#mainnav .sf-menu').removeClass('sf-js-enabled').addClass('sf-js-disabled');
          break;
      }
    });

    $(document).ready(function() {
      // initialise superfish nav
      $(function(){
        $('#menu-wrapper a.item-global').on('click', function() {
          $('#siteSearch.sf-menu').hide();
          $('#menu-wrapper a.item-search').removeClass('active');

          $('#mainnav .sf-menu').slideToggle();
          if ($(this).hasClass('active')) {
            $(this).removeClass('active');
          }
          else {
            $(this).addClass('active');
          }
        });

        $('#menu-wrapper a.item-search').on('click', function() {
          $('#mainnav .sf-menu').hide();
          $('#menu-wrapper a.item-global').removeClass('active');

          $('#siteSearch.sf-menu').slideToggle();
          if ($(this).hasClass('active')) {
            $(this).removeClass('active');
          }
          else {
            $(this).addClass('active');
          }
        });
      });
    });
  </script>


