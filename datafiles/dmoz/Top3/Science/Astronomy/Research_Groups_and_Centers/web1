  <!-- START main heading -->
  
  <h2>Welcome to the Institute for Astronomy</h2>
  

  <!-- END main heading -->
  <!-- START text box floating right -->
  <!-- show column depending on layout switch -->
  
    
          
            
  
                        	
    
    	
	      <div class="box-border">
	        <div class="box">
	          <p class="boxtitle">Research Groups</p>
	          
