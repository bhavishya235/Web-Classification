var state = 'show';
function toggleLayer(id,img)
{
	if (!document.getElementById) return;
	var s = document.getElementById(id).style;
	s.display = s.display ? '' : 'block';
	state = s.display ? 'hide' : 'show';
	document.images[img].src = '"pigwad/images/' + state + 'over.gif';
}
function moImg(img,w)
{
	document.images[img].src = 'pigwad/images/' + state + w + '.gif';
}
