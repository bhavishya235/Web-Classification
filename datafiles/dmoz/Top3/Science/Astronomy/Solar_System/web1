<SCRIPT LANGUAGE="JavaScript">
function flashScript(url,w,h) {
	leftPos = 0;
	if (url.indexOf('http') == 0)
	{
		var modifiedURL = url;
	}
	else
	{
		var modifiedURL = ('' + url);
	}
	var flash = window.open(modifiedURL,'flash','toolbar=0,location=0,directories=0,status=0,menubar=no,scrollbars=no,resizable=no,width='+w+',height='+h+',left='+leftPos+',top=0');
	flash.focus();
	flash.focus();
}
</SCRIPT>





