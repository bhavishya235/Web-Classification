// Clear Search box
	function clearSearch () {
		if (document.SSESearch.Criteria.value == 'Search') {
			document.SSESearch.Criteria.value = '';
		}
	}
	
// Reinstate Search box
	function fillSearch () {
		if (document.SSESearch.Criteria.value == '') {
			document.SSESearch.Criteria.value = 'Search';
		}
	}

