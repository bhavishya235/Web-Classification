<HTML>
<HEAD><TITLE>Welcome to the Planets</TITLE>


<SCRIPT LANGUAGE="JavaScript">

// Define Variables
	var rolloversOK = 0;
// Check browser version being used...
	browserName = navigator.appName;
	browserVer = parseInt(navigator.appVersion);
	if (browserName == "Netscape" && browserVer >= 3) 
		rolloversOK = 1;
	else if (browserName == "Microsoft Internet Explorer" && browserVer >= 4) 	
		rolloversOK = 1;
// Pre-load rollover iface
	if (rolloversOK) { 
	button1off= new Image(89,50)
	button1off.src = "iface/whatsnew.gif"
	button1on= new Image(89,50)
	button1on.src = "iface/whatsnew_r.gif"
	button2off= new Image(60,50)
	button2off.src = "iface/credits.gif"
	button2on= new Image(60,50)
	button2on.src = "iface/credits_r.gif"
	button3off= new Image(89,50)
	button3off.src = "iface/order.gif"
	button3on= new Image(89,50)
	button3on.src = "iface/order_r.gif"
	button4off= new Image(89,50)
	button4off.src = "iface/glossary.gif"
	button4on= new Image(89,50)
	button4on.src = "iface/glossary_r.gif"
	button5off= new Image(89,50)
	button5off.src = "iface/explorers.gif"
	button5on= new Image(89,50)
	button5on.src = "iface/explorers_r.gif"
	button6off= new Image(111,28) 
	button6off.src = "iface/pj_off.gif"
	button6on= new Image(111,28) 
	button6on.src = "iface/pj_on.gif"
	button7off= new Image(89,50) 
	button7off.src = "iface/planets.gif"
	button7on= new Image(89,50) 
	button7on.src = "iface/planets_r.gif"
   }

// Swap rollover iface

	function buttonOn (imgName) {
    	  if (rolloversOK) {
    	    eval ("document [imgName].src = " + imgName + "on.src");
  	  }
	}

	function buttonOff (imgName) {
    	  if (rolloversOK) {
	    eval ("document [imgName].src = " + imgName + "off.src");
	  }
	}

// - END rollover iface script

</SCRIPT>
</HEAD>
