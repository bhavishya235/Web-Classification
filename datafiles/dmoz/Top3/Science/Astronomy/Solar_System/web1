		timelineArray[1][65][0] = 'Deep Impact';
		timelineArray[1][65][1] = '4 July 2005: First comet impact (Tempel 1)';
		timelineArray[1][65][2] = 'missions/profile.cfm?MCode=DeepImpact&Target=Comets';
	
		timelineArray[1][66] = new Array();
		timelineArray[1][66][0] = 'Stardust';
		timelineArray[1][66][1] = '15 January 2006: First sample return of material from a comet (Wild 2)';
		timelineArray[1][66][2] = 'missions/profile.cfm?MCode=STARDUST&Target=Comets';
	
		timelineArray[1][67] = new Array();
		timelineArray[1][67][0] = 'Opportunity';
		timelineArray[1][67][1] = '20 May 2010: Surface endurance record: six years and 116 days operating on Mars (surpassing Viking 1)';
		timelineArray[1][67][2] = 'missions/profile.cfm?MCode=MER_B';
	
		timelineArray[1][68] = new Array();
		timelineArray[1][68][0] = 'Hayabusa';
		timelineArray[1][68][1] = '13 June 2010: First sample return from an asteroid (Itokawa)';
		timelineArray[1][68][2] = 'missions/profile.cfm?MCode=MUSES';
	
		timelineArray[1][69] = new Array();
		timelineArray[1][69][0] = 'MESSENGER';
		timelineArray[1][69][1] = '18 March 2011: First spacecraft to orbit Mercury';
		timelineArray[1][69][2] = 'missions/profile.cfm?MCode=MESSENGER&Target=Mercury';
	
		timelineArray[1][70] = new Array();
		timelineArray[1][70][0] = 'MSL/Curiosity';
		timelineArray[1][70][1] = '6 August 2012: First use of \"sky crane\" landing technique; largest rover (about the size of a compact car) to land on another planet (Mars).';
		timelineArray[1][70][2] = 'missions/profile.cfm?MCode=MarsSciLab';
	
		timelineArray[1][71] = new Array();
		timelineArray[1][71][0] = 'Rosetta';
		timelineArray[1][71][1] = 'November 2014: First comet landing (67P/Churyumov-Gerasimenko)';
		timelineArray[1][71][2] = 'missions/profile.cfm?MCode=Rosetta';
	
		timelineArray[1][72] = new Array();
		timelineArray[1][72][0] = 'Dawn';
		timelineArray[1][72][1] = 'February 2015: First spacecraft to orbit a dwarf planet (Ceres)';
		timelineArray[1][72][2] = 'missions/profile.cfm?MCode=Dawn&Target=Dwarf';
	
		timelineArray[1][73] = new Array();
		timelineArray[1][73][0] = 'New Horizons';
		timelineArray[1][73][1] = '14 July 2015: First Pluto flby';
		timelineArray[1][73][2] = 'missions/profile.cfm?MCode=PKB&Target=Pluto';
	
		timelineArray[1][74] = new Array();
		timelineArray[1][74][0] = 'Juno';
		timelineArray[1][74][1] = 'July 2016: First solar-powered spacecraft at Jupiter';
		timelineArray[1][74][2] = 'missions/profile.cfm?MCode=Juno';
	

function changetimelinedesc(a,b)
{
	document.getElementById("timelinedesc").innerHTML = '<A HREF=\"' + timelineArray[a][b][2] + '\" CLASS=\"timelinetextheader\">' + timelineArray[a][b][0] + '</A><BR>' + timelineArray[a][b][1] + '&nbsp;&nbsp;&nbsp;<A HREF=\"' + timelineArray[a][b][2] + '\" TARGET=\"_blank\" CLASS=\"timelinelink\">More&nbsp;<IMG SRC=\"images/arrow2-transparent-ltblue.png\" ALIGN=\"absmiddle\" WIDTH=\"7\" HEIGHT=\"7\" BORDER=\"0\" ALT=\"\"></A>';
}

var currenttimeline = 1;
