                          <div id="nomenclature_menu" class="menu">
                            <div class="home_menu" onClick="location.href='/nomenclature/';"><a href="/nomenclature/">Home</a></div>
                            <div class="previous_menu" onClick="animateRight('#nomenclature_menu'); animateRight('#main_menu');"> Previous</div>
                            <div class="parent_menu">Nomenclature</div>
                            <div class="sub_menu">
                              <div class="menu_item" onClick="location.href='/nomenclature/Page/MERCURY/target';"                ><a href="/nomenclature/Page/MERCURY/target">Mercury   </a></div>
                              <div class="menu_item" onClick="location.href='/nomenclature/Page/VENUS/target';"                  ><a href="/nomenclature/Page/VENUS/target">Venus     </a></div>
