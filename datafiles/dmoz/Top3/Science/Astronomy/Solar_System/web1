<SCRIPT>
var scrollLeftTimer;

function startScrollLeft(divid)
{
	scrollLeftTimer = setInterval("scrollLeft('" + divid + "')", 20);
}

function stopScrollLeft(divid)
{
	clearInterval(scrollLeftTimer);
}

function scrollLeft(divid)
{
	document.getElementById(divid).scrollLeft = document.getElementById(divid).scrollLeft - 5;
}

var scrollRightTimer;

function startScrollRight(divid)
{
	scrollRightTimer = setInterval("scrollRight('" + divid + "')", 20);
}

function stopScrollRight(divid)
{
	clearInterval(scrollRightTimer);
}

function scrollRight(divid)
{
	document.getElementById(divid).scrollLeft = document.getElementById(divid).scrollLeft + 5;
}

var timelineArray = new Array();

	timelineArray[1] = new Array();
	timelineArray[1][0] = new Array();
	timelineArray[1][0][0] = 'timelinefirstsimage';
	timelineArray[1][0][1] = 'timelinefirstsnav';
	
		timelineArray[1][1] = new Array();
		timelineArray[1][1][0] = 'Sputnik 1';
		timelineArray[1][1][1] = '4 October 1957: First spacecraft  to orbit Earth';
		timelineArray[1][1][2] = 'missions/profile.cfm?MCode=Sputnik';
	
		timelineArray[1][2] = new Array();
		timelineArray[1][2][0] = 'Explorer 1';
		timelineArray[1][2][1] = '1 February 1958: First U.S. spacecraft';
		timelineArray[1][2][2] = 'missions/profile.cfm?MCode=Explorer_01';
	
		timelineArray[1][3] = new Array();
		timelineArray[1][3][0] = 'Luna 1';
		timelineArray[1][3][1] = '2 January 1959: First probe to escape Earth\'s gravity';
		timelineArray[1][3][2] = 'missions/profile.cfm?MCode=Luna_01';
	
		timelineArray[1][4] = new Array();
		timelineArray[1][4][0] = 'Luna 2';
		timelineArray[1][4][1] = '14 September 1959: First lunar impact';
		timelineArray[1][4][2] = 'missions/profile.cfm?MCode=Luna_02';
	
		timelineArray[1][5] = new Array();
		timelineArray[1][5][0] = 'Luna 3';
		timelineArray[1][5][1] = '6 October 1959: First lunar flyby and first photo of Moon\'s farside';
		timelineArray[1][5][2] = 'missions/profile.cfm?MCode=Luna_03';
	
