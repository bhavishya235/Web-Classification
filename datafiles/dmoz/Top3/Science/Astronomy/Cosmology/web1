      hoverIntent: false,
      sticky: true,
      mouseOutClose: true,
      closePosition: 'title',
      closeText: '[x]'
    });
 
 
    $( ".banner" ).animate({ opacity: 1	}, {	duration: 2000,	queue: false	});
  
  });

  // Show/Hide toggle
  function ReverseDisplay(d) {
    if(document.getElementById(d).style.display == "none") { 
      document.getElementById(d).style.display = "block"; 
    } else { 
      document.getElementById(d).style.display = "none"; 
    }
  }

  $(document).ready(function(){setTimeout( function(){var banner = $('.banner');banner.animate({"height":"+=0px"},"slow");$('.close').click(function(){var bannerHeight=banner.height();if(bannerHeight=="0"){banner.animate({"height":"+=40px"},"slow");$(this).removeClass('active')}else{banner.animate({"height":"-=40px"},"slow");$(this).addClass('active');};});},2000);});

  loadTabs = function() {
    var availableTabs = [];
    var defaultTabId = 'tab-overview';
    var hash = window.location.hash;

    if (hash) {
      $('div.TabbedPanelsContent[id|="tab"]').each(function() {
        availableTabs.push($(this).attr('id').replace('tab-', ''));
      });
      if (availableTabs.indexOf(hash.substr(1)) > 0) {
        defaultTabId = 'tab-' + hash.substr(1);
      }
    }

    var TabbedPanels1 = new Spry.Widget.TabbedPanels("TabbedPanels1", {
      defaultTab: defaultTabId
    });    
  }

  $(loadTabs);

    //Multimedia toggle video
  
  $(document).ready(function(){
   $('.videoWrapper:first').parent('.selection').show();
   $('.audioWrapper:first').parent('.selection').show();
   
   $('.toggle:first').addClass('arrowdown');
   $('.toggle:not(:first)').addClass("arrowup");
   
  

    $('.toggle').click(function(){
        var next = $(this).next('.selection');
        $(this).siblings('.selection').not(next).slideUp("slow");
        next.slideToggle("slow");
    $(this).toggleClass("arrowdown").toggleClass("arrowup");

    
    });
    
    $('.selection').click(function(){
        $(this).siblings('.selection').slideUp("slow");
        $(this).slideToggle("slow");
    });

  });

  // Multimedia Audio Player Flash Detection
  $(function(){
    if(isAndroidDefault){
            alert("Sorry, your browser does not support this implementation of the player. It will be using the standard HTML5 audio player instead")
              }

       $(".audio").mb_miniPlayer({
          
          inLine: true,
          downloadable: true,
          wmode:"window"
        });

      });
  


</script> 


