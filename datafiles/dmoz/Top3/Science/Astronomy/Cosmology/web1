      <script language="JavaScript" type="text/javascript">
        function RightslinkPopUp() {
          var url = "https://s100.copyright.com/AppDispatchServlet";
          var location = url 
            + "?publisherName=" 
            + escape('NAP') 
            + "&imprint=" 
            + escape('NAP')
            + "&publication=" + escape('book')
            + "&title=" + escape('Cosmology: A Research Briefing')
            + "&contentID=" + escape('9293')
            + "&publicationDate=" + escape('1995')
            + "&author=" + escape('Panel on Cosmology, Board on Physics and Astronomy, National Research Council')
            + "&sp=" + escape('n')
            + "&pageCount=" + escape('51')
            + "&orderBeanReset=true";
          PopUp = window.open( location,'Rightslink','location=no,toolbar=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=650,height=550');
        }
      </script> 

