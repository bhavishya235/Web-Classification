                        (function(){
                                var visitNum;
                                var parts = document.cookie.split("s_vnum=");
                                if (parts.length === 2) {
                                    vn = parts.pop().split(";").shift().split("vn%3D").pop();
                                }
                                vn = parseInt(vn);
                                if (vn === 1) {
                                    visitNum = "1";
                                } 
                                else if (vn > 1 && vn < 5) {
                                    visitNum = "2-4";
                                }
                                else if (vn > 4) {
                                    visitNum = "5 or more";
                                }
                                else {
                                    visitNum = "unknown";
                                }
