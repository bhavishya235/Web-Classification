			(function()
			{
				window.googletag = window.googletag || {};
				window.googletag.cmd = window.googletag.cmd || [];
			
				googletag.cmd.push(function()
				{
					var _startTime = new Date();
					var _log = googletag.debug_log.log;
					var addEvent = function( name, match )
					{
						events.push(
						{
							"name":name,
							"match":match
						});
					}
	
					var events = [];
	
					addEvent( "gpt-page_load_complete",			/Page load complete/ig);
					addEvent( "gpt-slot_rendered",				/Completed rendering ad for slot (.*)/ig);
				
					googletag.debug_log.log = function( level, message, service, slot, reference)
					{
						// "arguments" is an array-like object corresponding to the arguments passed to a function
						// Array.prototype.slice.call() turns it into an array so that we can pass the same arguments to the trigger
						var _arguments = Array.prototype.slice.call( arguments );
						
						// Add timing info
						_arguments.push( new Date().getTime() - _startTime.getTime() );
						
						// See whether the log message matches any of our desired events
						for( var e = 0,el = events.length; e < el; e++ )
						{
							//if(message.search(events[e].match) > -1){
							//	jQuery(googletag).trigger( events[e].name, _arguments );
							//}
						}
						
						return _log.apply( this,arguments );
					};
				});
			})();

			//on page load complete, we submit collected info to omniture
			jQuery( googletag ).on( 'gpt-page_load_complete', function( e,level,message,service,slot,reference )
			{
				if( typeof s != 'undefined' )
				{
					s.linkTrackVars='prop3,prop4,prop5,prop21,prop36,prop40,prop44,prop45,prop46,prop47,prop48,eVar36,events';
					s.linkTrackEvents='event3';
					s.events='event3';
					s.prop44 = googletag.adUnitTiming[ 0 ];
					s.prop45 = googletag.adUnitTiming[ 1 ];
					s.prop46 = googletag.adUnitTiming[ 2 ];
					s.prop47 = googletag.adUnitTiming[ 3 ];
					s.prop48 = googletag.adUnitTiming[ 4 ];

					s.tl( true, 'o', 'Ad load complete' );
				}
			});
			
			//stores loading time for specific ad slots
			jQuery( googletag ).on( 'gpt-slot_rendered', function( e,level,message,service,slot,reference,timing )
			{
				var adSlotNames = [  /728x90A/ig, /300x250-600-1050/ig, /300x250B/ig, /300x250C/ig, /728x90B/ig  ];
				googletag.adUnitTiming  = googletag.adUnitTiming || [ '', '', '', '', '' ];
				for( var i = 0; i < adSlotNames.length; i++ )
				{
					if( message.search( adSlotNames[ i ] ) > -1 )
					{
						googletag.adUnitTiming[ i ] = timing;
					}
				}
			});
			
		
