<script>
  function addToShelf(r) {
    $.getJSON('/napsave.php', { type: 'shelf', r: r }, function(response) {
      if (response.status === "1") {
        $('#bkshlf').remove();
        $('.setactionbox').append('<li class="saveit_active">Added</li>');
        alert("Cosmology: has been added to your bookshelf!");
        trackEvent('mynap', 'save book', 9293);
      } else {
        // error
      }
    });
  }
</script>
