$(".mobilemenu").slideUp();
});

$("#mobile_userBtn").click(function () {
$(".mobileuser").slideToggle("slow");
$(".mobilesearch").slideUp();
$(".mobilemenu").slideUp();
});

$("#mobile_menuBtn").click(function () {
$(".mobilemenu").slideToggle("slow");
$(".mobilesearch").slideUp();
$(".mobileuser").slideUp();
});
</script>    </div>
  </header>
<!-- close header section -->

          
