<!-- PAGE INLINE EXECUTABLE start up code to set visibilties.
	MODS NEEDED:  CSS/html should set defaults and let JS change them as needed, that way there is some graceful degradation
    if JS not present and for VOX browsers etc.
------------------------------------------------->
<script type="text/javascript">
try {
	$(document).ready(function() {  // STARTUP STATE: tabs and widgets...
		tabLine_show('#tabbedBoxWidget','1');
        tabLine_show('#NewsWebCamTabbedBox','2');
		setZIndex('#scExplorer','102');
		sc_show("#sc2");
	});
	
} // if not jquery
catch (exp) {
	alert("ERROR: jQuery $ not found.");
}
</script>

<script type="text/javascript">
try {

	// set focus on TOP widget clicked anywhere
	// Intro/Facts box
	$('#tabbedBoxWidget').on('click',function() {  // set starting tabs for all widgets...
		setZIndex('#tabbedBoxWidget','102');
		$('#tabbedBoxWidget').fadeTo(250,1);

		setZIndex('#scExplorer','101');
		$('#scExplorer').fadeTo(250,.5);
		//sc_reset("#sc1");
	});
	
// SC_Expl
	$('#scExplorer').on('click',function() {  // set starting tabs for all widgets...
		setZIndex('#scExplorer','102');
		$('#scExplorer').fadeTo(250,1);

		setZIndex('#tabbedBoxWidget','101');
		$('#tabbedBoxWidget').fadeTo(250,.5);
	});	
	
} // if not jquery
catch (exp) {
	alert("ERROR: jQuery $ not found.");
}
</script>

