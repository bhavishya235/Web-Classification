<!-- tabbox javascript ------------------------------------------------------------------>
<script type="text/javascript">
function tabLine_show(wid, tnum) {	  //reveals content div and sets selected class on tab number passed, not dependency on ID names
try {
  for (i=1;i<=4;i++){  // be better to grab all and count them in next version 
	$(wid + "  .tabContent"+i).hide(10); // hide all, sureThis could be more elegant, IE find the one with display:block and set it to "none"
	$(wid + "  .tab"+i).removeClass("selected"); // turn off selected tab
	}


// poss add a show of the motion blurred image between the fade out and fade in!!
//alert("showing"+"#ff"+id);
  $(wid + "  .tabContent"+tnum).fadeIn(600);
  $(wid + "  .tab"+tnum).addClass("selected");
}
catch (exp) {
	// 
	alert("ERROR: tabLine_show() failed.");
	if ( !$ ) alert("Jquery not found.");
}

}
</script>

