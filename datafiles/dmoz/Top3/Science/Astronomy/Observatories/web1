        <!-- The JavaScript -->
        <script type="text/javascript" src="js/jquery.min.js"></script>
		<script type="text/javascript" src="js/jquery.easing.1.3.js"></script>
        <script type="text/javascript">
			<!-- Body Content -->
			function goto(id, t){
				//animate to the div id.
				$(".contentbox-wrapper").animate({"left": -($(id).position().left)}, 600);
			 
			 	//Set the Breadcrumbs
				var idName;
					idName = id.substring(1).toUpperCase();
				
					if(idName == "ORBITAL"){ idName =  "Orbital IDS of Minor Planets and Bodies";}
					if(idName == "EARTH"){ idName =  "Close approaches to Earth";}
					if(idName == "INNER"){ idName =  "Close approaches to Venus, Mars, and Mercury";}					
					if(idName == "MINOR"){ idName =  "Potentially Dangerous Objects on close approaches to Earth";}					
					if(idName == "SMALL"){ idName =  "Small Minor Planets H>22 approaching Earth";}					
					if(idName == "PLANET"){ idName =  "Close approaches of asteroids with close approaches to Venus, Mars, and Mercury";}					
				
				
					document.getElementById("breadcrumbs").innerHTML = "<a href='INDEX.html'>HOME</a> -> " + idName;
			 
				// remove "active" class from all links inside #nav
				$('#nav a').removeClass('active');
			 
				// add active class to the current link
				$(t).addClass('active');
			}
			<!-- Menu -->
            $(function() {
				/**
				* for each menu element, on mouseenter, 
				* we enlarge the image, and show both sdt_active span and 
				* sdt_wrap span. If the element has a sub menu (sdt_box),
				* then we slide it - if the element is the last one in the menu
				* we slide it to the left, otherwise to the right
				*/
                $('#sdt_menu > li').bind('mouseenter',function(){
					var $elem = $(this);
					$elem.find('img')
						 .stop(true)
						 .animate({
							'width':'170px',
							'height':'170px',
							'left':'0px'
						 },400,'easeOutBack')
						 .andSelf()
						 .find('.sdt_wrap')
					     .stop(true)
						 .animate({'top':'140px'},500,'easeOutBack')
						 .andSelf()
						 .find('.sdt_active')
					     .stop(true)
						 .animate({'height':'170px'},300,function(){
						var $sub_menu = $elem.find('.sdt_box');
						if($sub_menu.length){
							var left = '170px';
							if($elem.parent().children().length == $elem.index()+1)
								left = '-170px';
							$sub_menu.show().animate({'left':left},200);
						}	
					});
				}).bind('mouseleave',function(){
					var $elem = $(this);
					var $sub_menu = $elem.find('.sdt_box');
					if($sub_menu.length)
						$sub_menu.hide().css('left','0px');
					
					$elem.find('.sdt_active')
						 .stop(true)
						 .animate({'height':'0px'},300)
						 .andSelf().find('img')
						 .stop(true)
						 .animate({
							'width':'0px',
							'height':'0px',
							'left':'85px'},400)
						 .andSelf()
						 .find('.sdt_wrap')
						 .stop(true)
						 .animate({'top':'25px'},500);
				});
            });
        </script>        
</body>
