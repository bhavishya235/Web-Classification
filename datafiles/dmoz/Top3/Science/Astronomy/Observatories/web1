$(document).ready(function () {
	/* key message hover */
	var main_msg = "ESO, the European Southern Observatory, builds and operates a suite of the world\u0027s most advanced ground\u002Dbased astronomical telescopes";
	
	
	
	$("#elt").hover( function () { $("#main_message").text( "The European Extremely Large Telescope  the Worlds Biggest Eye on the Sky" ); }, function () { $("#main_message").text( main_msg ); } );
	
	
	$("#alma").hover( function () { $("#main_message").text( "ALMA  In search of our Cosmic Origins" ); }, function () { $("#main_message").text( main_msg ); } );
	
	
	$("#vlt").hover( function () { $("#main_message").text( "VLT  the worlds most advanced visible\u002Dlight astronomical observatory" ); }, function () { $("#main_message").text( main_msg ); } );
	
	
	$("#lasilla").hover( function () { $("#main_message").text( "La Silla  ESOs first observatory" ); }, function () { $("#main_message").text( main_msg ); } );
	
	
	/* pr rotator */
	$('.pr_items' ).cycle({ 
		fx:    'fade', 
		timeout: 5000,
		speed:  800,
		pause:  1,
		pager: '#numbers',
		containerResize: 0,
		height: '425px',
		fit: 1,
		width: '100%'
	});
	$('.pr_items').removeClass("init_hide"); // css hides all but the first PR. Now that cycle script is loaded, remove the class hiding them, so they will be displayed. 
});
</script>



