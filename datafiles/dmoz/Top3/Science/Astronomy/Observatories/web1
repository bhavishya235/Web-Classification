<script type="text/javascript">
function toggleMenuItem(element,action)
{
	document.getElementById(element).className = action;
}
function toggleMenu(menuBar)
{
	 document.getElementById('news').className = "hideMenu"
	 document.getElementById('membership').className = "hideMenu"
	 document.getElementById('governance').className = "hideMenu"
	 document.getElementById('events').className = "hideMenu"
	 document.getElementById('diversity').className = "hideMenu"
	 document.getElementById('resources').className = "hideMenu"
	 document.getElementById('about').className = "hideMenu"
	 
	 if (document.getElementById(menuBar) != null)
	 {
		 document.getElementById(menuBar).className = "showMenu"
	 }
}
function PrintThisPage() 
{ 
   var sOption="toolbar=yes,location=no,directories=yes,menubar=yes,"; 
         sOption+="scrollbars=yes,width=750,left=100,top=25"; 
   
   var winprint=window.open("/print.asp","Print",sOption);
  
	window.focus(winprint);   

}
function DetectBrowser()
{
	var isSafari = (navigator.userAgent.indexOf("Safari") != -1) ? true : false; 
	var isWindows = (navigator.userAgent.indexOf("Windows") != -1) ? true : false; 
	var isNotChrome = (navigator.userAgent.indexOf("Chrome") == -1) ? true : false;
	if (isSafari && isWindows && isNotChrome) document.getElementById('flashMovie').style.display = 'none'; 
	if (isSafari && isWindows && isNotChrome) document.getElementById('SafariWinPic').style.display = 'block';
//	if (isSafari && isWindows) document.getElementById('yourflashid').style.Zindex = '-1000';
}

function Pop(path)
{
	window.open (path,"Feedback","width=800,height=500,resizable,scrollbars,dependent,screenx=10,screeny=10,left=50,top=50");
}
function Clickheretoprint()
{ 
  var disp_setting="toolbar=yes,location=no,directories=yes,menubar=yes,"; 
      disp_setting+="scrollbars=yes,width=650, height=600, left=100, top=25"; 
  var content_vlue = document.getElementById("print_content").innerHTML; 
  
  var docprint=window.open("","",disp_setting); 
   docprint.document.open();
   docprint.document.write('<html><head><link rel=stylesheet href="/css/reset.css" type="text/css"><link rel=stylesheet href="/css/styles.css" type="text/css"><link rel=stylesheet href="/css/Menu.css" type="text/css"><link rel=stylesheet href="/css/print.css" type="text/css"><title></title></head>'); 
   docprint.document.write('<body class="printPageProperties" link="#800000" vlink="#800000" alink="#800000" onLoad="self.print()"><left>');
   docprint.document.write(content_vlue);
   docprint.document.write('</left></body></html>'); 
   docprint.document.close(); 
   docprint.focus(); 
}
</script>

