  <OPTION value="RASS-Cnt Broad">RASS 3 
  <OPTION value="Digitized Sky Survey" SELECTED>DSS 
  <OPTION value="IRAS 100 micron">IRAS 
  <OPTION value="0408MHz">0408MHz 
</SELECT>  
