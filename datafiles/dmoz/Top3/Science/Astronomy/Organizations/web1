          <div id="block-views-who_online-block_1" class="block block-views default who-online">
  <div class="block-inner">

          <h2 class="block-title">Who's Online</h2>
    
    <div class="block-content"><div class="view view-who-online view-id-who_online view-display-id-block_1 view-dom-id-5">
    
  
  
      <div class="view-content">
      <table class="views-view-grid">
  <tbody>
                <tr class="row-1 row-first">
                  <td class="col-1">
              
  <span class="views-field-picture">
                <div class="field-content">
