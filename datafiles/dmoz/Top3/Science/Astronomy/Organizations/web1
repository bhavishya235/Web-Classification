function are_cookies_enabled(){
    var cookieEnabled = (navigator.cookieEnabled) ? true : false;

    if (typeof navigator.cookieEnabled == "undefined" && !cookieEnabled)    { 
        document.cookie="testcookie";
        cookieEnabled = (document.cookie.indexOf("testcookie") != -1) ? true : false;
    }
    return (cookieEnabled);
}
        $(document).ready(function(){
            setTimeout(function() {
                var cookieValue = $.cookie("visited"); //read cookie
                if(cookieValue!="1" && are_cookies_enabled()==true){
                    // Triggering Popup when page load is complete
                    $('#popup').bPopup({
                        transition: "slideIn",
                        speed: 850,
                        easing: "easeOutBack",
                        content:'iframe', 
                        contentContainer:'.content',
                        loadUrl:'http://theplanetarysociety.org/modal/signup.php' //Uses jQuery.load()
                    });
                    $.cookie("visited", 1, {expires:365});//set cookie
                }
            }, 2000); // wait 2 seconds after page load.
        });
// ]]></script>
