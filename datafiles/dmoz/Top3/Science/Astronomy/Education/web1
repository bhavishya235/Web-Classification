<div id="Sun Data" class="hide">
<script language="JavaScript" type="text/javascript">
var results=new Array(8); for (i=0; i <8; i++) results[i]=new Array(10);
results[0][0]="8";results[1][0]="0";results[2][0]="1";results[3][0]="3";
results[4][0]="4";results[5][0]="5";results[6][0]="6";results[7][0]="7";
var now=new Date();var nowstring=now.toUTCString();
var Lat=53.4; var Long=-3;
var atlas=new Array(new place(" ",Lat,Long));
var observer=new observatory(atlas[0],2000,1,1,12,0,0,0);		
observer.year=now.getFullYear(); observer.month=now.getMonth()+1;
observer.day=now.getDate(); observer.hours=now.getHours();
observer.minutes=now.getMinutes(); observer.tz=now.getTimezoneOffset(); 
var TZ2=0-(observer.tz/60);
for (i=0; i <8; i++) {
  var times=risetPlan(observer,results[i][0]);
  results[i][1]=times[1];results[i][4]=times[2];results[i][7]=times[3];

  observer.hours=Math.floor(times[1]); 
  observer.minutes=(times[1]-observer.hours)*60;
  var position=posPlan(observer,results[i][0]);
  results[i][2]=Math.round(position[4]);

  observer.hours=Math.floor(times[2]); 
  observer.minutes=(times[2]-observer.hours)*60;
  var position=posPlan(observer,results[i][0]);
  results[i][5]=Math.round(position[3]);

  observer.hours=Math.floor(times[3]); 
  observer.minutes=(times[3]-observer.hours)*60;
  var position=posPlan(observer,results[i][0]);
  results[i][8]=Math.round(position[4]);

  results[0][3]="#494949";results[0][6]="#494949";results[0][9]="#494949";
  if (results[i][0]<8) {
    if (results[i][1]>results[0][1]  && results[i][1]<results[0][7]) {
      results[i][3]="#bb0000";
    } else {
      results[i][3]="#00bb00";
    }
    if (results[i][4]>results[0][1]  && results[i][4]<results[0][7]) {
      results[i][6]="#bb0000";
    } else {
      results[i][6]="#00bb00";
    }
    if (results[i][7]>results[0][1]  && results[i][7]<results[0][7]) {
      results[i][9]="#bb0000";
    } else {
      results[i][9]="#00bb00";
    }
  }
}

var suntimes=SunRiseSet(observer.year,observer.month,observer.day,Lat,Long);
results[0][1]=suntimes[0]+TZ2;results[0][7]=suntimes[1]+TZ2;

document.write("<center><table style=\"width:170px;text-align:center;\"><tbody style=\"border-top: none;\"><tr><td>Sunrise</td><td>Sunset</td></tr><tr><td>");
document.write("<font style=\"color:#bb0000\">"+hmstring(results[0][1])+"</td><td><font style=\"color:#bb0000\">"+hmstring(results[0][7])+"");
document.write("</td></tr></tbody></table></center>");
document.write("<p style=\"text-align:center;margin-top:2px;\">");
document.write("<img src=\"/sites/default/files/decor/sohonow.jpg\" width=120 height=120>");
document.write("<br/>Latest Sun Image");
document.write("<p style=\"margin-top:16px;text-align:center;\">");
document.write("<font style=\"color:#336699;font-size:10px;\">Liverpool : "+nowstring.substr(0,16)+"</font>");
document.write("</div>");

document.write("<div id=\"Moon Data\" class=\"show\">");
observer.hours=now.getHours();observer.minutes=now.getMinutes();
var phase=MoonPhase(observer.year,observer.month,observer.day,observer.hours);
var phaser=Math.abs(Math.round(phase)-360);
var phasetext=null; var phasepic=Math.round((phaser)/10);
if ((phaser < 5) || (phaser>355)){phasetext="Full Moon";}
if ((phaser>=5) && (phaser<=85)){phasetext="Waning Gibbous";}
if ((phaser>85) && (phaser<95)){phasetext="Last Quarter";}
if ((phaser>=95) && (phaser<=175)){phasetext="Waning Crescent";}
if ((phaser>175) && (phaser<185)){phasetext="New Moon";}
if ((phaser>=185) && (phaser<=265)){phasetext="Waxing Crescent";}
if ((phaser>265) && (phaser<275)){phasetext="First Quarter";}
if ((phaser>=275) && (phaser<=355)){phasetext="Waxing Gibbous";}
var times=MoonRise(observer.year,observer.month,observer.day,TZ2,Lat,Long);

document.write("<center><table style=\"width:170px;text-align:center;\">");
document.write("<tbody style=\"border-top:none;\"><tr>");
document.write("<td>Moonrise</td><td>Moonset</td></tr><tr><td>");
if (hmstring(times[0])=="-2:00 AM") {
  document.write("<font style=\"color:#bb0000\">--:--</td><td>");
  } else {
  document.write("<font style=\"color:#bb0000\">"+hmstring(times[0])+"</td><td>");
  }
if (hmstring(times[1])=="-1:00 AM") {
  document.write("<font style=\"color:#bb0000\">--:--");
  } else {
  document.write("<font style=\"color:#bb0000\">"+hmstring(times[1]));
  }
document.write("</td></tr></tbody></table>");
document.write("<p style=\"text-align:center;margin-top:0px;\">");
document.write("<img src=\"/sites/default/files/phases/phase_100_"+phasepic+".gif\" width=100 height=100>");
document.write("<p style=\"text-align:center;margin-top:-5px;\">");
document.write("<font style=\"color:#bb0000\">"+phasetext+"</font><br>");
document.write(+Math.round(100.0*(1.0+cosd(phase))/2.0)+"% of Full<br>");
document.write("<p style=\"text-align:center;margin-top:0px;\">");
document.write("<font style=\"color:#336699;font-size:10px;\">Liverpool : "+nowstring.substr(0,16)+"</font>");
document.write("</div>");

document.write("<div id=\"Planet Data\" class=\"hide\">");
document.write("<div id=\"slide\">");
document.write("  <div id=\"p1\">");
document.write("    <table><tbody style=\"border-top:none;\">");
document.write("    <tr><td>Rises</td><td>Az.</td></tr>");
for (i=0; i <6; i++) {
  document.write("    <tr><td><font style=\"color:"+results[i][3]+";\">"+hmstring(results[i][1])+"</font></td><td>"+results[i][2]+"&deg;</td></tr>");
}
document.write("    </tbody></table>");
document.write("  </div>");
document.write("  <div id=\"p2\">");
document.write("    <table><tbody style=\"border-top:none;\">");
document.write("    <tr><td>Transit</td><td>Alt.</td></tr>");
for (i=0; i <6; i++) {
  document.write("    <tr><td><font style=\"color:"+results[i][6]+";\">"+hmstring(results[i][4])+"</td><td>"+results[i][5]+"&deg;</td></tr>");
}
document.write("    </tbody></table>");
document.write("  </div>");
document.write("  <div id=\"p3\">");
document.write("    <table><tbody style=\"border-top:none;\">");
document.write("    <tr><td>Sets</td><td>Az.</td></tr>");
for (i=0; i <6; i++) {
  document.write("    <tr><td><font style=\"color:"+results[i][9]+";\">"+hmstring(results[i][7])+"</td><td>"+results[i][8]+"&deg;</td></tr>");
}
document.write("    </tbody></table>");
document.write("  </div>");
document.write("</div>");
document.write("<div id=\"previous\"><b>Previous</b></div>");
document.write("<div id=\"next\"><b>Next</b></div>");

document.write("<table style=\"line-height:1.5em; font-size:11px;margin-top:32px;margin-left:10px;text-align:left;color:#336699;\">");
document.write("<tbody style=\"border-top:none;\">");
document.write("    <tr><td></td></tr>");
document.write("    <tr><td>Sun</td></tr>");
document.write("    <tr><td>Mercury</td></tr>");
document.write("    <tr><td>Venus</td></tr>");
document.write("    <tr><td>Mars</td></tr>");
document.write("    <tr><td>Jupiter</td></tr>");
document.write("    <tr><td>Saturn</td></tr>");
document.write("</tbody></table>");
document.write("<p style=\"text-align:center;margin-top:0px;font-size:11px;\">");
document.write("<font style=\"color:#00bb00;\">");
document.write("Green events occur at night</font></p>");
document.write("<p style=\"text-align:center;margin-top:17px;font-size:11px;\">");
document.write(nowstring.substr(0,16)+"</p>");
document.write("</div>");
</script>

