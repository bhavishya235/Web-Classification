  #celest div.celon {color:#c00; padding:0 5px; margin-right:3px; margin-top:1px; float:left; background:url(/sites/default/files/decor/tabs_2.gif) repeat-x left bottom; border:1px solid #000; cursor:pointer; border-bottom:0; height:15px; line-height:14px; position:relative; z-index:100;}
