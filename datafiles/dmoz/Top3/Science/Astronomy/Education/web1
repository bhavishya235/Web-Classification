<script type="text/javascript">
onload = function() {
  var e, i = 0;
  while (e = document.getElementById('celest').getElementsByTagName ('DIV') [i++]) {
    if (e.className == 'celon' || e.className == 'celoff') {
    e.onclick = function () {
	var getEls = document.getElementsByTagName('DIV');
	for (var z=0; z<getEls.length; z++) {
	  getEls[z].className=getEls[z].className.replace('show', 'hide');
	  getEls[z].className=getEls[z].className.replace('celon', 'celoff');
	}
	this.className = 'celon';
	var max = this.getAttribute('title');
	document.getElementById(max).className = "show";
	}
    }
  }
}
</script>

<style type="text/css">
  #celest {font:11px verdana,arial,sans-serif; width:190px; padding:0 0 0 0; line-height:15px;}
