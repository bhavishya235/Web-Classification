            //Account popup
            var clearli;
            $(".accountLink").hover(function() {
                clearTimeout(clearli);
                $("ul#nav li").removeClass('current');
                $(this).addClass('current');
                }, function() {
                    var getthis = $(this);
                    clearli = setTimeout(function() {
                    $(getthis).removeClass('current');
                }, 100);
            });
            //Embed video
            $('a.ksPopVideo').popupVideo();
        });
