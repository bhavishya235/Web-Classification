<script type="text/javascript">

    $(document).ready(function() {

        
            AWcClearHDCookie();
         

        //check if allowchangepwd function is available
        if(typeof AllowChangePwd == 'function') 
            AllowChangePwd();
    });

    function GetHDCookieValue(userId, displayName, lastUpdatedInTicks, emailAddress) {
        return userId + "|" + displayName + "|" + lastUpdatedInTicks + "|" + emailAddress;
    }
    
    function AWcClearHDCookie() {
        var tmp = null;
        var hdDomain = 'www.aviationweek.com';
        var lastIndex = hdDomain.lastIndexOf('.');
        if (lastIndex != -1) {
            lastIndex = hdDomain.lastIndexOf('.', lastIndex - 1);
            tmp = hdDomain.substring(lastIndex, hdDomain.length);
        }

        if (tmp != null)
            hdDomain = tmp;
        var d = new Date();
        document.cookie = "hd=;path=/;domain=" + hdDomain + ";expires=" + d.toString() + ";";
    }
    
    jQuery.fn.center = function() {
        this.css("position", "absolute");
        this.css("top", (($(window).height() - this.outerHeight()) / 2) + $(window).scrollTop() + "px");
        this.css("left", (($(window).width() - this.outerWidth()) / 2) + $(window).scrollLeft() + "px");
        return this;
    }
    jQuery.fn.btnTRPosition = function(targetEle, zIndex) {
        var tEleLeft = parseInt($(targetEle).css("left"));
        var tEleTop = parseInt($(targetEle).css("top"));
        var thisTop = tEleTop + 15;
        var thisLeft = ($(targetEle).outerWidth() - 40) + tEleLeft;
        this.css('z-index', zIndex);
        this.css("position", "absolute");
        this.css("top", thisTop + 'px');
        this.css("left", thisLeft + 'px');
        return this;
    }

    function showloginpop(obj) {
        var lnkText = $(obj).html();
        if (lnkText == 'Login') {
            $('.loginBackground').show();
            $('.pnlLogin').prop("src", $('#hdnLoginUrl').val());
            $('.pnlLogin').css("position", "absolute");
            $('.pnlLogin').css("top", (($(window).height() - $('.pnlLogin').outerHeight()) / 2) + $(window).scrollTop() + "px");
            $('.pnlLogin').css("left", (($(window).width() - $('.pnlLogin').outerWidth()) / 2) + $(window).scrollLeft() + "px");
            $('.pnlLogin').css("z-index", '10001');
            //$('.pnlLogin').center();
            $('.pnlLogin').show();
            var tEleLeft = parseInt($('.pnlLogin').css("left"));
            var tEleTop = parseInt($('.pnlLogin').css("top"));
            var thisTop = tEleTop + 15;
            var thisLeft = ($('.pnlLogin').outerWidth() - 40) + tEleLeft;
            $('.lightboxClose').css('z-index', '10002');
            $('.lightboxClose').css("position", "absolute");
            $('.lightboxClose').css("top", thisTop + 'px');
            $('.lightboxClose').css("left", thisLeft + 'px');
            $('.lightboxClose').show();
            //$('.lightboxClose').btnTRPosition('.pnlLogin', '10001');
            return false;
        }

        AWcClearHDCookie();
        return true;
    }

    function showRegisterPop() {
        $('.modalBackground').show();
        $('.pnlFrame').prop("src", $('#hdnRegisterUrl').val());
        $('.pnlFrame').css("left", (($(window).width() - $('.pnlFrame').outerWidth()) / 2) + $(window).scrollLeft() + "px");
        $('.pnlFrame').show();

        $('.lightboxClose').show();
        $('.lightboxClose').btnTRPosition('.pnlLogin', '10001');
        return false;
    }
    
    function hidelogin_noreload() {
        $('.loginBackground').hide();
        $('.pnlLogin').hide();
        $('.lightboxClose').hide();
    }
    
    function hideloginpop() {
        $('.loginBackground').hide();
        $('.pnlLogin').hide();
        $('.lightboxClose').hide();
        
        //reload page
        var lnk = document.getElementById("dnn_dnnLOGIN_cmdLogin");
        if (lnk != null && $(lnk).text() == "Login") {
            //remove 'loginAction=true'
            
            window.location.href = window.location.href.replace("?loginaction=true", "").replace("?loginaction=true&", "?").replace("&loginaction=true", "");
            
            //window.location.reload();
        }
    }
</script>
