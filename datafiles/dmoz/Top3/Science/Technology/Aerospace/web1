<script type="text/javascript">

    var _tag;
    var s;

    $(function () {
        /**
        * Function to Initiate Webtrend tracking
        */
        function InitiateWebtrendsCall() {
            try {                
                WTTag();
                //Call Omniture tag : Changed the Omniture call for bugfix of both page view and tab selected.               
                trackingTagMod_initializeOmniture();
            }
            catch (exception) {
                if (window.console) {
                    window.console.log("Caught an exception\n" + exception.message);
                }   
            }
        }

        /**
        * Initiate webtrends and Omniture tracking
        */
        InitiateWebtrendsCall();

        /**
        * Initiate webtrends tracking for Error Messages
        */

        /**
        * Function to Initiate Error Webtrend tracking
        */
        function InitiateErrorWebtrendsCall() {
            try {
                var isAWIN = false;
                if (dnn_wt_site != undefined && dnn_wt_site.indexOf("aw_awin") > -1)
                    isAWIN = true;
                if ($('.dnnFormMessage.dnnFormValidationSummary') != undefined &&
                    $('.dnnFormMessage.dnnFormValidationSummary:visible') && isAWIN == true) {                    
                    
                    $('.dnnFormMessage.dnnFormValidationSummary:visible').each(function () {
                   
                        var errorMsg = ($(this).find(".dnnModMessageHeading:visible"))? $(this).find(".dnnModMessageHeading").text() : "";
                        errorMsg += ($(this).find("[id$='lblMessage']:visible")) ? $(this).find("[id$='lblMessage']").text() : "";  
                                          
                        TrackUriAndErrorMessage("AWIN Error Message", errorMsg);
                    });
                }
            }
            catch (exception) {
                _log("Caught an exception: " + exception.message);
            }
        }
        InitiateErrorWebtrendsCall();
    });    
    </script>
