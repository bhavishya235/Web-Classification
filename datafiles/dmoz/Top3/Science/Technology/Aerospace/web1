// chartbeat blocks the website in Safari 5/6.
// So, detect if browser is from Apple (which means Safari)
// and if it's one of the problem versions, don't run chartbeat.
(function () {
    var condition = (window.navigator && navigator.vendor
                     && navigator.vendor === 'Apple Computer, Inc.'
                     && navigator.appVersion
                     && navigator.appVersion.match('Version/5') !== null);

    if (condition) {
        return;
