function OAS_NORMAL(pos) {
    try {
        document.write('<A HREF="' + OAS_url + 'click_nx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + pos + '?' + OAS_query + '" TARGET=' + OAS_target + '>');
        document.write('<IMG SRC="' + OAS_url + 'adstream_nx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + pos + '?' + OAS_query + '" BORDER=0></A>');
    } catch (exception) {
        alert("Exception: " + exception.message);
        OASLog("(OAS_NORMAL) Caught an exception while dynamically loading the OAS script: " + exception.message);
    }
}

OAS_version = 11;
if (navigator.userAgent.indexOf('Mozilla/3') != -1 || navigator.userAgent.indexOf('Mozilla/4.0 WebTV') != -1)
    OAS_version = 10;
if (OAS_version >= 11) {
    try {
        var scriptFile = '<SCR' + 'IPT LANGUAGE=JavaScript1.1 SRC="' + OAS_url + 'adstream_mjx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '?' + OAS_query + '"><\/SCRIPT>';
        document.write('<SCR' + 'IPT LANGUAGE=JavaScript1.1 SRC="' + OAS_url + 'adstream_mjx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '?' + OAS_query + '"><\/SCRIPT>');
    } catch (exception) {
        alert("Exception: " + exception.message);
        OASLog("Caught an exception while dynamically loading the OAS script: " + exception.message);
    }
}

document.write('');
function OAS_AD(pos) {
    if (OAS_version >= 11)
        OAS_RICH(pos);
    else
        OAS_NORMAL(pos);
}

/* BACKGROUND ADS START*/
function OAS_BG() {
    var imgBG = $(".background_ads .oas_advertisement img").attr("src");

    if (imgBG) {
        var imgLink = $(".background_ads .oas_advertisement a").attr("href");
        imgLink = (imgLink) ? imgLink : "#";

        var mco = $('#main-content-wrap');
        var mcoPosition = mco.position();
        var mcoTop = parseInt(mcoPosition.top);
        var imgBGHeight = $(".background_ads .oas_advertisement img").height();
        var mcoHeight = mco.height() - imgBGHeight;

        var mcoWidth = mco.width();
        var documentWidth = $(document).width();
        var sideLinkWidth = parseInt((documentWidth - mcoWidth) / 2);

        var leftSideStyle = 'style = "overflow: hidden; display: block; position: absolute; left:0px; top: ' + mcoTop + 'px; height:' + mcoHeight + 'px; width: ' + sideLinkWidth + 'px;"';
        var rightSideStyle = 'style="overflow: hidden; display: block; position: absolute; right:0px; top: ' + mcoTop + 'px; height:' + mcoHeight + 'px; width: ' + sideLinkWidth + 'px;"';
        var traspImage = '<img src="/DesktopModules/AWeek.OAS/images/transparentBG_link.gif" width="' + sideLinkWidth + 'px" height="' + mcoHeight + 'px" style="border: 0">';
        var leftSideLink = '<a href="' + imgLink + '" ' + leftSideStyle + ' id="aLeftBG" target="_blank">' + traspImage + '</a>';
        var rightSideLink = '<a href="' + imgLink + '" ' + rightSideStyle + ' id="aRightBG" target="_blank">' + traspImage + '</a>';

        $(".background_ads .oas_advertisement").hide();
        if (navigator.appVersion.indexOf("MSIE 7.") != -1) {
            var bgADS = "<div class='backgroundADS' style='overflow: hidden;display: block; position: absolute;top: " + mcoTop + "px;height:" + imgBGHeight + "px;width:100%;z-index:0;'><IMG src='" + imgBG + "' alt='' style='margin-left:auto;margin-right:auto;height:" + imgBGHeight + "px;display:block;'/></div>";
            $('.main_content_wrap_bg').append(leftSideLink + rightSideLink);
            $('body').prepend(bgADS);
            $('#breadcrumbPane').attr("style", "z-index:1;position:relative;");
        } else {
            $('.main_content_wrap_bg').attr("style", "background-image: url(" + imgBG + ");");
            $('.main_content_wrap_bg').append(leftSideLink + rightSideLink);
        }

    }
}
$(function() {
    if ($(".background_ads .oas_advertisement img")) {
        OAS_BG();
    }
});
/* BACKGROUND ADS END*/</script>
