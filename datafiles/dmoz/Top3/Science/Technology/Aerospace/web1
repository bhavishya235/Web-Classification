            {{#if instagramPreview}}
                <a href="{{=postUrl}}" target="_blank">
                  <img src="{{=instagramPreview.instagramPreviewImage}}" class="instagramPreviewImage" alt="{{=feed}}" />
                </a>
                <!--<a href="{{=postUrl}}" target="_blank" class="instagramPreviewTitle">{{=instagramPreview.instagramPreviewTitle}}</a>-->
                <a href="{{=postUrl}}" target="_blank">
                  <span class="instagramPreviewDescription">{{=instagramPreview.instagramPreviewDescription}}</span>
                </a>
            {{/if}}

            {{#if flickrPreview}}
                <a href="{{=postUrl}}" target="_blank">
                  <img src="{{=flickrPreview.flickrPreviewImage}}" class="flickrPreviewImage" alt="{{=feed}}" />
                </a>
                <!--<a href="{{=postUrl}}" target="_blank" class="flickrPreviewTitle">{{=flickrPreview.flickrPreviewTitle}}</a>-->
                <a href="{{=postUrl}}" target="_blank">
                  <span class="flickrPreviewDescription">{{=flickrPreview.flickrPreviewDescription}}</span>
                </a>
            {{/if}}

            {{#if facebookPreview}}

            {{#else flickrPreview}}

            {{#else instagramPreview}}

            {{#else}}

                <!-- If neither facebookPreview nor flickrPreview nor instagramPreview -->
                <a href="{{=postUrl}}" target="_blank" class="socialAction">{{=actionCopy}}</a>

            {{/if}}

                </div>
            </div>
        </span>
    </script>



