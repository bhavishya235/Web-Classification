     // start the slideshow 
	    $ss.after('<div id="bannernav">').cycle({
		fx: 'fade',
		speed: 1000,
		timeout: 8000,
		pager:  '#bannernav',
		pause: 2,
		//before: function() { if (window.console) console.log(this.src); }
	});
		
