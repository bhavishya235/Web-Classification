        $(document).ready(function() {
            $('#dnn_ctr4891_NavView_hlnkSubNavNews').click(function() {
                $('#dnn_ctr4891_NavView_channelProductsPane').parent().hide();
                $('#dnn_ctr4891_NavView_channelBlogsPane').parent().hide();
                $('#dnn_ctr4891_NavView_channelSpecialTopicsPane').parent().hide();
                $('#dnn_ctr4891_NavView_channelDownloadsPane').parent().hide();
                $('#dnn_ctr4891_NavView_channelEventsPane').parent().hide();
                $('#dnn_ctr4891_NavView_channelPhotosPane').parent().hide();
                resetSubNavHeader(1);
                if (($('#dnn_ctr4891_NavView_hlnkSubNavNews').parent().css("background-color") == "transparent") ||
                ($('#dnn_ctr4891_NavView_hlnkSubNavNews').parent().css("background-color") == "rgba(0, 0, 0, 0)")) {
                    $('#dnn_ctr4891_NavView_hlnkSubNavNews').parent().attr("style",
                        "background-color:#fff;border:solid 1px #8f8f8f;border-bottom:0px;padding-top:6px;");
                    $('#dnn_ctr4891_NavView_hlnkSubNavNews').attr("style", "color:#408cff;background-position:right");
                }
                else {
                    $('#dnn_ctr4891_NavView_hlnkSubNavNews').parent().attr("style",
                        "background-color:transparent;border:solid 0px #8f8f8f;border-bottom:0px;padding-top:8px;");
                    $('#dnn_ctr4891_NavView_hlnkSubNavNews').attr("style", "color:#fff;background-position:right 7px");
