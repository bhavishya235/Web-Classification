        function resetSubNavHeader(navid) {
            if (navid != 1) {
                $('#dnn_ctr4891_NavView_hlnkSubNavNews').parent().attr("style",
                        "background-color:transparent;border:solid 0px #8f8f8f;border-bottom:0px;padding-top:8px;");
