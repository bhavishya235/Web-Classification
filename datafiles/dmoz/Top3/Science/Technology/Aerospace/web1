                $('#leftrailblock > :last-child').after($('#leftnav'));
                $( "#leftnav" ).accordion({ autoHeight: false });
                if ( $('#leftnav-ad-top').length ) {
                    $('#leftnav').before($('#leftnav-ad-top'));
