<script type="text/javascript">
    jQuery.fn.btnTRPosition = function(targetEle, zIndex) {
        var tEleLeft = parseInt($(targetEle).css("left"));
        var tEleTop = parseInt($(targetEle).css("top"));
        var thisTop = tEleTop + 15;
        var thisLeft = ($(targetEle).outerWidth() - 40) + tEleLeft;
        this.css('z-index', zIndex);
        this.css("position", "absolute");
        this.css("top", thisTop + 'px');
        this.css("left", thisLeft + 'px');
        return this;
    }
    function showpop(obj) {
        if ('False' == "True") {
            window.location.href = 'http://www.aviationweek.com/UserRegistration/UserRegistrationStep1/tabid/426/Default.aspx?returnurl=http%3a%2f%2fwww.aviationweek.com%2fHomePage.aspx';
            return false;
        }
        var lnkText = $(obj).html();
        if (lnkText != null) {
            $('.modalBackground').show();
            $('.pnlFrame').prop("src", $('#hdnRegisterUrl').val());
            $('.pnlFrame').css("left", (($(window).width() - $('.pnlFrame').outerWidth()) / 2) + $(window).scrollLeft() + "px");
            $('.pnlFrame').delay(1000).fadeIn(1);

            $('.popClose').delay(1000).fadeIn(1);
            var tEleLeft = parseInt($('.pnlFrame').css("left"));
            var tEleTop = parseInt($('.pnlFrame').css("top"));
            var thisTop = tEleTop + 15;
            var thisLeft = ($('.pnlFrame').outerWidth() - 40) + tEleLeft;
            $('.popClose').css('z-index', '10002');
            $('.popClose').css("position", "absolute");
            $('.popClose').css("top", thisTop + 'px');
            $('.popClose').css("left", thisLeft + 'px');

            //$('.popClose').btnTRPosition('.pnlFrame', '10001');
            return false;
        }        
        return true;
    }

    function hidepop() {
        $('.modalBackground').hide();
        $('.pnlFrame').hide();
        $('.popClose').hide();
        
        //reload page
        window.location.href = window.location.href.toLowerCase().replace("?registeraction=true", "").replace("?registeraction=true&", "?").replace("&registeraction=true", "");
        //window.location.reload();
    }
</script>

