            <div class="DnnModule DnnModule-AWeekMultiView DnnModule-7715"><a name="7715"></a>
<div class="blue-border-grad-bot">
	<h2 class="bbgb-heading">
        <span id="dnn_ctr7715_dnnTITLE_titleLabel" class="Head">Special Topics</span>



    </h2>
    
    <div id="dnn_ctr7715_ContentPane" class="bbgb-body"><!-- Start_Module_7715 --><div id="dnn_ctr7715_ModuleContent" class="DNNModuleContent ModAWeekMultiViewC">
	

<script type="text/javascript">

        var dnn_ctr7715_MultiView_ctl00_divSpecialTopicimageHeight =69,
            dnn_ctr7715_MultiView_ctl00_divSpecialTopicimageWidth =132,
            dnn_ctr7715_MultiView_ctl00_divSpecialTopicimageCount =12;



    $(document).ready(function() {
        //set ul width
        var currentModuleId = "#" + "dnn_ctr7715_MultiView_ctl00_divSpecialTopic";
        $contObj = $(currentModuleId);
        var computedWidth = ((parseInt(window[$contObj.attr('id') + 'imageWidth']) * parseInt(window[$contObj.attr('id') + 'imageCount'])) 
                                + (5 * parseInt(window[$contObj.attr('id') + 'imageCount'])));
        $contObj.find("ul").css("width", computedWidth);
        
        //fires on next btn click
        $(currentModuleId + ' .next').click(function() {      
            var left_indent = parseInt($contObj.find("ul").css('left')) - parseInt(window[$contObj.attr('id') + 'imageWidth']);
            $contObj.find("ul:not(:animated)").animate({ 'left': left_indent }, 500, function() {
                $contObj.find("ul li:last").after($contObj.find("ul li:first"));
                $contObj.find("ul").css({ 'left': '-139px' });
            });
        });
    });
</script>

<div id="dnn_ctr7715_MultiView_ctl00_divSpecialTopic" class="basicBlockInner specialTopics">
