<link rel="stylesheet" type="text/css" media="all" href="/public/js/fancybox/jquery.fancybox.css">
<script type="text/javascript" src="/public/js/fancybox/jquery.fancybox.js?v=2.0.6"></script>

<script>
		$(function(){
			
			$(".modalbox").click(function(){
				$(".modalbox").fancybox();
			}); 
			
			$("#newreport").show();
			$("#upcomingrep").hide();
			$("#inthenews").hide();
			$("#pressreleases").hide();
			
			$('#slides').slides({
				preload: true,
				preloadImage: '/public/images/loader.gif',
				play:100000000,
				pause: 2500,
				hoverPause: true,
				animationStart: function(current){
					$('.caption').animate({
						bottom:-35
					},100);
					if (window.console && console.log) {
						// example return of current slide number
						console.log('animationStart on slide: ', current);
					};
				}
			});
		});
		function showdiv(divid,divid1,divid2,divid3)
		{
			var path=""+'/public/images/tab_';
			var tab1=divid+'t';
			var tab2=divid1+'t';
			var tab3=divid2+'t';
			var tab4=divid3+'t';
			document.getElementById(divid).style.display='';
			document.getElementById(divid1).style.display='none';
			document.getElementById(divid2).style.display='none';
			document.getElementById(divid3).style.display='none';
			document[tab1].src=path+divid+'_h.gif';
			document[tab2].src=path+divid1+'.gif';
			document[tab3].src=path+divid2+'.gif';
			document[tab4].src=path+divid3+'.gif';
		}
	</script>
