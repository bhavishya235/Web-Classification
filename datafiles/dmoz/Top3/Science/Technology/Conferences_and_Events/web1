<script type="text/javascript">//<![CDATA[
chain.wait(function () {
	jQuery(function($) {

		var form = $('form#grid');

		$('ul.radio.list > li > input').change(function(event) {
			var input = $(event.currentTarget);
			var prefixes = {
				orderedby: 'Resize by: ',
				tagid: 'Show talks related to: '
			};
			var header = prefixes[input.attr('name')];
			$.GA_trackEvent('Homepage Filter Click', header + input.next('label').html(), '', undefined, true);

			form.submit();
		});

		form.bind('submit', function(e) {
			e.preventDefault();
			var query = form.serialize();
			theGridApp.loadTalks('/talks/browse.json?' + query);
			$('div.homeloading img').show();
		})

		$('div.homeloading img').ajaxStop(function() {
			$(this).hide();
		});
	});
});
chain.wait(function () {
	$(function() {
		var form = $('#surpriseForm');

		$('a.action').on('click', function(event) {
		  event.preventDefault();
		  form.submit();
		});

		$(".sf-selection.time, .sf-selection.rating").on('click', function(e) {
			var box = $('.popover_box.' + $(this).data('popover'));
			e.stopPropagation();
			// position popover box
			var h = box.height();
			var w = box.width();
			// where is the minutes box?
			var xpos = $(this).position().left + $(this).width()/2 -2;
			// center minutes popover according to this
			var popover_x = xpos - (w/2);
			var popover_y = h+16;
			box.siblings('.popover_box')
				.removeClass('open')
				.hide();
			box.css({"left":popover_x,"top":-popover_y})
				.addClass("open")
				.fadeIn(120);
		});

		$(".sm-selection a").on('click', function(event) {
			event.preventDefault();
			var value = $(this).data('value');
			if ($(this).closest(".popover_box").hasClass("time")) {
				$(".sf-selection.time").html(value);
				$("input[name=minutes]").val(value);
			} else if ($(this).closest(".popover_box").hasClass("rating")) {
				$(".sf-selection.rating").html($(this).html());
				$("input[name=ratingword_id]").val(value);
			}
			GridTile.prototype.isSkipNext = true;
			$(this).closest(".popover_box").fadeOut(200);
		})

		// if click anywhere onbody, close the surprise me popover
		$("body").on("click", function(e){
			$(".popover_box").removeClass("open").hide();
		});

					var loading_tpl  = "<p><img src='http://assets.tedcdn.com/images/ajax-loader.gif' height='16' width='16'> Creating your playlist ...</p>";
			var complete     = "<a class='watch button sm' style='display: inline-block' href='#'><span>Watch playlist</span></a><em>or <a class='restart' href='#'>start over</a></em>";
			var try_again    = "<a href='#'>Let's try again.</a>";
			var problem      = "There was a problem.";
			var pl_form_tpl  = '<form id="playlist_form" action="/playlists/surprise" method="GET"><div class="playlist-results clearfix"><input name="minutes" type="hidden" value="{minutes}" /><input name="ratingword_id" type="hidden" value="{rating_id}" /><ul></ul></div></form>';
			var item_tpl     = '<li data-id="{id}"><a href="#"><div><img src="{image_url}" alt="{title}" /></div><span class="talk-title">{title}<em> {duration}</em></span></a></li>';
			var summary_tpl  = "Your playlist<br /><span>{minutes} minutes of {rating_word} talks – just for you.</span>";

			$('#playlist_form a.restart, #playlist_form a.watch').live('click', function(e) {
				e.preventDefault();
				if ($(this).hasClass('restart')) {
					var talk_ids = $('#playlist_form li').map(function(i, v) {
						return $(v).data('id');
					});
					var values = get_values(form);
					$.GA_trackEvent('surprise generator',
						'start over',
						[values.minutes, values.ratingword_id, talk_ids.get().join('-')].join('|')
					);

					form.show();
					$('p.summary').fadeOut(200, function() {
						$('#playlist_form').slideUp(250, 'easeInOutQuart', function() {
							$(this).remove();
						});
						$(this).html("Alright, let's go again:").fadeIn(250);
					});
				} else {
					$(this).closest('form').submit();
				}
			});

			var loading = $(loading_tpl).appendTo($(".sf-container"))
				.css({'position': 'absolute', 'top': '7px'})
				.hide();

			form.hide = function() {
				var width = form.innerWidth();

				$(".sf-container").css({'overflow':'hidden'});

				return form
					.animate({ 'left': '-=' + width }, 400, 'easeInOutQuad')
					.promise()
					.done(function () {
						loading.fadeIn("fast");
					});
			}

			form.show = function() {
				form.css('left', 0);
				loading.stop(true).hide();
				$(".sf-container").css({'overflow':'visible'});
			}

			form.on('submit', function(e) {
				var values = get_values(form);
				$.GA_trackEvent('surprise generator',
					'surprise me',
					[values.minutes, values.ratingword_id].join('|')
				);

				form.stop(true).hide().promise().done(function () {
					$.post(form.attr('action'), form.serializeArray())
						.complete(function () {
							loading.fadeOut('fast');
						})
						.success(function(data) {
							var playlist_form = $($.nano(pl_form_tpl, data));

							$.each(data.items, function(index, item) {
								playlist_form.find('ul').append($.nano(item_tpl, item));
							});

							// playlist_form.children(':last').after(complete);
							playlist_form.find('ul').children(':last').after('<li class=\'sf-action\'>'+complete+'</li>')

							$('p.summary').fadeOut('slow', function() {
								$(this).html($.nano(summary_tpl, data))
									.fadeIn('slow');
							});

							var width = $(".sf-container").outerWidth(true);

							playlist_form
								.appendTo($('.sf-container'))
								.css({
									'position': 'relative',
									'left': width
								})
								.animate({
									'left': '-=' + width
								}, 450, 'easeInOutQuart');
						})
						.error(function(error) {
							$(try_again).on('click', function(e) {
								e.preventDefault();
								$(this).hide();
								form.show();
								$('p.summary').html("Alright, let's go again:");
							}).insertAfter($('p.summary').html(problem));
						});
				});

				return false;
			});

			function get_values(_form) {
				var values = {};
				$.each(_form.serializeArray(), function(i, v) {
					values[v.name] = v.value;
				});
				return values;
			}
			});
});
//]]></script>
