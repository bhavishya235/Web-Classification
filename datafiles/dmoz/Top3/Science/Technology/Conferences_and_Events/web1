    <script type="text/javascript">

    //Search Modal Code
    $(document).ready(function() {

        // ajax for getting dropdown category
        // added by deepak
        if(!("console" in window) || !("firebug" in console)){
            var names = [
                "log", "debug", "info", "warn", "error", "assert", "dir",
                "dirxml", "group", "groupEnd", "time", "timeEnd", "count", "trace",
                "profile", "profileEnd"
            ];
            window.console = {};
            for(var i = 0; i< names.length; ++i) window.console[names[i]] = function() {};
        }
        $.ajax({
            url: "/default/index/catdropdown",
            success: function(msg){
                $('#categoryList').html(msg);
            }
        });

        // check if upcoming checkbox is checked then disable publish date
        // added by deepak
        $("input[name='upcoming']").click(function(){
            if($('#upcoming').is(':checked')){
                $("#fromdate").val('');
                $("#todate").val('');
                $("#upcoming_div").show();
                $("#publish_div").hide();
            }else{
                $("#upcoming_div").hide();
                $("#publish_div").show();
                $("#upcoming_fromdate").val('');
                $("#upcoming_todate").val('');
            }
        });
    });

    /* This function is for Advance Search Popup*/
    function advsearch_formpopup(){
        var id = "#adv_dialog1";
        //$("#mask").show('slow');
        //Get the screen height and width
        var maskHeight = $(document).height();
        var maskWidth = $(window).width();

        //Set heigth and width to mask to fill up the whole screen
        $('#adv_mask').css({'width':maskWidth,'height':maskHeight});

        //transition effect
        $('#adv_mask').fadeIn(500);
        $('#adv_mask').fadeTo("slow",0.8);

        //Get the window height and width
        var winH = $(window).height();
        var winW = $(window).width();

        //Set the popup window to center
        $(id).css('top',  148.5);
        $(id).css('left', winW/2-$(id).width()/2);

        //transition effect
        $(id).fadeIn(1000);
        $("#search_keyword").focus();
        //  openAdvanceSearch();
    }
    $(document).ready(function() {
        $("#search_keyword").focus(function()
        {
            $("#search_keyword").addClass("inputfld_blue");
            $("#search_keyword").removeClass("inputfld2");
        });
        $("#search_keyword").blur(function()
        {
            $("#search_keyword").removeClass("inputfld_blue");
            $("#search_keyword").addClass("inputfld2");
        });


        $("input[name='report']").click(function(){
            if($('#report').is(':checked')){
                $('#report_chapter').show();
                $("#topic_title").attr('checked', true);

            }else{
                $('#report_chapter').hide();
                $("#topic_title").removeAttr('checked');
                $("#topic_content").removeAttr('checked');

            }
        });


        $(".collapsible").hide();


        $("#fancyboxclose").click(function() {
            $("#adv_dialog1").fadeOut('slow');
            $("#adv_mask").fadeOut('slow');
            $(".collapsible").hide();
        });

        // close the modal when the user presses ESC
        $('body').keypress(function(e){

            if(e.keyCode == 27){
                $("#adv_dialog1").fadeOut('slow');
                $("#adv_mask").fadeOut('slow');
                $(".collapsible").hide();
            }
        });

    });


    function searchPost(){
        /****Added by Deepak for tracking error been thrown when none of the checkboxes are selected in Advance Search. Feb 02'12 ****/

        var chk=0;
        $("input[type=checkbox][checked]").each(
        function() {
            chk=chk+1;
        }
    );
        if(chk==0)
        {
            $("input[type=checkbox][name=all]").attr('checked', true);
        }

    }
</script>
