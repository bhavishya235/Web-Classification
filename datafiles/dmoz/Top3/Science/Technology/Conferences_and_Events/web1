<script>
$(document).ready(function(){
    if(navigator.appName=="Microsoft Internet Explorer"){
        $("#follow").css("left",($(document).width()-140)+"px");
        $("#assist_message").css("left",($(document).width()-260)+"px");
    }else{
        $("#follow").css("left",($(document).width()-132)+"px");
        $("#assist_message").css("left",($(document).width()-244)+"px");
    }
     setTimeout(function(){
       $("#follow").css("bottom","0px");
       $("#follow").show();
       $("#followanchor").html("Contact us -");
       followus();
    },5000);

});

function followus(){
	$("#na").text("");$("#em").text("");$("#ph").text("");$("#me").text("");$("#res").text("");$("#comp").text("")
	
     if( $("#followanchor").html() == 'Contact us -')
     {
         $("#removeMayI").css("bottom","20px"); //
		 $("#assist_message").hide();
         $("#followanchor").html("Contact us +");
         $("#assist_message").css("display","none");
         $("#follow").css("bottom","0px");
         $('#assistfrm')[0].reset();
		 $("#removeMayI").css("bottom","20px;"); //
     }
     else
     {
          $("#removeMayI").css("bottom",$("#assist_message").height()+26); //
		 $("#assist_message").show();
         $("#assist_message").css("display","");
         $("#followanchor").html("Contact us -");
         if(navigator.appName=="Microsoft Internet Explorer")
         {
            //$("#follow").css("bottom","238px");
            $("#follow").css("bottom",$("#assist_message").height()+7);
     	 }
     	 else
     	 {
            //$("#follow").css("bottom","241px");
            $("#follow").css("bottom",$("#assist_message").height()+7);
     	 }
     }
}


function addMessage(){
        $("#follow").hide();
		$("#mayiassist").val("Sending...");
	var url = '/index/addmessage'; 
	$("#na").text("");
        $("#em").text("");
        $("#ph").text("");
        $("#me").text("");
        $("#res").text("");
        $("#comp").text("");
        $("#follow").addClass("help_button");
        $("#follow").addClass("green");
	$.ajax({
		url: url,
		type: "POST",
		data: $("#assistfrm").serialize(),
		success: function(msg){
						$("#mayiassist").val("Send");
                        var response = eval('('+msg+')');  //parsing json

                        if(response.error_name=="No")
                            $("#na").html('<div class = "assist_popup_error">Name can not be blank!</div>');

                        if(response.error_mail=="No")
                              $("#em").html('<div class = "assist_popup_error">Please enter valid email!</div>');

                        if(response.error_phone_blank=="No")
                              $("#ph").html('<div class = "assist_popup_error">Phone can not be blank!</div>');
                        else if(response.error_phone_type=="No")
                              $("#ph").html('<div class = "assist_popup_error">Phone should be Numeric!</div>');

                        if(response.error_message=="No")
                            $("#me").html('<div class = "assist_popup_error">Message can not be blank!</div>');
                        
                        if(response.error_company=="No")
                            $("#comp").html('<div class = "assist_popup_error">Company can not be blank!</div>');

                       if(response.error_duplicate=="No")
			{
                            $('#res').html('<div style = "text-align:center; color:red;font-weight:bold ">Detail already exists!</div>');
                            $("#follow").css("bottom",$("#assist_message").height()+17);
                            $("#usrName").val('');  $("#usrEmail").val(''); $("#usrPhone").val(''); $("#usrMessage").val('');$("#usrCompany").val('');
			}
                        if(response.error_ip=="No")
                        {
                                $('#res').html('<div style = "text-align:center; color:red;">You have exceeded the limit of the day</div>');
                                $("#follow").css("bottom",$("#assist_message").height()+17);
                                $("#usrName").val('');  $("#usrEmail").val(''); $("#usrPhone").val(''); $("#usrMessage").val('');$("#usrCompany").val('');
                        }         
                        $("#follow").css("bottom",$("#assist_message").height()+7);	
                        $("#removeMayI").css("bottom",$("#assist_message").height()+26);    
                        if(response.success=="Yes")
                        {
                                $('#res').html('<div style = "text-align:center; color:green;font-weight:bold ">We will contact you soon</div>');
                                $("#follow").css("bottom",$("#assist_message").height()+7);
								$("#removeMayI").css("bottom",$("#assist_message").height()+26);
                                $("#usrName").val('');  $("#usrEmail").val(''); $("#usrPhone").val(''); $("#usrMessage").val('');$("#usrCompany").val('');		
                        }
                      }
	  				
       		});
                $("#follow").fadeIn(3000);
}
function removethisoption(){
    /*var conf = confirm("Are you sure you want to close this form?\nThis will reappear when you open the browser again.");
    if(conf){*/
        $("#assistfrm").remove();
        $.ajax({
        url: "/index/remove-may-i-assist",
        type: "POST",
        success: function(data){
            
        }
    });
      
   //}
  }
</script>
