<script type="text/javascript">

	// Set current page path
	var thispage = window.location.pathname;
	
	// Initialize Main Menu
	if ($mysjquery('.submenu', '#navMain').length) {
		$mysjquery('#navMain').mysDropDownMenu();
	}
	
	// Highlight Current Section
	$mysjquery('a', '#navMain').each(function() {
		if ($mysjquery(this).attr('href') == thispage) {
			$mysjquery(this).closest('li.navMainLi').addClass('selected');
		}
	});
</script>

