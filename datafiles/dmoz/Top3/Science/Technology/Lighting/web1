<script type="text/javascript">
                var timerStoerer;
                function animateStoerer(){
                               $('#blockStoerer').animate({
                                               right: '0px',
                                               top: '0'                                 
                               }, 500, function() {
                                               startTimerStoerer('6000');
                               });
                }
                $('#blockStoerer').hover( function () {
                               clearTimeout(timerStoerer);
                  }, function () {
                    startTimerStoerer('5000');
                  }
                );
                
                setTimeout(animateStoerer, 1000);
                
                function startTimerStoerer(delay){
                               timerStoerer = setTimeout(function(){
                                               $('#blockStoerer').animate({
                                                               right: '-330px',
                                                               top: '-80px'
                                               }, 500);
                               }, delay);
                }
                
                $('#closeStoerer').click( function(){
                               clearTimeout(timerStoerer);
                               $('#blockStoerer').animate({
                                               right: '-330px',
                                               top: '-80px'
                               }, 500);
                });

</script>
