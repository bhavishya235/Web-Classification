function Toggle(item) {
//	alert ("in toggle " + item );
   obj=document.getElementById(item);
   visible=(obj.style.display!="none")
   key=document.getElementById("x" + item);
   if (visible) {
     obj.style.display="none";
      key.innerHTML="<img src='/global/img/plusSign.gif' width='11' height='11' hspace='0' vspace='0' border='0' align='absmiddle'>";
   } else {
      obj.style.display="block";
     key.innerHTML="<img src='/global/img/minusSign.gif' width='11' height='11' hspace='0' vspace='0' border='0' align='absmiddle'>";
   }
}

