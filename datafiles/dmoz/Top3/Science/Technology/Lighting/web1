<script type="text/javascript">
	$(document).ready(function(){
		$("#block-views-artikel-themenbuehne-block-1 li").hover(function(){
			$("#block-views-artikel-themenbuehne-block li.active, #block-views-artikel-themenbuehne-block-1 li.active").removeClass("active");
			var hoveredElement = $(this).data("themeid");
			$("li[data-themeid=" + hoveredElement + "]").addClass("active");
		});
	});
</script>
<script type="text/javascript">
	$(document).ready(function(){
		$("#block-views-artikel-themenbuehne-block-3 li").hover(function(){
