	<script type="text/javascript">
	$(document).ready(function(){	
        //When mouse rolls over
        $("#login").bind('mouseover mouseenter focusin',function(){
            $("#logo").stop().delay(500)
            .animate({top:'-64px'},
            {queue:false, duration:600, easing: 'swing'})
			
			$("#login").stop()
            .animate({top:'89px'},
            {queue:false, duration:600, easing: 'easeOutBounce'})
        });
		
		$("input.error").focus();

        //When mouse is removed
        $("#login").bind('mouseout mouseleave focusout',function(){
			if (($("#edit-name").is(":focus")) || ($("#edit-pass").is(":focus"))) {					
			} else {
				$("#logo").stop().animate({top:'-20px'},
					{queue:false, duration:600, easing: 'easeOutBounce'})
			
					$("#login").stop().animate({top:'40px'},
					{queue:false, duration:600, easing: 'swing'})
			}
        });
	});
	</script>
