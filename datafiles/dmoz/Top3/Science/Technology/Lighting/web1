	.om-forum #auswahl_status	{
		background-color: #006ab0;
	}

	.om-forum h1, .om-forum h2, .om-forum h3, .om-forum h4, .om-forum h5,
	.om-forum .benutzerstatus {
		color: #006ab0;
	}
	
	.jm-forum header.main section.main .picture-shadow,
	.jm-forum header.main,
	.jm-forum header.main section.main .seperator,
	.jm-forum header.main section.main .seperator div,
	.jm-forum header.main article.login,
	.jm-forum #auswahl_status	{
		background-color: #bbce00;
	}

	.jm-forum h1, .jm-forum h2, .jm-forum h3, .jm-forum h4, .jm-forum h5,
	.jm-forum .benutzerstatus {
		color: #bbce00;
	}
	
	header.main section.main .picture-shadow,
	header.main,
	header.main section.main .seperator,
	header.main section.main .seperator div,
	header.main article.login, #auswahl_status {
		background-color: #A7A7A7;
	}
	
	header.main section.main .picture {
		background-image: url("../../../sites/all/themes/vpltemplate/site/img/header/junior.jpg");
	}
	
		</style>
</head>

