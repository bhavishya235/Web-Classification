<script type="text/javascript">

/************* DO NOT ALTER ANYTHING BELOW THIS LINE !!**************/
//if AG_LOCALE is not present then metrics will be submitted after overlay comes

var country = document.getElementById("country");
var country_flyout = document.getElementById("country_flyout");
if( country == null || (country != null && country_flyout == null ) || (!cookieEnabled || getCookie("AG_LOCALE").length!=0)){
	var s_code=s.t();if(s_code)document.write(s_code);
}

</script>

							
						
						


<!-- Custom Variable Tag -->
<script type="text/javascript" charset="UTF-8">
if(typeof custom_var != "undefined") {
	if(typeof s.pageName != "undefined"){
    if(typeof jsAgilentProductLine == "undefined"){
    custom_var=s.pageName.replace(/\|/g, "_") + '|' + s.prop15 + '|' + s.prop16 + '|' + 'null';
    }else{
      custom_var=s.pageName.replace(/\|/g, "_") + '|' + s.prop15 + '|' + s.prop16 + '|' + jsAgilentProductLine;
    }
	}
  
	if(typeof s.products == "undefined"){
		custom_var = custom_var + '|' + 'null';
	}else{
		custom_var = custom_var + '|' + s.products;
	}
	custom_var=escape(custom_var);
} else {
	custom_var = 'null';
}
	</script>

					</div>
				</div>
