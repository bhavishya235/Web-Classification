MM_reloadPage(true);

function inArray(e, str) {
  ret = false;
  for (j =0; j < e.length; j++) {
    if (e[j] == str) {
      ret = true;
      break;
    }
  }
  return ret;
}

function getRandom(max) {
  return Math.round(Math.random()*max);
}

// LISTE DES IMAGES RANDOM
var sources = new Array('im_home110x110_1.jpg', 
                        'im_01.gif',
                        'si_110.jpg', 
                        'im_home110x110_2.jpg',
                        'im_02.gif',
                        'im_metrologia_110.gif',
                        'comites.jpg', 
                        'im_home110x110_3.jpg',
                        'im_03.gif',
                        'big_g_100.jpg',
                        'image1.gif',
                        'im_06.gif');
 

var img;
var images = new Array();
var sel    = new Array();
var imgDisplayed = 3;
for (i = 0; i < imgDisplayed; i++){
  img = getRandom(sources.length-1);
  while (inArray(sel, img)) {
    img = getRandom(sources.length-1);
  }
  sel[i]    = img;
  images[i] = '<img src="/utils/common/img2/'+sources[img]+'" width="110" height="110" alt="BIPM image">';
}

  //-->
