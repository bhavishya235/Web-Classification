      function playFeatureRotation()
      {
        $("#rotationControl").tabs('rotate', '7000', true);
        $("#rotationControl").find('a.pause').css('display', 'block');
        $("#rotationControl").find('a.play').css('display', 'none');
      }

      function pauseFeatureRotation()
      {
        $("#rotationControl").tabs('abort');
        $("#rotationControl").tabs('rotate', null, true);
        $("#rotationControl").find('a.pause').css('display', 'none');
        $("#rotationControl").find('a.play').css('display', 'block');
      }
