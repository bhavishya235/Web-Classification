        function doTranslate(lang_pair) {if(lang_pair.value)lang_pair=lang_pair.value;if(lang_pair=='')return;if(location.hostname=='epic.uncc.edu' && lang_pair=='en|en')return;var lang=lang_pair.split('|')[1];if(location.hostname!='epic.uncc.edu' && lang_pair=='en|en')location.href=unescape(gfg('u'));else if(location.hostname=='epic.uncc.edu' && lang_pair!='en|en')location.href='http://translate.google.com/translate?client=tmpg&hl=en&langpair='+lang_pair+'&u='+escape(location.href);else location.href='http://translate.google.com/translate?client=tmpg&hl=en&langpair='+lang_pair+'&u='+unescape(gfg('u'));}
