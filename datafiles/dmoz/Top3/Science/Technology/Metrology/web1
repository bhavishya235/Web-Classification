	function checkSearch() {
		var minLength = 3; // Minimum length
		var field1 = document.searchBox.searchterm.value;

		if ((document.searchBox.searchterm.value.length < minLength) && (field1 !== '')) {
			alert("Mindestens 3 Zeichen eingeben!");
			return false;
		}
	}
	</script>
	<form name="searchBox" onSubmit="return checkSearch()" action="/metasweb;internal&action=search" method="post">
