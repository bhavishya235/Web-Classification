  // Code to make sure that the __utmc cookie is re-instated if the __utmb cookie still exists.
  // This overrides the "new session on browser close" behaviour of Google Analytics
  var cookiePath = '/'; // should be set to '/' or whatever the GA cookie path is
  var cookieDomain = '.bis.gov.uk'; // should be set to the domain that the GA cookies would use
  var utmc = 0;
  var utmb = 0;
  try {
    utmc = (/__utmc=/.test(document.cookie) ? document.cookie.match(/__utmc=([^;]*)/)[1] : 0);
  } catch(err) {}
  if (!utmc) {
    try {
      utmb = (/__utmb=/.test(document.cookie) ? document.cookie.match(/__utmb=([^;]*)/)[1] : 0);
    } catch(err) {}
    if (utmb) {
      var domainHash = utmb.split('.')[0];
      document.cookie = '__utmc=' + escape(domainHash) + '; path=' + cookiePath + '; domain=' + cookieDomain;
