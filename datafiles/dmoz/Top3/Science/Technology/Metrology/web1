<TITLE>The ITS-90 Resource Site, Temperature and Scales</TITLE>
<SCRIPT language="JavaScript"><!--
browserName = navigator.appName;
browserVer = parseInt ( navigator.appVersion );

version = "n2";
if ( browserName == "Netscape" && browserVer >= 3 ) version = "n3";
if ( browserName == "Microsoft Internet Explorer" && browserVer >=4 ) version = "e4";

if ( version == "n3" || version == "e4" )
{

        light_on = new Image ( 27, 23 );
        light_on.src = "images/butt-on.gif";
        light_off = new Image ( 27, 23 );
        light_off.src = "images/butt-off.gif";
}

function button_on ( imgName )
{
        if ( version == "n3" || version == "e4" )
        {
                document [imgName].src = light_on.src;
        }
}

function button_off ( imgName )
{
        if ( version == "n3" || version == "e4" )
        {
                document [imgName].src = light_off.src;
        }
}

function email_test() 
  {
  email_in=document.email_form.email.value;
  var dot=email_in.indexOf(".")
  var at=email_in.indexOf("@")
  if (dot==-1 || at==-1)
  { 
  alert("Invalid Email Address, please correct");
   return false;
  }
  return true
  }

// -->
</SCRIPT>
</HEAD>
