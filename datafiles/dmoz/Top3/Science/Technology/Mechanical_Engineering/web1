				function swaptab(obj)
				{
				if(obj.id=='header1')
			{	
				document.getElementById('content2').style.display='none';document.getElementById('content1').style.display='block'
				obj.className="newsHyper";
				document.getElementById('header1').className='eventsHyper';
				document.getElementById('Nspan1').className='TabLeft';
				document.getElementById('Nspan2').className='TabCenter';
				document.getElementById('Nspan3').className='TabRight';
				document.getElementById('header2').className='eventsHyper';
				document.getElementById('EVspan1').className='TabLeftActive';
				document.getElementById('EVspan2').className='TabCenterActive';
				document.getElementById('EVspan3').className='TabRightActive';
				
			}
		else
			{	document.getElementById('content1').style.display='none';document.getElementById('content2').style.display='block'
							obj.className="newsHyper";
						document.getElementById('header1').className='eventsHyper';
						document.getElementById('Nspan1').className='TabLeftActive';
						document.getElementById('Nspan2').className='TabCenterActive';
						document.getElementById('Nspan3').className='TabRightActive';
						document.getElementById('EVspan1').className='TabLeft';
						document.getElementById('EVspan2').className='TabCenter';
						document.getElementById('EVspan3').className='TabRight';
		

			}
				}
