.tabs-widget li a:hover,.tabs-widget li a.tabs-widget-current{background:url(http://1.bp.blogspot.com/-O8DVAutRyKk/T1h_HVFvUDI/AAAAAAAADgg/VeyFKw1SO3M/s000/tabs-bg.png) left -128px repeat-x;color:#fff;text-shadow:0px 1px 0px #000;text-decoration:none}
.tabs-widget-content{}
