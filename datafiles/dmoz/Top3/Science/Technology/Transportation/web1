<script type="text/javascript">

var packcount = 0;
var totalpacks;

$(document).ready(function(){
	$(".moves").hide();
	$(".pack").show();
	$(".moves").fadeOut('fast',function(){
		$(".moves").css("top","0px");
		$(".moves").fadeIn();
		setTimeout("moveIt()",4000);
	});
	totalpacks = $(".moves .pack").length;
});

function moveIt(){
	if(packcount<totalpacks-1){
		$(".moves").animate({"top":"-=112px"});
		setTimeout("moveIt()",4000);
		packcount+=1;
	} else{
		$(".moves").fadeOut('fast',function(){
			$(".moves").css("top","0px");
			$(".moves").fadeIn();
			setTimeout("moveIt()",4000);
		});
		packcount=0;
	}
}
</script>
