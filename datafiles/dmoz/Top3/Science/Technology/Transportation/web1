	function searchday()
	{
		
		var currentTime = new Date();
		var oldTime = currentTime - 60 *1000 *60*24*7*2;
              var oldDate = new Date(oldTime);
		var month1 = currentTime.getMonth()+1; 
		var date1 = currentTime.getDate();	 
	       var year1 = currentTime.getFullYear(); 
	       var month = oldDate.getMonth()+1;
		if (month< 10) 
		     month = "0" + month; 
		var date  = oldDate.getDate();
		if (date< 10)
			date = "0" + date; 
		var year = oldDate.getFullYear();             
		var dateString_cur = year1 + "" + month1 + "" + date1;
	       var dateString = year + "" + month + "" + date;
  		var dislink= document.getElementById('displaylink');
		<!-- dislink.href = "http://ntlsearch.bts.gov/tris/latestsearch.do?b1=1&f1=0&t1=mde%3A%5B"+dateString+"+TO+"+dateString_cur+"%5D&r=&d=dc+OR+po&p=0&z=50&s=mte%2Cf%2Cr&o=&new=y&f7=st&b7=1&t7=Approved"; -->
		<!-- alert(dislink.href); -->
              <!-- dislink.href = "http://ntlsearch.bts.gov/tris/search.do?new=&d=tr&b1=1&t1=&f1=kw&b2=1&t2=&f2=kw&b3=1&t3=&f3=kw&Submit=Search&f12=yr&b12=1&t12="+dateString+"-"+dateString_cur+"&f16=ft&b16=1&s=yr&o=1&z=10"; -->

       }
</script>
