                <td width="600" align="left" valign="top"><div align="center">
                  <script type="text/javascript">
AC_FL_RunContent( 'codebase','http://download.macromedia.com/pub/shockwave/cabs/flash/swflash.cab#version=6,0,29,0','width','318','height','182','src','IMAGES/web_md','quality','high','pluginspage','http://www.macromedia.com/go/getflashplayer','movie','IMAGES/web_md' ); //end AC code
