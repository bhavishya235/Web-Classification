<script type="text/javascript">
	function ntlSearch()
	{
   		var dbs1 = document.getElementById('dbselect1');
		var dbs2 = document.getElementById('dbselect2');
 		if (dbs1.checked){   		
   			document.srhFormIS.t1.value = document.srhFormMain.searchText.value;
   			document.srhFormIS.submit();
   		}
   		else if (dbs2.checked){
    		      document.srhFormHQ.txtSearch.value = document.srhFormMain.searchText.value;
   		      document.srhFormHQ.submit();  		
   		}
		else { 
			document.srhFormFAQ.p_search_text.value = document.srhFormMain.searchText.value;
   			document.srhFormFAQ.submit();
			
		}
   		
   		return false;
   }
</script>
