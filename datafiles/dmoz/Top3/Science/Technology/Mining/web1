		                 <script type="text/javascript">
$(document).ready(function() {
                      
                               $("a[rel^='prettyPhoto']").prettyPhoto();
                       
               
                /////////////////////////////////////////
               $("#rotator-links a:first").addClass("active");

               var imageWidth = $(".box").width();
               var imageSum = $("#rotator_content div").size();
               var imageReelWidth = imageWidth * imageSum;

               $("#rotator_content").css({'width' : imageReelWidth});

               rotate = function(){
                       var triggerID = $active.attr("rel") - 1;
                       var image_reelPosition = triggerID * imageWidth;
                       $("#rotator-links a").removeClass('active');
                       $active.addClass('active');
                       $("#rotator_content ").animate({
                               left: -image_reelPosition
                       }, 500 );
               };

               rotateSwitch = function(){
                       play = setInterval(function(){
                               $active = $('#rotator-links a.active').next();
                               if ( $active.length === 0) {
                                      $active = $('#rotator-links a:first');
                               }
                               rotate();
                       }, 5000);
               };

               rotateSwitch();

               $("#rotator_content .box").hover(function() {
                       clearInterval(play);
               }, function() {
                       rotateSwitch();
               });

               $("#rotator-links a").click(function() {
                       $active = $(this);
                       clearInterval(play);
                       rotate();
                       rotateSwitch();
                       return false;
                       });
/////////////////////////////////////////////////////
               $("#rotator-links-video a:first").addClass("active");

               var imageWidthVideo = $(".box-video").width();
               var imageSumVideo = $("#rotator_content_video div").size();
               var imageReelWidthVideo = imageWidthVideo * imageSumVideo;

               $("#rotator_content_video").css({'width' : imageReelWidthVideo});

               rotateVideo = function(){
                       var triggerIDVideo = $active.attr("rel") - 1;
                       var image_reelPositionVideo = triggerIDVideo * imageWidthVideo;
                       $("#rotator-links-video a").removeClass('active');
                       $active.addClass('active');
                       $("#rotator_content_video").animate({
                              left: -image_reelPositionVideo
                       }, 500 );
               };

               rotateSwitchVideo = function(){
                        playVideo = setInterval(function(){
                               $active = $('#rotator-links-video a.active').next();
                               if ( $active.length === 0) {
                                      $active = $('#rotator-links-video a:first');
                               }
                               rotateVideo();
                       }, 4000); 
               }; 
 
               rotateSwitchVideo();

               $("#rotator_content_video .box-video").hover(function() {
                       clearInterval(playVideo);
               }, function() {
                       rotateSwitchVideo();
               });

               $("#rotator-links-video a").click(function() {
                       $active = $(this);
                       clearInterval(playVideo);
                       rotateVideo();
                       rotateSwitchVideo();
                       return false;
                       });
                               
                               


            });



</script>
		  
