<!--Sitemap function script.-->
	<script>
	$(document).ready(function(){
		var myUtils = myUtils || {};
		// Default click event is "click".
		myUtils.events = myUtils.events || {
			click : "click"
		};
		// Check for touchscreen, and if so, remap "click" to "touchstart"
		if (typeof document.body.ontouchstart !== "undefined") {
			myUtils.events.click = "touchstart";
		}
		$(function() {
		  $("#siteDirectoryClick").bind(myUtils.events.click, function(e) {
			  $( "#siteDirectory" ).toggle( "blind", 800);
		  });
		});
	});
	</script>
