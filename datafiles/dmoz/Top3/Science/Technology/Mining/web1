  <script type="text/javascript">
	function validate_form(this_form)
	{
		
		if (check_required(this_form.searchterm, "Searchterm"))	
			
			return true;
		else	
			return false;
	}
function check_required(field, field_name){
	if (!field.value.length){	
		alert('Please enter '+field_name+'.');
		field.focus();
		return false;
	}
	else
		return true;
}
</script> 
