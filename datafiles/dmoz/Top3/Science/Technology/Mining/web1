<script>
$(function(){
	var CURRENT_BANNER_NUM = 0;
	var TITLE_BANNERS = ["<a class=titlebanner href=\"bclick.php?id=268&amp;url=http%3A%2F%2Fwww.rmipsl.com\" target=\"_blank\"><img src=\"bview.php?placeholder=1&amp;id=268\" width=\"468\" border=\"0\"><\/a>"];
	var fadeInTitleBanner = function() {
		$(".titlebannerinner").remove();
		$(".titlebanner").html(
			"<div class=titlebannerinner>"
			+TITLE_BANNERS[CURRENT_BANNER_NUM]
			+"</div>"
		);
		$(".titlebannerinner").css("display","none");
		$(".titlebannerinner").fadeIn("slow");
		var notrack = TITLE_BANNERS[CURRENT_BANNER_NUM];
		notrack = notrack.replace("bview.php?placeholder","bview.php?notrack");
		TITLE_BANNERS[CURRENT_BANNER_NUM] = notrack;
		CURRENT_BANNER_NUM+=1;
		if (CURRENT_BANNER_NUM >= TITLE_BANNERS.length)
			CURRENT_BANNER_NUM = 0;
		setTimeout(fadeOutTitleBanner,10000);
	}
	var fadeOutTitleBanner = function() {
		$(".titlebannerinner").fadeOut("slow",function(){
			fadeInTitleBanner();
		});
	}
	fadeInTitleBanner();
});
</script>
