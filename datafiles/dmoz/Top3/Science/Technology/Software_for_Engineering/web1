// Begin
// news ticker functioon

var newslist=new Array();
var cnt=0;			// current news item
var curr = "";
var i=-1;			// current letter being typed
newslist[0]=new Array("Leider sind keine aktuellen News vorhanden","Unfortunately, no latest news available")
function newsticker()
{
	// next character of current item
	if (i < newslist[cnt][0].length - 1)
	{
		i++;
		temp1 = newslist[cnt][0];	
		temp1 = temp1.split('');	
  		curr = curr+temp1[i];
		temp2 = newslist[cnt][1];	
