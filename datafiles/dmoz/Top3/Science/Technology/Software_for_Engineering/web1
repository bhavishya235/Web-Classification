  <script>
$(document).ready(function() {
    // toggle megamenus

	var mmTimer = false;
	var activeMM  = false;
	var hideOnMouseOut = true;
	
	$(".megamenu-launch").click(function() { 
		return true;
	});
	
   $(".megamenu-launch").mouseover(function() {
		hideOnMouseOut = true;
   		clearTimeout(mmTimer);
   		var mm = $(this).children("li").attr('id').substring(4);
   		if (activeMM && activeMM != mm) {
			$("#megamenu-" + activeMM).fadeOut(150);
			$("#menu-" + activeMM).fadeOut(150);
		}
   		if ($("#megamenu-" + mm).length > 0 && ((!activeMM)|| activeMM != mm)) {
	        $("#megamenu-" + mm).fadeIn(150);
	    }
   		if ($("#menu-" + mm).length > 0 && ((!activeMM)|| activeMM != mm)) {
	        $("#menu-" + mm).fadeIn(150);
	    }
   		activeMM = mm;
		return false;
    });
    
	$(".megamenu-hide").mouseover(function() {
		clearTimeout(mmTimer);
	});
	
   $(".megamenu-launch, .megamenu-hide").mouseout(function() {
	   if (hideOnMouseOut) {
		mmTimer = setTimeout(function() {
			$("#megamenu-" + activeMM).fadeOut(150);
			$("#menu-" + activeMM).fadeOut(150);
			activeMM = false;
		}, 100);
	   }
	});

    // swap active state bg for first li item - rounded corner
    
    if ( $('#section-menu ul li:first').hasClass('nav-active') )
        $('#section-menu ul li:first').removeClass('nav-active').addClass('nav-active-first');
      
    // add hover state bg for li items
    $('#section-menu ul li').hover(
          function() {
              $(this).addClass('nav-hover');
          },
          function() {
              $(this).removeClass('nav-hover');
    });
     
    // swap hover state bg for first li item - rounded corner
    $('#section-menu ul li:first').hover(
        function() {
            $(this).removeClass('nav-hover');
            $(this).addClass('nav-active-hover-first');
        },
        function() {
            $(this).removeClass('nav-active-hover-first');
    });

});


</script>

  
