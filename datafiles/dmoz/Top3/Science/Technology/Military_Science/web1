      <script type="text/javascript">
        $(document).ready(function() {
          var headerSearchPlaceholder = 'Search the CAC Public Website';
          $('#headerSearch').val(headerSearchPlaceholder);
         $('#headerSearch').css('color','#CCCCCC');
         $('#headerSearch').on('focus', function() {
            if ($('#headerSearch').val() == headerSearchPlaceholder) {
              $('#headerSearch').val('');
              $('#headerSearch').css('color','#000000');
            }
          });
          $('#headerSearch').on('blur', function() {
            if ($('#headerSearch').val() == '') {
              $('#headerSearch').val(headerSearchPlaceholder);
              $('#headerSearch').css('color','#CCCCCC');
            }
          });

        });
      </script>
    </form>
