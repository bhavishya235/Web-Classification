        function updateLocalTime(utc) {
            var localdiff = parseInt($('#utcOffset').val());
            var local_now = new Date(utc.getTime() + localdiff);
            $('#spanTime').text(formatTime(local_now));
        }

        jQuery(function () {
            onClockTick = updateLocalTime;
            startClock();
        });
    </script>
    
