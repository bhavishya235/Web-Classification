                               $("#cboxPrevious").html("<a href='#' onclick='open_tip_site()'><center><font color='black'><b><span style='font-family: arial,helvetica,sans-serif;'>Proceed to TIP Website</span></b></font></center></a>");
                               $("#cboxPrevious").attr("id", "");
                           }
                      });
                      $('#cboxClose').remove();
                      $("#colorbox").show();
                  }
               });
               function open_anniv_site() {
                   window.location = 'icobtl.php';
                   $("html").colorbox.close();
               }
               function open_tip_site() {
                   window.location = 'index.php';
                   $("html").colorbox.close();
               }
            </script>  			<!-- end pop-up box -->
            
