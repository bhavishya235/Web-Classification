<script type='text/javascript'>
var _googleLogin = null;
function googlePop()
{
if ( !$('googleOptions') )
{
return false;
}
if( !_googleLogin )
{
 _googleLogin = new ipb.Popup( 'google', { 
type: 'pane',
initial: $('googleOptions').innerHTML,
hideAtStart: false,
w: '600px',
modal: 'true'
} );
}
else
{
 _googleLogin.show();
}
}
 $('googleIcon').observe( 'click', function(e) {
googlePop();
        Event.stop(e);
        return false;
  });
</script>
