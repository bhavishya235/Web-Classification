function favori(phrase,lien)
{
   if (window.sidebar)
   {
   window.sidebar.addPanel(phrase, lien,"");
   }
   else if( document.all )
   {
   window.external.AddFavorite(lien, phrase);
   }
   else
        {
   return true;
   }
}


var a_vars = Array();
var pagename='';

var phpmyvisitesSite = 1;
var phpmyvisitesURL = "http://www.ensc-lille.fr/phpmv2/phpmyvisites.php";
</script>
<script language="javascript" src="http://www.ensc-lille.fr/phpmv2/phpmyvisites.js" type="text/javascript"></script>
<!-- Fin JS Inouit -->
  </head>

