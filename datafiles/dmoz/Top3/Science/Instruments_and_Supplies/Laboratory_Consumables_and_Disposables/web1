    <script type="text/javascript">
        function RenderClick(e, btnID, validationGroup) 
        {
            if (window.event) // IE
            {
                keynum = e.keyCode;
            }
            else if (e.which) // Netscape/Firefox/Opera
            {
                keynum = e.which;
            }
            // Si Enter
            
            if (keynum == 13) {
                // Lancer un postback sur le bouton passÃ© en paramÃ¨tre
               
                if (Page_ClientValidate(validationGroup)) {
                   
                    __doPostBack(btnID, '');

                }
                else {
                    CheckMediaSendForm();
                 }
                return false;
            }
            return true;
        }
    </script>
