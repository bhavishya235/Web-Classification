<script type="text/javascript">
//<![CDATA[
    function ShowError(errorTitle, errorText, errArray) {
        jQuery('#errorTitle').html(errorTitle);
        if (errorText != '') jQuery('#errorDescription').show(); else jQuery('#errorDescription').hide();
        jQuery('#errorDescription').html(errorText);
        if (errArray != null && errArray.length > 0) {
            jQuery('#errorItems').empty();
            jQuery('<ul></ul>').appendTo('#errorItems');
            for (var i = 0; i < errArray.length; i++)
                jQuery('<li>' + errArray[i] + '</li>').appendTo('div#errorItems ul');
            jQuery('#errorItems').show(); 
        }
        else jQuery('#errorItems').hide();
        {
            tb_show(null, '#TB_inline?height=120&amp;width=300&amp;inlineId=ErreurPopup', false);
        }
    }

    /* - Fonction : Validation des champs de formulaire - */
    function ValidateForm(groupName, OnlyOneErrorByField, classError) {
        var msg = new Array();
        var ctl = new Array();

        if (classError != '') jQuery('.'+classError).removeClass(classError);

        //Ne pas oublier d'appliquer "Page_ClientValidate()" avant d'appeler cette procÃ©dure
        for (var i = 0; i < Page_Validators.length; i++) {
            if (Page_Validators[i].validationGroup == groupName && !Page_Validators[i].isvalid) {
                if (OnlyOneErrorByField) {
                    var exist = false;
                    for (var y = 0; y < ctl.length; y++) {
                        if (ctl[y] == Page_Validators[i].controltovalidate)
                            exist = true;
                    }

                    if (!exist) {
                        msg[msg.length] = Page_Validators[i].errormessage;
                        if (classError != '') {
                            jQuery('#' + Page_Validators[i].controltovalidate).addClass(classError);
                            if(jQuery("#" + Page_Validators[i].controltovalidate + "_fake").length > 0)
                                jQuery("#" + Page_Validators[i].controltovalidate + "_fake").addClass(classError);
                        }
                    }
                }
                else {
                    msg[msg.length] = Page_Validators[i].errormessage;
                    if (classError != '') {
                        jQuery('#' + Page_Validators[i].controltovalidate).addClass(classError);
                        if (jQuery("#" + Page_Validators[i].controltovalidate + "_fake").length > 0)
                            jQuery("#" + Page_Validators[i].controltovalidate + "_fake").addClass(classError);
                    }
                }

                ctl[i] = Page_Validators[i].controltovalidate;
            }

        }

        return msg;
    }
//]]>
</script>

    
