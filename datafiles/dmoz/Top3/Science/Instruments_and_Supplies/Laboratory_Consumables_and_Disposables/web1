function set(x){
for(i=1;i<=3;i++)
if(i==x) {
document.getElementById("tab"+i).className="lit";
document.getElementById("cell"+i).style.display="block";

} else {
document.getElementById("tab"+i).className="";
document.getElementById("cell"+i).style.display="none";

}
}
