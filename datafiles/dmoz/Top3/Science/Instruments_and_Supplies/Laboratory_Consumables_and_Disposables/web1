			    <script type="text/javascript">
			        var sSearchTextboxID = "ctl00_txtSearch";
                    var sButtonSearch = "ctl00$btnSearch";
                    var sModuleNameSearch = "Search";
                    
                    function CheckCOVEOForm() {
                        if (typeof (Page_ClientValidate) == 'function') {
                            if (!Page_ClientValidate("COVEOSearch")) {
                                var msg = ValidateForm("COVEOSearch", true, "Erreur");
                                ShowError(sModuleNameSearch, "message", "message 2");
                                return false;
                            }
                        }
                        return true;
                    }
			    
                     function CheckCOVEOFormWithPostback(e, btnId) {
                        if (window.event) // IE
                        {
                            keynum = e.keyCode;
                        }
                        else if (e.which) // Netscape/Firefox/Opera
                        {
                            keynum = e.which;
                        }
                        // Si Enter
                        if (keynum == 13) 
                        {
                            if (typeof (Page_ClientValidate) == 'function') 
                            {
                                if (!Page_ClientValidate("COVEOSearch"))
                                 {
                                    var msg = ValidateForm("COVEOSearch", true, "Erreur");
                                    ShowError(sModuleNameSearch, "message", "message 2");
                                    return false;
                                }
                                else
                                {
                                    __doPostBack(btnId, '');
                                }
                            }
                        }  
                        return true;
                    }
			    </script> 
			
