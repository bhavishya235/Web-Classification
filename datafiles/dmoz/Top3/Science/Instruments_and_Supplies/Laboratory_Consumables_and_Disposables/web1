$(document).ready(function(){
    $('#login_form').live('submit', function(event){
        event.preventDefault();
        $('#ajaxauth_login_error_container').hide();
        
        $.post(
            'http://www.porvair-sciences.com/?ACT=59',
            $('#login_form').serialize(),
            function(msg) {
                
                if (msg.indexOf('You are now logged in.') >= 0)
                {
                    
                $('#login_form').replaceWith("You are now logged in.");
                    a2();
                    
                    
                } else {
                    var out = /<ul>[\s\S]*<\/ul>/.exec(msg);
                    $('#ajaxauth_login_error_container').html(''+out);
                    $('#ajaxauth_login_error_container').show();
                }
                return false;
            }
        );
        return false;
    });
});
</script>
  
