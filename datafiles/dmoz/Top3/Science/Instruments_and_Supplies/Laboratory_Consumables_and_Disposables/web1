<script type="text/javascript">

function slideSwitch() {

	var $active = parseInt($('#slideshow IMG.active').attr("id"));
	var $len = $('#slideshow img.img').length;
	
    if ($active == 0 ) $active = 1;
	if ($active > $len-1 )
	{
		$active = 0;
		$('img#'+$len).removeClass('active').css({opacity: 0.0});
	}
	
    // use this to pull the images in the order they appear in the markup
    var $next =  $('img#'+($active+1));
	$('img#'+$active).removeClass('active').css({opacity: 0.0});
    $next.addClass('active').animate({opacity: 0.99}, 500);
	$("#movers-row").find("a.thumbactive").find("img").attr("src",$("#movers-row").find("a.thumbactive").attr("rel3"));
	$("#movers-row").find("a.thumbactive").removeClass("thumbactive");
	$("#movers-row").find("a[rel='"+$('#slideshow').find('IMG.active').attr("id")+"']").addClass("thumbactive").find("img").attr("src",$("#movers-row").find("a[rel='"+$('#slideshow').find('IMG.active').attr("id")+"']").attr("rel2"));
	
}


jQuery(document).ready(function($){
setInterval( "slideSwitch()", 5000 );
	$('#get_img').elevateZoom();
	
	$("#movers-row a.cross-link").live("click",function()
	{
		$('img#'+$(this).attr("rel")).parent().parent().find("img.active").removeClass('active').css({opacity: 0.0});
		$('img#'+$(this).attr("rel")).css({opacity: 0.0}).addClass('active').animate({opacity: 0.99}, 500);
		$("#movers-row").find("a.thumbactive").find("img").attr("src",$("#movers-row").find("a.thumbactive").attr("rel3"));
		$("#movers-row").find("a.thumbactive").removeClass("thumbactive");
		$(this).addClass("thumbactive").find("img").attr("src",$(this).attr("rel2"));
	});


})

</script>
<script>
function sel_img(val){
		//alert(val);
		$('#get_img').attr('src',val);
			$('#get_img').elevateZoom();
				}
				</script>  
                 
<script type="text/javascript">

$(document).ready(function() {

	//Default Action
	$(".tab_content").hide(); //Hide all content
	$("ul.tabs li:first").addClass("active").show(); //Activate first tab
	$(".tab_content:first").show(); //Show first tab content
	
	//On Click Event
	$("ul.tabs li").click(function() {
		$("ul.tabs li").removeClass("active"); //Remove any "active" class
		$(this).addClass("active"); //Add "active" class to selected tab
		$(".tab_content").hide(); //Hide all tab content
		var activeTab = $(this).find("a").attr("href"); //Find the rel attribute value to identify the active tab + content
		$(activeTab).fadeIn(); //Fade in the active content
		return false;
	});
	
	//Default Action
	$(".tab_content1").hide(); //Hide all content
	$("ul.tabs_prdt li:first").addClass("active").show(); //Activate first tab
	$(".tab_content1:first").show(); //Show first tab content
	
	//On Click Event
	$("ul.tabs_prdt li").click(function() {
		$("ul.tabs_prdt li").removeClass("active"); //Remove any "active" class
		$(this).addClass("active"); //Add "active" class to selected tab
		$(".tab_content1").hide(); //Hide all tab content
		var activeTab1 = $(this).find("a").attr("href"); //Find the rel attribute value to identify the active tab + content
		$(activeTab1).fadeIn(); //Fade in the active content
		return false;
	});

});
</script>
