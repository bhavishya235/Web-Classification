		<script language="JScript"><!--
{

        document.body.onmouseover=makeCool;

        document.body.onmouseout=makeNormal;

	   

		}



    function makeCool() {

        src = event.toElement;

        if (src.tagName == "A") {

            src.oldcol = src.style.color;

            src.style.color = "008000"; 

        }

    }



    function makeNormal() {

        src=event.fromElement;

        if (src.tagName == "A") {

            src.style.color = src.oldcol;

        }

    }
// -->
		</script>
