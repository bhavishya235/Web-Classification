<script type="text/javascript">
    //API Call configuration
    var api_server = 'http://mcc.ugc.bazaarvoice.com';
    var api_key = 'e8kg4m7mfs1kio3uqd3xgay5t';
    var num_items = 100;
</script>

<script type="text/javascript">
    $(document).ready(function () {
        var _reviewCount;
        var _overallRating;
        var _stars;

        // Disable cache-busting parameter names because we want to allow for API results to be cached
        $.ajaxSetup({ cache: true });
        // Make the call to the API using the base URL and key we set at the top of the file
        $.getJSON(api_server + "/data/statistics.json?callback=?",
      "apiversion=5.4&passkey=" + api_key + "&limit=" + num_items + "&filter=productid:15,16,55,765,840,842,867,1593,1756,1758,1760,1827,1859,1865,1871&stats=reviews",
      function (json) {
          jQuery.each(json.Results, function (index, Review) {
              //alert(Review.ProductStatistics.ReviewStatistics['TotalReviewCount']);
              _reviewCount = Review.ProductStatistics.ReviewStatistics['TotalReviewCount'];
              _overallRating = Math.round(Review.ProductStatistics.ReviewStatistics['AverageOverallRating'] * 10) / 10;
              _overallRating = (_overallRating + "").split(".");
              _overallRating = _overallRating.toString().replace(",", "_");
              //_int = Math.floor(_overallRating);
              stars = api_server + "/9901-en_us/" + _overallRating + "/5/rating.gif"
              $("#bvratings_" + Review.ProductStatistics.ProductId).append("<img src='" + stars + "' style='vertical-align:middle;'>");
              if (_reviewCount > 1) {
                  $("#href_" + Review.ProductStatistics.ProductId).text("(" + _reviewCount + " reviews)");
              }
              else {
                  $("#href_" + Review.ProductStatistics.ProductId).text("(" + _reviewCount + " review)");
              }

              
          });
      });


    });

</script>

