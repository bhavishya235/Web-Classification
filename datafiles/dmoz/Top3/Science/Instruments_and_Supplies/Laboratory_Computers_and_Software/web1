		function rollover(imgSwap,imgName) {

			if (document.images != null) {

				imgSwap = eval(imgSwap + ".src");

				document [imgName].src = imgSwap;

				}

			}



		function gotoURL(location) {

			if (location != "") {

				window.location=location;

				}

			}



