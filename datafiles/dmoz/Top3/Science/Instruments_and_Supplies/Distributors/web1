<script type="text/javascript">
function initMenu() {
  $('#menuz1 ul').hide();
  $('#menuz1 li a').click(
    function() {
        $(this).next().slideToggle('normal');	
      }
    );
  }
$(document).ready(function() {initMenu();});
</script>

