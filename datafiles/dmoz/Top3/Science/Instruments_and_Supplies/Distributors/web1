<script type="text/javascript">
var $j = jQuery.noConflict(); 
$j(document).ready(function(){
    $j('#areaSwitcher div').not('div:first').hide(); // Hide all areaPosters except for the first one
    $j('#areaSwitcher ul li:first').addClass('active');
    
    $j('#areaSwitcher ul li').mouseenter(function() {    // Register mouseovers on each area Switch list item
       if (!($j(this).hasClass('active'))) { // only switch if element is not already active
           $j('#areaSwitcher ul li:animated').hide();
           $j('#areaSwitcher ul li').removeClass('active');
           $j(this).addClass('active');
           var idContent = '#'+$j(this).children().attr("id")+'Content';
           $j(this).parents("#areaSwitcher").children("div").hide();
           $j('#areaSwitcher div'+idContent).fadeIn(300); 
       }
    });
});
function switchImg (targetElement, URL) {
    $j(targetElement).attr('src',URL);
}
</script>



