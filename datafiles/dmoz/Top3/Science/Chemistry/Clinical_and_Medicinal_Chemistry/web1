<p>
    <a href="http://tuition.ku.edu/" title="Tuition">Tuition</a> | <a href="http://affordability.ku.edu/billing/" title="Bill Payments">Bill Payments</a> | <a href="http://affordability.ku.edu/cs/search.shtml" title="Scholarship Search">Scholarship Search</a><br>
    <a href="http://affordability.ku.edu/financialaid/" title="Financial Aid">Financial Aid</a> | <a href="http://affordability.ku.edu/financialaid/loans/" title="Loans">Loans</a> | <a href="http://www.kucard.ku.edu/beak_em_bucks/default.aspx" title="Beak 'em Bucks">Beak 'em Bucks</a>
</p>    </div>
  </div>
</div><div class="block block-ku-static block-ku-static-nvg-weather-module block-ku-static-ku-static-nvg-weather-module even block-without-title" id="block-ku-static-ku-static-nvg-weather-module">
  <div class="block-inner clearfix">
                
    <div class="content clearfix">
      <div id="weatherholder"></div>
<script type="text/javascript">
    //Use javascript to avoid weather caching
    $kuj(document).ready(function() {
        if(!$kuj.cookie('myweather')){
            var url = 'http://webmedia.ku.edu/~http/ssi/2012/widgets/cmslawrenceweather.php';
            var weatherval = '<div class="sectiontitle">Current Weather</div>';
            var date = new Date();
            var minutes = 30;
            date.setTime(date.getTime() + (minutes * 60 * 1000));
            try {
            //cross script issue with IE8 workaround
            if (navigator.appVersion.match(/MSIE/) && window.XDomainRequest) {
                // Use Microsoft XDR
                var xdr = new XDomainRequest();
                xdr.open("get", url);
                xdr.onload = function () {
                    var result = weatherval+xdr.responseText;
                    $kuj.cookie('myweather', result, { expires: date });
                    $kuj('#weatherholder').html(result);
                };
                xdr.send();
            } else if (!navigator.appVersion.match(/MSIE/)) {
                $kuj.ajax({
                    type: 'GET',
                    url: url,
                    success: function (data) {
                        date.setTime(date.getTime() + (minutes * 60 * 1000));
                        $kuj.cookie('myweather', weatherval+data, { expires: date });
                        $kuj('#weatherholder').html($kuj.cookie('myweather'));
                    }
                });
            }
        } catch (e) {}
        }else{
            $kuj('#weatherholder').html($kuj.cookie('myweather'));
        }

    });
</script>
    </div>
  </div>
</div>  </div>
