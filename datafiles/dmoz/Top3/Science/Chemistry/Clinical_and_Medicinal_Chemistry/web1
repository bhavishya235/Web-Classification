        $kuj(document).ready(function() {
            $kuj('.why_ku_title').click(function() {
                $kuj('.bragpoints').css('display', 'block');
                $kuj('.applyblock').css('display', 'none');
            });
            $kuj('#applydropdown').click(function() {
                if($kuj('.bragpoints').css('display') == 'none'){
                $kuj('.bragpoints').css('display', 'block');
                $kuj('.applyblock').css('display', 'none');
                                $kuj('#applydropdown').removeClass('open');

                }else{
                                                    $kuj('#applydropdown').addClass('open');

                $kuj('.bragpoints').css('display', 'none');
                $kuj('.applyblock').css('display', 'block');
            }
            });
            /* Show one item of ku today content at random */
            childcount = $kuj('.whyku_random').children().length;
            randomnum = Math.floor(Math.random() * childcount) + 1;
            $kuj('div.whyku_random div:nth-child(' + randomnum + ')').css('display', 'block');
            $kuj('div.whyku_random div').vAlign(5);
        });

    </script>

