}
function marquee(){
 var cleft = parseInt(mObj.style.left) ;
 var tlen = mObj.clientWidth;
 if(Math.abs(cleft)<(tlen/2)){
  mObj.style.left = cleft - 1;
 }else{
  mObj.style.left = 0;
