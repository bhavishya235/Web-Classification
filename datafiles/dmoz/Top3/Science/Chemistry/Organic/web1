	$(document).ready(function(){
		// Fix all inline PNG images except those in a Google map and anything with class pngfixthis.
		// Background images set to 'crop' by default. If 'scale' is desired, give the element a class of 'scale'.
			$("#container, #footer, .pngfixthis, .pngfixme, :not #map").pngFix();
	});
</script>
