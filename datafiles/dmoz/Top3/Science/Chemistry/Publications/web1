document.write('<script language="JavaScript" src="http://ad.doubleclick.net/N8868/adj/c_and_en_online/IndexLP;pos=1;tile=' + dc_tile + ';Index=Homepage;' + dcoptTag + 'sz=728x90;ord=' + ord + '?" type="text/javascript"><\/script>');
</script>
