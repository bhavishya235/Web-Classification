     $(function() {
         $('#ctl00_CbnSearch1_ddlCategories').jqTransSelect();
         $('#ctl00_CbnSearch1_ddlSubCat').jqTransSelect();
         $('#ctl00_CbnSearch1_ddlMedia').jqTransSelect();
     });

     function transformDDL() {
         $('#ctl00_CbnSearch1_ddlCategories').jqTransSelect();
         $('#ctl00_CbnSearch1_ddlSubCat').jqTransSelect();
         $('#ctl00_CbnSearch1_ddlMedia').jqTransSelect();
     }
     Sys.WebForms.PageRequestManager.getInstance().add_endRequest(transformDDL);
