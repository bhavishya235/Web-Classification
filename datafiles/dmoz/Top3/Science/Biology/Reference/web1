	function redirectme() {
		var loc = 'http://www.ecologydictionary.org/' + document.getElementById('define').term.value;
		if (document.myform.operation) {
			if (document.myform.operation[1].checked == true) { 
			var loc = 'http://www.ecologydictionary.org/' + document.getElementById('define').term.value;
			}
		}
		var myExp = /\s/g;
		var loc2 = loc.replace(myExp,"_");
		location = loc2;
	}
	
	function bar(e){
		var keycode;
		if (window.event) keycode = window.event.keyCode;
		else if (e) keycode = e.which;
		else return true;
		if (keycode == 13) {
			redirectme();
		   return false;
		   }
		else
		   return true;
	}
