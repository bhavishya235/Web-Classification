====================<http://www.imb-jena.de/IMAGE_AA.html>====================
<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 3.2//EN">
<HTML>
<HEAD>
    <META NAME="Generator" CONTENT="Cosmo Create 1.0.3">
    <META HTTP-EQUIV="Content-Type" CONTENT="text/html; charset=iso-8859-1">
    <META NAME="Template" CONTENT="C:\PROGRAMME\MICROSOFT OFFICE\OFFICE\html.dot">
    <TITLE>The Amino Acid Repository - JenaLib</TITLE>
  <LINK REL='stylesheet' type='text/css' HREF="/ImgLibPDB/ImgLib-rh.css">
  <LINK REL='SHORTCUT ICON' type='image/x-icon' HREF='http://jenalib.fli-leibniz.de/ImgLibPDB/images/jenalib_minilogo.ico'>
  <base target='_self'>

  <script type="text/javascript">

    var expanded_info_names  = new Array("individual","images","classification","hydrophobicity","oneimage","amino","modifications","solvent","mutation","graph","ramachandran");
    var collapsed_info_names = new Array("images","classification","hydrophobicity","oneimage","","solvent","mutation","graph","ramachandran");

    var table_cell_mark        = new Array();      /* used for permanent highlighting of specific table cells */
    var table_cell_color       = new Array();      /* used for permanent highlighting of specific table cells */
    var table_row_color        = new Array();      /* used for permanent highlighting of specific table rows */
    var table_column_color     = new Array();      /* used for permanent highlighting of specific table columns */

    function idbShowControl(control,action) {      /* show or hide control elements in atlas page */
      switch(control) {
        default:
          if (action == "show") {
            if (document.getElementById(control + "_info")) {
              document.getElementById(control + "_info").style.display     = "inline";
            }
            if (document.getElementById(control + "_show")) {
              document.getElementById(control + "_show").style.display     = "none";
            }
            if (document.getElementById(control + "_hide")) {
              document.getElementById(control + "_hide").style.display     = "inline";
            }
          } else {
            if (document.getElementById(control + "_info")) {
              document.getElementById(control + "_info").style.display     = "none";
            }
            if (document.getElementById(control + "_show")) {
              document.getElementById(control + "_show").style.display     = "inline";
            }
            if (document.getElementById(control + "_hide")) {
              document.getElementById(control + "_hide").style.display     = "none";
            }
          }
          break;
      }
    }

    function idbExpandAll(action,info_names)  {      /* show or hide all control elements supplied by list in atlas page */
      for (var control in info_names) {
        idbShowControl(info_names[control],action);
      }
    }

    function idbColorTableCells(name,column_count,row_count, color1, color2) {
      var background_color = new Array (color1, color2);
      /* color cells with 'color1' */
      for (var row_number=1; row_number<=row_count; row_number++) {
        var row_start_color_index = row_number % 2;
        for (var column_number=1; column_number<=column_count; column_number++) {
          var cell_id = name + "_" + column_number + "_" + row_number;
          var color_index = (row_start_color_index + column_number) % 2;
          if (document.getElementById(cell_id)) {
            document.getElementById(cell_id).style.backgroundColor = background_color[color_index];
          }
        }
      }
    }

    function idbMTP(table_name, column_number, row_number, action, header_row_count) {
      var current_row_number    = 0;
      var current_column_number = 0;
      var header_row_number     = 0;
      var max_header_row_number = 0;
      var cell_color            = "";
      var column_header_color   = "";
      var row_header_color      = "";
      var header_color          = "#dddddd";
      var mark_color            = "#7fb5f8";    /* for permanent cell marks */
      var mark_line_color       = "#d7a277";    /* for permanent line marks (column/row) */
      var background_color      = new Array ("#FFFFFF","#EEEEEE");

      var cell_id          = table_name + "_" + column_number + "_" + row_number;
      var column_header_id = table_name + "_" + column_number + "_0";
      var row_header_id    = table_name + "_0_" + row_number;

      if (action == "+") {   /* on */
        cell_color          = "#FFAAAA";
        column_header_color = cell_color;
        row_header_color    = cell_color;
      } else {
        if (table_cell_color[cell_id]) {
          cell_color = table_cell_color[cell_id];
        } else {
          if (table_row_color[table_name + row_number]) {
            cell_color = table_row_color[table_name + row_number];
          } else {
            if (table_column_color[table_name + column_number]) {
              cell_color = table_column_color[table_name + column_number];
            } else {
              cell_color = background_color[0];
            }
          }
        }

        if (header_row_count) {
          column_header_id = column_header_id + "_1";   /* color of first header row is used, colors for other rows are identical */
        }

        if (table_cell_color[column_header_id]) {
          column_header_color = table_cell_color[column_header_id];
        } else {
          if (table_column_color[table_name + column_number]) {
            column_header_color = table_column_color[table_name + column_number];
          } else {
            column_header_color = header_color;
          }
        }

        if (table_cell_color[row_header_id]) {
          row_header_color = table_cell_color[row_header_id];
        } else {
          if (table_row_color[table_name + row_number]) {
            row_header_color = table_row_color[table_name + row_number];
          } else {
              row_header_color = header_color;
          }
        }
      }

      if (action == "toggle_row") {
        if (table_row_color[table_name + row_number]) {
          delete table_row_color[table_name + row_number];
        } else {
          table_row_color[table_name + row_number] = mark_line_color;
        }
        for (current_column_number=0; current_column_number<=column_number; current_column_number++) {
          cell_id = table_name + "_" + current_column_number + "_" + row_number;
          if (table_cell_mark[cell_id]) {
            cell_color = table_cell_color[cell_id];
          } else {
            if (table_row_color[table_name + row_number]) {
              cell_color = table_row_color[table_name + row_number];
            } else {
              if (current_column_number == 0) {
                cell_color = header_color;
              } else {
                if (table_column_color[table_name + current_column_number]) {
                  cell_color = table_column_color[table_name + current_column_number];
                } else {
                  cell_color = background_color[0];
                }
              }
            }
          }
          if (document.getElementById(cell_id)) {
           document.getElementById(cell_id).style.backgroundColor = cell_color;
          }
        }

      } else {

        if (action == "toggle_column") {

          if (table_column_color[table_name + column_number]) {
            delete table_column_color[table_name + column_number];
          } else {
            table_column_color[table_name + column_number] = mark_line_color;
          }

          if (header_row_count) {
            max_header_row_number = header_row_count;
          } else {
            max_header_row_number = 1;
          }
          current_row_number = 0;
          for (header_row_number=1; header_row_number<=max_header_row_number; header_row_number++) {
            cell_id = table_name + "_" + column_number + "_" + current_row_number;
            if (header_row_count) {
              cell_id = cell_id + "_" + header_row_number;
            }
            if (table_cell_mark[cell_id]) {
              cell_color = table_cell_color[cell_id];
            } else {
              if (table_column_color[table_name + column_number]) {
                cell_color = table_column_color[table_name + column_number];
              } else {
                cell_color = header_color;
              }
            }
            if (document.getElementById(cell_id)) {
             document.getElementById(cell_id).style.backgroundColor = cell_color;
            }
          }


          for (current_row_number=1; current_row_number<=row_number; current_row_number++) {
            cell_id = table_name + "_" + column_number + "_" + current_row_number;
            if (table_cell_mark[cell_id]) {
              cell_color = table_cell_color[cell_id];
            } else {
              if (table_column_color[table_name + column_number]) {
                cell_color = table_column_color[table_name + column_number];
              } else {
                if (table_row_color[table_name + current_row_number]) {
                  cell_color = table_row_color[table_name + current_row_number];
                } else {
                  cell_color = background_color[0];
                }
              }
            }
            if (document.getElementById(cell_id)) {
             document.getElementById(cell_id).style.backgroundColor = cell_color;
            }
          }

        } else {

          if (action == "t") {   /* toggle (used shortcut to reduce transfer volume significantly */
            if (table_cell_mark[cell_id]) {
              delete table_cell_mark[cell_id];
              delete table_cell_color[cell_id];
              if (header_row_count) {
                for (header_row_number=1; header_row_number<=header_row_count; header_row_number++) {
                  column_header_id = table_name + "_" + column_number + "_0_" + header_row_number;
                  if (table_cell_mark[column_header_id]) {
                    if (table_cell_mark[column_header_id] > 1) {
                      table_cell_mark[column_header_id]--;
                    } else {
                      delete table_cell_mark[column_header_id];
                      delete table_cell_color[column_header_id];
                    }
                  }
                }
              } else {
                if (table_cell_mark[column_header_id]) {
                  if (table_cell_mark[column_header_id] > 1) {
                    table_cell_mark[column_header_id]--;
                  } else {
                    delete table_cell_mark[column_header_id];
                    delete table_cell_color[column_header_id];
                  }
                }
              }
              if (table_cell_mark[row_header_id]) {
                if (table_cell_mark[row_header_id] > 1) {
                  table_cell_mark[row_header_id]--;
                } else {
                  delete table_cell_mark[row_header_id];
                  delete table_cell_color[row_header_id];
                }
              }
            } else {
              table_cell_mark[cell_id] = 1;
              table_cell_color[cell_id] = mark_color;
              if (header_row_count) {
                for (header_row_number=1; header_row_number<=header_row_count; header_row_number++) {
                 column_header_id = table_name + "_" + column_number + "_0_" + header_row_number;
                  if (table_cell_mark[column_header_id]) {
                    table_cell_mark[column_header_id]++;
                  } else {
                    table_cell_mark[column_header_id] = 1;
                    table_cell_color[column_header_id] = mark_color;
                  }
                }
              } else {
                if (table_cell_mark[column_header_id]) {
                  table_cell_mark[column_header_id]++;
                } else {
                  table_cell_mark[column_header_id] = 1;
                  table_cell_color[column_header_id] = mark_color;
                }
              }
              if (table_cell_mark[row_header_id]) {
                table_cell_mark[row_header_id]++;
              } else {
                table_cell_mark[row_header_id] = 1;
                table_cell_color[row_header_id] = mark_color;
              }
            }

            if (table_cell_color[cell_id]) {
              cell_color = table_cell_color[cell_id];
            } else {
              if (table_row_color[table_name + row_number]) {
                cell_color = table_row_color[table_name + row_number];
              } else {
                if (table_column_color[table_name + column_number]) {
                  cell_color = table_column_color[table_name + column_number];
                } else {
                  cell_color = background_color[0];
                }
              }
            }
            if (header_row_count) {
              for (header_row_number=1; header_row_number<=header_row_count; header_row_number++) {
                column_header_id = table_name + "_" + column_number + "_0_" + header_row_number;
                if (table_cell_color[column_header_id]) {
                  column_header_color = table_cell_color[column_header_id];
                } else {
                  if (table_column_color[table_name + column_number]) {
                    column_header_color = table_column_color[table_name + column_number];
                  } else {
                    column_header_color = header_color;
                  }
                }
              }
            } else {
              if (table_cell_color[column_header_id]) {
                column_header_color = table_cell_color[column_header_id];
              } else {
                if (table_column_color[table_name + column_number]) {
                  column_header_color = table_column_color[table_name + column_number];
                } else {
                  column_header_color = header_color;
                }
              }
            }
            if (table_cell_color[row_header_id]) {
              row_header_color = table_cell_color[row_header_id];
            } else {
              if (table_row_color[table_name + row_number]) {
                row_header_color = table_row_color[table_name + row_number];
              } else {
                  row_header_color = header_color;
              }
            }
          }

          if (document.getElementById(cell_id)) {
            document.getElementById(cell_id).style.backgroundColor = cell_color;
          }
          if (header_row_count) {
            for (header_row_number=1; header_row_number<=header_row_count; header_row_number++) {
              column_header_id = table_name + "_" + column_number + "_0_" + header_row_number;
              if (document.getElementById(column_header_id)) {
                document.getElementById(column_header_id).style.backgroundColor = column_header_color;
              }
            }
          } else {
            if (document.getElementById(column_header_id)) {
              document.getElementById(column_header_id).style.backgroundColor = column_header_color;
            }
          }
          if (document.getElementById(row_header_id)) {
            document.getElementById(row_header_id).style.backgroundColor = row_header_color;
          }
        }
      }
    }

  </script>
