function setPlantImage() {
	var strPath, strImage, strNumber, strExtension, strFullPlantPath;
	var strImageStart, strImageEnd, strImage
	strPath = "/templates/1020_images/";
	strImage = "plant";
	strNumber = GenRandomTwo(1, 85);
	strExtension = ".jpg"
	strImageStart = "<img src=\""
	strImageEnd = "\" width=1020 border=0 >"
	strFullPlantPath = strPath + strImage + strNumber + strExtension
	strImage = strImageStart + strFullPlantPath + strImageEnd
	document.write(strImage);
}
function GenRandomOne() {
	var intRandomNumber
	intRandomNumber = parseInt(85 * Math.random() + 1);
	alert(intRandomNumber);
}
function GenRandomTwo(r1, r2) {
  	if (r2 > r1) { 
  		return (Math.round(Math.random()*(r2-r1))+r1);
 	}	
  	else {
		return (Math.round(Math.random()*(r1-r2))+r2);
	}	
}
//-->
