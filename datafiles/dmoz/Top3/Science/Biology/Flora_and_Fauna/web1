	<script type="text/javascript">
		$(document).ready(function(){
			$("#datepicker").datepicker({ dateFormat: 'yy-mm-dd' });
			$(".tiptopclass").tipTip({maxWidth: "300px", edgeOffset: 3, defaultPosition: "top"});
            $(".tiptopclass2").tipTip({maxWidth: "300px", edgeOffset: 3, defaultPosition: "right"});
			$("a[rel^='prettyPhoto']").prettyPhoto({
				keyboard_shortcuts: false,
				deeplinking: false,
				social_tools: false,
				theme: 'light_rounded'
			});
			$( "#taxafinder_searchfield" ).autocomplete({
				source: "include_search_taxa_autocomplete.asp",
				contentType: "application/json; charset=iso-8859-1",
				minLength: 3,
				select: function( event, ui ) {
					document.getElementById('taxafinder_searchfield').value=ui.item.value.split("#")[0];
					document.getElementById('taxafinder_searchfield_thingid').value=ui.item.value.split("#")[1];

					$('#seektop').submit();
				}
			});
		});
	</script>
