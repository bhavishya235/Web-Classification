    <div id="top_search_div">
        <div id="head_search_results_div2"></div>
    </div>
    <script>
                                var timer, value;
                                $('#logoarea_searchfield').bind('keyup', function() {
                                    clearTimeout(timer);
                                    var str = $(this).val();
                                   // document.getElementById('taxafinder_searchfield_2').value=document.getElementById('taxafinder_searchfield_2_').value;
                                    if (str.length > 2 && value != str) {
                                        timer = setTimeout(function() {
                                            value = str;
                                      //      document.getElementById('taxafinder_searchfield_2').value=document.getElementById('taxafinder_searchfield_2_').value;
                                            document.getElementById('swapiframe').src='include_head_search_results2.asp?w='+document.getElementById('logoarea_searchfield').value;
                                            document.getElementById('logoarea_search_searchicon_div2').style.display = 'none';
                                            document.getElementById('logoarea_search_loading_div2').style.display = 'block';
                                            showSearchDiv();
                                        }, 1000);
                                    }
                                });
        function showSearchDiv() {
            $('#top_search_div').css({'top':$("#top_menu_search_button").offset().top+40,'left':$("#top_menu_search_button").offset().left}).show('fast', function() {});
    	}
        function searchDone() {
             //$('#top_search_div').css({'top':$("#top_menu_search_button").offset().top+40,'left':$("#top_menu_search_button").offset().left}).fadeToggle('slow', function() {});
             $("#top_search_div").animate({
              width: "973",
              height: "280"
              }, 200 );
          //  $('#top_search_div').width("970");
          //  $('#top_search_div').height("300");
            $('#head_search_results_div2').show();
       //     $('#head_search_person_div').show();
            $('#head_search_close_div').show();
       //     $('#taxafinder_searchfield_2_').removeClass('loadinggif')
       //     $('#taxafinder_searchfield_2_').addClass('unloadloadinggif');
          //  $('#head_search_loading_div2').hide();
          //  $('#head_search_searchicon_div2').show();
         //   $('#head_search_searchicon_img').src="images/icon_searchbox.png";
            document.getElementById('logoarea_search_searchicon_div2').style.display = 'block';
            document.getElementById('logoarea_search_loading_div2').style.display = 'none';

         //   $('#head_search_person_div').slimScroll({ height: '215px',railVisible: true,    alwaysVisible: true});
        }
        function hidesearchDone() {
             $("#top_search_div").animate({
              width: "1",
              height: "1"
              }, 200 );
          //  $('#top_search_div').width("970");
          //  $('#top_search_div').height("300");
         //   $("#top_search_div").hide();
      //      $('#head_search_person_div').hide();
            $('#head_search_close_div').hide();
            $('#top_search_div').css({'top':$("#top_menu_search_button").offset().top+40,'left':$("#top_menu_search_button").offset().left}).hide('fast', function() {});
        }
    </script>
    
