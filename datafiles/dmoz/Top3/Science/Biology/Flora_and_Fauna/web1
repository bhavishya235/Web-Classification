<script type="text/javascript">
      var map;
      var topmap;
      var maplatest;

      
      function initializeObsMap(koordinater,zoomlevel) {
      	geocoder = new google.maps.Geocoder();
      	var mySplitResult = koordinater.split(",");

        var myOptions = {
          zoom: zoomlevel,
          panControl: false,
          rotateControl: false,
          center: new google.maps.LatLng(30.14512718337613, 10.1953125),
          mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        map = new google.maps.Map(document.getElementById('observations_map_canvas'),
            myOptions);

        var posn = new google.maps.LatLng(mySplitResult[0],mySplitResult[1]);
		map.setCenter(posn);

		var image = 'images/cross-hairs.gif';
	    var beachMarker = new google.maps.Marker({
	        position: posn,
	        map: map,
	        icon: image
	    });
	    beachMarker.bindTo('position', map, 'center');

	    google.maps.event.addListener(map, 'zoom_changed', function() {
		    var center = map.getZoom();
			document.getElementById("zoomlevel").value = center.toString();
		  });

		google.maps.event.addListener(map, 'dragend', function() {
		    var MylatLng = beachMarker.getPosition()


		//    var lat = myLatLng.lat();
		//    var lng = myLatLng.lng();
		    document.getElementById("koordut").value = MylatLng.lat()+","+MylatLng.lng();
		//	var koord = document.getElementById("koordut").value;
		//	var koord2 = koord.replace(/[\(\)]/g, "");
		//	var koord3 = koord2.split(/\s*,\s*/);
			document.getElementById("inKoordX").value=[MylatLng.lat()];
			document.getElementById("inKoordY").value=[MylatLng.lng()];
			geocoder.geocode({'latLng': beachMarker.getPosition()}, function(results, status) {
		      if (status == google.maps.GeocoderStatus.OK) {
		        if (results[1]) {
		          document.getElementById("inAdress").value=results[0].formatted_address;
		        }
		      }

		  	});
		  });


      }

      function initialize(koordinater,zoomlevel) {
      	geocoder = new google.maps.Geocoder();
      	var mySplitResult = koordinater.split(",");

        var myOptions = {
          zoom: zoomlevel,
          panControl: false,
          rotateControl: false,
          center: new google.maps.LatLng(30.14512718337613, 10.1953125),
          mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        map = new google.maps.Map(document.getElementById('map_canvas'),
            myOptions);

        var posn = new google.maps.LatLng(mySplitResult[0],mySplitResult[1]);
		map.setCenter(posn);

		var image = 'images/cross-hairs.gif';
	    var beachMarker = new google.maps.Marker({
	        position: posn,
	        map: map,
	        icon: image
	    });
	    beachMarker.bindTo('position', map, 'center');

	    google.maps.event.addListener(map, 'zoom_changed', function() {
		    var center = map.getZoom();
			document.getElementById("zoomlevel").value = center.toString();
		  });

		google.maps.event.addListener(map, 'dragend', function() {
		    var MylatLng = beachMarker.getPosition()


		//    var lat = myLatLng.lat();
		//    var lng = myLatLng.lng();
		    document.getElementById("koordut").value = MylatLng.lat()+","+MylatLng.lng();
		//	var koord = document.getElementById("koordut").value;
		//	var koord2 = koord.replace(/[\(\)]/g, "");
		//	var koord3 = koord2.split(/\s*,\s*/);
			document.getElementById("inKoordX").value=[MylatLng.lat()];
			document.getElementById("inKoordY").value=[MylatLng.lng()];
		  });


      }

      function GetCoords() {
      		var myLatLng = beachMarker.latLng;
		    var lat = myLatLng.lat();
		    var lng = myLatLng.lng();
		    document.twitch.koordut.value = lat.toString();
      }

      function initializeTop() {
        var myOptions = {
          zoom: 1,
          center: new google.maps.LatLng(30.14512718337613, 10.1953125),
          mapTypeId: google.maps.MapTypeId.ROADMAP
        };
        topmap = new google.maps.Map(document.getElementById('map_top'),
            myOptions);
      }

      //google.maps.event.addDomListener(window, 'load', initialize);
      function ShowMarkers(koordinater,returnancor) {

      		toggleLayer('googlemaparea_wrapper');
      		$('html, body').animate({scrollTop:0}, 'fast');
      		initializeTop()

      		var mySplitResult = koordinater.split(",");

      		var posn = new google.maps.LatLng(mySplitResult[0],mySplitResult[1]);
			topmap.setCenter(posn);
			topmap.setZoom(9);
			var marker = new google.maps.Marker({
		        position: posn,
		        map: topmap
		    });
			document.getElementById('googlemaparea_returnancor').value = returnancor;

		}

	  function HideMap() {
	  		var returnancor='#' + document.getElementById('googlemaparea_returnancor').value;
      		document.getElementById('googlemaparea_wrapper').style.display = "none";

      		if (returnancor!='') {
      			$('html, body').animate({scrollTop: $(returnancor).offset().top}, 1000);
      		}
		}

      function showCountry() {
      		var sel = document.twitch.country.options[document.twitch.country.selectedIndex]
		  	var address = sel.text
		    geocoder.geocode( { 'address': address}, function(results, status) {
		      if (status == google.maps.GeocoderStatus.OK) {
		        map.setCenter((results[0].geometry.location));
		        map.setZoom(4);


                    document.getElementById("koordut").value = results[0].geometry.location.lat()+","+results[0].geometry.location.lng();
                    document.getElementById("inKoordX").value=results[0].geometry.location.lat();
			        document.getElementById("inKoordY").value=results[0].geometry.location.lng();
		      }
		    });
		}

		function showArea() {
			  var sel1 = document.twitch.undercountry.options[document.twitch.undercountry.selectedIndex]
			  var sel2 = document.twitch.country.options[document.twitch.country.selectedIndex]

			  var address=sel1.text+', '+sel2.text;
			  geocoder.geocode( { 'address': address}, function(results, status) {
		      if (status == google.maps.GeocoderStatus.OK) {
			        map.setCenter(results[0].geometry.location);
			        map.setZoom(9);


                    document.getElementById("koordut").value = results[0].geometry.location.lat()+","+results[0].geometry.location.lng();
                    document.getElementById("inKoordX").value=results[0].geometry.location.lat();
			        document.getElementById("inKoordY").value=results[0].geometry.location.lng();
			      }
			    });
			}
		function showLocation() {
			  var address=document.getElementById("lokal").value;
			  var sel2 = document.twitch.country.options[document.twitch.country.selectedIndex]
			  var address2=address+', '+sel2.text;
			  geocoder.geocode( { 'address': address2}, function(results, status) {
		      if (status == google.maps.GeocoderStatus.OK) {
			        map.setCenter(results[0].geometry.location);
			        map.setZoom(12);

                    document.getElementById("koordut").value = results[0].geometry.location.lat()+","+results[0].geometry.location.lng();
                    document.getElementById("inKoordX").value=results[0].geometry.location.lat();
			        document.getElementById("inKoordY").value=results[0].geometry.location.lng();

			      }
			    });
			}

		function showLocID() {
			  var posn = new google.maps.LatLng(document.twitch.inKoordX.value,document.twitch.inKoordY.value);
				map.setCenter(posn);
				map.setZoom(12);
				$("#setcoords").prop("checked", true);
			}

		function popLoc() {
				document.twitch.lokal.value = document.twitch.underlokaler.value.split("$")[0];
				document.twitch.inKoordX.value = document.twitch.underlokaler.value.split("$")[1];
				document.twitch.inKoordY.value = document.twitch.underlokaler.value.split("$")[2];
				document.twitch.underlokaler.ListIndex = 0;
				var posn = new google.maps.LatLng(document.twitch.inKoordX.value,document.twitch.inKoordY.value);
				map.setCenter(posn);
				map.setZoom(14);
				document.getElementById('nocoordinates').checked = false;
				document.getElementById('mapcover').className = 'mapcover_off';
				document.getElementById('map').className = 'map_on';
			}
        function newMessage(toid,toname) {
                $("#in_text").cleditor({
            	    width:        900,
                  height:       300
            	});
                toggleLayer('message_new_wrapper');
				document.getElementById('in_to').value = toid;
                document.getElementById('in_to_span').innerHTML = toname;
			}

            function checkEnter(e){
             e = e || event;
             var txtArea = /textarea/i.test((e.target || e.srcElement).tagName);
             return txtArea || (e.keyCode || e.which || e.charCode || 0) !== 13;
            }
