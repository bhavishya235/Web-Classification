    function initLightbox(){
        new Lightbox({
            fileLoadingImage: 'http://www.montreal-biotech.com/skin/frontend/default/default/images/lightbox/loading.gif',
            fileBottomNavCloseImage: 'http://www.montreal-biotech.com/skin/frontend/default/default/images/lightbox/closelabel.gif',
            overlayOpacity: 0.3,
            animate: 1,
            resizeSpeed: 9,
            borderSize: 3,
            labelImage: 'Image',
            labelOf: 'of'
        });
    }


    if (Prototype.Browser.IE) {
        Event.observe(window, 'load', function(){ //KB927917 fix
            initLightbox();
        });
    } else {
        document.observe("dom:loaded", function(){
            initLightbox();
        });
    }
</script>    </div>
