<script type="text/javascript">

    function initHeaderFooter() {
        var globalQuickLinks = new ApplicationLinks(true);
        globalQuickLinks.setGlobalQuickLinks();
        globalQuickLinks.setCartLinks();

        initGreeting();
        initRedeemQuote();
        initB2bPunchoutLinks();
        initLanguageSelect('www.invitrogen.com');
        initCountrySelect('http://www.lifetechnologies.com/in/en/home/technical-resources.html', 'http://www.invitrogen.com');
        //initScmsMenu();
        initCountryFlag();
        initCustomerServiceNumber();
        initBrandsImage();
        B2bmessaging.Messenger.initB2bLogoAndMessage('https://www.lifetechnologies.com/order/catalog/en/US/procurement/lt');
        initB2BEditMode();
        $(".globalHeader, .globalFooter").show();
    }

    function callCookieService() {
        var isSafari = (/Safari/.test(navigator.userAgent)) && !(/Chrome/.test(navigator.userAgent));
        var daylink = window.location.protocol + '//' + environmentURL.WCMSERVER + '/site.ivgnCookie.html?c=getCookie&callback=?';
        if (isSafari) {
            daylink = window.location.protocol + '//' + environmentURL.WCMSERVER + '/site.ivgnCookie.html?saf=1&c=getCookie&callback=?';
        }
        $.getJSON(daylink, getCookieCallback);
    }

    function lifetechHeaderAjaxCall() {
        var isSafari = (/Safari/.test(navigator.userAgent)) && !(/Chrome/.test(navigator.userAgent));

        if (isSafari) {
            $("body").append('<iframe id="ivgncookieframe" name="ivgncookieframe" src="http://www.invitrogen.com/site/us/en/home/Batch-X/safaricookie.html" style="display: none;"></iframe>' +
            '<form id="ivgncookieform" enctype="application/x-www-form-urlencoded" action="http://www.invitrogen.com/site.ivgnCookie.html?saf=1&c=getCookie" target="ivgncookieframe" method="post"></form>');
            $("#ivgncookieform").submit();
            setTimeout('callCookieService()', 3000);
        }
        else {
            callCookieService();
        }
    }


        setCrossDomainCookies();
        initHeaderFooter();


    $('.footer-help-icon').bt();
    $('.footer-social ul li a').bt({
        positions : [ 'bottom'] ,
        heightAdjustment : 5
    });

    $(document).ready(function() {
        $("a#hfB2BContinueShoppingLink").attr("href",$("a#hfLifetechLogoLink").attr("href"));
	 });
</script>

<script type="text/javascript">
function punchoutForFeedback() {
    if(getIsB2B() || getIsB2BCustomerType()) {
        var punchoutMessage = $('#punchoutMessage').html();
        var punchoutOk = $('#punchoutOk').html();
        var punchoutCancel = $('#punchoutCancel').html();
        $.ivgnStringModal("<div style=\"padding-bottom: 20px;\">" + punchoutMessage + "</div>" +
                          "<button type=\"button\" class=\"button primary-button\" style=\"font-size: 16px; color: white;\" onclick=\"k_config.actionOpenFFButtonLT(); $.modal.close();\">" + punchoutOk + "</button>&nbsp;" +
                          "<button type=\"button\" class=\"button secondary-button\" style=\"font-size: 16px; color: white;\" onclick=\"$.modal.close();\">" + punchoutCancel + "</button>", {width: 500, height: 200});
    }else{
		k_config.actionOpenFFButtonLT();
	}
}

</script>

