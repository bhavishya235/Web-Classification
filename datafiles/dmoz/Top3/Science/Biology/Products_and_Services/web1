    <script type="text/javascript">
        function findPos(obj) {
            var obj2 = obj;
            var curtop = 0;
            var curleft = 0;
            if (document.getElementById || document.all) {
                do {
                    curleft += obj.offsetLeft - obj.scrollLeft;
                    curtop += obj.offsetTop - obj.scrollTop;
                    obj = obj.offsetParent;
                    obj2 = obj2.parentNode;
                    while (obj2 != obj) {
                        curleft -= obj2.scrollLeft;
                        curtop -= obj2.scrollTop;
                        obj2 = obj2.parentNode;
                    }
                } while (obj.offsetParent)
            } else if (document.layers) {
                curtop += obj.y;
                curleft += obj.x;
            }
            return [curtop, curleft];
        } 

        function CollapseAll(){
            for (i=0; i<dropDownMenus.length; i++) {
                var temp = dropDownMenus[i];
                var img = temp[0];
                var src = temp[1];
                var div = temp[2];
                img.src = src;
                div.style.visibility = "hidden";
                div.style.height = "0px";
            }
        }

        function ToggleMenuMainItems() {

            var collapsedSrc = "images/menu.jpg";
            var expandedSrc = "images/menu_checked.jpg";
            if (!e) var e = window.event;
            var c = e.srcElement;
            if (c.src.indexOf(collapsedSrc) == -1) {
                CollapseAll();
            }
            else {
                CollapseAll();
                c.src = expandedSrc;
                var pos = findPos(c);
                var itemContainer = document.getElementById("MainItems");
                itemContainer.style.width = c.offsetWidth + "px";
                if (itemContainer.style.position == "relative")
                {
                    itemContainer.style.left = "0px";
                    itemContainer.style.top = "0px";
                }
                else
                {
                    itemContainer.style.left = pos[1] + "px";
                    itemContainer.style.top = (pos[0] + c.offsetHeight) + "px";
                }
                itemContainer.style.visibility = "visible";
                itemContainer.style.height = "";
            }
        }
    </script>
    
