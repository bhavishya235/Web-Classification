function SetSame() {
	document.forms[0].bfirstname.value = document.forms[0].firstname.value;
	document.forms[0].blastname.value = document.forms[0].lastname.value;
	document.forms[0].baddress1.value = document.forms[0].address1.value;
	document.forms[0].baddress2.value = document.forms[0].address2.value;
	document.forms[0].bcity.value = document.forms[0].city.value;
	//document.forms[0].bstate_list.selectedindex = document.forms[0].state_list.selectedindex;
	document.forms[0].bstate.value = document.forms[0].state.value;
	//document.forms[0].bcountry.value = document.forms[0].country.value;
	document.forms[0].bzipcode.value = document.forms[0].zipcode.value;
	document.forms[0].bphone.value = document.forms[0].phone.value;
	document.forms[0].bemail.value = document.forms[0].email.value;
	var jhn = document.forms[0].bstate_list.length;
	//alert(count);
	//alert(x_count);
	var jhm = document.forms[0].country.length;
	//alert(jhm);
	//alert(jhn);
	//alert(document.paymentForm.country.options[22].value);
	for(var i=0; i< jhn; i++){
		if(document.forms[0].bstate_list.options[i].value == document.forms[0].state_list.value){
			document.forms[0].bstate_list.options[i].selected = true;
		}
	}
	for(var i=0; i< jhm; i++){
		if(document.forms[0].bcountry.options[i].value == document.forms[0].country.value){
			document.forms[0].bcountry.options[i].selected = true;
		}
	}
}
function mask(end,middle,start,subject){
var one ='mai';
var two='lto:';
var three='?Subject=';
//start,middle,end,subject;
var putogether= one+two+start+middle+end+three+subject;
document.location.href=eval('"'+putogether+'"');
window.status=putogether;
}


//-->
</script>
