<script language="Javascript" type="text/javascript">
//<![CDATA[
    myPix = new Array("/prod/groups/cfans/@pub/@cfans/@agronomy/documents/asset/cfans_asset_287276.jpg", "/prod/groups/cfans/@pub/@cfans/@agronomy/documents/asset/cfans_asset_287277.jpg", "/prod/groups/cfans/@pub/@cfans/@agronomy/documents/asset/cfans_asset_287279.jpg")
    imgCt = myPix.length 

    function choosePic() {
        if (document.images) {
            randomNum = Math.floor((Math.random() * imgCt))
            document.myPicture.src = myPix[randomNum]
        }
    }
//]]>
</script>



