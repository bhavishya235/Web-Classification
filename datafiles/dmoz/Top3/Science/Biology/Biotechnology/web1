<script type="text/javascript">
(function($) {
    var timer = 0;
    var hover = '';
    var $links = $("#collaplinks-21479 .collaplinks-link a");
    var $contents = $("#collaplinks-21479 .collaplinks-content > div");
    $contents.hide();
        
    $links.each(function() {
        var sel = $(this).attr('href');
        $(this).add($(sel)).hover(function() {
            if(timer != 0) window.clearTimeout(timer);
            timer = 0;
            if(hover != sel) {
                $contents.stop(true, true).hide();
                if(hover == '') {
                    $(sel).delay(500).slideDown(600);
                } else {
                    $(sel).show();
                }
                $links.removeClass('hover');
                $links.filter('a[href='+sel+']').addClass('hover');
                hover = sel;
            }
        }, function() {
            if(timer != 0) window.clearTimeout(timer);
            timer = window.setTimeout(function() {
                timer = 0;
                $contents.stop(true, true).slideUp(600);
                $links.removeClass('hover');
                hover = '';
            }, 200);
            return false;
        });
    });
    
    $links.click(function(){return false;});
})(jQuery);
</script>
</div>
    </div>
</div>
