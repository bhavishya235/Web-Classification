        var spotlight = new Glider('profiles', {duration:0.5, frequency:15.0});
        spotlight.start();
        var controls = $('spotlight-controls')
        controls.innerHTML = '<a class="prev left" href="#">Previous</a><a class="next right" href="#">Next</a><p>&nbsp;</p>';
