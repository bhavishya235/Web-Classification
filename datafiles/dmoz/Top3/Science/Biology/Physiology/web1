<script language="JavaScript"><!-- Hide from JavaScript-Disabled Browsers


function getFullYear(n)
{
  var yr = n.getYear()
  if (yr < 1000) yr += 1900
  return yr
}

function adddate() {
var isnMonths = new Array("January","February","March","April","May","June","July","August","September","October","November","December");
var isnDays   = new Array("Sunday","Monday","Tuesday","Wednesday","Thursday","Friday","Saturday","Sunday");
var now       = new Date();
var day       = now.getDate();
var month     = now.getMonth();
var year      = getFullYear(now);
var dname     = isnDays[now.getDay()];
var mname     = isnMonths[now.getMonth()]
document.write(dname + ", " + mname + " " + day + ", " + year + "&nbsp;&nbsp;");
}
var today     = new Date();
hrs=today.getHours();
min=today.getMinutes();
sec=today.getSeconds();
clckh=""+((hrs>12)?hrs-12:hrs);
clckm=((min<10)?"0":"")+min;
clcks=((sec<10)?"0":"")+sec;
clck=(hrs>=12)?"P.M.":"A.M.";
// End Hiding -->
</script>
