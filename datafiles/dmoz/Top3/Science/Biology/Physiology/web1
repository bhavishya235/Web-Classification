            }
            if (txtgetEvents == 'Yes'){ // 2011Dec06 Waiyee Lai - Keeps option value, Yes means nTreePoint
                $.get(
                    "/include/splash-feeds1-5-jquery.php",
                    {feed: '1398', cutoff_story: 50, cutoff_title: 23, source_type: 'NTreePointEvents', cat_heading: 'Events'}, 
                    function(responseText){
                        $("#eventContent").html(responseText);
                    },
                    "html"
                );
            } 
            if (txtgetEvents == 'ActiveData') { // 2011Nov28 Waiyee Lai - added for Active Data link
                $.get(
                    "/include/splash-feeds1-5-jquery.php",
                    {feed: '&lt;A href=&quot;http://events.umanitoba.ca/RSSSyndicator.aspx?type=N&amp;number=5&amp;category=47-131&amp;sortorder=ASC&quot;&gt;&lt;img src=&quot;http://events.umanitoba.ca/images/rss_1.gif&quot; border=&quot;0&quot; /&gt;&lt;/A&gt;', cutoff_story: 50, cutoff_title: 23, source_type: 'ActiveData', cat_heading: 'Events'}, 
                    function(responseText){
                        $("#eventContent").html(responseText);
                    },
                    "html"
                );
            }
    });
    
 </script>        


<script language="javascript" type="text/javascript">
function init()
{
    txtgetEvents = 'ActiveData';
    // check slideshowImagePath before calling the function
    if (''.length !="") { 
    slideshow('','#slideshowlinks div','http://umanitoba.ca/include/LandingPageImageRotate_v4.php','#slider');  
       
    }
}
</script>

 
</head>
