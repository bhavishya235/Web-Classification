pageTracker._trackPageview();} catch(err) {}</script>


            


<script type="text/javascript">


var extDoc = [".pdf",".swf",".flv",]; // liste des extensions Ã  suivre
function addLinkerEvents() {
    
    var as = document.getElementsByTagName("a"); // on rÃ©cupÃ¨re toutes les balises <a>
    
    for(var i=0; i<as.length; i++) {
        var flag = 0;
        var tmp = as[i].getAttribute("onclick");

        // IE6-IE7 fix (null values error) with thanks to Julien Bissonnette for this
        if (tmp != null) {
            tmp = String(tmp);
            if (tmp.indexOf('urchinTracker') > -1 || tmp.indexOf('_trackPageview') > -1) continue;
        }
        
        for (var j=0; j<extDoc.length; j++) {
            if (as[i].href.indexOf(extDoc[j]) != -1) {
                as[i].onclick = function(){ var splitResult = this.href.split('/'); pageTracker._trackEvent('Telechargements', 'Fichiers', splitResult[splitResult.length-1]); }
                break;
            }
        }

    }
}

</script>

	</body>
</html>









