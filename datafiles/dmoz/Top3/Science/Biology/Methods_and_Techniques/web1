<script type="text/javascript" src="http://www.lifetechnologies.com/apps/lifetech/js/countriesInfo.js"></script>

<script type="text/javascript">

	///  ----  Exceptions objects   ----
	var InvalidUrlException = function (message)
	{
		this.type = "InvalidUrlException";
		this.message = message;
	};

	var DOMElemNotFoundException = function (message)
	{
		this.type = "DOMElemNotFoundException";
		this.message = message;
	};
	/// END OF Exceptions objects

	ltHeader = new function()
	{
        this.scmsEuLogoLinkHref = 'https://commerce.lifetechnologies.com/index.cfm?fuseaction=customersite.schome';
        this.scmsEuLogoImageIe6Path = 'https://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/supplycentre-logo-blue.png';
        this.scmsEuLogoImagePath = 'http://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/supplycentre-logo-blue.png';
        this.scmsEuLogoImagePrintPath = 'http://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/logo-black.png';
        this.scmsShipToLabLogoLinkHref = 'http://www.lifetechnologies.com/in/en/home/order';
        this.scmsShipToLabLogoImageIe6Path = 'https://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/life-logo.png';
        this.scmsShipToLabLogoImagePath = 'http://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/life-logo.png';
        this.scmsShipToLabLogoImagePrintPath = 'http://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/logo-black.png';
        this.scmsEuShipToLabLogoLinkHref = 'http://www.lifetechnologies.com/in/en/home/order';
        this.scmsEuShipToLabLogoImageIe6Path = 'https://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/life-logo.png';
        this.scmsEuShipToLabLogoImagePath = 'http://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/life-logo.png';
        this.scmsEuShipToLabLogoImagePrintPath = 'http://www.lifetechnologies.com/content/dam/LifeTech/Images/Header/logo-black.png';

        this.scmsNavLinkTextForEU = 'Supply Centre';
        this.scmsNavUnlockLinkTextForEU = 'Unlock From Secure Storage Location';
        this.scmsNavChangeLocationLinkTextForEU = 'Change My Location';
        this.scmsNavSetStartPageLinkTextForEU = 'Supply Centre';
        this.scmsNavSupplyCenterHomeLinkTextForEU = 'Supply Centre Home';
        this.scmsNavSupplyCenterHomeForLabLinkTextForEU = 'Supply Centre Home';

        this.utilityBarSignInForwardLinkHref = 'http://www.lifetechnologies.com';

        //If in author mode the CK_ISO_CODE and CK_LANG_CODE are not set automatically
        this.defaultCookies = function()
        {
            if (!getCookie('CK_ISO_CODE')) {
                $.cookie('CK_ISO_CODE', 'us', {path: '/', domain:'.lifetechnologies.com', expires: 90});
            }
            else if (!getCookie('CK_LANG_CODE')) {
                $.cookie('CK_LANG_CODE', 'en', {path: '/', domain:'.lifetechnologies.com', expires: 90});
            }
        };

		//sets up an object that defines publicly accessible functions that can be used to modify the syndicated header
		//NOTE: this object comprises an API that is used inside and outside the header
		this.hideMiniCart = function()
		{
			//hides the minicart button in the commerce utility bar
			hideElementById("miniCartButton");
		};

		this.hideMyCustomProducts = function()
		{
			//hides the "My Custom Products" menu item in the commerce utility bar under the "my account" menu
			hideElementById("hfMyCustomProductsLi");
		};

		this.hideMySettings = function()
		{
			//hides the "My Settings" menu item in the commerce utility bar under the "my account" menu
			hideElementById("hfMySettingsLi");
		};

        this.hideSignIn = function()
        {
            //hides the "Sign In" menu item in the commerce utility bar
            hideElementById("hfSignInLi");
			hideElementById("hfSignInHr");
        };
		this.hideMyAccountOptions = function()
        {
            //hides the "My Account Drop downs" menu item in the commerce utility bar
            hideElementById("hfMyAccountOptions");
        };
		this.hideMyAccountMenu = function()
        {
            //hides the "Sign In" menu item in the commerce utility bar
		   var isoCode = getCookieOrDefault("CK_ISO_CODE", "us");
            if (stringEqualsIgnoreCase(ltCountryInfo[isoCode].signInHidden, 'true') && stringEqualsIgnoreCase(ltCountryInfo[isoCode].registerHidden, 'true'))
            {
				hideElementById("hfGreetByName");
				hideElementById("hfGreetAnonymous");
            }
        };

        this.hideRegister = function()
        {
            //hides the "Register" menu item in the commerce utility bar
            hideElementById("hfRegisterLi");
        };

        this.hideScmsMenu = function()
        {
            hideElementById("scms-home");
            hideElementById("hfScmsSettings");
            hideElementById("scms-unlock");
            hideElementById("scms-change-location");
            hideElementById("scms-set-start");
        };

        this.hideScmsHomeForScLink = function()
        {
            hideElementById("hfScmsHomeLink");
        };

        this.hideScmsChangeLocationLink = function()
        {
            hideElementById("scms-change-location");
        };

        this.hideScmsAlertIcons = function()
        {
            hideElementsByClassName("scms-alert-icon");
        };

        this.hideScmsStartPageLink = function()
        {
            hideElementById("scms-set-start");
        };

        this.hideScmsUnlock = function()
        {
            hideElementById("scms-unlock");
        };

        this.showScmsShipToLab = function()
        {
            showElementById("hfScmsShipToLab");
        };

        this.showScmsShipToSC = function()
        {
            var maxChars = 45;
            var scidSCName = getUnescapedCookie("scidSCName");
            var fullLabelText = $("#hfScmsShipToSC").text() + " : " + scidSCName;
            var shortLabelText = $("#hfScmsShipToSC").text() + " : " + shortenString(scidSCName, maxChars);
            if (!stringEquals(fullLabelText, shortLabelText))
            {
				$("#hfScmsShipToSC").text(shortLabelText);
                $('#hfScmsShipToSC').bt(fullLabelText,
                {
                    positions : ['bottom'],
                    heightAdjustment : 5,
                    offsetParent : '.search',
                    cssStyles : {color : 'black'}
                });
            }
            else
            {
                $("#hfScmsShipToSC").text(fullLabelText);
            }
            showElementById("hfScmsShipToSC");
        };

        this.showScmsShipToSCForEU = function()
        {
            var maxChars = 45;
            var scidSCName = getUnescapedCookie("scidSCName");
            var fullLabelText = $("#hfScmsShipToSCForEU").text() + " : " + scidSCName;
            var shortLabelText = $("#hfScmsShipToSCForEU").text() + " : " + shortenString(scidSCName, maxChars);
            if (!stringEquals(fullLabelText, shortLabelText))
            {
				$("#hfScmsShipToSCForEU").text(shortLabelText);
                $('#hfScmsShipToSCForEU').bt(fullLabelText,
                {
                    positions : ['bottom'],
                    heightAdjustment : 5,
                    offsetParent : '.search',
                    cssStyles : {color : 'black'}
                });
            }
            else
            {
                $("#hfScmsShipToSCForEU").text(fullLabelText);
            }
            showElementById("hfScmsShipToSCForEU");
        };

        this.showScmsHomeForLabLink = function()
        {
            showElementById("hfScmsHomeForLabLink");
        };

        this.hideSignInBasedOnCountry = function()
        {
            var isoCode = getCookieOrDefault("CK_ISO_CODE", "us");
            if (stringEqualsIgnoreCase(ltCountryInfo[isoCode].signInHidden, 'true'))
            {
                this.hideSignIn();
                this.hideMyAccountOptions();

            }
        };

        this.hideRegisterBasedOnCountry = function()
        {
            var isoCode = getCookieOrDefault("CK_ISO_CODE", "us");
            if (stringEqualsIgnoreCase(ltCountryInfo[isoCode].registerHidden, 'true'))
            {
                this.hideRegister();
            }
        };

        this.getCustInfoDisplayType = function()
        {
        	var ckCustInfo = getCookie('CK_CUSTOMER_INFO');
        	var arr = ckCustInfo.match(/displayType(?:%3A|\:)([a-zA-Z]*)/i);
        	console.log(arr);
        	// if there was a match, arr should be like ['displayType:cmgtpscms','cmgtpscms']
        	if(arr) {
            	var displayType = arr[1];
            	// displayType should be something like cmgtpscms
            	return displayType;
        	}
        	// fallback to CK_DISPLAY_TYPE
        	return getCookie('CK_DISPLAY_TYPE');
        }

        this.showAppropriateScmsElements = function()
        {
            var displayType = getCookie("CK_DISPLAY_TYPE");
            var scShipType = getCookie("scShipType");
            var scStartPage = getCookie("scStartPage");
            var scList = getCookie("scList");
            var displayChangeSC = getCookie("displayChangeSC");
            var isScKiosk = getCookie("ISSCKIOSK");
            var isSecuredStorageAccess = getCookie('isSecuredStorageAccess');
            var isoCode = getCookieOrDefault("CK_ISO_CODE", "us");
            var scCustomerId = getCookie('CUSTOMERID');
	        var scChangeLocationUrl = $('#scms-change-location').find('a').attr('href') + scCustomerId + "#changeSCLocation";
	        var scSetStartPage = $('#scms-set-start').find('a').attr('href')+"&UserKey=" + scCustomerId + "#SCLocationSelected";
            var euCountry = ltCountryInfo[isoCode].euCountry;

            if (stringEqualsIgnoreCase(displayType, 'cmgtsc') || stringEqualsIgnoreCase(displayType, 'cmgtpscms') || stringEqualsIgnoreCase(displayType, 'pscmsmaster'))
            {
                if(stringEqualsIgnoreCase(scShipType, "SC"))
                {
                    if (stringEqualsIgnoreCase(euCountry, 'true'))
                    {
                        $("#hfLifetechLogoLink").attr('href', this.scmsEuLogoLinkHref);
                        $("#hfLifetechLogoIe6Image").attr('src', this.scmsEuLogoImageIe6Path);
                        $("#hfLifetechLogoImage").attr('src', this.scmsEuLogoImagePath);
                        $("#hfLifetechLogoPrintImage").attr('src', this.scmsEuLogoImagePrintPath);

                        $("#hfScmsMenuLinkText").text(this.scmsNavLinkTextForEU);
                        $("#hfScmsUnlockLink").text(this.scmsNavUnlockLinkTextForEU);
                        $("#hfScmsChangeLocationLink").text(this.scmsNavChangeLocationLinkTextForEU);
                        $("#hfScmsSetStartLink").text(this.scmsNavSetStartPageLinkTextForEU);
                        $("#hfScmsHomeLink").text(this.scmsNavSupplyCenterHomeLinkTextForEU);

                        this.showScmsShipToSCForEU();
                    }
                    else
                    {
                        this.showScmsShipToSC();
                    }
                }
                else if (stringEqualsIgnoreCase(scShipType, 'LAB') || !scShipType)
                {
                    if (stringEqualsIgnoreCase(euCountry, 'true'))
                    {
                        $("#hfLifetechLogoLink").attr('href', this.scmsEuShipToLabLogoLinkHref);
                        $("#hfLifetechLogoIe6Image").attr('src', this.scmsEuShipToLabLogoImageIe6Path);
                        $("#hfLifetechLogoImage").attr('src', this.scmsEuShipToLabLogoImagePath);
                        $("#hfLifetechLogoPrintImage").attr('src', this.scmsEuShipToLabLogoImagePrintPath);

                        $("#hfScmsMenuLinkText").text(this.scmsNavLinkTextForEU);
                        $("#hfScmsUnlockLink").text(this.scmsNavUnlockLinkTextForEU);
                        $("#hfScmsChangeLocationLink").text(this.scmsNavChangeLocationLinkTextForEU);
                        $("#hfScmsSetStartLink").text(this.scmsNavSetStartPageLinkTextForEU);
                        $("#hfScmsHomeForLabLink").text(this.scmsNavSupplyCenterHomeForLabLinkTextForEU);
                    }
                    else
                    {
                        $("#hfLifetechLogoLink").attr('href', this.scmsShipToLabLogoLinkHref);
                        $("#hfLifetechLogoIe6Image").attr('src', this.scmsShipToLabLogoImageIe6Path);
                        $("#hfLifetechLogoImage").attr('src', this.scmsShipToLabLogoImagePath);
                        $("#hfLifetechLogoPrintImage").attr('src', this.scmsShipToLabLogoImagePrintPath);
                    }

                    if (stringEqualsIgnoreCase(displayType, 'cmgtpscms'))
                    {
                        //this.hideScmsMenu();
						this.hideScmsUnlock();
                        this.hideScmsStartPageLink();
                        this.hideScmsChangeLocationLink();
 						hideElementById("scms-home");
                        hideElementById("hfScmsSettings");
                    }
                    else
                    {
                        this.hideScmsHomeForScLink();
                        this.showScmsHomeForLabLink();
                        this.showScmsShipToLab();
                    }
                }
            }

            if (!scStartPage || stringEquals(scStartPage, '0'))
            {
                this.hideScmsStartPageLink();
            }
            else
            {
                $('#scms-set-start').find('a').attr("href", scSetStartPage);
            }

            if (!displayChangeSC || stringEquals(displayChangeSC, '0') || !scList || stringEquals(scList, '0'))
            {
                this.hideScmsChangeLocationLink();
            }
            else
            {
                $('#scms-change-location').find('a').attr("href", scChangeLocationUrl);
            }

            if (!isScKiosk || stringEquals(isScKiosk, '0') || !isSecuredStorageAccess || stringEquals(isSecuredStorageAccess, '0'))
            {
                this.hideScmsAlertIcons();
                this.hideScmsUnlock();
            }
        };

        this.syncHomeBreadcrumbLink = function()
        {
            var hfLifetechLogoLinkHref = $("#hfLifetechLogoLink").attr('href');
	        var breadCrumbLink = $('.breadcrumbs li:first a');
	        breadCrumbLink.attr('href', hfLifetechLogoLinkHref);
        };

        // set the fowardURL param to the appropriate link based on the current domain
        this.initSignInReturnUrl = function()
        {
            var signInLink =  $('#hfSignInLink');
            //#23928: Need an explicit check for signInLink.length !=0, to avoid an expcetion in IE
            if (isLifetechSite() && signInLink!=null && signInLink
            		&& signInLink.length!=0 && this.utilityBarSignInForwardLinkHref && $.trim(this.utilityBarSignInForwardLinkHref).length > 0)
            {
                this.setReturnUrlToPage(this.utilityBarSignInForwardLinkHref);
            }
        };

        // set the returnURL param to the sign-in link to the current page
        this.setReturnUrlToCurrentPage  = function ()
		{
        	//set current URL as return page
        	this.setReturnUrlToPage(window.location.href);
		};

		// set the returnURL param to the sign-in link to the specified page
        this.setReturnUrlToPage= function (url)
		{
        	if (!validUrl(url))
        	{
        		throw new InvalidUrlException("The specified return url is not valid: " + url);
        	}

        	var SIGNIN_LINK_ID = "hfSignInLink";						//id of the sign-in link
        	var RETURN_URL_PARAM_NAME = "forwardURL";					//param name for the return URL
        	var signInLink =  document.getElementById(SIGNIN_LINK_ID);	//

        	if (signInLink == null || signInLink.href.indexOf("cmd=partnerMktLogin") === -1)
        	{
        		throw new DOMElemNotFoundException("SignIn link not found or has an invalid href");
        	}

        	var signInLinkUrl = signInLink.href;
        	var lastSignInChar = signInLinkUrl.charAt(signInLinkUrl.length-1);
        	if (signInLinkUrl.indexOf('?') === -1)	//question mark not found - appending it
        	{
        		signInLinkUrl = signInLinkUrl + '?';
        	} else if (lastSignInChar !== '&' && lastSignInChar !== '?')
        		signInLinkUrl = signInLinkUrl + '&';

        	//set the new link with the return URL
        	signInLink.href = signInLinkUrl+RETURN_URL_PARAM_NAME+"="+encodeURIComponent(url);

		};

       	function validUrl(url)
    	{
       		var validUrl = true;

       		//validate URL is string, not empty  and starts with HTTP:// or HTTPS://
       		if (	(typeof url !== 'string') ||  !url.match(/^http(s)?:\/\//gi)	 )
       		{
       			validUrl = false;
       		}
       		return validUrl;
    	}

		function hideElementById(idString)
		{
			var styleString = "#" + idString + "{display: none !important;}";
			addStyle(styleString);
		}

        function showElementById(idString)
        {
            var styleString = "#" + idString + "{display: block !important;}";
            addStyle(styleString);
        }

        function hideElementsByClassName(className)
        {
            var styleString = "." + className + "{display: none !important;}";
            addStyle(styleString);
        }

		function getCookie(name)
		{
			var cname = name + '=';
			var dc = document.cookie;
			if (dc == null) return '';
			if (dc.length > 0)
			{
				var begin = dc.indexOf(cname);
				if (begin != -1)
				{
					begin += cname.length;
					var end = dc.indexOf(';', begin);
					if (end == -1) end = dc.length;
					return dc.substring(begin, end);
				}
			}
			return '';
		}

        function getCookieOrDefault(name, defaultVal)
        {
            var cookieVal = getCookie(name);
            if (!cookieVal)
            {
                cookieVal = defaultVal;
            }
            return cookieVal;
        }

        function getUnescapedCookie(name)
		{
			var cname = name + '=';
			var dc = document.cookie;
			if (dc == null) return '';
			if (dc.length > 0)
			{
				var begin = dc.indexOf(cname);
				if (begin != -1)
				{
					begin += cname.length;
					var end = dc.indexOf(';', begin);
					if (end == -1) end = dc.length;
					return unescape(dc.substring(begin, end));
				}
			}
			return '';
		}

		function addStyle(styleText)
		{
			var headDomObj = document.getElementsByTagName("head")[0];
			var styleDomObj = document.createElement("style");
			var rules = document.createTextNode(styleText);
			styleDomObj.type = "text/css";

			if (styleDomObj.styleSheet) //IE
			{
				styleDomObj.styleSheet.cssText = rules.nodeValue;
			} else
			{
				styleDomObj.appendChild(rules);
			}

			headDomObj.appendChild(styleDomObj);
		}

        function stringEquals(s1, s2)
        {
            if (typeof s1 == 'string' && typeof s2 == 'string')
            {
                return s1 === s2;
            }

            return false;
        }

        function stringEqualsIgnoreCase(s1, s2)
        {
            if (typeof s1 == 'string' && typeof s2 == 'string')
            {
                return s1.toUpperCase() === s2.toUpperCase();
            }

            return false;
        }

        // Take a string and truncate it if the length exceeds maxChars.  If so append an ellipse at the end.
        function shortenString(s, maxChars)
        {
            var overflowString = '...';

            if (typeof s !== 'string' || typeof maxChars !== 'number')
            {
                return '';
            }

            if (s.length > maxChars)
            {
                return s.substr(0, (maxChars - overflowString.length)) + overflowString;
            }

            return s;
        }

        function isLifetechSite() {
            return window.location.hostname.indexOf('lifetechnologies.com') > -1 ||
                   window.location.hostname.indexOf('f3daya01t.test.life') > -1 ||
                   window.location.hostname.indexOf('f3d5abq01t.test.life') > -1 ||
                   window.location.hostname.indexOf('f3d5asq01t.test.life') > -1 ||
                   window.location.hostname.indexOf('f1dy5wa02p.corp.life') > -1 ||
                   window.location.hostname.indexOf('f3d5aqa01t.test.life') > -1 ||
                   window.location.hostname.indexOf('localhost') > -1;
        }
	};

    ltHeader.defaultCookies();
    ltHeader.hideSignInBasedOnCountry();
    ltHeader.hideRegisterBasedOnCountry();
    ltHeader.hideMyAccountMenu();
    ltHeader.showAppropriateScmsElements();
    //ltHeader.initSignInReturnUrl();

    (function applyBgiframeToCommerceBar()
	{
		if ($.fn.bgiframe)
		{
			$("#hfGlobalCommerceBar").bgiframe();
		}
	})();

	(function setUpLtHeaderAPI() {
		//this function uses ltHeaderConfig as a preferences object example:
		//ltHeaderConfig = {minicart: false}; //runs ltHeader.hideMiniCart() which hides the minicart button in the commerce utility bar
		if (typeof ltHeader != "object") {
			throw "ltHeader must exist before calling this function";
		}

		if (!configFileExists()) {
			return false;
		} else {
			checkHideMiniCart()
		}

		function configFileExists() {
			return typeof ltHeaderConfig === "object";
		}

		function checkHideMiniCart() {
			if (ltHeaderConfig.minicart === false) {
				ltHeader.hideMiniCart();
			}
		}
	})();
</script>


