    function initializeMiniAudioPlayer(){
         jQuery("a[href*='.mp3'] ,a[href*='.m4a']").not(".map_excuded").mb_miniPlayer({
				inLine:true,
                width:"200",
				skin:"gray",
				animate:true,
				volume:1,
				autoplay:false,
				showVolumeLevel:true,
				showTime:true,
				id3:false,
				showRew:true,
				downloadable:false,
				downloadPage:"http://www.savingantiquities.org/wp-content/plugins/wp-miniaudioplayer/map_download.php",
				swfPath:"http://www.savingantiquities.org/wp-content/plugins/wp-miniaudioplayer/js/"
		});
    }

	jQuery(function(){
          initializeMiniAudioPlayer();
          jQuery(document).ajaxSuccess(function(event, xhr, settings) {
           	initializeMiniAudioPlayer();
          });
	});
	</script>
