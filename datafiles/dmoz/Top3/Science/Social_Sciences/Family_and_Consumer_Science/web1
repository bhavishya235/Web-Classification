$(document).ready(function() {$('#nav-main .has-sub').removeClass('has-sub');
$('#nav-main .dropdown').each(function() {$(this).parent('.nav-item').addClass('has-sub');});







$('#email_signup').ajaxForm({
	beforeSubmit: function (arr, $form, options){
    	return $('#email_signup').valid();
    },
	dataType: 'json',
	success: function(resp, stat, xhr, $form){
    	if (typeof(resp) != 'undefined')
        {
        	if (resp.status == 'success')
            {
            	$('.email-success').show();
                $('#freeform_email_address').val('');
                return;
            }
        }
        $('.email-error').show();
        return false;
    }
});

if ($('.system-msg .close').length) {
	$('.close').click(function() {
    	$(this).parent('.msg').parent('.system-msg').slideUp(200);
        return false;
    });
}

$('#email_signup').validate({
    rules: {
        email_address: {
            required: true,
            email: true
        }
    },
    messages: {
    	email_address: "Please enter a valid email address"
    }
});
	 $(".slides").slidesjs({
       width: 614,
       height: 320,
       play: {
            interval: 5000,
            effect: 'fade',
            auto: true
        },
        pagination: {
          effect: 'fade'
        },
        effect: {
            fade: {
                speed: 800,
                crossfade: true
            }
        }

});
});
</script>



