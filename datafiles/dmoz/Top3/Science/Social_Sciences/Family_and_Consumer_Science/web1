<script type="text/javascript">
  Drupal.behaviors.myModuleBehavior = function (context) {
    $('#header li').hover(function() {
    	$(this).addClass('ie-hover');
    }, function() {
    	$(this).removeClass('ie-hover');
    });

    $('#sidebar-left .block:first').addClass('first');
    $('#sidebar-left .block:last').addClass('last');
    $('#sidebar-right .block:last').addClass('last');
    $('.event:last').addClass('last');
    $('#hd .nice-menu').find('> li:first').addClass('first');
    $('#hd .nice-menu').find('> li:last').addClass('last');
    $('#hd .nice-menu a.active').parents('li').addClass('active-trail');

    var home_col_height = ( Math.max($('#main').height(), $('#sidebar-left').height(), $('#sidebar-right').height()) );
    $('#main').css({'min-height' : home_col_height});

  };
</script>
