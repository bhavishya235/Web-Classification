  if ($(this).parent().hasClass('on')) {
    $(this).parent().removeClass('on');
    $(this).parent().next().toggle();
    $("#login_extra").toggle();
    return false;
  }
  else { 
    $(this).parent().addClass('on');
    $(this).parent().next().toggle();
    $("#login_extra").toggle();
    return false;
   }
  });
});
</script>

