	<script type="text/javascript">
	 

//added pause on hover	
function mycarousel_initCallback(carousel)
{
	carousel.clip.hover(function() {
	    carousel.stopAuto();
	}, function() {
	    carousel.startAuto();
	});
};



$(document).ready(function() {
	
	// scrollende content	  

	$("ul.webshop-caroussel").jcarousel({
	    visible: 1,        
	    scroll: 1,        
	    auto: 5,
	    wrap: 'circular',
		animation:300,
		hoverPause: true,
		initCallback: mycarousel_initCallback	    
	});


	
	$(".view-actueel .view-content").jcarousel({
	    vertical: true,    
	    visible: 4,        
	    scroll: 1,        
	    auto: 5,
	    wrap: 'circular'
	});	
	
	$(".tweets-pulled-listing ul").jcarousel({
	    vertical: true,    
	    visible: 2,        
	    scroll: 1,        
	    auto: 8,
	    wrap: 'circular'
	});	
	
	
	$(".view-partners .view-content").jcarousel({
	    visible: 6,        
	    scroll: 1,        
	    auto: 5,
	    wrap: 'circular'
	});


  
	// Voor het al dan niet tonen van welkomstgeschenk
	if ($('#uc-cart-checkout-form td.products:contains("abonnement")').length > 0) {
	    $("#extra_information-pane").addClass("show");
	}
	
	if ($('#uc-cart-checkout-form td.products:contains("subscription")').length > 0) {
	    $("#extra_information-pane").addClass("show");
	}
	    
	// 	extra body classes toevoegen	    
	var url = window.location.href; 
	if (url.indexOf("webwinkel/boekman-tijdschrift")>0) {
	$("body").addClass("boekmantijdschrift");
	}
	if (url.indexOf("webshop/boekman-journal")>0) {
	$("body").addClass("boekmantijdschrift");
	}
	if (url.indexOf("comment/reply")>0) {
	$("body").addClass("comment");
	}
	  
	//  Deze adresvelden in de chekout vertalen niet via interface
	$('.i18n-nl .address-pane-table tr:eq(4) td:contains("Address:")').text( "Adres:" ); 
	$('.i18n-nl .address-pane-table tr:eq(13) td:contains("Address:")').text( "Adres:" ); 
	

});
	</script>
