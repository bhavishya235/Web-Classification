<script type="text/javascript">
// <![CDATA[
var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-798015-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
  

$(function () {
  var track = function(x,y) {
    _gaq.push(['_trackEvent', 'Homepage - Monet 2.0', x, y]);
  };
  
  var track_region = function (selector, name) { 
    $(selector).live('click', function(e){
      var _t = $(this);
      track(name,  (_t.text() || _t.children('img:first').attr('alt')));
    })
  };
  
   track_region('#logo a', 'Logo');
   track_region('#secondaryGlobalNav li a', 'Secondary Nav Link');
   track_region('.subLinks li a', 'Secondary Sub Nav Link');
   track_region('.globalNavigation ul li a', 'Primary Navigation Link');
   track_region('#quickLinks-menu li a', 'Quicklinks Links');
