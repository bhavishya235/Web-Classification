<!-- Begin Container -->  
<div id="container" class="home">        
  <!-- Begin Header -->      
  <div id="header" class="home">
    <div id="masthead">
      <h1><img src="images/logo.gif" id="logo" width="600" height="96" alt="Department of Ethnic Studies, UC Berkeley" /></h1>
      <form action="http://ethnicstudies.berkeley.edu/search.php" id="cse-search-box" class="text right">
      <div>
      <input type="hidden" name="cx" value="005611847287229241580:z75x1l-_atm" />
      <input type="hidden" name="cof" value="FORID:11" />
