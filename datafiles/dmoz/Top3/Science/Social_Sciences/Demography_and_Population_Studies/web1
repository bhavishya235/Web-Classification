                        <SCRIPT language=JavaScript>
<!-- Begin
var how_many_ads = 17;
var now = new Date()
var sec = now.getSeconds()
var ad = sec % how_many_ads;
ad +=1;
if (ad==1) {
url="english/modules/social/pgr/index.html";
alt="Did you know&#8230;?";
banner="images/did-you-know01.gif";
width="130";
height="162";
}
if (ad==2) {
url="english/modules/social/pgr/index.html";
alt="Did you know&#8230;?";
banner="images/did-you-know02.gif";
width="130";
height="162";
}
if (ad==3) {
url="english/modules/social/pgr/index.html";
alt="Did you know&#8230;?";
banner="images/did-you-know03.gif";
width="130";
height="162";
}
if (ad==4) {
url="english/modules/social/life/index.html";
alt="Did you know&#8230;?";
banner="images/did-you-know04.gif";
width="130";
height="162";
}
if (ad==5) {
