<SCRIPT language=Javascript>
function verify()
{
	if ((document.vote.elements[0].checked)||(document.vote.elements[1].checked))
	{
		window.open("","votewindow","left=300,top=300,height=350,width=500,status=no,toolbar=no,menubar=no,location=no");
		document.vote.submit();
	}
}
</SCRIPT>

