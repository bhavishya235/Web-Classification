<title>Census and Statistics Department </title>
<script type="text/javascript">
function isMobile(){
                var agent = navigator.userAgent.toLowerCase();
                var mobiles = ["midp","240x320","blackberry","netfront","nokia","panasonic","portalmmm","sharp","sie-","sonyericsson","symbian",
								"windows ce","benq","mda","mot-","opera mini","philips","pocket pc","sagem","samsung","sda",
								"sgh-","vodafone","xda","palm","iphone","ipod","ipad","android","iemobile","webos","windows phone"]; // list of mobile device
                for (var cnt=0;cnt<mobiles.length;cnt++)
                                if(agent.search(mobiles[cnt].toLowerCase())>-1) document.location = "/m/o.jsp"// set the new destination
}
function chgcss(oId, oClass) { document.getElementById(oId).className=oClass; }
isMobile();
</script>
