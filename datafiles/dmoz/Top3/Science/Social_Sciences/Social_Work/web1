     	            <script  type="text/javascript">

     	                $(document).ready(function () {

     	                    function equalHeight() {

     	                        var heightArray = $(".firstrow>li").map(function () {

     	                            return $(this).height();

     	                        }).get();

     	                        var maxHeight = Math.max.apply(Math, heightArray);

     	                        $(".firstrow>li").height(maxHeight);

     	                    }

     	                    equalHeight();

     	                });

    	   
            </script>





