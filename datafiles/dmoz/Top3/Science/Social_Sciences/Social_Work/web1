function getData(vars,div,count){
	var arrTwo=new Array();
	var request = window.XMLHttpRequest ? new XMLHttpRequest() : new ActiveXObject("MSXML2.XMLHTTP.3.0");
	var nocache=new Date();
	nocache=nocache.valueOf();
	var myPage="/swAjax.php?nocache="+nocache;
	request.open("POST", myPage, true);
	request.setRequestHeader("Content-Type", "application/x-www-form-urlencoded"); 
	request.onreadystatechange = function(){
		if (request.readyState == 4 && request.status == 200) {
			if (request.responseText){
				var arrOne=request.responseText.split("~~");
				for (a=0;a<(arrOne.length-1);a++){
					var arrThree=arrOne[a].split("||");
					divContent[div][a]=new Array(arrThree[0],arrThree[1]);
				}
				popDiv(div,count);
			}
		}
	};
	request.send(vars);
}

function popDiv(div,count){
	var tempContent=divContent[div];
	var maxLength=tempContent.length;
	var content='';
	var minHit=0;
	var counter=0;
		if(div=='country'){
			if(count==10){
				tempContent.sort(sortByHits);	
				maxLength=(tempContent.length>10)?10:tempContent.length;
				minHitItem=maxLength-1;
				if(tempContent.length>10){
				   content="<div class='listDisplay' onclick='popDiv(\""+div+"\",0);'>See all...</div>";
				}		
 				minHit=tempContent[minHitItem][1];
				tempContent.sort(sortByItems);
			} else {
				if(maxLength>10){
				   content="<div class='listDisplay' onclick='popDiv(\""+div+"\",10);'>See top ten...</div>";
				}
			}
		}
		var oldUrl=window.location.href;
		for(b=0;b<tempContent.length;b++){
			if(parseInt(tempContent[b][1])>=minHit && counter<=maxLength){
			   oldUrl=oldUrl.replace(/(related|id)\/[0-9]+\/$/,"/");
			   oldUrl=oldUrl.replace(/\/[0-9]+\/$/,"/");
			   var linkName=tempContent[b][0];
			   tempContent[b][0]=tempContent[b][0].replace(/\./g,'[dot]');
			   if(div=='category'){
			   		var newUrl=(oldUrl.indexOf("/category/")>-1)?oldUrl.replace(/\/category\/([^/\.]+)\// ,"/category/"+tempContent[b][0]+"/"):oldUrl+"category/"+tempContent[b][0]+"/";
			   }
			   if(div=='country'){
			   		var newUrl=(oldUrl.indexOf("/country/")>-1)?oldUrl.replace(/\/country\/([^/\.]+)\// ,"/country/"+tempContent[b][0]+"/"):oldUrl+"country/"+tempContent[b][0]+"/";
			   }
			   if(div=='source'){
			   		var newUrl=(oldUrl.indexOf("/source/")>-1)?oldUrl.replace(/\/source\/([^/\.]+)\// ,"/source/"+tempContent[b][0]+"/"):oldUrl+"source/"+tempContent[b][0]+"/";
			   }	
			   if(div=='time'){
			   		var newUrl=(oldUrl.indexOf("/time/")>-1)?oldUrl.replace(/\/time\/([^/\.]+)\// ,"/time/"+tempContent[b][0]+"/"):oldUrl+"time/"+tempContent[b][0]+"/";
			   }			   		   			   
			   counter++;
			   //content+="<a href='"+newUrl+"'>"+linkName+" ("+tempContent[b][1]+")</a><br />";
			   content+="<a href='"+newUrl+"'>"+linkName+"</a><br />";
			}
		}
		document.getElementById(div).innerHTML=content;
}

function performSearch(searchstr){
	searchstr=searchstr.replace(/[^A-Za-z0-9 ]+/,"");
	var oldUrl=window.location.href;
	oldUrl=oldUrl.replace(/(related|id)\/[0-9]+\/$/,"/");
	oldUrl=oldUrl.replace(/\/[0-9]+\/$/,"/");
	var newUrl=(oldUrl.indexOf("/search/")>-1)?oldUrl.replace(/\/search\/([^/\.]+)\// ,"/search/"+searchstr+"/"):oldUrl+"search/"+searchstr+"/";
	window.location.href=newUrl;
}
