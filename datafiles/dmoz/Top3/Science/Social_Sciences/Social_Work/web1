<!-- Document onload for font -->
<script>
    $(document).ready(function () {

        var inc = $.cookie('fontSize');
        var id = $.cookie('activeLink');
        if (inc == undefined) {
            inc = '10'
            $.cookie('fontSize', inc, { path: '/' });
        }
        if (id == undefined) {
            id = 'a1';
            $.cookie('activeLink', id, { path: '/' });
        }

        changeActive();

        var p = document.getElementsByTagName('body');
        if (p[0].style.fontSize != inc) {
            changeFontSize(inc, id);
        }
    });

</script> 

<!-- Font Changer -->
<script type="text/javascript">

    function changeFontSize(inc, id) {
        var p = document.getElementsByTagName('body');
        for (n = 0; n < p.length; n++) {
            if (p[n].style.fontSize) {
                var size = parseInt(p[n].style.fontSize.replace("px", ""));
            } else {
                var size = 10;
            }
            p[n].style.fontSize = inc + 'px';
        }
        $.cookie('fontSize', inc, { path: '/' });
        $.cookie('activeLink', id, { path: '/' });
        changeActive();
    }

    function changeActive() {
        if ($.cookie("activeLink") == "a1") {
            $("#textList>li>a").removeClass("activeLink");
            $("#a1").addClass("activeLink");
        }
        else if ($.cookie("activeLink") == "a2") {
            $("#textList>li>a").removeClass("activeLink");
            $("#a2").addClass("activeLink");
        }
        else if ($.cookie("activeLink") == "a3") {
            $("#textList>li>a").removeClass("activeLink");
            $("#a3").addClass("activeLink");
        }
    }
</script>


