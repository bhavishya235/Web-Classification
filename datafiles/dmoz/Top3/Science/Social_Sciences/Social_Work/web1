<!--Google Analytics Code To Track PDF Downloads-->
<script type="text/javascript">
    $("a[href$='pdf']").each(function (index) {
        pdfLabel = $(this).attr('href');
        pdfOnClick = "_gaq.push(['_trackEvent', 'Download', 'PDF', '" + pdfLabel + "']);";
        $(this).attr("onClick", pdfOnClick);
    });
</script>
<!--End Google Analytics Code To Track PDF Downloads-->

<!--Google Analytics Code To Track DOC Downloads-->
<script type="text/javascript">
    $("a[href$='doc'], a[href$='docx']").each(function (index) {
        docLabel = $(this).attr('href');
        docOnClick = "_gaq.push(['_trackEvent', 'Download', 'DOC', '" + docLabel + "']);";
        $(this).attr("onClick", docOnClick);
    });
</script>
<!--End Google Analytics Code To Track PDF Downloads-->

<!--Google Analytics Code To Track ZIP Downloads-->
<script type="text/javascript">
    $("a[href$='zip']").each(function (index) {
        zipLabel = $(this).attr('href');
        zipOnClick = "_gaq.push(['_trackEvent', 'Download', 'ZIP', '" + zipLabel + "']);";
        $(this).attr("onClick", zipOnClick);
    });
</script>
<!--End Google Analytics Code To Track PDF Downloads-->

<!--Google Analytics Code To Track External Link-->
<script type="text/javascript">
    $("a[href*='http://']:not([href*='.pdf']):not([href*='.doc']):not([href*='.zip']):not([href*='scie.org.uk']), a[href*='https://']:not([href*='.pdf']):not([href*='.doc']):not([href*='.zip']):not([href*='scie.org.uk'])").each(function (index) {
        extLink = $(this).attr('href');
        extLinkClick = "_gaq.push(['_trackEvent', 'External', 'Links', '" + extLink + "']);";
        $(this).attr("onClick", extLinkClick);
    });
</script>
<!--End Google Analytics Code To Track External Link-->

<!--Google Analytics Code To Track Video Downloads-->
<script type="text/javascript">
    $("a[href$='.mp4'], a[href$='.mov'], a[href$='.avi'], a[href$='.wmv'] ").each(function (index) {
        vidLabel = $(this).attr('href');
        vidOnClick = "_gaq.push(['_trackEvent', 'Download', 'Video', '" + vidLabel + "']);";
        $(this).attr("onClick", vidOnClick);
    });
</script>
<!--End Google Analytics Code To Track Video Downloads-->

<!--Google Analytics Code To Track Accordion Click-->
<script type="text/javascript">
    $(".accContent h3").each(function (index) {
        accText = $(this).text().replace("Open", "").replace("Close", "");
        accLabel = document.URL + " " + accText;
        accOnClick = "_gaq.push(['_trackEvent', 'Action', 'Accordion', '" + accLabel + "']);";
        $(this).attr("onClick", accOnClick);
    });    
</script>
<!--End Google Analytics Code To Track Accordion Click-->

<!--Google Analytics Code To Track Tabs Click-->
<script type="text/javascript">
    $("#tabs > ul li a").each(function (index) {
        tabText = $(this).text();
        tabLabel = document.URL + " " + tabText;
        tabOnClick = "_gaq.push(['_trackEvent', 'Action', 'Tab', '" + tabLabel + "']);";
        $(this).attr("onClick", tabOnClick);
    });    
</script>
<!--End Google Analytics Code To Track Tabs Click-->

