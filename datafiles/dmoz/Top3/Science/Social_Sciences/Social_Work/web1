        <script>

                $(window).scroll(function () {
                    if ($(this).scrollTop() > 200) {
                        $('#hb-gotop').fadeIn();
                    } else {
                        $('#hb-gotop').fadeOut();
                    }
                });

                $('#hb-gotop').click(function () {
                    $("html, body").animate({ scrollTop: 0 }, 300);
                    return false;
                });

</script> 

