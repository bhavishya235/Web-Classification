        <a href="../services/tours.html" onMouseOut="MM_swapImgRestore()" onMouseOver="MM_swapImage('nav2','','../services/images/2visitors-on.gif',1)"><img name="nav2" border="0" src="../services/images/2pagenav.gif" width="120" height="23" alt="Visitor &amp; Tours"></a><br>
        <a href="../services/gallery.html" onMouseOut="MM_swapImgRestore()" onMouseOver="MM_swapImage('nav5','','../services/images/5pagenav-on.gif',1)"><img name="nav5" border="0" src="../services/images/5pagenav.gif" width="120" height="23" alt="Gallery"></a><br>
