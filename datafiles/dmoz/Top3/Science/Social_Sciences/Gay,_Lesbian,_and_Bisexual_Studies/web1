<script type="text/javascript">
    window.nymag.dcads.article = true;

    function truncateParagraph(el,lessTxt, moreTxt){
    	$("span.truncatedText").toggle();
        var curText = $(el).text();
        if (curText == moreTxt) {
            $(el).text(lessTxt);
        } else {
            $(el).text(moreTxt);
        }
    }

    $(document).ready(function() {
    	nym.echo.loadMostCommented({
    		items:10
    	});
    });
</script>



