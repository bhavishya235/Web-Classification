  $(document).ready(function(){

    var path = window.location.pathname,
        sectURL, sectName;
    for( i in sectionLookup ) {

      if( path.match(i) ) {

        sectName = sectionLookup[i]["title"]
        $('#sectionTitle').text(sectName);

        if (sectURL !== '') {
          sectURL = sectionLookup[i]["link"];
          $('#sectionURL').attr('href', sectURL);
        } else {
          $('#sectionURL').attr('href', 'javascript:;');
          $('#sectionURL').css('pointer-events', 'none');
        }
      }
    };

    $('#txt-ny-search').focus(function(){
      $('.searchDiv').addClass('focus');
    }).blur(function(){
      $('.searchDiv').removeClass('focus');
    });

  });
  </script>
 
