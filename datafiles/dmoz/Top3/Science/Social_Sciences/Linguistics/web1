<script type="text/javascript">
    var itemOver = null;

    function doMouseOver(sender, args)
    {
        var found = false;
        if(itemOver != null)
            for(var i = 0; i < itemOver.Items.length && !found; i++)
                found = itemOver.Items[i] == args.Item;
        if(!found && itemOver != null)
            itemOver.Collapse();
            
        itemOver = args.Item;
        
        if(itemOver.Items.length > 0)
            itemOver.Expand();
    }

    function doMouseOut(sender, args)
    {
        //args.Item.Collapse();
    }
</script>




