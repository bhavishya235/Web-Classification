        function onClientItemClicking(sender, eventArgs) {
            //var item = eventArgs.get_item(); 

            var item = eventArgs.get_item();
            var itemText = item.get_text();

            if (itemText == "General") {
                eventArgs.set_cancel(true);
                window.location = "http://www.reading.org/General/default.aspx";

            }

        } 
