    dc:identifier="http://languagelog.ldc.upenn.edu/nll/?p=10664"
    dc:title="More Metadata Muddles on Google Books"
    trackback:ping="http://languagelog.ldc.upenn.edu/nll/wp-trackback.php?p=10664" />
