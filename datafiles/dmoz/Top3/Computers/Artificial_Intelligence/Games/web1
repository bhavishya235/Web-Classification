<script type="text/javascript">

var placeholder = 'Search...';

function check_value() {
    var s_box = $('#s');
    
    if( s_box.val() == placeholder )
        s_box.val('');

} 

function searchfocusfn(e) {
	if ($(e).val() == placeholder) {
   	    $(e).val('');
	}
	if ($(e))
		$(e).css('color', '#000000');
}

function searchblurfn(e) {
    if ($(e).val() == '') {
        try {
	        var s = getVar('s');
		} catch (err) {
		    return;
		}
		
	    if (s) {
        	$(e).value(s);
	    } else {
		    $(e).value(placeholder);
	    }
    }
    
    $(e).css('color', '#939393' );
}


$(document).ready(function() { 
	searchblurfn('#s');
});

</script>
    

