      function OnLoad() {

        // create a feed control
        var feedControl = new google.feeds.FeedControl();

        feedControl.addFeed("http://alta.asn.au/topnews",
        "News");

        feedControl.addFeed("http://www.google.com/calendar/feeds/3tpn29fbkj3k7hbf2mmvegqsn0@group.calendar.google.com/public/basic",
        "Events");

