    <script type="text/javascript">
        $(function() {
            $('.jnlCover img').load(function() {

            var jnlCover = $('.jnlCover');
            var jnlCTA = $('.jnlCTA');

            var jnlCoverHeight = parseInt(jnlCover.height()) + parseInt(jnlCover.css('padding-top')) + parseInt(jnlCover.css('padding-bottom')) - 10;

            if (jnlCoverHeight > jnlCTA.height())
                jnlCTA.css('height', jnlCoverHeight + 'px');

            });
        });
    </script>

