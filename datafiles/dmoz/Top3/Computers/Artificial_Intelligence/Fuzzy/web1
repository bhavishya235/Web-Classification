        <div class="jqmWindow" id="modalEmailFriend">
            <h2>Email a Friend</h2>
            <div id="modalEmailFriend_Form">
                <p>Complete the short form below to let your friends and colleagues know about this page.</p>
                <p>Don't worry, the details you provide on this page will not be used to send unsolicited e-mail. Find out more about our <a href="http://www.elsevier.com/wps/find/privacypolicy.cws_home/privacypolicy">privacy policy</a>.</p>
                <div style="margin: 0px -20px -20px 0px;">
                    <table class="shortForm">
                        <tr>
                            <th>Your Name</th>
                            <td><input id="txtEmailSenderName" type="text" /></td>
                        </tr>
                        <tr>
                            <th>Your Email</th>
                            <td><input id="txtEmailSenderAddress" type="text" /></td>
                        </tr>
                        <tr>
                            <th>Friend's Name</th>
                            <td><input id="txtEmailRecipientName" type="text" /></td>
                        </tr>
                        <tr>
                            <th>Friend's Email</th>
                            <td><input id="txtEmailRecipientAddress" type="text" /></td>
                        </tr>
                        <tr>
                            <td colspan="2">
                                <input id="btnEmail" type="button" class="button" value="Send Email" />
                            </td>
                        </tr>
                    </table>
                </div>
            </div>
            <div id="modalEmailFriend_Confirm" style="display: none;">
                <p style="font-size: 1.4em;">Thank you for recommending this page to a friend or colleague.</p>
                <p><a href="#" id="modalEmailFriend_Hide">Click here to close this dialogue box</a></p>
            </div>
            <script type="text/javascript">
                $(function () {
                    addthis.init();

                    $('#modalEmailFriend').jqm({
                        onHide: function (h) {
                            h.w.slideUp("slow", function () {
                                if (h.o) { h.o.remove(); }

                                $('#modalEmailFriend_Form').show();
                                $('#modalEmailFriend_Confirm').hide();
                            });
                        }
                    })
                    .jqmAddTrigger($('#emailFriend'))
                    .jqmAddClose($('#modalEmailFriend_Hide'));

                    var pageTitle = $(this).attr('title');

                    $('#btnEmail').click(function () {
                        var taggedUrl = addthis.addClickTag($(location).attr('href'), 'email');
                        $.ajax({
                            url: '/ajax/emailToFriend.ashx',
                            data: {
                                senderName: $('#txtEmailSenderName').val(),
                                senderAddress: $('#txtEmailSenderAddress').val(),
                                recipientName: $('#txtEmailRecipientName').val(),
                                recipientAddress: $('#txtEmailRecipientAddress').val(),
                                pageTitle: pageTitle,
                                pageUrl: taggedUrl,
                                publicationId: 621920
                            },
                            success: function (msg) {
                                if (msg.indexOf('1|') > -1) {
                                    $('#modalEmailFriend_Form').hide();
                                    $('#modalEmailFriend_Confirm').show();

                                    addthis.logShare(taggedUrl);
                                    _gaq.push(['_trackEvent', 'Form Submitted', 'Send to a Friend']);
                                } else if (msg.indexOf('0|') > -1) {
                                    alert(msg.split('|')[1]);
                                } else {
                                    alert('There was a problem sending your email. Please try again later.');
                                }
                            }
                        });
                    });
                });
            </script>
        </div>
    </div>
    

