    <script type="text/javascript">
        $(function () {
            var shortDesc = $('textarea#txtShortScope').val();
            if (shortDesc != undefined) {
                $('#shortScope').html(shortDesc).show();
                $('#fullScope').hide();

                $('#linkScope a').toggle(function () {
                    $('#fullScope').show();
                    $('#shortScope').hide();
                    $(this).html('Hide full aims and scope');
                }, function () {
                    $('#fullScope').hide();
                    $('#shortScope').show();
                    $(this).html('View full aims and scope');
                });
            }
        });
    </script>
