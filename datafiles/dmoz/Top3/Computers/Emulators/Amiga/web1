<SCRIPT LANGUAGE="JavaScript">
<!--

// Copyright (c) 1997-1998 Allan Lindqvist. All rights reserved.
// Classics For UAE - http://www.lindqvist.it/retired/uae
//
// Do not even THINK of cutting/pasting anything from this source code
// Without checking with me first
//
//
//

defaultStatus="Classics for UAE, Copyright 1998-99 Allan Lindqvist"

function JukeBox(modfile) {
window.open('modbox.html?tune='+modfile,'Jukebox','resizable=no,menubar=no,scrollbars=no,width=300,height=275');
}

function jukeinfo() {
window.open("jukeboxinfo.html", "Info",'resizable=no,menubar=no,scrollbars=no,width=550,height=350');
}

function SignGuestBook() {
window.open("signguestbook.html", "SignBook",'resizable=no,menubar=no,scrollbars=yes,width=550,height=400')
}

function AwardsWindow() {
window.open("awards.html", "Awards",'resizable=no,menubar=no,scrollbars=yes,width=600,height=350');
}

function NewsWindow() {
window.open('news.html', 'News','resizable=no,menubar=no,scrollbars=no,width=450,height=250')
}

function HelpForm(game) {
window.open('helpform.html?game='+game,'Helpform','resizable=no,menubar=no,scrollbars=yes,width=600,height=350');
}

function ReadDoc(doc) {
window.open(doc,"Documentation",'resizable=no,menubar=no,scrollbars=yes,width=600,height=350');
}

function MailMe() {
if (window.confirm("If you want to ask for Kickstarts or Workbench ADFs please don't bother. It would be totally illegal for me to mail them to you, and my page could go down as a consequence. Even if you do own an Amiga and thereby is legally entitled to the Kickstart, I can't risk sending it to you. Sorry. Now, with that out of the way, if you just wanted to ask a simple question, I'm sorry to have kept you. Please proceed.")) {
window.location="MAILTO:cfuae@lindqvist.it"
}

}

//--></SCRIPT>

