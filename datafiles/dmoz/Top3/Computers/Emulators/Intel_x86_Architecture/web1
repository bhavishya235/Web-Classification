        if (typeof playerMap[link.attr("rel")] != "undefined") {
            link.removeAttr("target").addClass("ctx-video-lightbox-reveal").attr("href", "javascript:void(0);").attr("rel", "videoLightbox-" + playerMap[link.attr("rel")]);
            }
    });
</script></div>
<div class="parbase section reference"><div style="display:inline;" class="cq-dd-paragraph"><div class="buttons">
<div align=left class="ctx-btns-left"><p>
                <a class="ctx-medium-btn ctx-medium-blue ctx-no-external" href="/products/xenserver/try.html" target="_blank"><span>Download Free</span></a>
                
