    <div class="vid-container reveal-modal-inner"></div>
    <span class="close-reveal-modal cboxClose" rel="videoLightbox-2d80a27"></span>
  </div>
  <script thirdpartynav="true" type="text/javascript">
      /*
            Adding a check in here because when this is included in an iframe we lose the JS
            Defensive coding.
       */
      if(typeof setVideoLightboxReveal != "undefined"){
          setVideoLightboxReveal("#videoLightbox-2d80a27");
      }else{
          jQuery("#videoLightbox-2d80a27").hide();
      }

  </script></div>
