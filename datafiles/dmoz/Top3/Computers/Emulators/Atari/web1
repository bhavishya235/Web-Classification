		//if (top==self) document.location.href="http://www.chrislam.co.uk"

		function myDay(iDay)
		{
			if (iDay==0) return "Sunday"
			if (iDay==1) return "Monday"
			if (iDay==2) return "Tuesday"
			if (iDay==3) return "Wednesday"
			if (iDay==4) return "Thursday"
			if (iDay==5) return "Friday"
			if (iDay==6) return "Saturday"
		}

		function myDateExt(iDay)
		{
			if (iDay==11) return "th"
			if (iDay==12) return "th"
			if (iDay==13) return "th"

			i = iDay % 10

			if (i==1) return "st"
			if (i==2) return "nd"
			if (i==3) return "rd"

			return "th"
		}

		function myMonth(iMonth)
		{
			if (iMonth==0) return "January"
			if (iMonth==1) return "February"
			if (iMonth==2) return "March"
			if (iMonth==3) return "April"
			if (iMonth==4) return "May"
			if (iMonth==5) return "June"
			if (iMonth==6) return "July"
			if (iMonth==7) return "August"
			if (iMonth==8) return "September"
			if (iMonth==9) return "October"
			if (iMonth==10) return "November"
			if (iMonth==11) return "December"
		}
		-->
