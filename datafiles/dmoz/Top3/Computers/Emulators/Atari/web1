            (function(){
              var immersionCompliment = false;
              if ((!immersion_adcode && !immersionCompliment)  || (immersion_adcode && immersionCompliment)) {
                document.write('<iframe src="http://ad.doubleclick.net/N4140/adi/ostg.sourceforge/cons_none_p85_medrec;pg=%2Fprojects;psrch=0;logged_in=0;tpc=castaway;tpc=c;tpc=emulators;tpc=system;aud=developers;aud=endusers;dc_ref=http%3A%2F%2Fsourceforge.net%2Fprojects%2Fcastaway%2F;tile=7;sz=300x250;u=e%3A68f90d6a-9e75-11e3-a6ea-0200ac1d1d8e;ord=3389036219463622?" width="300" height="250" marginwidth="0" marginheight="0" hspace="0" vspace="0" frameborder="0" scrolling="no" seamless></iframe>');
              }
            })();
       </script>
        </section>
        
        

