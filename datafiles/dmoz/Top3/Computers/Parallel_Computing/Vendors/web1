function updateversions(product)
{
    var product_array = product.split(",");
        document.getElementById("product").value=product_array[0];
        document.getElementById("versions").value=product_array[1];
}
function isNotQuote(evt) {
    var charCode = (evt.which) ? evt.which : event.keyCode
    if (charCode == "'".charCodeAt(0))
      return false;
    return true;
}
  function isNumberKey(evt) {
    var charCode = (evt.which) ? evt.which : event.keyCode
    if (charCode > 31 && (charCode < 48 || charCode > 57) && charCode != ",".charCodeAt(0) && charCode != " ".charCodeAt(0) && charCode != "\n".charCodeAt(0))
      return false;
    return true;
}
</script>

