                                s.prop4 = s.prop3;
                                s.prop5 = s.prop3;
                                s.prop6 = s.prop3;
                                s.prop7 = s.prop3;
                            }
                        }
                        else{
                            s.prop3 = s.channel;
                            s.prop4 = s.channel;
                            s.prop5 = s.channel;
                            s.prop6 = s.channel;
                            s.prop7 = s.channel;
                        }

                        var i = 1;
                        var pagen = "";
                        for (i = 1; i < temp.length; i++) {
                            pagen += temp[i];
                            if (i != temp.length - 1) {
                                pagen += ":";
                            }
                        }
                        if(actualPageName != ""){
                                pagen += ":" + actualPageName;
                            }

                        if (pagen.indexOf(":") == 0)
                            s.pageName = "select" + pagen;
                        else
                            s.pageName = "select:" + pagen;
                                
                       // s.pageName = pagen;
                    }
                }
            }

