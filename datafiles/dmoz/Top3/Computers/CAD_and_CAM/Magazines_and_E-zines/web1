<script type="text/javascript">
if (document.images)
{
adImages = new Array("images/HPBD_Banner468_60S.JPG","images/cad-user3.gif");
					 
adURLs = new Array("www.caddealer.com/click_counter/click.php?id=652","www.caddealer.com/click_counter/click.php?id=3967");
				   
thisAd = 0;
}
function cycleAds()
{
    if (document.images)
    {
        if (document.adBanner.complete)
        {
            if (++thisAd == adImages.length)
                thisAd = 0;
			
            document.adBanner.src = adImages[thisAd];
        }
	}
	setTimeout("cycleAds()", 10000);
}

function gotoAd()
{
    window.open("http://" + adURLs[thisAd]);
}
</script>

