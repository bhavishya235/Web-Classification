	<!--"Remember me" scripts-->
	<script type="text/javascript"\>
		/*
		onload = function() {
			doPosition('frmLogInTop','rememberMeBlock');			
		}
		*/
	
	function doPosition(id1, id2) {
		
		var curleft = curtop = 0;
		var curbot;
		
		var obj1 = document.getElementById(id1);
		var obj2 = document.getElementById(id2);
				
		var obj = obj1;

		if (obj.offsetParent) {

			do {
				curleft += obj.offsetLeft;
				curtop += obj.offsetTop;
			} while (obj = obj.offsetParent); //Sets obj to parent and continues execution if there's a value

		curbot = curtop + obj1.offsetHeight;
		
		obj2.style.top = curbot;
		obj2.style.left = curleft;
		obj2.style.visibility = "visible";

		}
	}

	</script>
	






