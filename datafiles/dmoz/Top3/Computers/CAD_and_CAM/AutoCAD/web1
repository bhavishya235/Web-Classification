<head>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1" />
<title>The Autodesk File</title>
<link rel="stylesheet" href="/documents/styles/standard_screen.css" type="text/css" />
<meta name="author" content="John Walker" />
<meta name="description" content="The Autodesk File: A history of Autodesk and AutoCAD in documents" />
<meta name="keywords" content="Autodesk, AutoCAD, history, walker, drake, green, bartz" />
<script type="text/javascript" language="JavaScript">
<!--
    var footnote;

    function popWin(href) {
        if (footnote == null || footnote.closed) {
            footnote = window.open(href, "footnote", "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=350,height=300")
        } else { 
            if (navigator.appName.substring(0.8) == "Netscape") {
                footnote.location.href = href;
                footnote.focus();
            } else {
                footnote = window.open(href, "footnote", "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=350,height=300")
            }
        }
        footnote.focus();
    }

    function footOver(which) {
        top.defaultStatus= "Displaying footnote";
        popWin("/autofile/www/f/" + which + ".html");
    }
