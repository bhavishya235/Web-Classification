    jQuery(document).ready( function() {
            Shadowbox.init({
        players: ['img', 'iframe', 'html'],
        skipSetup: true
    });

    g_base_video_size = {};
    g_sb_oversize_width = "50";
    g_sb_oversize_height = "75";
    g_segment_display = 'shadowbox';
    g_video_window = null;
    g_debug = 1;
    g_video_base_url = "http://www.streamingteacher.com";

    g_video_popup_width = "1000";
    g_video_popup_height = "675";

    g_video_popup_max_screen_width = "10000";
    g_video_popup_max_screen_height = "775";

        jQuery(".expand").each( function() {
           if( jQuery(this).html().indexOf('Free') != -1 ) {
                jQuery(this).addClass( 'expand-free' );
           }
        });

        jQuery("q").prepend("&ldquo;").append("&rdquo;");
        jQuery("q q").prepend("&lsquo;").append("&rsquo;");
        // Add classes to input pseudotypes
        jQuery("input[type='text'], input[type='password']").addClass("text");
        jQuery("input[type='button'], input[type='submit'], input[type='checkbox'], input[type='image'], input[type='radio'], input[type='reset'], button").addClass("nontext");
        jQuery("input[type='button'], input[type='submit'], input[type='reset'], button").addClass("button");
        // Focus on the first text input field in the first field on the page
        jQuery("input[type='text']:first").focus();
        // Add focus class to inputs
        focusfix("input.text", "focus");
        // Courseware buttons
        jQuery("#accordion").accordion({ active: false, autoHeight: false, collapsible: true });
        // Add scrollbars

        //jQuery('#channels, #articles').jScrollPane({showArrows:true, scrollbarMargin: 0,  scrollbarWidth: 17, arrowSize: 16});
        //jQuery('#channels').jScrollPane({showArrows:true, scrollbarMargin: 0,  scrollbarWidth: 17, arrowSize: 16});
       // jQuery('#channels').jScrollPane({showArrows:true});
       // jQuery('.jScrollPaneDrag').append('<div class="jScrollPaneGrip" style="width: 9px"></div>');
        // Pop-up windows for videos
        //jQuery('.popupwindow').popupwindow(profiles);

        // Round Corners
        jQuery('#merchant_info, #dealer-logo').addClass('ui-corner-all');
        jQuery('.moreInfo').addClass('ui-corner-bottom');
        jQuery('#courseware h3 a span').addClass('ui-corner-left');
    });

    // Adds a class focus to input text when focused
    function focusfix(selector, className) {
        jQuery(selector).focus(function() {
            jQuery(this).addClass(className);
        });
        // Removes class when focus is lost
        jQuery(selector).blur(function() {
            jQuery(this).removeClass(className);
        });
    }

</script>                    </div>
