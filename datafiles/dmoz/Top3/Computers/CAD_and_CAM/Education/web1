<div id="courseware">
    <h2>COURSEWARE AVAILABLE</h2>
    <div class="jScrollPaneContainer" style="height: 724px; width: 478px;">
        <div id="channels" style="">
            <div id="accordion" style="height: 400px;" class="ui-accordion ui-widget ui-helper-reset">
                                                <!-- Mastercam X5 Channel -->
                <h3 style="padding-left: 0px;" id="button-47" class="button ui-accordion-header ui-helper-reset ui-state-default ui-corner-all">
                    <span class="ui-icon ui-icon-triangle-1-e">&nbsp;</span>
                    <a tabindex="-1"  href="http://www.streamingteacher.com/mastercam-x7.html" style="background-image: url( http://www.streamingteacher.com/media/catalog/product/cache/1/steach_catalog_image/9df78eab33525d08d6e5fb8d27136e95/m/a/mastercamvc_1_1_2.gif );">
                        <span style="display: none;">Mastercam Video Courseware</span>
                        <span class="ui-corner-left" >
