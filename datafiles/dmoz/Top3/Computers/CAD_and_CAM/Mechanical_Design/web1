	                    	//If the cookied country is not in the geolocation(node name) product catalog, then default to US Teaser	 
	                    	 if(userlocation===teasersJson.teasers[i].country.toUpperCase()){
	                               url = teasersJson.teasers[i].teaserPath;
	                               found = true;
	                               break;
	                    	 }      
	                 }
	                 if(!found){
	                	  //defaulting to US teaser for the users who don't have the geo-location in the product catalog
                          url = teasersJson.defaultTeaser.teaserPath;
