                     var teaserSuffix = "/jcr:content/par.html?wcmmode=disabled";
                     var authorMode='false';
                     var url="";
                     var found=false;
                     
                     if(authorMode=='true'){
                    	 userlocation = "";
                     }else{
                    	 userlocation = common.cookie.getClientCookie('userlocation');
                     }
                     
                     if(userlocation!==null && userlocation!==""){
                    	 userlocation=userlocation.toUpperCase();
                     }else{
                    	 userlocation="US";
