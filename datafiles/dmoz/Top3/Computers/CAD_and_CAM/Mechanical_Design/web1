    <script type="text/javascript">
        $(document).ready(function(){
            $('#TCACC_accord_12353 h3:first').addClass('active');
            $('#TCACC_accord_12353 h3:first img').attr('src','/includes/images/tcacc_down.png');
            $('#TCACC_accord_12353 div:not(:first)').hide();
            $('#TCACC_accord_12353 h3').click(function(){
                $('#TCACC_accord_12353 h3 img').attr('src','/includes/images/tcacc_right.png');
                $(this).next("div").slideToggle("fast")
                .siblings("div:visible").slideUp("fast");
                $('#'+this.id+' img').attr('src','/includes/images/tcacc_down.png');
            });
        });
    </script>
                
