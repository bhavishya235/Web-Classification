                     var userlocation = "";
                     var teaserSuffix = "/jcr:content/par.html?wcmmode=disabled";
                     var authorMode='false';
                     var url="";
                     var found=false;
                     
                     if(authorMode=='true'){
                    	 userlocation = "";
                     }else{
                    	 userlocation = common.cookie.getClientCookie('userlocation');
                     }
                     
                     if(userlocation!==null && userlocation!==""){
                    	 userlocation=userlocation.toUpperCase();
                     }else{
                    	 userlocation="US";
                     }
                     
	                 for(var i=0; i < teasersJson.teasers.length; i++){
	                    	//If the cookied country is not in the geolocation(node name) product catalog, then default to US Teaser	 
	                    	 if(userlocation===teasersJson.teasers[i].country.toUpperCase()){
	                               url = teasersJson.teasers[i].teaserPath;
	                               found = true;
	                               break;
	                    	 }      
	                 }
	                 if(!found){
	                	  //defaulting to US teaser for the users who don't have the geo-location in the product catalog
                          url = teasersJson.defaultTeaser.teaserPath;
                     }
	                 if(url!==""){
	                	 var time = new Date().getTime();
                         url = url + teaserSuffix+'&time='+time;
