function showOrNot(){
        var showDisclaimer = 0;
        if(showDisclaimer==1 && ( !$.cookie("show_cookies_disclaimer") || $.cookie("show_cookies_disclaimer") != 1)){
           $("#footer").addClass("cookies_disclaimer");
            $("#content").addClass("cookies_disclaimer");
            $("#cookies_disclaimer").css('display','block');
            $("#cookies_disclaimer_btn").click(function (){
                $("#footer").removeClass("cookies_disclaimer");
                $("#content").removeClass("cookies_disclaimer");
                $("#cookies_disclaimer").css('display','none');
                $("#cookies_disclaimer").remove();
                $.cookie("show_cookies_disclaimer",1,{expires:90,path:'/'}) ;
            });
        }else{
            $("#cookies_disclaimer").remove();
        }
    }
    $(function() {
        showOrNot();
    });
    </script>
