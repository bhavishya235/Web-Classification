function Fenster(WinWidth,WinHeight,Link,WindowName) {
  var putItThere = null; 
  var chasm = screen.availWidth;
  var mount = screen.availHeight;

  window.open(Link,WindowName,'toolbar=0,width='+WinWidth+',height='+WinHeight+',left=' + ((chasm - WinWidth - 10) * .5) + ',top=' + ((mount - WinHeight - 30) * .5) +', location=0,directories=0,status=0,menubar=0,scrollbars=no,resizable=0,copyhistory=0');
}

function loadFrame() { 
//    Fenster('400','293','images/xmas.jpg')
//  vor Fenster Befehl setzen zum deaktivieren
	var start = document.location.href.lastIndexOf("?")+1;
	var end = document.location.href.indexOf("+",start);
	var label = "welcome";
	var params = "";
	if (start != -1 ) {
		if ( end != -1 ) {
			label = document.location.href.substring(start, end);
			params = "?" + document.location.href.substring(end+1);
			}
		else
			label = document.location.href.substring(start);
	}
	for(i=0; i<subList.length; i++){
		if(subList[i] == label)
			break;
	}
	if(i>=subList.length)
		i=0;
	if(i >= 0) {
		downFrame.location.href = subList[i+2] + params;
		topFrame.switchBild(subList[i+1]);
	}
}
