  <div class="views-field views-field-field-event-icon">        <div class="field-content"><img typeof="foaf:Image" src="http://www.edac.org/sites/default/files/images/events/dac2014_icon.png" alt="" /></div>  </div>  
  <div class="views-field views-field-title">        <strong class="field-content"><a href="http://www.dac.com/" target="_blank">DAC 	Design Automation Conference</a></strong>  </div>  
  <span class="views-field views-field-field-start-date">        <span class="field-content">Jun 1 - 5, 14</span>  </span>      &nbsp;  
  <span class="views-field views-field-field-location">        <span class="field-content">San Francisco, CA, USA</span>  </span>  </div>
