<!--video slider-->
    
<script type="text/javascript">
$(document).ready(function(){
      $("#customer_videos_list").each(function(){
            // get current ul
            var $ul = $(this);
            // get array of list items in current ul
            var $liPromo = $ul.children('li.promo');
            var $liArr = $ul.children('li:not(.promo)');
            for (var i = $liArr.length - 1; i > 0; i--) {
                var j = Math.floor(Math.random() * (i + 1));
                var temp = $liArr[i]; 
                $liArr[i] = $liArr[j];
                $liArr[j] = temp;
            }
            $("#customer_videos_list").html($.merge($liPromo,$liArr));    
      });
});

</script>
