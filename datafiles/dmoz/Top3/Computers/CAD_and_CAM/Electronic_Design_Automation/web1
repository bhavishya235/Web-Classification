<script type="text/javascript" language="javascript">
function TrapKP(e, keyword)
{
   var keycode;
    if (window.event) 
    {keycode = window.event.keyCode;}
    else if (e.which) 
    {keycode = e.which;}
    else 
    {return true;}   
    
    if (keycode == 13)
   {
        event.returnValue=false;
        event.cancelBubble = true;
        //location.href = "http://"+ location.host + "/searchcenter/Pages/results.aspx?k=" + keyword;
    }
}
function clearText() 
{

document.getElementById("ctl00_cadenceSearchBox_txtKeywords").value="";
document.getElementById("ctl00_cadenceSearchBox_txtKeywords").style.fontStyle= "normal"; 
}


</script>
