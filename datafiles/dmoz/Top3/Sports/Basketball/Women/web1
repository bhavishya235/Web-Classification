    //launch the lytebox viewer
    function showLytebox() {
        $lb.launch({
            url: 'http://www.triplecrownsports.com/tctv/games/2012-postWNIT-championship.asp',
            options: 'showPrint:false width:560 height:350 titleTop:true navTop:true',
