		$(this).css('paddingBottom' , (156 - $(this).height()) / 2 );
	});
	
	//Directory Search form
	var delay_submit = (function(){
		var timer = 0;
		return function(callback, ms){
			clearTimeout (timer);
			timer = setTimeout(callback, ms);
		};
	})();
	
	function set_loader(keywords, location) {
		if(keywords && keywords != ''){
			var search_type = $('.search_type').val();
			if(location == 'home') {
				var the_id = '#staff_search';
				$(the_id).fadeIn();
				$('.filter_input').addClass('results');
			} else if(location == 'footer') {
				var the_id = '#footer_staff_search';
				$(the_id).fadeIn();
				$('.filter_input').addClass('results');
			} else {
				var the_id = '#staff_search_inside';
			}
			var loader_data = '';
			if(search_type == 'contacts') {
				loader_data = '<div class="row loading">';
				loader_data += '<div class="loader">Searching</div>';
				loader_data += '</div>';
				$(the_id + ' .inner').html(loader_data);
			}
		}
	}

	$('.argument').bind('keyup', function(){
		var keywords = $(this).val();
		var location = $(this).parent().nextAll('.location').val();
		//console.log('keyup');
		//console.log('location: ' + location);
		if(location == 'directory') {
			var search_val = $(this).val();
			var selector = $('.type_filter li.option.selected').attr('data-filter');
			//console.log('keywords: ' + selector);
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry',
		        search_val : search_val,
		        filter : selector
		    });
		} else {
			set_loader(keywords, location);
			var url = $('.search_form').attr('action');
			contact_type = $('.type_id').val();
			if(keywords && keywords != '') {
				delay_submit(function(){
					submit_form(keywords, url, location);
				}, 300);
			} else {
				submit_form(keywords, url);
			}
		}
	});
	
	var search_type = $('.search_type').val();
	var contact_type = $('.type_id').val();
	
	function strstr_simple(haystack, needle, bool) {
	    var pos = 0;
	 
	    haystack += '';
	    pos = haystack.indexOf(needle);
	    if (pos == -1) {
	        return false;
	    } else {
	        return true;
	    }
	}


	function submit_form(keywords, url, location){
		$.ajax({
			type: 'POST',
			dataType: 'json',
			url: url,
			data: {keywords: keywords, location: location, search_type: search_type, contact_type: contact_type, method: 'ajax'},
			success: function(data){
				if(location == 'home') {
					var the_id = '#staff_search';
				} else if(location == 'footer') {
					var the_id = '#footer_staff_search';
				} else {
					var the_id = '#staff_search_inside';
				}
				$(the_id + ' .inner').empty();
				if(!(data.length)){
					if(search_type == 'contacts') {
						var html = '<div class="row no_results">';
						html += 'No matching results found.';
						html += '</div>';
					}
				} else if(data.length > 0) {
					var total = data.length;
					var html = '';
					for(i = 0; i < data.length; i++){
						var photo = (data[i].photo != false && data[i].photo != null) ? "/public/uploads/contacts/" + data[i].photo : false;
						var thumb_folder = (photo != false) ? photo.split(".").shift() + '/' : false;
						var contact_type = data[i].type_id == 11 ? 'staff' : 'faculty';
						var row = (i%2 == 1) ? 'row alt ' + contact_type : 'row ' + contact_type;
						var row_last = (i == (total-1)) ? ' last' : '';
						var ext = (data[i].ext != "") ? ' <b>Ext.</b> ' + data[i].ext : "";
						var phone = (data[i].phone != "") ? '<b>P:</b> <span class="phone">' + data[i].phone + ext + '</span>' : "";
						if(data[i].url != "" && data[i].url != null) { 
							if(strstr_simple(data[i].url, 'http') == true) { 
								var blog = ' | <a class="blog" target="_blank" href="' + data[i].url + '">Blog</a>';
							} else { 
								var blog = ' | <a class="blog" target="_blank" href="http://' + data[i].url + '">Blog</a>';
							}
						} else {
							 var blog = "";
						}
						var arr = $.trim(data[i].first_name).split(" ");
						var the_first_name = '';
						if(location == 'home') {
							var photo_width = '52';
						} else {
							var photo_width = '78';
						}
						if(arr[0].length > 2) { the_first_name = arr[0]; } else if(arr[1] && arr[1].length > 2) { the_first_name = arr[1]; } else if(arr[2] && arr[2].length > 2) { the_first_name = arr[2]; }
						var the_title = '';
						if(data[i].title && data[i].title != '') { the_title = data[i].title; } else { the_title = ''; }
						if(search_type == 'contacts')
						{
							html += '<div class="' + row + row_last + '">';
							if(location != 'home' && location != 'footer') {
								html += '<h3><a href="/about-us/staff-directory/contact_id/' + data[i].id + '"><span>' + data[i].last_name + ', ' + data[i].first_name + '</span></a></h3>';
							}
							html += '<a class="photo" href="/about-us/staff-directory/contact_id/' + data[i].id + '">';
							if(thumb_folder != false)
							{
								html += '<img src="' + thumb_folder + '78w_78h_' + data[i].photo + '" width="' + photo_width + '" height="' + photo_width + '" />';
							}
							else
							{
								html += '<img src="/public/img/contact_placeholder.png" width="' + photo_width + '" height="' + photo_width + '" />';
							}
							html += '</a>';

							html += '<ul>';
							if(location == 'home' || location == 'footer') {
								html += '<li class="name"><a href="/about-us/staff-directory/contact_id/' + data[i].id + '">' + data[i].last_name + ', ' + data[i].first_name + '</a></li>';
							}
							if(location == 'home' || location == 'footer') {
								html += '<li class="title"><a href="/about-us/staff-directory/contact_id/' + data[i].id + '">' + the_title + '</a></li>';
							} else {
								html += '<li class="title">' + the_title + '</li>';
							}
							html += '<li class="phone">';
							html += phone;
							html += '&nbsp;</li><li class="links">';
							html += '<a href="/about-us/staff-directory/contact_id/' + data[i].id + '" class="view_bio">View Bio</a> | ';
							if (data[i].email != "")
							{ 
								html += '<a class="email" href="mailto:' + data[i].email + '">Email</a> | ';
							}
							html += '<a class="vcard" href="/about-us/staff-directory/vcard/' + data[i].id + '">vCard</a>';
							html += blog;
							html += '&nbsp;</li></ul><div class="clear"></div>';
							html += '</div>';
						}
					}
				}
				if(search_type == 'contacts')
				{
					$(the_id + ' .inner').html(html);
					
					$('.row').hover(function() {
						$(this).toggleClass('hover');
					});
				}
				/*
if(location == 'directory') {
					$($iso_container).isotope({
				        itemSelector: '.row',
				        layoutMode : 'masonry'
				    });
				}
*/
			}
		});
	}
	
	$('.row').hover(function() {
		$(this).toggleClass('hover');
	});
	
	$('#inside_content.departments #main_content ul li ul').isotope({
        itemSelector: '#inside_content.departments #main_content ul li ul li',
        layoutMode : 'masonry'
    });
	
	$('.filter_input.home_search, .filter_input.footer_search').focusout(function(e){
		setTimeout("$('#staff_search, #footer_staff_search').fadeOut()", 500);
		$('.filter_row .clear.button').hide();
		$('.filter_row .search.button').show();
		$('.filter_input.home_search, .filter_input.footer_search').val('');
		//setTimeout("$('.filter_input').removeClass('results')", 500);
	});
	
	$('.filter_input.directory_search').focusout(function(){
		if( $(this).val() == '' ) {
			$(this).siblings('.clear.button').hide();
			$(this).siblings('.search.button').show();
			$(this).prev('label').show();
		}
		//setTimeout("$('.filter_input').removeClass('results')", 500);
	});

	//hide 'x' initially
	$(".filter_row .clear.button").hide();
    
    //hide/show title/ 'x' when clicking/leaving input
    $('.filter_row input').blur(function() { 
		if( $('.filter_row input').val() == '' ) {
	    	$(".filter_row .clear.button").hide();
	    	$(".filter_row .search.button").show();
		}
	}).focus(function() {
    	$(".filter_row .clear.button").show();
    	$(".filter_row .search.button").hide();
	});
    
    //hide 'x', show title when clicking 'x', reset filter
    $(".filter_row .clear.button").click(function(){
    	$(this).prevAll('.argument').val('');
    	$(this).prevAll('.argument').trigger('keyup');
    	$(".filter_row .clear.button").hide();
    	$(".filter_row .search.button").show();
    	$(this).siblings('label').show();
	});
	//End Directory Search form
	
		
	window.addEventListener('focus', function() {
	    $(window).resize();
	});
	
	$(window).resize(function() {
		body_width = $('body').width();
		$('#application_process .item .info').each(function(){
			item_height = Math.round($(this).height()/10)*10;
			$(this).height(item_height);
		});
		if(body_width_static > 1000 && body_width < 1000)
		{
			body_width_static = body_width;
			
			$($iso_container).isotope('destroy');
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		} else if(body_width_static < 1000 && body_width > 1000) {
			body_width_static = body_width;
			
			if($("#mobile_menu_btn").hasClass('open')) {
				$("#mobile_menu_btn").click();
			}
			
			$($iso_container).isotope('destroy');
		    $($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		    
		    //Vertical align quote inside bubble HOME PAGE
			$('.home .quote p').each(function(){
				$(this).css('paddingTop' , (190 - $(this).height()) / 2 );
				$(this).css('paddingBottom' , (190 - $(this).height()) / 2 );
			});
		
			//Vertical align quote inside bubble INSIDE PAGE
			$('.inside .quote p').each(function(){
				$(this).css('paddingTop' , (156 - $(this).height()) / 2 );
				$(this).css('paddingBottom' , (156 - $(this).height()) / 2 );
			});
		} else if(body_width_static > 740 && body_width < 740) {
			body_width_static = body_width;
			
			$($iso_container).isotope('destroy');
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		} else if(body_width_static < 740 && body_width > 740) {
			body_width_static = body_width;
			
			if($("#mobile_menu_btn").hasClass('open')) {
				$("#mobile_menu_btn").click();
			}
			
			$($iso_container).isotope('destroy');
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry'
		    });
		    
		    $('#inside_content.departments #main_content ul li ul').isotope('destroy');
			$('#inside_content.departments #main_content ul li ul').isotope({
		        itemSelector: '#inside_content.departments #main_content ul li ul li',
		        layoutMode : 'masonry'
		    });
		}
		//console.log('body_width_static: ' + body_width_static);
		//console.log('body_width: ' + body_width);
	});
	
});
