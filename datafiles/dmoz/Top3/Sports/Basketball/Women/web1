	    $(this).children('.cover').stop().fadeToggle('fast');
    });
	
	//Fake Select Toggle
	$('.select_toggle').click(function(e){
		e.stopPropagation();
		if($('.select_toggle').hasClass('active'))
		{
			$('.select_toggle').removeClass('active');
			$('.select_toggle').next('ul.select').hide();
		}
		$(this).toggleClass('active');
		$(this).next('ul.select').toggle();
	});
	
	$('.select_toggle').keydown(function(e){
	    if(e.keyCode == 13) {
	        $(this).click();
	    }
	});
	
	$('html').click(function(){
		if($('.select_toggle').hasClass('active'))
		{
			$('.select_toggle').removeClass('active');
			$('.select_toggle').next('ul.select').hide();
		}
	});
	
	$('ul.select li.option').click(function() {
		$('ul.select li.option').removeClass('selected');
		$(this).addClass('selected');
		var to = $(this).attr('name');
		var title = $(this).html();
		$(this).parent('ul.select').next('input').val(to);
		$(this).parent('ul.select').prev('.select_toggle').html('<a>'+title+'</a>');
		//For directory search only
		if($(this).parent().parent().hasClass('type_filter')){
			//$(this).parent().parent().prev().children('.argument').trigger('keyup');
		}
		$(this).parent().prev('.select_toggle').click();
		$(this).parent('ul.select').next('input').focus();
		if($(this).attr('data-filter') != undefined) {
			//console.log('filtered');
			var selector = $(this).attr('data-filter');
			$($iso_container).isotope({
		        itemSelector: '.row',
		        layoutMode : 'masonry',
		        filter : selector
		    });
		}
	});
	
	$('ul.select li.option').each(function(){
		if($(this).hasClass('selected')) {
			var title = $(this).html();
			$(this).parent('ul.select').prev('.select_toggle').html('<a>'+title+'</a>');
		}
	});
	
