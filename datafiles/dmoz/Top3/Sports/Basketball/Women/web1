<div id="scrolltotop"></div>
<script type="text/javascript">
$(window).scroll(function () {
	if ($(this).scrollTop() > 200) {

		$('#scrolltotop').fadeIn();
	} else {
		$('#scrolltotop').fadeOut();
	}
});
$('#scrolltotop').click(function () {
	$('body,html').animate({
		scrollTop: 0
	}, 800);
	return false;
});

$('.gallery_toggle').toggle(function() {
	$(this).toggleClass('open');
	$(this).parent().next('.gallery_slide').stop(true, true).slideDown();
}, function() {
	$(this).toggleClass('open');
	$(this).parent().next('.gallery_slide').stop(true, true).slideUp();
});
$('.first_photo_preview').click(function(){
	$(this).next().next('.gallery_toggle').trigger('click');
});

(function($) {
    $.fn.listenForChange = function(options) {
        settings = $.extend({
            interval: 200 // in microseconds
        }, options);

        var jquery_object = this;
        var current_focus = null;

        jquery_object.filter(":input").add(":input", jquery_object).focus( function() {
            current_focus = this;
        }).blur( function() {
            current_focus = null;
        });

        setInterval(function() {
            // allow
            jquery_object.filter(":input").add(":input", jquery_object).each(function() {
                // set data cache on element to input value if not yet set
                if ($(this).data('change_listener') == undefined) {
                    $(this).data('change_listener', $(this).val());
                    return;
                }
                // return if the value matches the cache
                if ($(this).data('change_listener') == $(this).val()) {
                    return;
                }
                // ignore if element is in focus (since change event will fire on blur)
                if (this == current_focus) {
                    return;
                }
                // if we make it here, manually fire the change event and set the new value
                $(this).trigger('change');
                $(this).data('change_listener', $(this).val());
            });
        }, settings.interval);
        return this;
    };
})(jQuery);

(function($) {
	$.fn.fileName = function() {
		var $this = $(this),
		$val = $this.val(),
		valArray = $val.split('\\'),
		newVal = valArray[valArray.length-1],
		$button = $this.siblings('.button');
		if(newVal !== '') {
			$button.text(newVal);
	  	}
	};
})(jQuery);

$(document).ready(function(){
	
	$(".datepicker").datepicker();
	$(".monthpicker").datepicker({
        dateFormat: 'MM yy',
        changeMonth: true,
        changeYear: true,
        showButtonPanel: true,

        onClose: function(dateText, inst) {
            var month = $("#ui-datepicker-div .ui-datepicker-month :selected").val();
            var year = $("#ui-datepicker-div .ui-datepicker-year :selected").val();
            $(this).val($.datepicker.formatDate('MM yy', new Date(year, month, 1)));
        }
    });
    
    $(".monthpicker").focus(function () {
        $(".ui-datepicker-calendar").hide();
    });

	//File Input Style
	$('input[type=file]').bind('change focus click', function() {$(this).fileName()});

	//tipsy
	 $('.office_forms [title]').tipsy({trigger: 'focus', gravity: 'w'});

/*
	//Mobile Nav Tap Color
    $("#mobile_nav li a").mouseup('click', function(){
      $(this).css("background", "blue");
    }).mousedown(function(){
      $(this).css("background", "#000");
    });
*/
	
	//Footer newsletter signup form
	$('#newsletter_col form .row.checkboxes input#ProspectiveStudents').change(function(){
		if($(this).attr('checked') == 'checked') {
			$('.row#entrance_year').show();
			$('#top_footer').addClass('taller');
		} else {
			$('.row#entrance_year').hide();
			$('#top_footer').removeClass('taller');
		}
	});
	
		
		
	$('#application_process .date_holder .title').each(function() {
		$(this).css('marginTop', (((30 - $(this).height()) / 2 + 14)));
	});
	
	$('a.next_item').smoothScroll({
		offset: -40
	});
	
	
	var item_height;
	$('#application_process .item .info').each(function(){
		item_height = Math.round($(this).height()/10)*10;
		$(this).height(item_height);
	});
	
	var menuStatus;
	var wrapperLeft;
	var ulLeft;
	var current_menu;
	var parent_class;
	var sibling_class;
	var smallSize = 100;
	var largeSize = 165;
	
	//stroll.bind($( '#mobile_nav ul' ));
	
	//function for setting the height of the view_menu buttons
	function resize_menu_btn() {
		$('a.view_menu').each(function() {
			//$(this).css('height', ($(this).parent().height() - 2));
		});
	}
	
	//run the function right off the bat
	resize_menu_btn()
	
	    $("#mobile_menu_btn").click(function(){
    	$(this).toggleClass('open');
        if(menuStatus != true){
        	$('ul.mobile_menu').show();
        	$('ul.mobile_menu').addClass('theparentmenu');
        	resize_menu_btn();
        	//$(".wrapper").css('position', 'fixed');
			$(".wrapper").animate({
				marginLeft: largeSize + "px"
			}, 300, function(){menuStatus = true}).addClass('open');
			/*
$("#mobile_header").animate({
				left: largeSize + "px",
			}, 300);
*/
			wrapperLeft = largeSize;
			ulLeft = 0;
		} else {
			//$(".wrapper").css('position', 'absolute');
			$(".wrapper").animate({
				marginLeft: "0px"
			}, 300, function(){menuStatus = false}).removeClass('open');
			/*
$("#mobile_header").animate({
				left: "0px",
			}, 300);
*/
			$('ul.mobile_menu').animate({
				left: "0px"
			}, 300);
			$("#mobile_nav li").each(function(){
				$(this).removeClass('open_children');
			});
			$('ul.mobile_menu').removeClass('small');
			$('ul.mobile_menu').find('ul').each(function(){
				$(this).css('left', 0);
				$(this).removeClass('small');
		    	$(this).hide();
	    	});
	    	$('#current_menu').html();
	    	$('#current_menu').hide();
			wrapperLeft = 0;
			ulLeft = 0;
		}
    });
    
   /*
 $(".wrapper").touchwipe({
	     wipeLeft: function() {
	     	//alert(dx + ' ' + dy);
			//return false; 
	     	if($("#mobile_menu_btn").hasClass('open')) {
				$("#mobile_menu_btn").click();
			} 
	     },
	     wipeRight: function() { 
	     	//alert(dx + ' ' + dy);
			//return false;
	     	if(!$("#mobile_menu_btn").hasClass('open')) {
				$("#mobile_menu_btn").click();
			}
	     }
	});
*/
	    
    //For menu button inside menu to drill down a level
    $('a.view_menu').live('click', function(){
    	//Get class of parent ul
    	parent_class = $(this).parent().parent().attr('class');
    	//Get class of sibling ul (the one that should be opening)
    	sibling_class = $(this).siblings('ul').attr('class');
    	
    	//Check for the current_menu id on the parent ul: decides whether you are drilling down, or just choosing another menu of the same level
		if($(this).parent().parent().attr('id') == 'current_menu') {
			//if you are drilling down, this means the view_menu button was the fake one, so find the ul with thecurrentmenu class
			$('ul.mobile_menu').find('.thecurrentmenu').each(function(){
				//loop through each child ul until you find the one with the class of sibling ul (the one that should be opening)
				$(this).find('ul').each(function(){
					if($(this).hasClass(sibling_class)) {
						//simulate the clicking of the ACTUAL view_menu button
						$(this).siblings('a.view_menu').click();	
					}
				});
			});
			return false;
		} else {
			//You are simply opening a differnt menu of the same level
			//Remove thecurrentmenu class from the old one listed
			$('#mobile_nav').find('.thecurrentmenu').each(function(){
				$(this).removeClass('thecurrentmenu');
			});
			//Remove theparentmenu class from the old one's parent
			$('#mobile_nav').find('.theparentmenu').each(function(){
				$(this).removeClass('theparentmenu');
			});
			//Add thecurrentmenu class to the new menu to display
			$(this).siblings('ul').addClass('thecurrentmenu');
			//Add thecurrentmenu class to the new parent menu
			$(this).parent().parent().addClass('theparentmenu');
		}
		//Remove all instances of the open_children class
    	$(this).parent().siblings().removeClass('open_children');
    	//Add the open_children class to the parent li
    	$(this).parent().addClass('open_children');
    	
    	//Remove all instances of the active_menu class
    	$(this).parent().parent().siblings('a.view_menu').removeClass('active_menu');
    	$(this).parent().siblings().children('a.view_menu').removeClass('active_menu');
    	//Add the open_children class to the parent li
    	$(this).addClass('active_menu');
    	
    	//Check if the overall menu holder has the "small" class
    	if($('ul.mobile_menu').hasClass('small')) {
    		//Check if the parent of the view_menu button clicked has "small" class
    		if($(this).parent().parent().hasClass('small')) {
    			//If it does, set ulLeft to the itself (0)
		    	ulLeft = ulLeft;
		    } else {
		    	//If it doesn't, set it to the small size
		    	ulLeft = ulLeft + smallSize;
		    }
    	} else {
    		//overall menu does have small class, so ulLeft is added to large size
	    	ulLeft = ulLeft + largeSize;
    	}
    	
    	//Check if parent ul has small class
    	if($(this).parent().parent().hasClass('small')) {
    		//if it does, set wrapperLeft to current val
    		wrapperLeft = wrapperLeft;
    	} else {
    		//if it doesn't, add the class and set wrapperLeft to itself plus smallSize
	    	$(this).parent().parent().addClass('small');
	    	wrapperLeft = wrapperLeft + smallSize;
    	}
    	
    	//If wrapperLeft is larger than small and alrge size combined, just set it to their total
    	if(wrapperLeft > (smallSize + largeSize)) {
	    	wrapperLeft = (smallSize + largeSize);
    	}
    	
    	//Hide the children of all other uls
    	$(this).parent().siblings().each(function(){
	    	$(this).children('ul').hide();
    	});
    	
    	//show the sibling ul to the view_menu button
	    $(this).siblings('ul').show();
	    
	    //Remove all the classes on the fake current menu
	    $('#current_menu').removeClass();
	    
	    //Add the classes from the new current menu
	    $('#current_menu').addClass($(this).siblings('ul').attr('class'));
	    
	    //Show the fake menu
	    $('#current_menu').show();
    	
    	//get the html of the new real current menu
    	current_menu = $(this).siblings('ul').html();
		
		//Add that html to the fake menu
		$('#current_menu').html(current_menu);
		
		//Run the resize_menu_btn function
		resize_menu_btn();
		
		//set left to 0 of real current menu
		$(this).siblings('ul').animate({
			left: "0"
		}, 100);
	    
	    //set left of wrapper to wrapperLeft	    
		$(".wrapper").animate({
			marginLeft: wrapperLeft + "px"
		}, 100, function(){menuStatus = true});

		/*
//animate the header as well
		$("#mobile_header").animate({
			left: wrapperLeft + "px",
		}, 100);
*/
		
		//set the left of the parent ul to the large size
		$(this).parent().parent().animate({
			left: largeSize + "px"
		}, 100);
		
		//set the left of the parent parent ul to large plus small so it's off screen
		$(this).parent().parent().parent().parent().animate({
			left: (largeSize + smallSize) + "px"
		}, 0);
		
		//set left of the overall menu to ul left
		$('ul.mobile_menu').animate({
			left: ulLeft + "px"
		}, 0);
		
		return false;
    });
    
    //END MOBILE NAV
		
	var body_width = $('body').width();
	var body_width_static = $('body').width();
	
	$('.no_enter_submit').keypress(function(e){
	    if ( e.which == 13 ) return false;
	});
		
	//Logo enlarge
	$('#logo').mouseenter(function(e){
		if(body_width > 700) {
			$(this).doTimeout('logo_hover', 100, logoHoverIn, e.target);
