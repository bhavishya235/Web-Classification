	//Twitter vert center
	if(body_width > 740) {
		if($('.twitter_left').height() > $('.twitter_right').height()) {
			//If left is taller than right, just apply to "top" of right
			$('.tweet .twitter_right').each(function(){
				$(this).css('top' , ($('.twitter_left').height() - $(this).height()) / 2 );
			});
		} else {
			//If right is taller than left, apply as padding to left so right border extends
			$('.tweet .twitter_left').each(function(){
				$(this).css('paddingTop' , ($('.twitter_right').height() - $(this).height()) / 2 );
				$(this).css('paddingBottom' , ($('.twitter_right').height() - $(this).height()) / 2 );
			});
		}
	}
	
