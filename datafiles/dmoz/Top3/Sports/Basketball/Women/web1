	}).mouseleave(function(e){
		if(body_width > 700) {
			$(this).doTimeout('logo_hover', 100, logoHoverOut, e.target);
		}
	});
	
	function logoHoverIn(selector){
		$('#logo').find('img').animate({
			height: 130,
			width: 130,
			marginTop: 20,
			marginLeft: -10
		}, 200);
	}
	
	function logoHoverOut(selector){
		$('#logo').find('img').animate({
			height: 110,
			width: 110,
			marginTop: 30,
			marginLeft: 0
		}, 200);
	}
	
