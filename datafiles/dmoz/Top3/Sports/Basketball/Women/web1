	//Form label behaviour
	$('form input.text, form input.text_argument').each(function(){
		if( $(this).val() != '' ) {
	    	$(this).prev('label').not('.stop_label_action, .outside').hide();
		}
	});
	
	$('form textarea').each(function(){
		if( $(this).val() != '' ) {
	    	$(this).prev('label').not('.stop_label_action, .outside').hide();
		}
	});
	
	$('form label').click(function() { 
		$(this).next("input").focus();
		$(this).next("textarea").focus();

	}), (function() {
		$(this).next("input").blur();
		$(this).next("textarea").blur();
	});
	
	$('form input.text, form input.text_argument').blur(function() { 
		var this_field = $(this);
		
		//delay checking if field has value to give datepicker time to fill the field
		setTimeout(function(){
			if( this_field.val() == '' ) {
		    	this_field.prev('label').show();
			}
		}, 300)
	}).focus(function() {
    	$(this).prev('label').not('.stop_label_action, .outside').hide();
	});
	
	$('form textarea').blur(function() { 
	if( $(this).val() == '' ) {
    	$(this).prev('label').show();
	}
	}).focus(function() {
    	$(this).prev('label').not('.stop_label_action, .outside').hide();
	});
	
	$("form").listenForChange();

	//End form label behaviour
	
