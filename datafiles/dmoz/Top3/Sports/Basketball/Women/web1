            $( document ).ready(function() {
                $("#survey-form-body").on("click", "#feedback-submit", function(e) {
                    e.preventDefault();
                    var whoIs = $("input[type='radio'][name='who-is']:checked").val();
                    var feedbackComment = $("#feedback-input textarea").val();
                    var feedbackPageID = '65791';
                    var email = $('#survey-form-body input[name="email"]').val();
                    var userName = $('#survey-form-body input[name="userProfileName"]').val();
                    saveFeedback( whoIs, feedbackComment, feedbackPageID, email, userName )
                    .done(function(){
                        $('#survey-form-body').html(
                            "<p style='color:#fff;'>Thank you for your feedback</p>"
                        );
                        $('#survey-form').animate({
                            height: "50px"
                        });
                    })
                    .fail(function(jqXHR, textStatus, errorThrown){
                        $('#survey-form-body').html(
                            "<p style='color:#fff;'>An error occurred when submitting your feedback. Please try again later.</p>" +
                            "<!-- error: " + errorThrown + "-->"
                        );
                        $('#survey-form').animate({
                            height: "50px"
                        });
                    });
                });
            });
        </script>
