			}
			
			slideHTML+= '</tr></table></p>'
		}
		slideHTML+= '</div>'
		picobj.innerHTML=slideHTML
	}
	 
	 
	fadeshow.prototype.rotateimage=function(){
		if (this.pausecheck==1) //if pause onMouseover enabled, cache object
		var cacheobj=this
		if (this.mouseovercheck==1)
			setTimeout(function(){cacheobj.rotateimage()}, 100)
		else if (iebrowser&&dom||dom){
			this.resetit()
			var crossobj=this.tempobj=iebrowser? iebrowser[this.curcanvas] : document.getElementById(this.curcanvas)
			crossobj.style.zIndex++
			fadeclear[this.slideshowid]=setInterval("fadepic(fadearray["+this.slideshowid+"])",50)
			this.curcanvas=(this.curcanvas==this.canvasbase+"_0")? this.canvasbase+"_1" : this.canvasbase+"_0"
		}
		else{
			var ns4imgobj=document.images['defaultslide'+this.slideshowid]
			ns4imgobj.src=this.postimages[this.curimageindex].src
		}
		this.curimageindex=(this.curimageindex<this.postimages.length-1)? this.curimageindex+1 : 0
	}
	 
	fadeshow.prototype.resetit=function(){
		this.degree=10
		var crossobj=iebrowser? iebrowser[this.curcanvas] : document.getElementById(this.curcanvas)
		if (crossobj.filters&&crossobj.filters[0]){
			if (typeof crossobj.filters[0].opacity=="number") //if IE6+
				crossobj.filters(0).opacity=this.degree
			else //else if IE5.5-
				crossobj.style.filter="alpha(opacity="+this.degree+")"
			}
		else if (crossobj.style.MozOpacity)
			crossobj.style.MozOpacity=this.degree/101
		else if (crossobj.style.KhtmlOpacity)
			crossobj.style.KhtmlOpacity=this.degree/100
	}
	 
	 
	fadeshow.prototype.startit=function(){
		var crossobj=iebrowser? iebrowser[this.curcanvas] : document.getElementById(this.curcanvas)
		this.populateslide(crossobj, this.curimageindex)
		if (this.pausecheck==1){ //IF SLIDESHOW SHOULD PAUSE ONMOUSEOVER
			var cacheobj=this
			var crossobjcontainer=iebrowser? iebrowser["master"+this.slideshowid] : document.getElementById("master"+this.slideshowid)
			crossobjcontainer.onmouseover=function(){cacheobj.mouseovercheck=1}
			crossobjcontainer.onmouseout=function(){cacheobj.mouseovercheck=0}
		}
		this.rotateimage()
	}
 
</script><script type="text/javascript">
new fadeshow(fadeimages, 150, 272, 266, 0, 5000, 1, "R")
</script>
</div>

<!--CMTODUAL INI2/25/2014 1:42:34 PM-->

<script type="text/javascript">
function chkCMT()
{

if (document.getElementById("captchacode").value.length != 6){
 alert('Please insert text from the image to the right in the field above the Post Comments button.');
 document.getElementById("captchacode").focus();
 return false;
}

	var maxCC=1000;
	var minCC=4;
	
	   // email check if was typed
	   if (document.getElementById("cmEmail").value.length>0)
	   {
	       var jcmtEmail=document.getElementById("cmEmail").value;
	       var jcmtEmailTest=(jcmtEmail.indexOf("@") > 0) && (jcmtEmail.indexOf(".") > 2)
		   if (jcmtEmailTest!=true)
		   {
			   alert('Email address is not valid, please type a valid email or simply leave it empty.');
			   document.getElementById("cmEmail").focus();
			   return false; 
		   }
	   }
	if(document.getElementById("cmName").value.length < 1)
	  {
	   alert('Please type your name.');
	   document.getElementById("cmName").focus();
	   return false; 
	  }

	if(document.getElementById("cmText").value.length < minCC) 
	  {
	   alert('Comment should be at least ('+minCC+') characters please type '+ (minCC - document.getElementById("cmText").value.length)+ ' more characters.');
       document.getElementById("cmText").focus();
	   return false; 
	  }		
	else if(document.getElementById("cmText").value.length > maxCC) 
	  {
	   alert('Comment exceeded the maximum allowed characters ('+maxCC+')\nplease remove '+ (document.getElementById("cmText").value.length - maxCC)+ ' characters, and post the comment.');
	   document.getElementById("cmText").focus();
	   return false; 
	   }
	else
	 {
	   return true; 
     }
 
}

var gComments=[["353252","2"],["353247","1"],["352845","1"],["350490","1"],["352884","1"],["201479","1"],["348176","1"],["260255","2"],["236935","3"],["185465","1"],["283857","4"],["283485","5"],["226981","3"],["224609","1"],["195223","1"],["210378","1"],["188044","1"],["187774","1"],["336425","1"],["314861","2"]];
function getCommentsModule()
{
	
	var dil = document.getElementsByTagName('div');
	for (var i = 0; i < dil.length; i++){                      
	      var ename = dil[i].id;
	      //alert(divname);
	      if(ename.match("cmtNewsID")) { 
	      		vtsi="<img src='http://www.eurobasket.com/comments/cmticon.gif' border=0>"
				vtstream="<a href='http://www.eurobasket.com/comments/jsScr.asp?newsid="+ename.replace('cmtNewsID','')+"&showcomments=1'>"+vtsi+" Comments</a>";
				document.getElementById(ename).innerHTML=vtstream;
				document.getElementById(ename).className="cmtStyleOn";
				for (x=0;x<=gComments.length;x++)
				{
					if (gComments[x])
					{
						if (ename=="cmtNewsID" + gComments[x][0].toString())
						{
						
							vtstream="<a href='http://www.eurobasket.com/comments/jsScr.asp?newsid="+gComments[x][0]+"&showcomments=1'>"+vtsi+" Comments (" + gComments[x][1] + ")</a>";
							document.getElementById("cmtNewsID" + gComments[x][0]).innerHTML=vtstream;
							document.getElementById("cmtNewsID" + gComments[x][0]).className="cmtStyleOn";
							break;
						}else{
							// nothing
						}
					}//gComments
				}//for xml
	      }// ename
	}//for div
	
	

}// getCommentsModule
function ToggleCMTFORM()
{
	hdivid="cmtFormShow";
	hfocusid="cmName";
	var fobj=document.getElementById(hdivid);
	var ffocusobj=document.getElementById(hfocusid);
	if (fobj)
	{
		if (fobj.style['display']=='none') {
			fobj.style['display']='block';
			if (ffocusobj) ffocusobj.focus();
			}
		else 
			fobj.style['display']='none';
	}
	
}

function RefreshImage() {
	var objImage = document.images["imgCaptcha"];
	if (objImage == undefined) {
		return;
	}
	var now = new Date();
	objImage.src = objImage.src.split('?')[0] + '?x=' + now.toUTCString();
}
</script>

