function siQuantcast()
{
	var lb = "Time Inc News Business and Sports,Sports Illustrated";
	var lb_ch = (jsmd.get("m:page.section[0]") ? jsmd.get("m:page.section[0]") : "");
	lb+=(lb_ch != null && typeof(lb_ch) == "string" && lb_ch.length > 0) ? "." + lb_ch:"";
	return lb;
}
