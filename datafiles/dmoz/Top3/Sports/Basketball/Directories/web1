<script language="javascript" type="text/javascript">
	function HideStatusWindow()
	{
		document.getElementById("divStatusWindow").style.display="none";
	}
	function submittoaction(LeageNo,ActionNo,PlayOffNo)
	{
		document.frmStatus.strAmitLeague.value=LeageNo;
		document.frmStatus.strAmitActive.value=ActionNo;
		document.frmStatus.strAmitPOff.value=PlayOffNo;
		document.frmStatus.submit();
	}
	function showupDropDown(intID,intValue)
	{
		if(intID < 10)
		{
			strName = "spanMen" + intID;
		}
		else
		{
			strName = "spanWomen" + intID;
		}
		if(intValue ==1)
		{
			document.getElementById(strName).style.display=""
		}
		else
		{
			document.getElementById(strName).style.display="none"
		}
	}
</script>

