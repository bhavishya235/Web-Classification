<script language="javascript" type="text/javascript">
	function submittovideo(Id,strImg, strTitle, sFolder)
	{
		document.frm.hiddenID.value = Id
		document.frm.hiddenImage.value = strImg
		document.frm.hiddenTitle.value = strTitle
		document.frm.hiddenFolder.value = sFolder
		document.frm.action ="/VideoView.asp"
		document.frm.submit();
	}
	function showhide_old(intNo,sFolder)
	{
		document.getElementById("tdTab0").className="ileftmenutd2";
		document.getElementById("tdTab1").className="ileftmenutd2";
		
		document.getElementById("trVideo0").style.display="none";
		document.getElementById("trVideo1").style.display="none";
		
		
		document.getElementById("tdTab"+intNo).className="aleftmenutd";
		document.getElementById("trVideo"+intNo).style.display="";
		document.frm.hiddenWhere.value = sFolder
	}
	function showhide(intNo,sFolder)
	{
		document.getElementById("tdTab0").className="ileftmenutd2";
		document.getElementById("tdTab1").className="ileftmenutd2";
		
		document.getElementById("trVideo0").style.display="none";
		document.getElementById("trVideo1").style.display="none";
		document.getElementById("tdTab2").className="ileftmenutd2";
		document.getElementById("trVideo2").style.display="none";
		
		
		document.getElementById("tdTab"+intNo).className="aleftmenutd";
		document.getElementById("trVideo"+intNo).style.display="";
		document.frm.hiddenWhere.value = sFolder
	}
	
	function ImageMouseOver(imgID)
	{
		document.getElementById(imgID).src="/images/PlayerBg.gif";
		document.getElementById(imgID).style.opacity=0.4;
		bName =	navigator.appName;
		if (bName=="MSIE")
		{
			document.getElementById(imgID).style.filters.alpha.opacity=40;
		}
		
	}
	function ImageMouseOut(imgID)
	{
		document.getElementById(imgID).src="/images/pic-bg.gif";
		document.getElementById(imgID).style.opacity=0.4;
		bName =	navigator.appName;
		if (bName=="MSIE")
		{
			document.getElementById(imgID).style.filters.alpha.opacity=40;
		}
	}
	function showhideTable(objName,id)
	{
		document.getElementById(objName +"1").style.display="none";
		document.getElementById(objName + "2").style.display="none";
		document.getElementById(objName + id).style.display="";
	}
</script>
