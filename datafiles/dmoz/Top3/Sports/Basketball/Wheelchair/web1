  <h3>Upcoming Competitions</h3>
</div>
<div class="calendar-content">
<script type='text/javascript' src='http://illinois.edu/calendar/pc/3719/1.js'></script>
<script type='text/javascript'>
$('.calendar-content div[class$="-time"] span').each(function() { 
    var content = $(this).html();
    $(this).parent().append(content);
    $(this).remove();
});
$('.calendar-content li[id^="event-"]').each(function() { 
    var time = $('[class$="-time"]', this);
    var timehtml = time.html();
    time.remove();
    $('[class$="-date"]', this).after("<div class='event-time'><div>" + timehtml + "</div></div>");
});
</script>
