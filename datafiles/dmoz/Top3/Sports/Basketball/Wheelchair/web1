			function controlEnter (obj, event,lang)
			{
				var keyCode = event.keyCode ? event.keyCode : event.which ? event.which : event.charCode;       
				if (keyCode == 13)
				{                   
					submitSearch(document.getElementById('search').value,lang);
                    return false;       
				}       
				else  
				{
                   return true;  
				}  
			}
			function submitSearch(txt,lang) 
			{
				if(lang=='en')
					window.location.href='/en/search.aspx?search=' + txt;
				else
					window.location.href='/fr/search.aspx?search=' + txt;
			}	
			$(function () {
				$('#rotator-wrapper').cycle({
					fx: 'fade',
					speed: '1000',
					timeout: 7000,
					pager: '#rotatorNav',
					slideExpr: 'div.flash-content'
				});
			});
