            $(document).ready(function () {
                $('#partnerlogos').cycle();
                $('.news').show();
                $('#rotator-wrapper').show();
                $('.fancybox').fancybox();
                $('.meltwaternews').show();
                $('.tabs > ul > li:first').each(function () {
                    $(this).css('display', 'block');
                });
                $('#twitter_update_list').children('li').children('span').each(function () {
                    $(this).children('a').each(function () { 
                        $(this).attr('target', '_blank');
                    })
                });
                $('#togglenews > a, #togglepress > a, #toggletwitter > a, #toggleevents > a').click(function () {
                    $(this).parent().hide();
                    $(this).parent().siblings('li').show();
                    var currentTab = $(this).attr('href');
                    $(this).parent().parent().parent().children('div').hide();
                    $(currentTab).show();
                    return false;
                });
