<div id="node-152" class="node node-article node-promoted node-sticky node-teaser clearfix" about="/node/152" typeof="sioc:Item foaf:Document">
   <!-- Article teasers get calendar format floated left of title -->
         <hr />
      <div class="calendar">
  	     <div class="month Aug">
    	    <div>Aug</div>
