    <div class="clear"></div>
</ul>

<script type="text/javascript">

    (function ($) {

        $(document).ready(function () {

            var slideSpeed = 300;
            var selectedClass = "selected";
            var $topNav = $("ul#dropdownmenu").children("li");

            $("ul#dropdownmenu").mouseleave(function () {
                // Close all menus
                var $allMenus = $(this).children("li");
                for (var i = 0; i < $allMenus.length; i++) {
                    var $menu = $($allMenus[i]);
                    if ($menu.text() != $(this).text()) {
                        $menu.children("ul").stop(true, true).slideUp(slideSpeed, function () {
                            $(this).parent().removeClass(selectedClass);
                        });
                    }
                }
            });

            $topNav.mouseenter(function () {
                // Close all menus
                var $allMenus = $(this).parent().children("li");
                for (var i = 0; i < $allMenus.length; i++) {
                    var $menu = $($allMenus[i]);
                    if ($menu.text() != $(this).text()) {
                        $menu.children("ul").stop(true, true).slideUp(slideSpeed, function () {
                            $(this).parent().removeClass(selectedClass);
                        });
                    }
                }

                // Open the selected menu
                $subNav = $(this).children("ul");

                if ($subNav.length != 0) {
                    $(this).addClass(selectedClass);
                    $subNav.css("minWidth", $(this).outerWidth()).stop(true, true).slideDown(slideSpeed);
                }
            });
            
            $topNav.children("ul").mouseleave(function () {
                $(this).stop(true, true).slideUp(slideSpeed, function () {
                    $(this).parent().removeClass(selectedClass);
                });
            });

        });

    })(jQuery);

</script>


</div>
    




    <div class="shadow"></div>
</div>

<script type="text/javascript">

    jQuery.noConflict();

    (function ($) {
        $(document).ready(function () {

            var $ngb = $('.ngb-icon');
            var $img = $ngb.find('img');
            var parentH = $ngb.parent().outerHeight();
            var imgH = $img.outerHeight();
            var m = (parentH - imgH) / 2;
            $img.css({ 'margin-top': m + 'px' });
        });
    })(jQuery);
    
</script>

