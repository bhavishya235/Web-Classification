        <script type="text/javascript">
            jQuery.noConflict();

            jQuery(document).ready(function() {
                // steal enter key from search boxes
                jQuery('.search-input-wrapper input[type=text], .search-box input[type=text]').bind('keypress', function (e) {
                    if (e.which == 13) {
                        document.location.href = '/USA-Boxing/Search?i=usa boxing&q=' + jQuery(this).val();
                        e.preventDefault();
                    }
                });

                // steal click from buttons (related to the above)
                jQuery('.search-input-wrapper input[type=image], .search-box input[type=image]').bind('click', function (e) {
                    document.location.href = '/USA-Boxing/Search?i=usa boxing&q=' + jQuery(this).prev('input[type=text]').val();
                    e.preventDefault();
                });
            });
        </script>

