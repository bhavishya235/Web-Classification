var picture_mediaBig
var announcement

function bigMedia()
{
if (!picture_mediaBig || picture_mediaBig.closed)
	{
	// media window has not been opened - first invokation
	picture_mediaBig = window.open("video/streetFest_2003/big1.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=360,width=440")
	}
else
	{
	// media window is opened somewhere, bring it into focus
	picture_mediaBig.focus()
	picture_mediaBig = window.open("video/streetFest_2003/big1.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=360,width=440")
	}
} // end function bigMedia

function beachball()
{
if (!picture_mediaBig || picture_mediaBig.closed)
	{
	// media window has not been opened - first invokation
	picture_mediaBig = window.open("video/webmercial.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=500,width=440")
	}
else
	{
	// media window is opened somewhere, bring it into focus
	picture_mediaBig.focus()
	picture_mediaBig = window.open("video/webmercial.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=500,width=440")
	}
} // end function beachball


function annWin()
{
if (!announcement || announcement.closed)
	{
	// announcement window has not been opened - first invokation
	announcement = window.open("misc/announcements/current.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=620,width=640")
//	announcement = window.open("misc/homePage_notice.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=520,width=500")
	}
else
	{
	// announcement window is opened somewhere, bring it into focus
	announcement.focus()
	announcement = window.open("misc/announcements/current.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=620,width=640")
//	announcement = window.open("misc/homePage_notice.htm","mediaBig","toolbar=no,menubar=no,scrollbars=yes,height=520,width=500")
	}
} // end function announceWindow

