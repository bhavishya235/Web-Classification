    var companyMarker = new google.maps.Marker({ 
     position: companyPos, 
     map: map, 
     title:"Panda Wellness Center" 
    }); 
     
    var companyPos = new google.maps.LatLng(27.557744, -99.501791); 
    var companyMarker = new google.maps.Marker({ 
     position: companyPos, 
     map: map, 
     title:"Morales Martial Arts" 
    }); 
 
    var companyPos = new google.maps.LatLng(39.935059, -91.344580); 
