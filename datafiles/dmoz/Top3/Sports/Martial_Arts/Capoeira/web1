<script type="text/javascript">
	function kide_help_show(id, type) {
		var id1 = 'kide_'+type+'_help_'+id;
		var id2 = 'kide_'+type+'_help_link_'+id;
		if (kide.css(id1, 'display') == 'none')
			kide.html(id2, '-');
		else
			kide.html(id2, '+');
		kide.show(id1);
	}
	</script>
