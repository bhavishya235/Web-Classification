		d.write("<a href=http://www.belstat.be/viewstat.asp?UserID=hakkogarcia target=_blank><img border=0 src=\"http://www.belstat.be/regstat.aspx?");
		d.write("UserID=hakkogarcia&Num=1&BColor=teller1&refer=" + ref + "&pag=" + pag + "&b=" + b + "&col=" + col + "&scr=" + scr);
		d.write("\" align=center width=45 height=45 alt=\"Monitored by BelStat - Your Site Counts\"><\/a>");
		// -->
    </script>
<!-- End of belstat.be Counter -->
      <p align="center">&nbsp;</td>
    <td height="39" style="border-bottom-style: solid; border-bottom-width: 1px" width="20%">&nbsp;</td>
    <td height="39" style="border-bottom-style: solid; border-bottom-width: 1px" width="1%">&nbsp;</td>
