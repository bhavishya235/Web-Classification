                el = document.getElementsByTagName("script")[0],
                headNode = el.parentNode;
            s.async = true;
            s.src = scriptUrl;
            headNode.insertBefore(s, el);
            if(el.src === s.src){
                headNode.removeChild(el);
            }
        })();        
    };

