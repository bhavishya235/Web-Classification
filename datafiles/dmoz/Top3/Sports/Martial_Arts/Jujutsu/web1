<script language="JavaScript">
function view_link(url,wname){
        window.open(url,wname,"toolbar=no,menubar=no,scroll=yes,status=no,resizable=yes,screenX=0,screenY=0,width=800,height=600");
}

function doSiteSearch(){
    if ( document.SearchForm.Keywords.value == "" || document.SearchForm.Keywords.value == "Searching for anything?"   ) {
      alert("Please type search keywrods");
      document.SearchForm.Keywords.focus();
      return;
    }
   else document.SearchForm.submit();
}

	function play_video(id)
	{
		if(id.length>0)
			{
				window.open("en_play_video.php?id="+id,"play","resizable=1,width=850,height=500,titlebar=0,menubar=0,scrollbars=1")
			}
		else
			{
				alert("Please, select the file you want to show");
				return;
			}
	}  
</script>
