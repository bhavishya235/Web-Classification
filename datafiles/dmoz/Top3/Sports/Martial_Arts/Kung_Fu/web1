	<!-- s:content -->

	<div id="content">



		<!-- s:left column -->

		<div id="left-column">

		    <div id="leftContent">

<div id="rotator">
    <img src="/images/promos/oceanside-mma.jpg" alt="" />
    <img src="/images/promos/mira-mesa-children.jpg" alt="" />
    <img src="/images/promos/san-diego-kickboxing.jpg" alt="" />
    <img src="/images/promos/san-diego-tai-chi.jpg" alt="" />
    <img src="/images/promos/temecula-children.jpg" alt="" />
    <img src="/images/promos/encinitas-tai-chi.jpg" alt="" />
		</div>

	<script type="text/javascript" src="/js/yourInfoValidation.js"></script>
	<script type="text/javascript" src="/js/jquery.infieldlabel.min.js"></script>
	<script type="text/javascript">
		$( document ).ready(function() {
			$("#formSuccess").hide();
			$("#sendingForm").hide();
			
			$("label").inFieldLabels();
			
			$(function() {
				$("#btnSubmit").click(function() {
				
					// get the form values
					var location = $("#Location>option:selected").val();
					var howFound = $("#HowFound>option:selected").val();
					var phone = $("#PhoneNumber").val();  
					var email = $("#Email").val();  
					var age = $("#Age").val();  
					var parent = $("#ParentsName").val();
					var name = $("#FullName").val();  
					var sex = $("input[name='Sex']:checked").val();
					
					var hasErrors = validateFields( location, howFound, phone, email, age, parent, name, sex );
					if ( !hasErrors ) {
						$("#btnSubmit").hide();
						$("#sendingForm").show();
						processForm( location, howFound, phone, email, age, parent, name, sex );
					}
					
					return false;  
				});
				
				$("#btnAnother").click(function() {
					$("#formSuccess").hide();
					$("#formFields").show();
					$("#btnSubmit").show();
					$("#sendingForm").hide();
					$("#formFields label").show();
					$("#formFields label").css({'opacity':1});
					$("label").inFieldLabels();
					return false;  
				});
			}); 
		});
		
		function validateFields( location, howFound, phone, email, age, parent, name, sex ) {
			$('.error').hide();
			var hasErrors = false;
			
			if ( location == "" ) {
				$("#errLocation").show();  
				$("#Location").focus();
				hasErrors = true;
			}
			
			if ( howFound == "" ) {
				$("#errHowHear").show();  
				$("#HowFound").focus();
				hasErrors = true;
			}
			
			if (phone == "") {  
				$("#errPhoneNumber").show();  
				$("#PhoneNumber").focus(); 
				hasErrors = true;
			}
			else if ( phone.length < 10 ) { 
				$("#errPhoneNumberVal").show();  
				$("#PhoneNumber").focus(); 
				hasErrors = true;
			}
			
			if (email == "") {  
				$("#errEmailReq").show();  
				$("#Email").focus(); 
				hasErrors = true;
			}
			else if ( !IsEmail(email) ) { 
				$("#errEmailVal").show();  
				$("#Email").focus(); 
				hasErrors = true;
			}
			
			if (age == "") {  
				$("#errAgeReq").show();  
				$("#Age").focus(); 
				hasErrors = true;
			}
			else if ( !isNumber(age) ) {
				$("#errAgeVal").show();  
				$("#Age").focus(); 
				hasErrors = true;
			}
			else {
				if ( parseInt(age) < 18 && parent == "") {  
					$("#errParentsName").show();  
					$("#ParentsName").focus(); 
				hasErrors = true;
				}
			}
			
			if (name == "" || name.indexOf(" ") <= 0) {  
				$("#errStudentsFullName").show();  
				$("#FullName").focus(); 
				hasErrors = true;
			}
			
			if (sex == null){
				// no checkboxes are checked
				$("#errSalutation").show();
				$("#FullName").focus();
				$(window).scrollTop($("#radSalutation").offset().top).scrollLeft($("#radSalutation").offset().left);
				hasErrors = true;
			}
			
			return hasErrors;
		}
		
		function processForm( location, howFound, phone, email, age, parent, name, sex ) {
			var dataString = 'Sex='+sex + '&FullName='+name + '&ParentsName='+parent 
			               + '&Age='+age + '&Email='+email + '&PhoneNumber='+phone
			               + '&HowFound='+howFound + '&Location='+location
			               + '&ref=SIDEBAR:/index.php';
			$.ajax({  
				type: "POST",  
				url: "/get-started/yourInfoSidebar.php",  
				data: dataString,
				datatype: "json",
				success: function( data ) {
					if ( data.status == 'ok' ) {
						$("#formSuccess").show();
						$("#formFields").hide();
						$('#form1')[0].reset();
											}
					else {
						//failure
						$('.error').hide();
						var errors = data.errors;
						for ( var i=0; i<errors.length; i++ ) {
							showError( errors[i] );
						}
					}
				}  
			});  
			return false;
		}
		
		function isNumber(n) {
			return !isNaN(parseFloat(n)) && isFinite(n);
		}
	</script>
		
	<style type="text/css">
		#sidebarForm label { font-weight:normal; }
		#sidebarForm span label { font-weight:normal; }
		#sidebarForm input.field { width:220px; }
		#sidebarForm select.field { width:224px; }
		#sidebarForm table { padding-left:15px; }
		#sidebarForm td { vertical-align:top; }
		#sidebarForm h3 { padding-top:15px; }
		#sidebarForm .error { color:yellow; font-size:11px; display:none; }
			</style>
		
	<div id="sidebarForm">

		<div class="heading">Get A Free Pass</div>
		
		<div id="formFields" class="sidebarFormDiv">
			<form id="form1" method="post" action="#">
				<center>
					<span class="subheading">Experience A Free Week Of Classes &<br/>A Free Personal Training Session.</span>
					<table>
						<tr>
							<td>
								<span id="radSalutation">
