<script type="text/javascript">
	var appointy = 'kravmaga',
		appointyHeight = '700',
		appointyWidth = '900',
		ShowSchedulemeImg = false, //if showSchedulemeImg is set to false then it will override the properties below. This can be used if you want to call overlay from your own custom link.
