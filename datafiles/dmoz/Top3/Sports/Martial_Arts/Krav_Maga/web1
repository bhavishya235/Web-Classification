<script type="text/javascript">
//<![CDATA[    

	function initRandomSlide(id, imgs, w, h) {
		if ((jQuery('#'+id).length > 0) && (imgs)) {
	        // setup side slideshow           
			 
			var myshow = new Slideshow(id, {
				type: 'combo',
				showTitleCaption: 1,
				captionHeight: 45,
				width: w,
				height: h,
				pan: 20,
				zoom: 30,
				loadingDiv: 1,
				resize: true,
				duration: [2000, 9000],
				transition: Fx.Transitions.Expo.easeOut,
				images: imgs,
				path: '/assets/photos/'
			});
			 
			myshow.caps.h2.setStyles({
				color: '#fff',
				fontSize: '13px'
			});
			myshow.caps.p.setStyles({
				color: '#ccc',
				fontSize: '11px'
			});
        }
	}
	
    jQuery(document).ready(function() {
        var instructor = '';
        if ('' != String(instructor).trim()) {
            if (jQuery('#menu-instructors-only-navigation').length > 0) {
                jQuery('#menu-instructors-only-navigation').parent().prepend('<small style="margin: 10px 0;text-align:center;display:block">Logged in as: ' + instructor + '</small>');
            } 
        }
        // init menus
        jQuery('ul.sf-menu').superfish({
            animation:  {opacity:'show', height:'show'},   // slide-down effect with fade-in 
            delay:      800
        }); 

        if (jQuery("iframe[src^='about:']").length > 0) {
        	jQuery("iframe[src^='about:']").css('display', 'none'); // some weird gravity forms thing
	    }
        
        // init search
        if (jQuery('#txtSearch').length > 0) {
            jQuery('#txtSearch').focus(function(e) {
                if ('search' == jQuery(this).val()) {
                    jQuery(this).val('');
                }
                jQuery(this).animate({
                    width: "150px"
                }, 555);
            }).blur(function(e) {
                if ('' == jQuery(this).val().trim()) {
                    jQuery(this).val('search');
                    jQuery(this).animate({
                        width: "100px"
                    }, 555);
                }
            }).keyup(function(e) {
                if ((13 == e.keyCode) && ('' != $(this).val().trim())) { // enter key causes submit
                    jQuery('#frmSearch').submit();
                } else if (27 == e.keyCode) {
                    jQuery(this).val('');
                    jQuery(this).blur();
                }
            });
        }
        
        jQuery.extend(jQuery.validator.messages, {
            required: '&larr; required',
            email: '&larr; e-mail required',
            digits: '&larr; number required', 
            date:  '&larr; date required'
        }); 
        
        var url = String(location.href);
        var photoVideo = ((-1 != url.indexOf('/media/photos')) || (-1 != url.indexOf('/media/video'))) ? true : false;
        var shop = (-1 == url.indexOf('/shop/')) ? false : true;
        if (!photoVideo && !shop) {
            if (jQuery('a[href$=mov]')) {
                jQuery('a[href$=mov]').each(function(index, value) { 
                    var hrf = value + '?width=320&height=240';
                    jQuery(this).attr('href', hrf);
                    jQuery(this).attr('rel', 'prettyPhoto[pageVideos]'); 
                });
            }
            if (jQuery('a[href$=jpg]')) {
                jQuery('a[href$=jpg]').each(function(index, value) { 
                    jQuery(this).attr('rel', 'prettyPhoto[pageImages]'); 
                });
            }
            if (jQuery('a[href$=gif]')) {
                jQuery('a[href$=gif]').each(function(index, value) { 
                    jQuery(this).attr('rel', 'prettyPhoto[pageImages]'); 
                });
            }
            if (jQuery('a[href$=jpeg]')) {                                                  
                jQuery('a[href$=jpeg]').each(function(index, value) { 
                    jQuery(this).attr('rel', 'prettyPhoto[pageImages]'); 
                });
            }
            if ((jQuery('a[href$=mov]')) || (jQuery('a[href$=jpg]')) || (jQuery('a[href$=jpeg]'))) {
                jQuery("a[rel^='prettyPhoto']").prettyPhoto({
                    theme: 'facebook',
                    social_tools: ''
				});                  
            }
        }
        if (jQuery('#twoColumnRightSidebar').length > 0) {
            var lht = jQuery('#twoColumnRightSidebar').height();
            var rht = jQuery('#twoColumnRightSidebar').height();
            if (lht < rht) {
                jQuery('#twoColumnRightSidebar').height(rht);
            }
        }
        //jQuery(document).bind('contextmenu', function(e) {
           // return false;
        //});
        
        if (jQuery('#homeSlider').length > 0) {
            jQuery('#homeSlider').nivoSlider({
            	effect: 'random', // Specify sets like: 'fold,fade,sliceDown'
		        slices: 15, // For slice animations
		        boxCols: 6, // For box animations
		        boxRows: 3, // For box animations
		        animSpeed: 500, // Slide transition speed
		        pauseTime: 5000, // How long each slide will show
		        startSlide: 0, // Set starting Slide (0 index)
		        directionNav: false, // Next & Prev navigation
		        directionNavHide: true, // Only show on hover
		        controlNav: true, // 1,2,3... navigation
		        controlNavThumbs: false, // Use thumbnails for Control Nav
		        controlNavThumbsFromRel: false, // Use image rel for thumbs
		        controlNavThumbsSearch: '.jpg', // Replace this with...
		        controlNavThumbsReplace: '_thumb.jpg', // ...this in thumb Image src
		        keyboardNav: true, // Use left & right arrows
		        pauseOnHover: true, // Stop animation while hovering
		        manualAdvance: false, // Force manual transitions
		        captionOpacity: 0, // Universal caption opacity
		        prevText: 'Prev', // Prev directionNav text
		        nextText: 'Next', // Next directionNav text
		        beforeChange: function(){}, // Triggers before a slide transition
		        afterChange: function() {}, // Triggers after a slide transition
		        slideshowEnd: function(){}, // Triggers after all slides have been shown
		        lastSlide: function(){}, // Triggers when last slide is shown
		        afterLoad: function(){} // Triggers when slider has loaded
            });
        }

        if (jQuery('.scroll-pane').length > 0) {
            if (jQuery.browser.msie) {
        		jQuery('.scroll-pane').jScrollPane({autoReinitialise: true});
            }
        }
		if (jQuery('.testimonials-slider .textwidget').length > 0) {
        	jQuery('.testimonials-slider .textwidget').scrollable({ 
            	vertical: true, 
            	keyboard: true, 
            	circular: true,
            	items: '.items'
            }).autoscroll({ 
                autoplay: true, 
                interval:4000 
            }); 
            jQuery('.next, .prev').click(function(e){ e.preventDefault(); }); 
		}

		//functionality for checkbox sliders as opposed to fixed radio buttons.
		if(jQuery('.rightColumn').length > 0){
			jQuery('.gfield_checkbox :input').each(function(i,v){
				jQuery(v).css({'position' : 'absolute', 'top' : '-9999px'});
				jQuery(v).after('<div class="custom-chkboxs"><div class="chkbox-slider chkbox-left"></div><span class="chkbox chkbox-on">On</span><span class="chkbox chkbox-off">Off</span></div>'); 
			});
			jQuery('.custom-chkboxs').click(function(){
				var slider = jQuery(this).find('.chkbox-slider');
				if(slider.hasClass('chkbox-left')){
					jQuery(slider, this).removeClass('chkbox-left').addClass('chkbox-right');
					jQuery(slider, this).animate({'left': "+="+slider.width()});
					jQuery(this).prev(':input').attr('checked', true)
					jQuery(this).next('label').css('fontWeight', 'bold');
				}else{
					jQuery(slider, this).removeClass('chkbox-right').addClass('chkbox-left');
					jQuery(slider, this).animate({'left': "-="+slider.width()}) ; 
					jQuery(this).prev(':input').attr('checked', false);
					jQuery(this).next('label').css('fontWeight', '400');
				}
			});
		}
    });
//]]>
</script>

