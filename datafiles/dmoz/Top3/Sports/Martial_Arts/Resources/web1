		<!-- THis javascript code is used for Banner Rotator  -->
  <script language="JavaScript" type="text/JavaScript"><!--

var urlArray = new Array(4); //for URL's, increase size as necessary
var banArray = new Array(4); //for banners, increase size as necessary
var urlLabelArray = new Array(4); //for URL's label, increase size as necessarym
var banDescArray = new Array(4); //for banners' description, increase size as necessary
var counter = 1;

var url = "http://www.worldblackbelt.com/Pages/KOFitnessDrink.asp"; //initial URL
var urlLabel = "<font color=\"red\">View Article</font>"; //initial URL label
var banDesc = "<h4><DIV align=left><B>Knock Out Your Thirst With KO Fitness Recovery Drink</B>...</div></h4>"; 

//initial URL label

//add your necessary URLs
urlArray[0] = "Pages/KOFitnessDrink.asp";
urlArray[1] = "mall/Products_DynamicFlex.asp";
urlArray[2] = "Pages/KOFitnessDrink.asp";
urlArray[3] = "Pages/BlackBeltVISA.asp";

//add your necessary Banners' short description 
banDescArray[0] = "<h4><DIV align=left><B>Knock Out Your Thirst With KO Fitness Recovery Drink</B>....</div></h4>";
banDescArray[1] = "<h4><DIV align=left><B>Finally, </B>a safe stretching machine that allows for....</div></h4>";
banDescArray[2] = "<h4><DIV align=left>Knock Out Your Thirst With KO Fitness Recovery Drink....</div></h4>";
banDescArray[3] = "<h4><DIV align=left>The Brand New Black Belt VISA....</div></h4>";


//add your necessary URLs' labels 
urlLabelArray[0] = "<font color=\"red\">View Article</font>";
urlLabelArray[1] = "<font color=\"red\">View Video and Article</font>";
urlLabelArray[2] = "<font color=\"red\">View Article</font>";
urlLabelArray[3] = "<font color=\"red\">Apply Here</font>";

if(document.images) //pre-load all banner images
{
  for(i = 0; i < 4; i++)
  {
    banArray[i] = new Image(250, 184);
    //banArray[i].src = "/images/pages/banner" + (i+1) + ".jpg";
    if(i==0)	
		banArray[i].src = "/images/pages/kofitnessrecoverydrink_Big.jpg";
	else if(i==1)
		banArray[i].src = "/images/pages/dynamicflex_Big.jpg";
	else if(i==2)
		banArray[i].src = "/images/pages/kofitnessrecoverydrink_Big.jpg";
	else if(i==3)
		banArray[i].src = "/images/pages/WBBMC_big.jpg";
	else 
		banArray[i].src = "/images/pages/kofitnessrecoverydrink_Big.jpg";

  }
}


function changeBanner() //banner changer function
{
  if(counter > 3)
   counter = 0;

  document.banner.src = banArray[counter].src; //sets a new banner
  
  url = urlArray[counter]; //sets a new URL to the banner

  urlLabel = urlLabelArray[counter]; //sets a new URL Label to the banner
  document.getElementById('banlink').innerHTML =  urlLabel;

  banDesc = banDescArray[counter]; //sets a new Description to the banner
  document.getElementById('bandescription').innerHTML =  banDesc;

  counter++; //increase the counter for the next banner
}


//calls the changeBanner() function every 6 seconds
//change the timer as necessary (minutes * 60000) or (seconds * 1000)
var timer = window.setInterval("changeBanner()", 6000);


function forwardTo(url)
{

	location.href = url;

}

//--></script>
  <script language="JavaScript1.2" type="text/javascript">
function CreateBookmarkLink() {
title = "World Black Belt";
	url = "http://www.worldblackbelt.com";
		if (window.sidebar) { // Mozilla Firefox Bookmark
			window.sidebar.addPanel(title, url,"");
		} else if( window.external ) { // IE Favorite
			window.external.AddFavorite( url, title); }
		else if(window.opera && window.print) { // Opera Hotlist
			return true; }
	}

<!-- Begin
var key = new Array();  // Define key launcher pages here
key['m'] = "/masters/world_black_belt_masters.asp";


function getKey(keyStroke) {
isNetscape=(document.layers);
// Cross-browser key capture routine couresty
// of Randy Bennett (rbennett@thezone.net)
eventChooser = (isNetscape) ? keyStroke.which : event.keyCode;
which = String.fromCharCode(eventChooser).toLowerCase();
for (var i in key) if (which == i) window.location = key[i];
}
document.onkeypress = getKey;
//  End -->

