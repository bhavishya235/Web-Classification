<script type="text/javascript" language="javascript">
var timeout         = 500;
var closetimer		= 0;
var ddmenuitem      = 0;

function jsddm_open()
{	jsddm_canceltimer();
	jsddm_close();
	var offset = $(this).position();
	var liWidth = $(this).width();
	ddmenuitem = $(this).find('div').eq(0).css('visibility', 'visible');
	ddmenuitem = $(this).find('.sub_menu').css('visibility', 'visible');
	ddmenuitem = $(this).find('.container_sub').eq(0).css('left', offset.left+'px');
	//ddmenuitem = $(this).find('.sub_menu').css('padding', '7px 5px 3px 10px');
	//ddmenuitem = $(this).find('.sub_menu').css('width', liWidth+5);
	//ddmenuitem = $(this).find('.container_sub').css('width', liWidth+5);
	ddmenuitem = $(this).find('.sub_menu').css('width','160px');
	ddmenuitem = $(this).find('.container_sub').css('width', '160px');
}


function jsddm_close()
{
    if(ddmenuitem) 
	{
		ddmenuitem.css('visibility', 'hidden');
		$('.sub_menu').css('visibility', 'hidden');
		$('.container_sub').css('visibility', 'hidden');
	}
}

function jsddm_timer()
{	
    closetimer = window.setTimeout(jsddm_close, timeout);
}

function jsddm_canceltimer()
{	if(closetimer)
	{	window.clearTimeout(closetimer);
		closetimer = null;}
}

$(document).ready(function()
{	$('#nav > li').bind('mouseover', jsddm_open);
	$('#nav > li').bind('mouseout',  jsddm_timer);
	$(".submenucontent li").click(function(){
	    $(".submenucontent li").removeClass('selet')
	    $(this).addClass('selet');
	});
	
});

document.onclick = jsddm_close;
</script>

                          
                  
                          
