    //<![CDATA[
    function closeMenu() {
        var menu = $("#OptionsSelector").data("tMenu");
        var item = $("> li", menu.element)[0];
        menu.close(item);
    }

     function onOptionsSelectorMenuOpen() {
         var tMenu = $("#OptionsSelector").data("tMenu");
         if (tMenu != null) {
            tMenu.nextItemZIndex = 1001;
         }
     }

    
    //]]>
