	var input = $('mod_search_searchword');
	input.addEvents({
		'blur' : function(){ if(input.value == '') input.value='Iskanje...'; },
		'focus' : function(){ if(input.value == 'Iskanje...') input.value='';	}
	});
	input.value = 'Iskanje...';
	
	if($('mod_search_button')){
		$('mod_search_button').addEvent('click', function(){ 
			input.focus(); 
		});
	}
});
</script>

