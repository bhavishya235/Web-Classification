jQuery(document).ready(function() {
		
	jQuery('#nivo_slider_134').show().nivoSlider({
			effect: 'random',
			slices: 15,
			boxCols: 8,
			boxRows: 4,
			animSpeed: 500,
			pauseTime: 5000,
			startSlide: 0,
