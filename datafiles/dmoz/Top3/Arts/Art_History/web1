<script type="text/javascript">
function mostraListaArtisti(e) {
	cancellaTimerLA();
	var obj = document.getElementById("divListaArtisti");
	if(document.all) {
		obj.style.top = parseInt(e.clientY + document.body.scrollTop);
	} else {
		obj.style.top = parseInt(e.clientY + document.body.scrollTop);
	}
	obj.style.left = e.clientX - 20;
	obj.style.display = "block";
}
var timerLA = false, timerLAct = false;
var timerLAev = null;
function nascondiListaArtisti(e) {
	if(timerLAct) {
		document.getElementById("divListaArtisti").style.display = "none";
		timerLAct = false;
		timerLA = false;
	} else {
		timerLAct = true;
		timerLAev = e;
		timerLA = setTimeout("nascondiListaArtisti(timerLAev)", 200);
	}
}
function cancellaTimerLA() {
	if(timerLA) {
		clearTimeout(timerLA);
		timerLAct = false;
	}
}
</script>
