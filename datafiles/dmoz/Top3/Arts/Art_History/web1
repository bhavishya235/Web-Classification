<script language="JavaScript">

function on(choice){
	var lit = eval('document.' + choice);
	lit.src = "images/sisterwendy_" + choice + "_on.jpg";
}

function off(choice){
	var lit = eval('document.' + choice);
	lit.src = "images/sisterwendy_" + choice + ".jpg";
}

function preload(){
	if(document.images) {
	  var ImgArray = new Array();
	  for (var i = 0; i < 5; i++) {
	    ImgArray[i] = new Image();
	  }
	ImgArray[0].src = "images/sisterwendy_works_on.jpg";
	ImgArray[1].src = "images/sisterwendy_museums_on.jpg";
	ImgArray[2].src = "images/sisterwendy_meet_on.jpg";
	ImgArray[3].src = "images/sisterwendy_about_on.jpg";
	ImgArray[4].src = "images/sisterwendy_video_on.jpg";
	}
}
</script>

