  text-decoration:none;
}

#puzzle {
  color:#882300;
 font: italic 12px/1.4 arial,sans-serif;
}
</style>
<script>
function francis() {

 var x=document.getElementById("myform"); 
 var mydata = "";
 var mydata_null = "";

var elem = document.createElement("img");
elem.setAttribute("src", "/NG/images/bigrotation2.gif");
elem.setAttribute("height", "32");
elem.setAttribute("width", "32");
elem.setAttribute("alt", "wait....");
document.getElementById("google2").appendChild(elem);

 
 $("#google").html(mydata)
 val = x.elements[0].value
 if ( val == "" ) {val = "berger"};
 mydata = mydata + "Site Search Results: <b>" + val + "</b><ol>"
 $(document).ready(function() {
  $.getJSON("/cgi-bin/googleSearch.py", { option: val } , function(data) {        
        contentType: "application/json; charset=utf-8",        
        $.each(data, function(i, val){
            mydata = mydata + "<li><a class=\"example\"  href=" + val.url + ">"  + val.txt + "</a><p>";
      });
      $("#google2").html(mydata_null)
      $("#google").append(mydata)
    });
});
}
