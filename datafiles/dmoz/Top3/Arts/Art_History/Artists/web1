															<script language="javascript">
function clickButton(e, buttonid){ 
      var bt = document.getElementById(buttonid); 
      if (typeof bt == 'object'){ 
            if(navigator.appName.indexOf("Netscape")>(-1)){ 
                  if (e.keyCode == 13){ 
                 //alert(bt.id + ' Netscape');
                        bt.click(); 
                        return false; 
                  } 
            } 
            if (navigator.appName.indexOf("Microsoft Internet Explorer")>(-1)){ 
                  if (event.keyCode == 13){ 
                  
                 // alert(bt.id + " -IE- " + document.frmSearch.action);
                  
                        bt.click(); 
                        return false; 
                  } 
            } 
      } 
} 
</script>
<script language="javascript">
	function AppendSubmitToQueryString(buttonid)
	{
		//alert("1 " +  " --- " + document.frmSearch.action + " : " + document.frmSearch.action.indexOf("?"));
		//	Is there a Query String
		if (document.frmSearch.action.indexOf("?") >= 0)
		{
			//	Query string contains contains 'SubmitArtistButton'
			//	Replace with 'SubmitButton'.
			if (document.frmSearch.action.indexOf("SubmitArtistButton") >= 0)
			{
				//	Append to Query String
				document.frmSearch.action = document.frmSearch.action.replace("SubmitArtistButton","SubmitButton") ;
				//alert ("3 replace executed");
			}
			//	Query string contains does not contain 'SubmitButton'
			else if (document.frmSearch.action.indexOf("SubmitButton") < 0)
			{
				//	Append to Query String
				//alert ("2 append SubmitButton attempted");
				document.frmSearch.action = document.frmSearch.action + "&Action=SubmitButton";
				//alert ("2 append SubmitButton executed");
			}
				//alert ("4 " + document.frmSearch.action);
		}
		else
		{
			//	Make a Query String
			//alert ("5 " + document.frmSearch.action);
			document.frmSearch.action = document.frmSearch.action + "?Action=SubmitButton";
		}
		//alert ("6 document.getElementById(buttonid).click(); buttonid=" + document.getElementById(buttonid).name);
		//frmSearch.submit();
		var bt = document.getElementById(buttonid);
		//alert ("7 bt=" + bt.name + " buttonid = " + buttonid);
		bt.click();
	}
</script>
<script language="javascript">
	function ClearActionSubmitFromQueryString()
	{
		//alert("1 " +  " --- " + document.frmSearch.action + " : " + document.frmSearch.action.indexOf("?"));
		if (document.frmSearch.action.indexOf("?") >= 0)
		{
			document.frmSearch.action = document.frmSearch.action.replace("&Action=SubmitButton","") ;
		}
		document.frmSearch.action = document.frmSearch.action + "&ClearSearch";
		window.location=document.frmSearch.action;
	}
</script>

