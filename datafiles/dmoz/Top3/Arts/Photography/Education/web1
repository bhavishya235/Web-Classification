function submitSearchForm()	//validate search against empty string or default text
{
	var query = document.search_form.q.value;
	query = query.replace(/^\s+|\s+$/g, '') ;
	//alert(query);
  	if ( (query == "") || (query == "Enter Keyword") )
  	{ 	 
		return false;
	}
	return true;
}
