<script language="javascript">
<!-- HIDE FROM OLD BROWSERS
 
function getBrowser2() 
{
	browser= navigator.appName;
	version=navigator.appVersion;
	accept=version.substring(0, 1);
	if ( (browser == "Netscape") && ( accept >= 3 ) ) return 1;
	if ( (browser == "Microsoft Internet Explorer") && ( accept >= 4) ) return 1;
	return 0;
}

function isEmpty2(s)
{   
	return ((s == null) || (s.length == 0))
}

function verifySearchSidebar(f)
{
	if (getBrowser2()){
		txt=f.szTerm.value;
		if(isEmpty2(txt))
		{
			_hbSet('pec','Please+enter+a+search+term.');
			_hbSet('hec','0');
			_hbSend(); 
			alert("Please enter a search term.");
			document.searchSidebar.szTerm.focus();
			return false;
		}
		if(f.szTerm.value == ' Enter search term ')
		{
			_hbSet('pec','Please+enter+a+search+term.');
			_hbSet('hec','0');
			_hbSend(); 
			alert("Please enter a search term.");
			document.searchSidebar.szTerm.focus();
			return false;
		}
	return true;
	}
}

//STOP HIDING -->
</script>


 <div id="searchWrapper">
    <div id="searchForm">
	<!-- search form -->
	<form method="post" action="/searchResults.asp" id="searchSidebar" name="searchSidebar" onSubmit="return verifySearchSidebar(this);">
		<div class="searchInput"><input class="inv_ink" maxLength="100" size="17" name="szTerm" value=" Enter search term " onFocus="if(this.value==' Enter search term ')this.value='';chgCSSClass('searchInput','normalTxtBox');"></div>
		<div class="searchgo"><input type="image" name="submit" src="/images/_siteheader/searchbttn.png" alt="Search" border="0" onclick="_hbLink('SearchGo')" align="left"><input type=hidden value="heavy" name="typ"><input type=hidden value="All" name="schMod"></div>
	</form> 
    </div>
 </div>

