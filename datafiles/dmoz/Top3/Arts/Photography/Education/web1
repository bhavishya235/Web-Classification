  <script type="text/javascript">
  var $jqPMA = jQuery.noConflict(); 
  $jqPMA(function() {
    
             $jqPMA('img.image0').data('ad-title', "<a target='_blank' href='film-pres-video'>Film Promo Video</a>");                              
             
             $jqPMA('img.image1').data('ad-title', "<a target='_blank' href='photo-pres-video'>Photography Promo Video</a>");                              
             
             $jqPMA('img.image2').data('ad-title', "<a target='_blank' href='apply-now'>Apply Now</a>");                              
             
             $jqPMA('img.image3').data('ad-title', "<a target='_blank' href='our-graduates'>Our Graduates</a>");                              
             
             $jqPMA('img.image4').data('ad-title', "<a target='_blank' href='summer-internships'>Summer Internships&nbsp;</a>");                              
             
    
    var galleries = $jqPMA('.ad-gallery').adGallery({
        effect: 'fade',width:false, height: false, slideshow: {animate_first_image: false, speed: 5000}
    });
    galleries[0].settings.effect = 'fade'; 
   
    $jqPMA('#toggle-slideshow').click(
      function() {
        galleries[0].slideshow.toggle();
        return false;
      }
    );
    $jqPMA('#toggle-description').click(
      function() {
        if(!galleries[0].settings.description_wrapper) {
          galleries[0].settings.description_wrapper = $jqPMA('#descriptions');
        } else {
          galleries[0].settings.description_wrapper = false;
        }
        return false;
      }
    );
  });
  </script>

