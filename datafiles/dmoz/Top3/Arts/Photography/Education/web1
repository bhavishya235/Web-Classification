        function openVC(theDropdown) {
            var selInd = theDropdown.selectedIndex;
            var selVal = theDropdown.options[selInd].value;
            if (selVal != '') {
                try {
                    recordOutboundLink(this, 'Outbound Links:Student Portal', selVal);
                }
                catch (err) {
                }
                window.open(selVal, 'vc', '');
            }
        }
