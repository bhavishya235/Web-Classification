                function nachricht(text)

                {

                   this.text = text;

                }





                var stopPoint =0;

                var startLeft =0;



                var browserVers = parseInt(navigator.appVersion)

                domBrowser = false;



                if (document.all||browserVers>4){

                                domBrowser = true;

                        }



                var moveMessage = new Array();

                function moveableObj(which)

                {

                         this.id = which;

                         if (document.layers)

                         {

                            this.obj = document.panel.document.layers[which];

                            this.obj.style = document.panel.document.layers[which];

                         }

                         this.obj.style.left=0;

                         this.move = moveBy;

                }



                function moveBy(dx,dy)

                {

                         curLeft = parseInt(this.obj.style.left);

                         curTop = parseInt(this.obj.style.top);

                         this.obj.style.left =(curLeft + dx);

                         this.obj.style.top = (curTop + dy);

                }



                function control()

                {

                         for (i=0; i<moveMessage.length;i++)

                         {

                             width = moveMessage[i].obj.clip.width;
                             //alert(width);

                             if (parseInt(moveMessage[i].obj.style.left) > (stopPoint-width)) moveMessage[i].move(-1,0);

                             else

                             {

                                 (i==0)?(which = moveMessage.length-1):(which = i-1);

                                 width = moveMessage[which].obj.clip.width;

                                 newPos = (parseInt(moveMessage[which].obj.style.left) + width+abstand);

                                 if (newPos < panelWidth) newPos = panelWidth;

                                 moveMessage[i].obj.style.left = newPos;

                                 moveMessage[i].move(-1,0);

                             }

                         }

                         window.setTimeout("control()",speed);

                }



                function initTicker()

                {

                         pos = panelWidth;

                         for (i=0; i<moveMessage.length;i++)

                         {

                             which = i-1;

                             if (which < 0) width=0;

                             else width = moveMessage[which].obj.clip.width;

                             moveMessage[i].obj.style.left =(pos +width+abstand);

                             moveMessage[i].obj.style.visibility = "show";

                             pos = parseInt(moveMessage[i].obj.style.left);

                        }

                        control();

                }



                function writeTicker()

                {

                       if (tickerOk)
                       {
                       
                         if (document.layers)

                         {

                            self.document.panel.document.open();

                            for (m=0;m<messages.length;m++)

                            {

                                message = 'message' + m;

                                self.document.panel.document.writeln('<div id="message' + m + '"><span class="TickerText">' +

                                messages[m].text + '</span></div>');

                                moveMessage[m] = new moveableObj(message);

                            }

                         self.document.panel.document.close();

                         initTicker();

                         }
                     }

                }
// -->
                </script>
		<style media="screen" type="text/css"><!--
#layer1   { position: absolute; top: 12px; left: 214px; width: 74px; visibility: hidden }
--></style>
