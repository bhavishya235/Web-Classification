<script type="text/javascript">
function changePP(){
	if($("#shop_prods").attr('src') == '/images/shop/prods1.jpg'){
		$("#shop_prods").attr('src','/images/shop/prods2.jpg');	
	}else{
		$("#shop_prods").attr('src','/images/shop/prods1.jpg');
	}
}
$(document).ready(function() { 
	$("a[rel='lightbox']").colorbox();
	$("a[rel='floatbox']").colorbox();
	$("a[rel='article_gallery']").colorbox({iframe:true, width:800, height:750});
	$(".iframeDigitalLogin").colorbox({iframe:true, width:445, height:450});
	$(".iframeDigitalLogin2").colorbox({iframe:true, width:445, height:450});
	$(".iframeDigitalLogin3").colorbox({iframe:true, width:445, height:450});
	$("a.youtube").colorbox({iframe:true, innerWidth:425, innerHeight:344});
	setInterval('changePP()',3000);
	
	$('#hmp-slideshow-container').cycle({		
		speed: 1000,
		timeout: 9000,
		delay: 0,
    	pager:  '#slide_number',
    	pause: true
	});
		$('#pauseButton').click(function() { 
	    $('#hmp-slideshow-container').cycle('pause');
	    $(this).hide();
	    $('#playButton').show();
	    return false; 
	});
	
	$('#playButton').click(function() { 
	    $('#hmp-slideshow-container').cycle('resume');
	    $(this).hide();
	    $('#pauseButton').show();
	    return false; 
	});
	
	$('#playButton').hide();
			
		
		
		
		
		
});
var addRules = function(){
    $(".contestant_fields").addClass('required').rules("add", {required:true});
}
</script>
