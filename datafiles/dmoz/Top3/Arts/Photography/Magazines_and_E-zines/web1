								<script language="JavaScript" name="ticker"><!--
if (tickerOk && N4 ){

                       var moveMessage = new Array();

               self.document.writeln('<style type=text/css>');

               self.document.writeln(' #panel{background-color: #0000CC;position:relative;width:' + panelWidth + ';clip:rect(0 ' + panelWidth + ' ' + panelHeight + ' auto);}  ');

               for (m=0;m<messages.length;m++)

               {

                       message = 'message' + m;

                   self.document.writeln('#message' + m + '{position:absolute; top:0; left:2;visibility:hidden;}');

