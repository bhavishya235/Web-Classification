<script language="javascript">
function OpenNewWin(Page, WinWidth, Secure)
{
	if ((Page.substr(0,3) == 'por') || (Page.substr(0,4) == 'ppor'))
	{
		WinHeight = 500;
	}
	else
	{
		WinHeight = screen.height - 175;
	}
	if(Secure)
	{
		NewWin = window.open(Page,"","width=" + WinWidth + ",height=" + WinHeight + ",location=0,toolbar=0,directories=0,status=1,menubar=0,scrollbars=1,resizable=1,top=50,left=50");
	}
	else
	{
		NewWin = window.open(Page,"","width=" + WinWidth + ",height=" + WinHeight + ",location=0,toolbar=0,directories=0,status=1,menubar=0,scrollbars=1,resizable=1,top=50,left=50");
	}
}

function OpenNewWin2(Page, WinWidth, Secure)
{
	if ((Page.substr(0,3) == 'por') || (Page.substr(0,4) == 'ppor'))
	{
		WinHeight = 500;
	}
	else
	{
		WinHeight = screen.height - 175;
	}
	if(Secure)
	{
		NewWin = window.open('http://dev.photolife.com/secure/photolife/'+Page,"","width=" + WinWidth + ",height=" + WinHeight + ",location=0,toolbar=0,directories=0,status=1,menubar=0,scrollbars=1,resizable=1,top=50,left=50");
	}
	else
	{
		NewWin = window.open(Page,"","width=" + WinWidth + ",height=" + WinHeight + ",location=0,toolbar=0,directories=0,status=1,menubar=0,scrollbars=1,resizable=1,top=50,left=50");
	}
}

function PopupPic(id) 
{
     window.open("/popup.php?id="+ id,"","resizable=YES,height=200,width=200");
} 

function PopupPic2(id) 
{
     window.open("/popupPage.php?id="+ id,"","resizable=YES,height=200,width=200");
} 

function doNothing()
{

}

var winCount=0;
function PopUp(page,winWidth,winHeight,scrolls,winID){
	// Initialization Block
		var winWidth, winHeight, scrolls, winID;
		if (winID == "undefined" || winID == null) {
			winCount++;	// Auto-increment winID
			var winID = "popup" + winCount;
		}
		if (scrolls == null) scrolls=1;	// Scroll specification is optional
		if (winWidth == null || isNaN(winWidth)) winWidth=350;	// Default window size
		if (winHeight == null || isNaN(winHeight)) winHeight=350;

	// Generating window
		window.open(page,winID,"width=" + winWidth + ",height=" + winHeight + ",location=0,toolbar=0,directories=0,status=0,menubar=0,scrollbars=" + scrolls + ", resizable=0,top=120,left=50");
}

function popup(adresse,largeur,hauteur, scroller, toolbar)
{
window.open(adresse, "ouvrirFenetre", "toolbar="+toolbar+", location=no, directories=no, status=no, scrollbars="+scroller+", resizable=no, copyhistory=no, width="+largeur+", height="+hauteur+", left=100, top=25");
}

</script>
</head>


<BODY  bgcolor="#000000" style="background-color: #000000;">
<CENTER>

