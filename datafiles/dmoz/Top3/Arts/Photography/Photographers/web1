function pcj_lookupSupportPhone(flag) {
var result = '(480) 624-2500';
if (flag==='au') { result = '02 8023 8602'; }
if (flag==='de') { result = '069 9508 6491'; }
if (flag==='es') { result = '91 275 4890'; }
if (flag==='fr') { result = '01 57 32 36 50'; }
if (flag==='ie') { result = '00 44 20 3027 4996'; }
if (flag==='in') { result = '1-800-121-0120'; }
if (flag==='it') { result = '02 9148 3553'; }
if (flag==='nl') { result = '020 206 1740'; }
if (flag==='uk') { result = '020 3027 4996'; }
if (flag==='us') { result = '(480) 624-2500'; }

return result;
}
