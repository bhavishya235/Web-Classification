<script>
	function redirectToCategory(galleryid) {
		var v = $('album_selection').value;
		if (v == '0') {
			// all albums for a gallery
			webs.redirect("/");
		} else if (v == '-1') {
			// do nothing
		} else if (v == 'my-photos') {
			webs.redirect("/my-photos");
		} else {
			webs.redirect("/category?id=" + v);
		}
	}

	function makeSafeSubmit(q){ 
        var q_new= new String();
        q_new=q;
        q_new=q_new.replace(new RegExp("<", 'g'),"&lt;");
        q_new=q_new.replace(new RegExp(">", 'g'),"&gt;");
        q_new=q_new.replace(new RegExp("\"", 'g'),"&quot;"); //"
        q_new=q_new.replace(new RegExp("'", 'g'),"&quot;");
        $('search').value=q_new;
        $('fw-search_form').submit();
    }
</script>

