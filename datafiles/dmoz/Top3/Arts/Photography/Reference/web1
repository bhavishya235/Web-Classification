    $(document).ready(function(){
      if($.browser.msie && $.browser.version < 8){
        if($("#details").exists() ){
          setTimeout("showtheDetailsIE7()",1000);
        }
      }
    });
    function showtheDetailsIE7(){
      $("#details").css("display","block");
