		$('.inproduction').mouseover(function () {
			$('.inproduction').attr("src", "images/rmcinema/inproductionhover.png");
		});

		$('.inproduction').mouseout(function () {
			$('.inproduction').attr("src", "images/rmcinema/inproduction.png");
		});

        $('#slideshowimagewrapper').cycle({
            fx:    'fade',
            speed:  500,
            timeout:  5000,
            prev:   '.slideshowleft',
            next:   '.slideshowright'
        });

        var currentImage = 0;

        function changeButton(up) {
            if (up) {
                currentImage += 1;
                if (currentImage > 3) {
                    currentImage = 0;
                }
            }

            else {
                currentImage -= 1;
                if (currentImage < 0) {
                    currentImage = 3;
                }
            }

            switch (currentImage) {
                case 0:
                    $('.ssnews').html("CineMacabre");
                    $('.sstitle').html("Nurse 3D");
                    $('.sstext').html("Join us for the Canadian premiere! <a href=\"http://www.rue-morgue.com/events/cinemacabre\"class=\"ssreadmore\">Read More</a>");
                    $('#slideshowbutton1').css("background-image", "url(/templates/ruemorgue/img/redcircle.png)");
                    $('#slideshowbutton2').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton3').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton4').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                break;

                case 1:
                    $('.ssnews').html("Rue Morgue Radio");
                    $('.sstitle').html("Behold! RMR France!");
                    $('.sstext').html("Back from the grave & ready to rock! <a href=\"http://rue-morgue.com/2014/02/behold-the-rise-of-rue-morgue-radio-france/\"class=\"ssreadmore\">Read More</a>");
                    $('#slideshowbutton1').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton2').css("background-image", "url(/templates/ruemorgue/img/redcircle.png)");
                    $('#slideshowbutton3').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton4').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                break;

                case 2:
                    $('.ssnews').html("Rue Morgue Podcast");
                    $('.sstitle').html("Slasher Films");
                    $('.sstext').html("Guitar legend SLASH answers fan questions. <a href=\"https://vimeo.com/83905852\"class=\"ssreadmore\">Read More</a>");
                    $('#slideshowbutton1').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton2').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton3').css("background-image", "url(/templates/ruemorgue/img/redcircle.png)");
                    $('#slideshowbutton4').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                break;

                case 3:
                    $('.ssnews').html("Conventions");
                    $('.sstitle').html("Attention, artists!");
                    $('.sstext').html("Design a cover for Rue Morgue and win big!  <a href=\"http://rue-morgue.com/2014/02/design-your-own-rue-morgue-cover/\"class=\"ssreadmore\">Read More</a>");
                    $('#slideshowbutton1').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton2').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton3').css("background-image", "url(/templates/ruemorgue/img/greycircle.png)");
                    $('#slideshowbutton4').css("background-image", "url(/templates/ruemorgue/img/redcircle.png)");
                break;
            }
        }

	$('.postbody img').parent().removeAttr("href")
	.css('display', 'block')
