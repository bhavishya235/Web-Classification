			<script type="text/javascript">
			
				function gotEventsResult(data)
				{
			
							
							$("#events_calendar").html(data);
				}
				
				var onSelectDate = function(dateText,inst) { 
					document.getElementById("events_calendar").innerHTML = "<h4>Loading...</h4><div class='loading'><img src='/images/tickets/purchase/loading.gif' alt='please wait, the content is still loading'/></div>"; 
					
					dateText = dateText.split("/");
				
					$.get ('/calendar/calendar_pod_events.cfm?byear=' + dateText[2] + '&bmonth=' + dateText[0] + '&bday=' + dateText[1], gotEventsResult);
					
				}; 
				
				function initCal() { 
					
					$("#calendar1").datepicker({defaultDate:null, onSelect:onSelectDate});
					
					
				   //cal1 = new YAHOO.widget.Calendar("cal1","cal1Container"); 
				   //cal1.select("2/25/2014");
				   //cal1.selectEvent.subscribe(mySelectHandler, cal1, true); 
				   
				   //cal1.render(); 
				   
				   $.get('/calendar/calendar_pod_events.cfm?byear=2014&bmonth=2&bday=25',gotEventsResult);
				} 
				
				$(document).ready(initCal);
				
				
			</script>
</div>
