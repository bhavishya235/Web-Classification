	<script type="text/javascript" language="javascript" src="/scripts/httprequest.js"></script>
	<script type="text/javascript" language="javascript">
	var loadingText = 'Loading...';
	
	
	function xmlReady(this_xml){
		var return_val = false;
		// if xmlhttp shows "loaded"
		if (this_xml.readyState==4) {
			// if "OK"
			if (this_xml.status==200) {
				return_val = true;							
			} else {
				alert("Error retrieving data");
			}
		}
		
		return return_val;
	}
	
	function videoStateChange(_init){
		if(xmlReady(xmlhttp)){
			
			var _text = xmlhttp.responseText;
			document.getElementById('video_interactive').innerHTML = _text;
			
			if(_init){
				_init(); 	
			}
		}	
	}
			
	function openVideo(_link){
		loadXMLDoc(_link,videoStateChange);
	}
	
	
</script>

	
		
		
