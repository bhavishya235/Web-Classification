	<script language = "javascript">
	<!--
	function ScrollerMemory_GetCoords()
	{
		var scrollX, scrollY;
		if (document.all)
		{
		if (!document.documentElement.scrollLeft)
			scrollX = document.body.scrollLeft;
		else
			scrollX = document.documentElement.scrollLeft;

		if (!document.documentElement.scrollTop)
			scrollY = document.body.scrollTop;
		else
			scrollY = document.documentElement.scrollTop;
		}
		else
		{
		scrollX = window.pageXOffset;
		scrollY = window.pageYOffset;
		}
		document.forms[0].EventsControl1_scrollLeft.value = scrollX;
		document.forms[0].EventsControl1_scrollTop.value = scrollY;
	}


	function ScrollerMemory_Scroll()
	{
		var x = document.forms[0].EventsControl1_scrollLeft.value;
		var y = document.forms[0].EventsControl1_scrollTop.value;
		window.scrollTo(x, y);
	}

	  
	window.onload = ScrollerMemory_Scroll;
	window.onscroll = ScrollerMemory_GetCoords;
	window.onclick = ScrollerMemory_GetCoords;
	window.onkeypress = ScrollerMemory_GetCoords;
	// -->
