        <script>
            $(document).ready(function() {
                $('.gordon_menu_mobile ul').siblings('a').click( 
                    function(event) {
                        if($(this).siblings('ul').is(':hidden'))
                            {		$(this).parent('li').children('ul').slideDown(); $(this).addClass('mmexpanded')     }
                        else { $(this).parent('li').children('ul').slideUp(); $(this).removeClass('mmexpanded');      }
                		if( $(this).parent('li').children('ul').length > 0 ) { event.preventDefault(); }
                	}
                );
                $('#top_sm').html( $('#foot_socialmedia').html() );
                $('#top_sm').children('span').removeAttr("id");
            });
        </script>
        
