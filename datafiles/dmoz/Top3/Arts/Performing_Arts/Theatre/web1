function createBigDoorUser(uId){
    var today = new Date();
    var tomorrow = new Date();
				tomorrow.setDate(today.getDate()+2);

	  var userId = uId;
	  var userName = registration.utils.cookies.getValue( "c_unm" );
	  var img = 'http://discussions.latimes.com/images/avatarPlaceHolder.jpg';
	  var bsk = '15988acb41e641b79c0682d360e555de';
	  var exp = Math.round(tomorrow/1000);
	  var asciiStr = 'display_name' + userName + 'end_user_login' + userId + 'expires' + exp + 'profile_image' + img + bsk;
	  var sig = CryptoJS.SHA256(asciiStr);	

	  var authUsr = 'display_name=' + encodeURIComponent(userName) + '&end_user_login=' + encodeURIComponent(userId) + 
					'&expires='+ encodeURIComponent(exp) + '&profile_image=' + encodeURIComponent(img) + '&sig=' + sig;

      return authUsr;
		}

	if (showBigDoor) {
		if (registration.user.isLoggedIn()) {
			id = registration.utils.cookies.getValue( "c_mId" );
			var bda = createBigDoorUser(id);
                        BDACWE = bda;
                        console.log('BDACWE ' + BDACWE);

		} else {
			registration.manager.events.getEventManagerInstance().addListener ('reg_after_login', function() {
				// your handler code
                        id = registration.utils.cookies.getValue( "c_mId" );
			var bda = createBigDoorUser(id);
                        BDACWE = bda;

				BDM.app.current().auth.authenticating(true);
				BDM.app.current().auth.check_authenticated_user(false, function(){
				    BDM.app.current().auth.authenticating(false);
				}, this);
			});
		}
		
	}
</script>
