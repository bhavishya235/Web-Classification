.center_videobar_title a {color: white; font: bold 16px arial,sans-serif; text-decoration: none; line-height: 1; padding: 10px 10px 55px; display: block;}
.center_videobar_thumb {float: left;}
.center_videobar_thumb_container {margin: 2px 0 0 0;}

/* Smaller-sized dynamic-collection based blog rolls headlines 2013-06-26 AE */
div.headlines.blog-roll h2.title {font-size:22px;}

</style>

<script type="text/javascript">
  (function() {
	var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
po.src = 'https://apis.google.com/js/plusone.js';
var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
