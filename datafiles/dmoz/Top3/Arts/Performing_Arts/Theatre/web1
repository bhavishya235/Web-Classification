    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
  })();
//]]>
</script>
<g:plusone size="medium" href="http://www.villagevoice.com/2007-05-15/theater/2006-2007-obie-award-winners/"></g:plusone></li></ul><div class="ClearIt"></div> </div>
