var scrollercontent='<div><span><a href=\"/services/inscription.php?sid=&lang_site=fr\" class=\"txtorangegras\"><font class=\"txtorangegras\">LAURENT BERETTA ILLUSIONNISTE (news)</A></span><br><span class=\"txtnoirgras\"><A href=\"javascript:dopopup(\'http://www.magiczoom.com/profil/fr/2771/magicien-magicienne-lulumessagere.htm?pup=1\')\"><span id=\'cl2771\' class=\'r1\'>lulumessagere</span></A> : <A href=\"/services/inscription.php?sid=&lang_site=fr\" class=\"txtnoir\" ><font class=\"txtnoir\">31 DECEMBRE  minuit LAURENT ......</A></span><br /><span class=\"txtgris\"> 31 Dcembre 2012  19:17:35</span><br></div><br><br><div><span><a href=\"/services/inscription.php?sid=&lang_site=fr\" class=\"txtorangegras\"><font class=\"txtorangegras\">VIKTOR VINCENT est sur facebook (news)</A></span><br><span class=\"txtnoirgras\"><A href=\"javascript:dopopup(\'http://www.magiczoom.com/profil/fr/2771/magicien-magicienne-lulumessagere.htm?pup=1\')\"><span id=\'cl2771\' class=\'r1\'>lulumessagere</span></A> : <A href=\"/services/inscription.php?sid=&lang_site=fr\" class=\"txtnoir\" ><font class=\"txtnoir\">Vous pouvez suivre les ......</A></span><br /><span class=\"txtgris\"> 14 Novembre 2012  18:06:59</span><br></div><br><br><div><span><a href=\"/services/inscription.php?sid=&lang_site=fr\" class=\"txtorangegras\"><font class=\"txtorangegras\">VIRTUOSES DE L\'ETRANGE/ SAMEDI 17 NOVEMBRE 17H00 (news)</A></span><br><span class=\"txtnoirgras\"><A href=\"javascript:dopopup(\'http://www.magiczoom.com/profil/fr/2771/magicien-magicienne-lulumessagere.htm?pup=1\')\"><span id=\'cl2771\' class=\'r1\'>lulumessagere</span></A> : <A href=\"/services/inscription.php?sid=&lang_site=fr\" class=\"txtnoir\" ><font class=\"txtnoir\">Programme du Samedi 17 ......</A></span><br /><span class=\"txtgris\"> 14 Novembre 2012  18:05:54</span><br></div><br><br><div><span><a href=\"/services/inscription.php?sid=&lang_site=fr\" class=\"txtorangegras\"><font class=\"txtorangegras\">Dlit de Magie  de Jack Barlet