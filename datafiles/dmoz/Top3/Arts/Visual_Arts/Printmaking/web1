	function AlignTwitter() {
		var tasterbottom = document.getElementById('tasters').offsetTop + 182;
		var twitheight = document.getElementById('twitterpanel').offsetHeight;
		if (twitheight == 0) { twitheight = 286; } //ie as usual can't cope so sometimes reterns 0 so fix value other browsers OK
		document.getElementById('twitterpanel').style.top = (eval(tasterbottom - twitheight)) + 'px';
	}
	window.onload = function () {
		AlignTwitter();
	}
