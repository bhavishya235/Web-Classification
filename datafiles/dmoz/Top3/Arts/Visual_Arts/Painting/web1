var _object;
var _objectCursor;
var mouseX;
var mouseY;

$(document).mousemove(function (e) {
	mouseX=e.clientX;
	mouseY=e.clientY;
});

function amazonClickView(object) {
    _object = object;
    _objectCursor = _object.style.cursor;
    busy(_object);
    setTimeout( 'scroll(0,0)', 0 ); 
	setTimeout( "document.getElementById('cpfAPC').click()", 500 ); 
	setTimeout( "$('#mobileBusyDiv').css('display','none')", 750 ); 
    setTimeout( "dimOff2(_object,_objectCursor)", 1000 ); 
    setTimeout( "", 1000 ); 
}

function amazonClickAdd(object) {
    _object = object;
    _objectCursor = _object.style.cursor;
    busy(_object);
    gaEvent( '', 'click',  'click-CartAdd', object.title ); 
    setTimeout( 'scroll(0,0)', 0 ); 
	setTimeout( "$('#mobileBusyDiv').css('display','none')", 750 ); 
    setTimeout( "dimOff2(_object,_objectCursor)", 1000 ); 
}

function busy(object) {
    //document.body.style.cursor = 'wait';
	dimOn();

    object.style.cursor='wait';

	$('#mobileBusyDiv').css('display','block');
	$('#mobileBusyDiv').css('background-color','blue');
	//$('#mobileBusyDiv').css('color','white');
	//$('#mobileBusyDiv').css('height','3em');
    $('#mobileBusyDiv').css('position','fixed');
    $('#mobileBusyDiv').css('left', mouseX+10+'px');
	$('#mobileBusyDiv').css('top', mouseY+'px');
}

function dimOn() {
    document.body.className = 'dim1 dim2';
    document.body.style.cursor = 'wait';
}

function dimOff() {
    document.body.className = '';
    document.body.style.cursor = 'default';
}

function dimOff2(object, objectCursor) {
    dimOff();
    object.style.cursor=objectCursor;
}

function trackAction(_subject, _message) {
    dimOn();
    document.body.style.cursor = 'wait';
}

function trackAddCart(item) {
    dimOn();
    document.body.style.cursor = 'wait';
}

function trackViewCart(item) {
    dimOn();
    document.body.style.cursor = 'wait';
}

function sendRequestGet(phpFile, keyValue) {
    if (true || phpFile=="" || keyValue=="") {
        return;
    } 

    if (window.XMLHttpRequest) {
        // code for IE7+, Firefox, Chrome, Opera, Safari
        xmlhttp=new XMLHttpRequest(); 
    }
    else {
        // code for IE6, IE5
        xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");     
    }
    xmlhttp.onreadystatechange=function() {
        if (xmlhttp.readyState==4 && xmlhttp.status==200) {
            
        }
    }
    xmlhttp.open("GET",phpFile+"?"+keyValue,true);
    xmlhttp.send();
}

var isMouseIn = false;
var isOffPending = false;
function allLinksMouseIn(key) {
    isMouseIn = true;
    $('.allLinksItem').hide();
    document.getElementById(key).style.display='inline';
}

function allLinksMouseOut(key) {
    isMouseIn = false;
    if (isOffPending == false) {
        isOffPending = true;
        //setTimeout( "allLinksOff()", 3000 ); 
    }
}

function allLinksOff() {
    if (isMouseIn == false) {
        $('.allLinksItem').hide();
    }
    isOffPending = false;
}



