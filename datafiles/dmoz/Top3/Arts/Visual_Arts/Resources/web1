		document.images.imm4.alt=document.modulo.nome[1].value;
		qualelink4 = 1;
	}
	else if(x==7){
		document.images.imm2.src = "imm/Dettagli/"+document.modulo.record[0].value+".jpg";
		document.images.imm2.alt=document.modulo.nome[0].value;
		qualelink2 = 0;
		document.images.imm3.src = "imm/Dettagli/"+document.modulo.record[1].value+".jpg";
		document.images.imm3.alt=document.modulo.nome[1].value;
