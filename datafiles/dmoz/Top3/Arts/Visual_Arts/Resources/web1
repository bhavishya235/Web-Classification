<!-- 
ns = (!document.all && document.getElementById)
ie = (document.all)? true:false
var x;
var qualelink1;
var qualelink2;
var qualelink3;
var qualelink4;

function Linko(indirizzo)
	{ 
	if (indirizzo=='')
		{
		indirizzo=='#'
		}
	window.location.href = indirizzo;
	} 

x = 0
function roller(){ 
	document.images.imm1.src = "imm/Dettagli/"+document.modulo.record[x].value+".jpg";
	document.images.imm1.alt=document.modulo.nome[x].value;
	qualelink1 = x;
	if (x==5){
		document.images.imm2.src = "imm/Dettagli/"+document.modulo.record[x+1].value+".jpg";
		document.images.imm2.alt=document.modulo.nome[x+1].value;
		qualelink2 = x+1;
		document.images.imm3.src = "imm/Dettagli/"+document.modulo.record[x+2].value+".jpg";
		document.images.imm3.alt=document.modulo.nome[x+2].value;
