		qualelink4 = x+3;
	}
	x = x + 1;
	if (x==8){
		x = 0;
	}
} 

function avvia(){
	if(ie){
	  document.images.imm2.style.visibility='visible';
	  document.images.imm3.style.visibility='visible';
	  document.images.imm4.style.visibility='visible';
	  }else if(ns){
	  document.images["imm2"].style.visibility='visible';
	  document.images["imm3"].style.visibility='visible';
	  document.images["imm4"].style.visibility='visible';
	}
	roller();
	clearInterval(timerID);
	timerID=setInterval('roller()',2000);
} 

function carica(){
	preloaddBottoneInMovimento = new Image();
	preloaddBottoneInMovimento.src = "imm/bottone2b.gif";
	preloadd1 = new Image();
	preloadd1.src = "imm/Dettagli/"+document.modulo.record[0].value+".jpg";
	preloadd2 = new Image();
	preloadd2.src = "imm/Dettagli/"+document.modulo.record[1].value+".jpg";
	preloadd3 = new Image();
	preloadd3.src = "imm/Dettagli/"+document.modulo.record[2].value+".jpg";
	preloadd4 = new Image();
	preloadd4.src = "imm/Dettagli/"+document.modulo.record[3].value+".jpg";
	preloadd5 = new Image();
	preloadd5.src = "imm/Dettagli/"+document.modulo.record[4].value+".jpg";
	preloadd6 = new Image();
	preloadd6.src = "imm/Dettagli/"+document.modulo.record[5].value+".jpg";
	preloadd7 = new Image();
	preloadd7.src = "imm/Dettagli/"+document.modulo.record[6].value+".jpg";
	preloadd8 = new Image();
	preloadd8.src = "imm/Dettagli/"+document.modulo.record[7].value+".jpg";
}

function controlla_caricamento(){
	if (preloadd1.complete==true && preloadd2.complete==true && preloadd3.complete==true && preloadd4.complete==true && preloadd5.complete==true && preloadd6.complete==true && preloadd7.complete==true && preloadd8.complete==true){
		avvia();
	}
}
//--> 
</script>
