                                           _/,__\,
                                        __/ _/o'o
                                      /  '-.___'/  __
                                     /__   /\  )__/_))\
          /_/,   __,____             // '-.____|--'  \\
         e,e / //  /___/|           |/     \/\        \\
         'o /))) : \___\|          /   ,    \/         \\
          -'  \\__,_/|             \/ /      \          \\
                  \_\|              \/        \          \\
                  | ||              &lt;    '_    \          \\
                  | ||             /    ,| /   /           \\
