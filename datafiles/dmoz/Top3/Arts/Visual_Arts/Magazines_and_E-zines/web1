links1[4] = "http://www.peterfiner.com/catalogue/"
links1[5] = "http://www.peterfiner.com/catalogue/"
links1[6] = "http://www.peterfiner.com/catalogue/"


code1 = new Array ();
code1[0] = "(['_trackEvent', 'Ads', 'Banner_1', 'Peter Finer']);"
code1[1] = "(['_trackEvent', 'Ads', 'Banner_1', 'Peter Finer']);"
code1[2] = "(['_trackEvent', 'Ads', 'Banner_1', 'Peter Finer']);"
