<SCRIPT LANGUAGE="JavaScript">


function controllo(prova) {
questo=document.myform;
ceccato = questo.elements.length;
var i=0;
for( i=0 ; i<ceccato ; i++) {
if (questo.elements[i].name=='lista[]') {
questo.elements[i].checked=prova;
}
}
}

function controllo_usa(prova) {
questo=document.myform;
ceccato = questo.elements.length;
for( i=238 ; i<ceccato ; i++) {
if (questo.elements[i].name=='lista[]') {
questo.elements[i].checked=prova;
}
}
}

function display()
{
myButton = document.getElementById("reg");
	myt = document.getElementById("Idregalo");
	if(myButton.checked!="")
		{
			myt.style.display="";
			} else {
			myt.style.display="none";
			}
}

function ValidateRequiredFields(FormName, RequiredFields)
{
var FieldList = RequiredFields.split(",");
var BadList = new Array();
var meil = true;

myButton = document.getElementById("reg");	
if(myButton.checked!="")
	{
		var s = eval('document.' + FormName + '.' + 'Rnome' + '.value');
		s = StripSpacesFromEnds(s);

		if(s.length < 1) {  BadList.push('Rnome');}
		
		var s = eval('document.' + FormName + '.' + 'cognomeRnome' + '.value');
		s = StripSpacesFromEnds(s);

		if(s.length < 1) {  BadList.push('cognomeRnome');}
		
		
	}	

for(var i = 0; i < FieldList.length; i++) {
		var s = eval('document.' + FormName + '.' + FieldList[i] + '.value');
		s = StripSpacesFromEnds(s);

	if(s.length < 1) {  BadList.push(FieldList[i]); }
	if(FieldList[i]=='mail') {
	meil = isEmail(s);	
	}
	}
if((BadList.length < 1) && (meil)) {return true; }
var ess = new String();
if(BadList.length > 0) { ess = 's'; 
var message = new String('\n\nThe field has not been correctly compiled:\n');
for(var i = 0; i < BadList.length; i++) 
	{ 
		if(BadList[i]=='cognome')
			{ message += '\n' + 'Surname'; }
		 if(BadList[i]=='nome')
			{ message += '\n' + 'First name'; }
		 if(BadList[i]=='mail')
			{ message += '\n' + 'E-mail address'; }
		 if(BadList[i]=='indirizzo')
			{ message += '\n' + 'Address'; }
		 if(BadList[i]=='citta')
			{ message += '\n' + 'Town'; }
		 if(BadList[i]=='paese')
			{ message += '\n' + 'Country'; }
		 if(BadList[i]=='numero')
			{ message += '\n' + 'Number'; }
		 if(BadList[i]=='cap')
			{ message += '\n' + 'Zip code'; }
		if(BadList[i]=='provincia')
			{ message += '\n' + 'County'; }
		if(BadList[i]=='telefono')
			{ message += '\n' + 'Phone no'; }
		if(BadList[i]=='Piva')
			{ message += '\n' + 'VAT number'; }
		if(BadList[i]=='Cfiscale')
			{ message += '\n' + 'Fiscal code'; }
		if(BadList[i]=='Rnome')
			{ message += '\n' + 'Present'; }
		if(BadList[i]=='cognomeRnome')
			{ message += '\n' + 'Surname present'; }	
		
			
	}//endwhile
if(!meil)	{
	message += '\n\n' + 'E-mail wrong';
}
alert(message);
return false;
}
return false;
}

function StripSpacesFromEnds(s)
{
while((s.indexOf(' ',0) == 0) && (s.length> 1)) {
	s = s.substring(1,s.length);
	}
while((s.lastIndexOf(' ') == (s.length - 1)) && (s.length> 1)) {
	s = s.substring(0,(s.length - 1));
	}
if((s.indexOf(' ',0) == 0) && (s.length == 1)) { s = ''; }
return s;
}

function isEmail(str) {
  // are regular expressions supported?
  var supported = 0;
  if (window.RegExp) {
    var tempStr = "a";
    var tempReg = new RegExp(tempStr);
    if (tempReg.test(tempStr)) supported = 1;
  }
  if (!supported) 
    return (str.indexOf(".") > 2) && (str.indexOf("@") > 0);
  var r1 = new RegExp("(@.*@)|(\\.\\.)|(@\\.)|(^\\.)");
  var r2 = new RegExp("^.+\\@(\\[?)[a-zA-Z0-9\\-\\.]+\\.([a-zA-Z]{2,3}|[0-9]{1,3})(\\]?)$");
  return (!r1.test(str) && r2.test(str));
}

function relocate(locatie)
	{
	self.location=locatie;
	}

</script>

