<script language="JavaScript">
var haveimgwin = false;
var conpage = '';
function viewimg(imgname, wd, ht, imgtitl, imgcapt)
{
    if (haveimgwin) { imgwin.close(); }
    wht = ht + 300;
    wwd = wd + 44;
    imgwin=open('','imgwin','scrollbars=yes,status=no,width='+wwd+',height='+wht+',dependent=yes');
    conpage = '<html><head>' +
              "<link rel='stylesheet' type='text/css' href='/az.css' />" +
               "<title> ARTEZINE: " +
               imgtitl +
               "</title></head><body bgcolor='#110512' text='#999999' link='yellow' vlink='yellow' alink='yellow'>\n" +
               "<a href='javascript: self.close();'>\n" +
               "<img src='lar.gif' border='0' align='left'> BACK </a>\n" +
               "<table border='0' cellpadding='8'><tr><td valign='top' align='center'>\n" +
               "<img src='" + imgname + "' border='0'><br clear='all'>" +
               "<p class='sm'>" + imgcapt + "</p>" +
               "</td></tr></table>" +
               // '<small>' + wd + ' x ' + ht + '</small><br>' +
               "</body></html>";
    imgwin.document.write(conpage);
    imgwin.focus();
    imgwin.document.close();
    haveimgwin = true;
}

var havearcwin = false;
function showarcwin()
{
    if (havearcwin) { imgwin.close(); }
    wht = ht + 300;
    wwd = wd + 44;
    arcwin=open('arcpage.html','arcwin','scrollbars=yes,status=no,width='+wwd+',height='+wht+',dependent=yes');
    arcwin.focus();
    havearcwin = true;
}

//
//  The following array deals only with 18gps2
//
imNama = new Array(
                   '18gps2_rt_ae_ss_53141422_mp4_1_png_md.jpg',
                   '18gps2_rt_ae_ss_53141422_mp4_2_png_md.jpg',
                   '18gps2_rt_ae_ss_53141422_mp4_3_png_md.jpg',
                   '18gps2_rt_ae_ss_53141422_mp4_4_png_md.jpg',
                   '18gps2_rt_ae_ss_53141422_mp4_5_png_md.jpg',
                   '18gps2_rt_kcoreainc_jpg_md.jpg',
                   '18gps2_rt_popular_sky_jpg_md.jpg',
                   '18gps2_rt_ready_jpg_md.jpg',
                   '18gps2_rt_roamie_view_jpg_md.jpg',
                   '18gps2_rt_sibling_topics2_jpg_md.jpg',
                   '18gps2_rt_sibling_topics_jpg_md.jpg',
                   '18gps2_rt_the_research2_jpg_md.jpg',
                   '18gps2_rt_the_research3_jpg_md.jpg',
                   '18gps2_rt_the_research_jpg_md.jpg');
imNamalen = imNama.length;

imOba = new Array(imNamalen);

var imob = new Image;
var k = -1;
var xok = 100;
var working = 0;
var ie5  = (document.all && document.getElementById);
var ns6 = (!document.all && document.getElementById);

function imfadeout(i) {
    xok -= 10;
    if (ie5) imslot.filters.item("DXImageTransform.Microsoft.Alpha").opacity = xok;
    if (ns6) {
      document.images.xox.style.opacity = xok/100;
      document.images.xox.style.filter = 'alpha(opacity=' + xok + ')';
    }
    if (xok > 10) { setTimeout('imfadeout()', 1); return; }
    working = 0;
}
function imfadein() {
    working = 1;
    xok += 20;
    if (ie5) imslot.filters.item("DXImageTransform.Microsoft.Alpha").opacity = xok;
    if(ns6) {
        document.images.xox.style.opacity = xok/100;
        document.images.xox.style.filter='alpha(opacity='+ xok + ')';
    }
    if (xok < 99) { setTimeout('imfadein()', 1); return; }
    setTimeout('imfadeout()', 2000);
}
function imfun(imPrefix) {
    for (i = 0; i < imNamalen; i++)
    {
        imOba[i] = new Image;
        imOba[i].src = imPrefix + '/' + imNama[i];
    }
    if (working == 0)
    {
        k += 1;
        if (k >= imNama.length) { k = 0; }
        document.images.xox.src = imOba[k].src;
        xok = 0;
        imfadein();
    }
    setTimeout('imfun("' + imPrefix + '")', 1000);
}
</script>
</head>

