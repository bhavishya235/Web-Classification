var r = Math.floor(163*Math.random())
var myimage = theImages[r]
var mylink = links[r]
var mytext = r_text[r]
function showImage(){
document.write('<A HREF="' + mylink + '" target="_blank"><IMG src="' + myimage + '" width="941" height="496" border="0"></A>')
}
function showText(){
document.write(r_text[r]);
}
function showLink(){
document.write('<FONT color="#CA0205">360&deg;&nbsp;</FONT><A HREF="' + mylink + '" target="_blank"><FONT color="#CA0205">' + mytext +'</FONT></A>')
}
//-->
</script>
