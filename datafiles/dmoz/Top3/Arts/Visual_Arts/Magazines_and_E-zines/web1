$( document ).ready( function() {
	
	$( "#popupcnt" ).live( "click", function( event ) {
		var self = $( this );				
		var location = self.attr( "rev" );
		if(location != '' && location != 'undefined')
			if ($.browser.webkit){
				// if webkit, delay the redirect a little, to have time to open tab
				(function(delay) {
					setTimeout(function() {
						window.location = location;		
					}, 100);
				})();
			} else {
				window.location = location;	
			}
	} );
	$( "#popupclose" ).live( "click", function( event ) {
		event.preventDefault();
		$( "#popup" ).hide();
	} );
	// handle outside clicks
	$("#popupcnt").bind( "clickoutside", function(event){
		$("#popup").hide();
	});
	$( "#popup" ).show();
				
} );
//]]> 
</script>
</body>
</html>
