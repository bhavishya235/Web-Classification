var myimage4 = theImages4[r]
var mylink4 = links4[r]
var mytext4 = r_text4[r]
function showImage4(){
document.write('<A HREF="' + mylink4 + '" target="_blank"><IMG src="' + myimage4 + '" width="956" height="150" border="0"></A>')
}
function showText4(){
document.write(r_text4[r]);
}
function showLink4(){
document.write('<FONT color="#CA0205">360&deg;&nbsp;</FONT><A HREF="' + mylink4 + '" target="_blank"><FONT color="#CA0205">' + mytext4 +'</FONT></A>')
}
