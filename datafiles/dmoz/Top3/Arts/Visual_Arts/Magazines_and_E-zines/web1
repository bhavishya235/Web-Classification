<script language="JavaScript">
var confirmMsg  = 'Confermi ';
function confirmLink(theLink, theSqlQuery)
{
    // Confirmation is not required in the configuration file
    // or browser is Opera (crappy js implementation)
    if (confirmMsg == '' || typeof(window.opera) != 'undefined') {
        return true;
    }

    var is_confirmed = confirm(confirmMsg + '' + theSqlQuery);
    if (is_confirmed) {
        theLink.href += '&is_js_confirmed=1';
    }

    return is_confirmed;
} // end of the 'confirmLink()' function


</script>
<script language="JavaScript">
function cambio_sfondo(tabella) {
 tabella.style.backgroundColor = "#BDDDFF";
}
function sfondo_bianco(tabella) {
 tabella.style.backgroundColor = "#FFFFFF";
}

function guarda(nome){
  tab=document.getElementById('tabella');
  riga=tab.rows.length;
  for(i=0;i<=riga-1;i++){ 
  if ((tab.rows(i).name=='nascosto')&&(tab.rows(i).id!=nome)){
		tab.rows(i).style.display='none';
	}	
  }
	if (document.getElementById(nome).style.display == 'none'){
		document.getElementById(nome).style.display = '';
	}else{ 
	  document.getElementById(nome).style.display = 'none';	
  }   
}
function mostra() {
if (document.modulo.submit.style.display == 'none'){
		document.modulo.submit.style.display = '';
		document.modulo.reset.style.display = '';
	}else{ 
	 document.modulo.submit.style.display == 'none';	
  }
}
</script>
<script language="JavaScript">
function aggiorna_campo(val) {
	//1
	if (val==8) {
		document.getElementById('si_campo').style.display='';
		//document.getElementById('no_campo').className='invisibile';
		}
	else {
		document.getElementById('si_campo').style.display='none';
		//document.getElementById('no_campo').className='visibile';
		}
	}
	
</script>
<script language="JavaScript">
function aggiorna_campo_riv(val) {
	//1
	if ((val==8) || (val==9) || (val==10) || (val==13) || (val==20) || (val==21)) {
		document.getElementById('si_campo_riv').style.display='';
		//document.getElementById('no_campo').className='invisibile';
		}
	else {
		document.getElementById('si_campo_riv').style.display='none';
		document.modulo.autore_rc.value='';
		document.modulo.edizione_rc.value='';
		//alert(document.modulo.edizione_rc.value);
		//document.getElementById('no_campo').className='visibile';
		}
	//2
	if ((val==8) || (val==9) || (val==10) || (val==13)) {
		document.getElementById('si_campo_riv2').style.display='';
		//document.getElementById('no_campo').className='invisibile';
		}
	else {
		document.getElementById('si_campo_riv2').style.display='none';
		document.modulo.autore_rc.value='';
		document.modulo.edizione_rc.value='';
		//alert(document.modulo.edizione_rc.value);
		//document.getElementById('no_campo').className='visibile';
		}
	}
	
</script>
