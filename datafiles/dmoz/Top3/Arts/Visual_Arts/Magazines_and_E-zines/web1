    <b:OplEcp type="OplEcp" priv="711">
    <b:Color>16777215</b:Color>
    </b:OplEcp>
    </b:Rgecp>
    <b:SzSchemeName priv="618">Bluebird</b:SzSchemeName>
    </b:ColorScheme>
