<!-- 19dtu.txt -->

<!-- set pclass to 'de_b' -->
<!--  Wave Hill:  Artists Respond to the Wild Garden -->
<!--  Donna Maria deCreeft 2012 -->

              <!-- 19dtu_wildgarden_title.jpg () -->
              <div align='center'>
                <table>
                  <tr>
                    <td align='center' valign='center'>
                      <img src='issues/19/19dtu_wildgarden_title.jpg'
                           class='i2'
                           border='0' align='center'
                           width='590' height='521'
                           alt='' 
                           title=''>
                    </td>
