		$('#header ul li ul').slideUp();
	});
});

function loadEditor(id)
{
    var instance = CKEDITOR.instances[id];
    if(instance)
    {
        CKEDITOR.remove(instance);
    }
	CKEDITOR.replace(id,{on:{instanceReady: function(ev){resizeEditor();}}});	
}

function resizeEditor() {
  var newHeight = window.innerHeight-260; 
  if (CKEDITOR){
	CKEDITOR.instances.desc.resize('100%',newHeight);
  }
}

window.onresize = function() {
  resizeEditor();
}

