<script type="text/javascript">
function blank(a) { if(a.value == a.defaultValue) a.value = ""; }
function unblank(a) { if(a.value == "") a.value = a.defaultValue; }
<!-- Slider Jquery Start -->
$(document).ready(function(){

	$(window).load(function() {
			$(".news_tick_load").show();
	});		
	
	$("ul#ticker01").liScroll({travelocity: 0.08});
	
	//Tabs
	$("#tab-box li").each(function(i){		
		i=i+1;					   
		$(this).click(function(){
			$("#tab-box li").removeClass("selected");				   
			$(".tabDetails").hide();
			$("#tabCont_"+i).show();
			$(this).addClass("selected");
			return false;
		});
	});
	
	$('.scroll-pane').jScrollPane();
	
	$('#slideshow').cycle({ 
		fx: 'fade', 
		speed: 'slow', 
		timeout: 3000, 
		pager: '#pagi' 
	});
	

	// for more column on home
	$(".col_first .columnsList .moreCols p a").html('More Columns').click(function(){
		$('.col_first .columnsList').find('ul').css('height','auto');
		return false;
	});

	$(".col_second .columnsList .moreCols p a").html('More Ac Boxing').click(function(){
		$('.col_second .columnsList').find('ul').css('height','auto');
		return false;
	});


	$('.connect_text_wrapper').css('border', '1px solid red');
	
});

</script>

