	#sideoffer .box { position: absolute; right:0; width: 78%; padding: 15px 10px; color: #ffffff; }
	#sideoffer p { margin-bottom: 5px !important; }
	</style>
    <!-- End SideOffer [HD] CSS -->
    </head>
