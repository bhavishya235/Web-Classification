	<script>
		
		var limitFriendCount = 5;
		var returnedFriendConnectiondataForSidebar,returnedSingleConnectiondataForSidebar;
	
		function getMyConnectionHTMLDisplay(data){
			myRealConnectionsData = data;
			
			myConnectionHTML='';
			
			if(myRealConnectionsData[0].BioConnections){
				//for(x=0; x < 3; x++){
				if($('#displayPageType').html()=="homepage"){
					hide = false;
					myConnectionHTML += getConnectionListHTML(myRealConnectionsData[0],myRealConnectionsData[0].BioConnections[0],"home",hide,0,'my');
					$('#your-connections-more-connections-button').html("FIND OUT MORE");
				}
				else{
					for(x=0; x < myRealConnectionsData[0].BioConnections.length; x++){
						if($('#displayPageType').html()=="other" || $('#displayPageType').html()=="homepage"){
							hide = (x > 2) ? true : false;
							myConnectionHTML += getConnectionListHTML(myRealConnectionsData[0],myRealConnectionsData[0].BioConnections[x],"other",hide,x,'my');
						}
						else if($('#displayPageType').html()=="group"){
							hide = (x > 2) ? true : false;
							myConnectionHTML += getConnectionListHTML(myRealConnectionsData[0],myRealConnectionsData[0].BioConnections[x],"group",hide,x,'my');
						}
						else{
							hide = (x > 2) ? true : false;
							myConnectionHTML += getConnectionListHTML(myRealConnectionsData[0],myRealConnectionsData[0].BioConnections[x],"profile",hide,x,'my');
						}
					}
					if(myRealConnectionsData[0].BioConnections.length<3){
						$('#your-connections-more-connections-button').hide();
					}
					else{
						//$('#your-connections-more-connections-button').html("Show More Connections ("+(myRealConnectionsData[0].BioConnections.length-3)+")");
						$('#your-connections-more-connections-button').html("See more bio connections");
					}
				}
				
			}
			else{
				if($('#displayPageType').html()=="homepage"){
					$('.logged-in-list-container .show-more').hide();
					myConnectionHTML +='<div id="your-connections-module" style="padding-top:10px" class="module"><h5><span>Your Connections</span></h5>'+showPageSpecificTypeEmptyText($('#displayPageType').html(),'You have no connections!',false)+'</div>';
				}
				else{
					myConnectionHTML += showPageSpecificTypeEmptyText($('#displayPageType').html(),'You have no connections!',false);
				}
				$('#your-connections-more-connections-button').hide();
				$('#specific-profile-connection').hide();
			}
	
			
			nameArray = myRealConnectionsData[0].facebookProfileName.split(" ");
			$("#facebook-user-name").html(nameArray[0]);
			$("#top-facebook-image").attr("src",myRealConnectionsData[0].facebookProfileImage);
			$('.loading-list-container').hide();
			$('.logged-in-list-container.logged-in').show();
			$('.logged-in-list-container.my').show();
			$("#my-connections-list").html(myConnectionHTML);
			
			//initialize logout button
			$(".small-login-link-html.logout").click(function(e){
				e.preventDefault();
				var params = {
					callback: socialze_logout_callback
				}
				aetn.socialize.logout(params);
			});
		}
		
		function getFriendConnectionHTMLDisplay(data){
			FriendDataReturn = data;
			//console.info(data); // json data 
			//console.info(FriendsConnectionsData); // json data 				
			friendConnectionHTML='';
			//FriendsConnectionsData={};
			
			for(z=0; z < FriendDataReturn.length;z++){
				if(FriendDataReturn[z].BioConnections){
					myRealFriendConnectionsData.push(FriendDataReturn[z]);
				}
				else{
					//
				}	
			}
			
			if(myRealFriendConnectionsData.length>0){
				if($('#displayPageType').html()=="homepage"){
					hide = false;
					friendConnectionHTML += getFriendConnectionListHTML(myRealFriendConnectionsData[0],myRealFriendConnectionsData[0].BioConnections[0],hide,0,"home",false);
				}
				else{
					for(x=0; x < myRealFriendConnectionsData.length; x++){
						hide = (x > 1) ? true : false;					
						friendConnectionHTML += getFriendConnectionListHTML(myRealFriendConnectionsData[x],myRealFriendConnectionsData[x].BioConnections[0],hide,x,'friend',true);
																																							
					}
				}
				
			}
			else{
				friendConnectionHTML='<li class="clearfix connection ">None of your friends have any connections!</li>';
			}
	
		
			$('.loading-list-container, .loading-friend').hide();
			$('.logged-in-list-container.logged-in').show();
			$('.logged-in-list-container.friend').show();
			$("#friends-connections-list").html(friendConnectionHTML);
		}
		
		function getConnectionListHTML(groupFriendObject,friendObject,style,hide,x,type){
			returnHTML =' '; 
			if(style=="home"){

					returnHTML +='<div class="clearfix logged-out-list-facebook-users">';
						returnHTML +='<strong class="facebook-heading-bold">Welcome '+groupFriendObject.facebookProfileName.split(' ')[0]+' You are connected with...</strong>';
						returnHTML +='<div style="padding-left: 40px;">';
							returnHTML +='<div class="facebook-user-container ">';
								returnHTML +='<div class="facebook-user ">';
									returnHTML +='<img class="facebook-image" src="'+groupFriendObject.facebookProfileImage+'">';
									returnHTML +='<img class="facebook-icon" src="/assets/images/common/facebook_favicon.png">';
								returnHTML +='</div>';
							returnHTML +='<a href="/" class="dot">You</a><br>';
							returnHTML +='<div id="small-login-link-html"><a href="'+$('#loggedinuser-sso-link').html() + "/user/" + username + '" class="small-login-link-html">settings</a> | <a href="#" class="small-login-link-html logout">logout</a></div>';
						returnHTML +='</div>';

								returnHTML +='<div class="facebook-user-container you">';
									returnHTML +='<div class="facebook-user you">';
									returnHTML +='<a href="'+friendObject.BioProfileURL+'"><img class="facebook-image" style="height:58x; width:58px; display:block;" src="'+friendObject.BioProfileImage+'" alt="'+friendObject.BioProfileURL+'"></a>';
									returnHTML +='</div>';
								returnHTML +='<a href="'+friendObject.BioProfileURL+'" class="dot">'+friendObject.BioProfileName+'</a>'; 
								returnHTML +='</div>';	

						returnHTML +='</div>';			
					returnHTML +='</div>';
			}
			else{
				returnHTML +='<li '; 
					if(style=="other"){ returnHTML +=' style="border-top:none;" '; } 
					returnHTML +=' class="clearfix connection ';
					if(hide){ returnHTML +=' hide '; } 
				returnHTML +='">';
				returnHTML +='<div class="float-left ">';
					returnHTML +='<a href="'+friendObject.BioProfileURL+'"><img class="facebook-image" src="'+friendObject.BioProfileImage+'"></a>';
				returnHTML +='</div>';
				returnHTML +='<div class="bio-profile-user float-right">';

					if(style=="other"){
						returnHTML +='<p><a class="dot" href="'+friendObject.BioProfileURL+'">'+friendObject.BioProfileName+'</a></p>';
						returnHTML +='<p>'+friendObject.BioProfileOccupation+'</p>';
					}
					else{
						returnHTML +='<p>'+friendObject.BioConnection;
						if(friendObject.moreConnection){
							returnHTML +='. <a href="#see-more-connections" class="red-underline see-more-link" id="moreconnection-'+type+'-'+x+'-'+friendObject.BioLinkProfileId+'"><span>see more connections</span></a>';
						}
						returnHTML +='</p>';					
						returnHTML +='<p class="social-share-connections">';
							returnHTML +='<a href="#" id="sharefacebookconnection-'+type+'-'+x+'-'+friendObject.BioLinkProfileId+'" class="fb-share-connection-button facebook-share-button"><img alt="facebook" src="/assets/images/common/facebook-share-button.png"></a>';
							//returnHTML +='<a href="#" id="sharetwitterconnection-'+type+'-'+x+'-'+friendObject.BioLinkProfileId+'" class="twitter-share-connection-button facebook-share-button"><img alt="twitter" src="/assets/images/common/twitter-share-button.png"></a>';
							//returnHTML +='<a style="display:none;" href="http://twitter.com/share?url=http://biography.com/bio-now&text='+friendObject.BioConnection.replace(/<\/?[^>]+>/gi, "")+'" class="twitter-share-button" data-url="http://biography.com/bio-now" data-text="'+friendObject.BioConnection+'" data-count="none"><img alt="twitter" src="/assets/images/common/twitter-share-button.png"></a>';
						returnHTML+='<a target="_blank" href="http://twitter.com/share?url=http://biography.com/bio-now&text='+friendObject.BioConnection.replace(/<\/?[^>]+>/gi, "")+encodeURIComponent(' #bio @bio')+' " class="twitter-share-button" data-url="http://biography.com/bio-now" data-text="'+friendObject.BioConnection.replace(/<\/?[^>]+>/gi, "")+encodeURIComponent('#bio @bio')+' " data-count="none"><img alt="twitter" src="/assets/images/common/twitter-share-button.png"></a>';	
						returnHTML +='</p>';
					}
					
					returnHTML +='</div>';
				returnHTML +='</li>';				
			}

			
			return returnHTML;
		}
		
		function getFriendConnectionListHTML(groupFriendObject,friendObject,hide,x,type,showMoreConnections){
			returnHTML =' '; 
			if(type=="home"){
					returnHTML +='<div class="clearfix logged-out-list-facebook-users">';
						returnHTML +='<strong class="facebook-heading-bold">Your friend '+groupFriendObject.facebookProfileName.split(' ')[0]+' is connected to...</strong>';
						returnHTML +='<div style="padding-left: 40px;">';
							returnHTML +='<div class="facebook-user-container ">';
								returnHTML +='<div class="facebook-user ">';
									returnHTML +='<img class="facebook-image" src="'+groupFriendObject.facebookProfileImage+'">';
									returnHTML +='<img class="facebook-icon" src="/assets/images/common/facebook_favicon.png">';
								returnHTML +='</div>';
							returnHTML +='<a href="/" class="dot">'+groupFriendObject.facebookProfileName+'</a>';
						returnHTML +='</div>';

								returnHTML +='<div class="facebook-user-container you">';
									returnHTML +='<div class="facebook-user you">';
									returnHTML +='<a href="'+friendObject.BioProfileURL+'"><img class="facebook-image" style="height:58x; width:58px; display:block;" src="'+friendObject.BioProfileImage+'" alt="'+friendObject.BioProfileURL+'"></a>';
									returnHTML +='</div>';
								returnHTML +='<a href="'+friendObject.BioProfileURL+'" class="dot">'+friendObject.BioProfileName+'</a>'; 
								returnHTML +='</div>';	

						returnHTML +='</div>';			
					returnHTML +='</div>';
					returnHTML +='<div class="show-more">'; 
						returnHTML +='<a href="/my-connections" id="your-connections-more-connections-button" class="blue-button" style="color:#fff;">See More Bio Connections</a>'; 
					returnHTML +='</div>';
			}
			else{
				returnHTML +='<li class="clearfix connection '; if(hide){ returnHTML +=' hide '; } returnHTML +='">';
				returnHTML +='<div class="float-left ">';
					returnHTML +='<img class="facebook-image" src="'+groupFriendObject.facebookProfileImage+'">';
				returnHTML +='</div>';
				returnHTML +='<div class="bio-profile-user float-right">';
					returnHTML +='<p>'+friendObject.BioConnection;
					if(friendObject.moreConnection && showMoreConnections){
						returnHTML +='. <a href="#see-more-connections" class="red-underline see-more-link" id="moreconnection-'+type+'-'+x+'-'+friendObject.BioLinkProfileId+'"><span>see more connections</span></a>';
					}
					returnHTML +='</p>';			
					returnHTML +='<p class="social-share-connections">';
						returnHTML +='<a href="#" id="sharefacebookconnection-'+type+'-'+x+'-'+friendObject.BioLinkProfileId+'" class="fb-share-connection-button facebook-share-button"><img alt="facebook" src="/assets/images/common/facebook-share-button.png"></a>';
						//returnHTML +='<a href="#" id="sharetwitterconnection-'+type+'-'+x+'-'+friendObject.BioLinkProfileId+'" class="twitter-share-connection-button facebook-share-button"><img alt="twitter" src="/assets/images/common/twitter-share-button.png"></a>';
						returnHTML+='<a target="_blank" href="http://twitter.com/share?url=http://biography.com/bio-now&text='+friendObject.BioConnection.replace(/<\/?[^>]+>/gi, "")+encodeURIComponent(' #bio @bio')+' " class="twitter-share-button" data-url="http://biography.com/bio-now" data-text="'+friendObject.BioConnection.replace(/<\/?[^>]+>/gi, "")+encodeURIComponent('#bio @bio')+' " data-count="none"><img alt="twitter" src="/assets/images/common/twitter-share-button.png"></a>';	
					returnHTML +='</p>';
					returnHTML +='</div>';
				returnHTML +='</li>';
				
			}
			
			return returnHTML;
		}
	
		function showPageSpecificTypeEmptyText(pageType,defaultText, showMoreText,friendsText){
			htmlForDisplay= (defaultText)? defaultText : (friendsText)? 'Your friends share no connections, but they have other connections' : 'You share no connections, but you have other connections';

			
			if(pageType=="group"){		
				if(friendsText){
					htmlForDisplay = (showMoreText) ? 'Your friends share no connections with '+$('#name-of-group-title').text() : 'Your friends share no connections with '+$('#name-of-group-title').text(); 		
				}
				else{
					htmlForDisplay = (showMoreText) ? 'You share no connections with '+$('#name-of-group-title').text()+', but you are connected to other people on biography.com' : 'You share no connections with '+$('#name-of-group-title').text()+', but you have other connections'; 		
				}
			}
			else if(pageType=="profile"){
				if(friendsText){
					htmlForDisplay = (showMoreText) ? 'Your friends  share no connections with '+$('#name-of-profile-title').text() : 'Your friends share no connections with '+$('#name-of-profile-title').text();
				}
				else{
					htmlForDisplay = (showMoreText) ? 'You share no connections with '+$('#name-of-profile-title').text()+', but you are connected to other people on biography.com' : 'You share no connections with '+$('#name-of-profile-title').text()+', but you have other connections';
				}
			}
			else if(pageType=="otd"){
				if(friendsText){
					htmlForDisplay = (showMoreText) ? 'Your friends share no connections with anyone on '+$('#name-of-onthisday-title').text() : 'You share no connections with anyone on '+$('#name-of-onthisday-title').text();
				}
				else{
					htmlForDisplay = (showMoreText) ? 'You share no connections with anyone on '+$('#name-of-onthisday-title').text()+', but you are connected to other people on biography.com ' : 'You share no connections with anyone on '+$('#name-of-onthisday-title').text()+', but you have other connections';
				}
			}
			
			return htmlForDisplay;
		}
	</script>
	
