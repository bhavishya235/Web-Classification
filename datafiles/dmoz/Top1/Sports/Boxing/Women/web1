					<h5><span>Your Friends' Connections</span></h5>
				
				
					
						<div id="specific-friend-connection" class="specific-friend-connection hide">specific friend connection</div>							
						<div id="friend-enter-up-connection-html"></div>
						<script>
							var returnedFriendConnectiondataForSidebar = {};
							$('#specific-friend-connection').addClass('loading').css({"height":"50px"}).html(' ').show();
							function displaySidebarFriendConnection(){
								$("#your-connection-dialog-pop-up").html('<div class="loading pop-up-connection-html" style="height:50px;"></div>');
								$('#friend-enter-up-connection-html').attr('rel','sidebarid-friend-0-'+($("#mainProfileIds").html()));
								if($("#mainProfileIds").html()){								
									getSingleFriendsProfileConnection('shortDescription');
								}
								else{						
									//htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'Your friends share no connections, but they have other connections',true,true);
									//never show other connections
								 	htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'Your friends share no connections, but they have other connections',false,true);
									$('#specific-friend-connection').html(htmlForDisplay).removeClass('loading');
									$('#friends-connections-list').show();
								}
							}
							function displayConnectionWithFriendProfile(data,type,objectIndex){								
								returnedFriendConnectiondataForSidebar = data;
								ignoreIdToList=false;
								var hasAtleastOneConnection = false;
								//alert('display side bar connections')
								if(returnedFriendConnectiondataForSidebar.length > 0 ){
									htmlForDisplay =' ';
									htmlForDisplay +='<ul>';
									
									if($('#displayPageType').html()=='group' || $('#displayPageType').html()=='otd' || $('#displayPageType').html()=='profile'){
										for(x = 0; x < returnedFriendConnectiondataForSidebar.length; x++){
											if(returnedFriendConnectiondataForSidebar[x].connections[0].BioConnections 
												&& returnedFriendConnectiondataForSidebar[x].connections[0].BioConnections != undefined){
												hasAtleastOneConnection=true;

												groupFriendObject = returnedFriendConnectiondataForSidebar[x].connections[0];
												friendObject = returnedFriendConnectiondataForSidebar[x].connections[0].BioConnections[0];
												htmlForDisplay += getFriendConnectionListHTML(groupFriendObject,friendObject,false,x,'friend');
												$("#specific-friend-connection").css({'border-bottom':'none'});
											}

										}	
										if(hasAtleastOneConnection == false){
											//htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'Your friends share no connections, but they have other connections',true,true);
											//never show other connections
											htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'Your friends share no connections, but they have other connections',false,true);
											
											$('#specific-friend-connection').html(htmlForDisplay).removeClass('loading');
											$('#friends-connections-list').show();
										}
									}
									else{//profile
										htmlForDisplay += returnedFriendConnectiondataForSidebar[0].connections[0].BioConnections[0].BioConnection;
									}	
									htmlForDisplay +='</ul>';

								
									$('#specific-friend-connection').css({'height':'auto'}).html(htmlForDisplay).removeClass('loading');//.css({"border-bottom":"1px solid #f2f2f2"});
									$('#friends-connections-list').hide();
								}
								else{
									//htmlForDisplay = 'You share no connections, but you have other connections';
									//htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'Your friends share no connections, but they have other connections',true,true);
									//never show other connections
									htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'Your friends share no connections, but they have other connections',false,true);

									 
									$('#specific-friend-connection').html(htmlForDisplay).removeClass('loading');
									$('#friends-connections-list').show();
								}
								$('.initial-fb-rail-load').hide();
								$('.logged-in-list-container').show();
							}
						</script>
					
					
				
				
				<ul id="friends-connections-list" class="connections-list">
					
				</ul>

			</div>
		</div>
	</div>
<!------- END: Your Connections Module -------> 
			
			
			
			
			
			
			
			
			
			
			
			
			

			
			
			
			
					

		
		
			






		
			
			
				
			
			
			
			
			
			
				
			

			
			
			
				



