<script type="text/javascript">
function Querystring(qs)
{ // optionally pass a querystring to parse
        this.params = {};

        if (qs == null) qs = location.search.substring(1, location.search.length);
        if (qs.length == 0) return;

        // Turn <plus> back to <space>
        qs = qs.replace(/\+/g, ' ');
        var args = qs.split('&'); // parse out name/value pairs separated via &

        // split out each name=value pair
        for (var i = 0; i < args.length; i++) {
            var pair = args[i].split('=');
            var name = decodeURIComponent(pair[0]);

            var value = (pair.length==2) ? decodeURIComponent(pair[1]) : "";

            this.params[name] = value;
        }
}

Querystring.prototype.get = function(key, default_)
{
        var value = this.params[key];
        return (value != null) ? value : default_;
}

Querystring.prototype.contains = function(key)
{
        var value = this.params[key];
        return (value != null);
}

var qs = new Querystring();
var dartTest = qs.get('test');
var profileId = '';

if(typeof dartTest == 'undefined') {
	dartTest = '';
}

if (20968407 != 0) {
	profileId = 20968407;
}

//TODO: Need to make sure we clean up certain parameters here according to dart rules (i.e., pid)
//TODO: Need to truncate certain dart tag values according to dart rules

var dart = {
	ord: Math.random()*10000000000000000,
	hpOrd: 0,
	siteName: "aetn.bio.people",
	s1: "profiles",
	s2: "",
	aid: profileId,
	kw: "",
	test: dartTest,
	pid: (location.pathname == "/") ? "home" : location.pathname.replace (/[^\w]/g, ""),
	
	group: [
	
	]
};

dart.hpOrd = dart.ord;

var numberOfTiles = 0;
function tileNumberAndDcopt() {
	numberOfTiles = numberOfTiles + 1;
	if (numberOfTiles == 1) {
		return numberOfTiles + ";dcopt=ist";
	}
	return numberOfTiles;
};

function getGroups() {
	var groupTag ='';
	
	for (var i = 0; i < dart.group.length; i++) {
		if(dart.group[i].length > 1)
			groupTag += 'group=' + dart.group[i] + ';'; 
	}
	
	return groupTag;
	
}

//If this is the home page and it is a box ad increment the ord values for each ad
function getOrd(s1, size) 
{
	if (s1 == "biohome" && size == "300x250") {
		dart.hpOrd = dart.hpOrd+1;
		return dart.hpOrd;
	} else {
		return dart.ord;
	}
}

/*************************************************
@description This is the callback function that quantcast will call with the resulting segments
@param result the list of segments from quantcast
*************************************************/
function qc_results(result) {
	var preQuantSeqs ='';
	var qcAdKeys = [];
	var qcValue = {};
	
	//preQuantSeqs='Q=MM:Q=100:Q=AA:Q=3549:Q=NC:Q=HHWK:';//test purpose only
	for (var i = 0; i < result.segments.length; i++) {
		if(result.segments[i].id.length >1)
			preQuantSeqs += 'Q=' + result.segments[i].id + ':'; //customizable per your ad server
			qcValue.q = result.segments[i].id;
			qcAdKeys.push(qcValue);
	}
	//console.log("creating cookie with segs: " + preQuantSeqs);
	window.bio.quantcast.createCookie(preQuantSeqs, 1); //set to expire in 24 hours (if the cookie does not already exist)
	window.bio.quantcast.setQuantSegs(preQuantSeqs.replace(/:/g,';'));
	window.bio.quantcast.setAdKeys(qcAdKeys);
}

/*************************************************
 * @description This function provide dart sitename and s1 information for tealium
 **************************************************/
function getDartInfoForTealium(){
    var omni_evar41 = dart.siteName+':'+dart.s1;
    if (dart.s2 != '') {
	    omni_evar41 = omni_evar41 + ':' + dart.s2;
    }
    return omni_evar41;
}

window.bio = window.bio || {};

/*************************************************
@class Provide the Logic for handling quantcast integration
@requires jQuery 1.3+
@returns 
*************************************************/

window.bio.quantcast = (function($) {

	var qcBaseURL = 'http://pixel.quantserve.com/api/segments.json';
	var qcCookieName = 'ckQuantSegs';
	
	var _s = {
		"$parent" : $(this),
		"quantSegs": '',
		"adKeys": {}
	};
	
	/**
	 * @private
	 * @param name the name of the cookie to create
	 * @param value the value to set in the cookie
	 * @param days the number of days the cookie should live for
	 */
	function createCookie(value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = qcCookieName+"="+value+expires+"; path=/";
	}
	
	/**
	 * @private
	 * @param name the name of the cookie to read
	 */
	function readCookie() {
		var nameEQ = qcCookieName + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}
	
	/**
	 * @public Called from the quantcast callback method (qc_results) 
	 * @param quantSegs the quantcast segments
	 */
	function setQuantSegs(quantSegs) {
		_s.quantSegs = quantSegs;
		
	}
	
	/**
	 * @public  
	 * @returns The current quantcast segments
	 */
	function getQuantSegs() {
		return _s.quantSegs;
		
	}
	
	function setAdKeys(qcAdKeys) {
		_s.adKeys = qcAdKeys;
	}
	
	function getAdKeys() {
		return _s.adKeys;
	}

	/**
	 * @public 
	 * @param qcAccountId the quantcast account id
	 */
	function init(qcAccountId) {
		if(readCookie(qcCookieName)==null || readCookie(qcCookieName) == ''){
			var container = document.getElementById('quantcast');
			var newScript = document.createElement('script');
			newScript.type = "text/javascript";
			newScript.src = qcBaseURL + '?a=' + qcAccountId + '&callback=qc_results';
			container.appendChild (newScript);
			//container.innerHTML = '<scri'+'pt type="text/javascript" src="' + qcBaseURL + '?a=' + qcAccountId + '&callback=qc_results" ></sc'+'ript>';
			//container.innerHTML = "hello world";
			//document.write('<scri'+'pt type="text/javascript" src="' + qcBaseURL + '?a=' + qcAccountId + '&callback=qc_results" ></sc'+'ript>');		
		}else{
			_s.quantSegs = readCookie(qcCookieName).replace(/:/g,';');
		}
	}
	
    return {
        "createCookie": createCookie,
        "readCookie" : readCookie,
        "setQuantSegs" : setQuantSegs,
        "getQuantSegs": getQuantSegs,
        "getAdKeys": getAdKeys,
        "setAdKeys": setAdKeys,
         "init": init
    };
	
})(jQuery);
//BIO - 610 - Felipe Arboleda - 23-May-13
//window.bio.quantcast.init('p-84eTroxoNX3JE');

function ifrmResize( width, height, ad_size_id, ad_id, time ){
	if(ad_size_id == 'ad_300x250_top_multisize'){
		//resizing ad
		//alert("width: "+ width + " height: " + height);
		document.getElementById('ad-box-iframe').height=height ;
		document.getElementById('ad-box-iframe').width=width ;
		
		if(width == 250) {
			document.getElementById('ad-box-iframe').setAttribute('style', 'border: 0px;');
		}
	}
}

</script>

        



