								<script>
									var returnedSingleConnectiondataForSidebar = {};
									var pageDataDisplayPageName = $('#displayPageType').html();
									$('#specific-profile-connection').addClass('loading').css({"height":"50px"}).html(' ').show();
									function displaySidebarProfileConnection(){
										$("#your-connection-dialog-pop-up").html('<div class="loading pop-up-connection-html" style="height:50px;"></div>');
										// id="moreconnection-'+type+'-'+x+'-'+friendObject.BioLinkProfileId+'"
										$('.pop-up-connection-html').attr('rel','sidebarid-my-0-'+($("#mainProfileIds").html()));
										if($("#mainProfileIds").html()){
											getSingleProfileConnection('shortDescription');
										}
										else{
											//htmlForDisplay = pageDataDisplayPageName+'You share no connections, but you have other connections';
											htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'You share no connections, but you have other connections',false);											
											$('#specific-profile-connection').html(htmlForDisplay).removeClass('loading');
											$('#my-connections-list').show()
										}
									}
									function displayConnectionWithSingleProfile(data,type,objectIndex){
										returnedSingleConnectiondataForSidebar = data;
										ignoreIdToList=false;
										
										if(returnedSingleConnectiondataForSidebar[0].connections[0]!=undefined){
											userTopInfoObj = returnedSingleConnectiondataForSidebar[0].connections[0];

											nameArray = userTopInfoObj.facebookProfileName.split(" ");
											$("#facebook-user-name").html(nameArray[0]);
											$("#top-facebook-image").attr("src",userTopInfoObj.facebookProfileImage).attr("alt",nameArray[0]);

										}
										

										if(returnedSingleConnectiondataForSidebar[0].connections[0].BioConnections == undefined){
											htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'You share no connections, but you have other connections',false);

											$('#specific-profile-connection').html(htmlForDisplay).removeClass('loading');
											$('#my-connections-list').show();
										}
										else if(returnedSingleConnectiondataForSidebar[0].connections 
												&& returnedSingleConnectiondataForSidebar[0].connections.length > 0 
												&& returnedSingleConnectiondataForSidebar[0].connections[0].BioConnections != undefined
												&& returnedSingleConnectiondataForSidebar[0].connections[0].BioConnections){
											htmlForDisplay ='';

											if($('#displayPageType').html()=='group' || $('#displayPageType').html()=='otd'){
												for(x = 0; x < returnedSingleConnectiondataForSidebar[0].connections.length; x++){
													
													if(returnedSingleConnectiondataForSidebar[0].connections.length>1){
														if(x==0){
															htmlForDisplay += '<p style="padding-bottom:5px;">'+returnedSingleConnectiondataForSidebar[0].connections[x].BioConnections[0].BioConnection+'.</p>';
															ignoreIdToList= returnedSingleConnectiondataForSidebar[0].connections[x].BioConnections[0].BioLinkProfileId;
															if($('#displayPageType').html()=='otd'){
																htmlForDisplay += '<p>Here are some other people on this day you have connections with</p>';
															}
															else{
																htmlForDisplay += '<p>Here are some other group members you have connections with</p>';
															}
															htmlForDisplay += '<ul style="display:block; margin-top:10px;">';
														}
														bioUserObject = returnedSingleConnectiondataForSidebar[0].connections[x].BioConnections[0];
														
														if(x<4 && bioUserObject.BioLinkProfileId!=ignoreIdToList){
															htmlForDisplay +='<li  style="border-bottom:none; border-top:1px solid #ccc" class="clearfix connection">';
															htmlForDisplay +='<div class="float-left ">';
																htmlForDisplay +='<a href="'+bioUserObject.BioProfileURL+'"><img class="facebook-image" src="'+bioUserObject.BioProfileImage+'"></a>';
															htmlForDisplay +='</div>';
															htmlForDisplay +='<div class="bio-profile-user float-right">';

																	htmlForDisplay +='<p><a class="dot" href="'+bioUserObject.BioProfileURL+'" style="color:#000; font-size:14px;">'+bioUserObject.BioProfileName+'</a></p>';
																	htmlForDisplay +='<p>'+bioUserObject.BioProfileOccupation+'</p>';
													
																
																	htmlForDisplay +='</div>';
																htmlForDisplay +='</li>';	
														}
														
														if(x == returnedSingleConnectiondataForSidebar[0].connections.length-1){
															htmlForDisplay += '</ul>';
														}
													}
													else{
														htmlForDisplay += returnedSingleConnectiondataForSidebar[0].connections[x].BioConnections[0].BioConnection;
													}
												}	
											}
											else{//profile
												htmlForDisplay += returnedSingleConnectiondataForSidebar[0].connections[0].BioConnections[0].BioConnection;
											}											
										
											$('#specific-profile-connection').css({'height':'auto'}).html(htmlForDisplay).removeClass('loading').css({"border-bottom":"1px solid #f2f2f2"});
											$('#my-connections-list').hide()
										}
										else if(returnedSingleConnectiondataForSidebar[0].connections[0].BioConnections == undefined 
												&& myRealConnectionsData[0].BioConnections == undefined){
											
											justHideMoreDesc=true;

											//alert("no more connections or undefined")
											//htmlForDisplay = 'You share no connections';
											 htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'You share no connections',false);
											
											$('#specific-profile-connection').html(htmlForDisplay).removeClass('loading').hide();
											$('#my-connections-list').show();
										}
										else{
											//htmlForDisplay = 'You share no connections, but you have other connections';
											 htmlForDisplay = showPageSpecificTypeEmptyText($('#displayPageType').html(),'You share no connections, but you have other connections',false);

											$('#specific-profile-connection').html(htmlForDisplay).removeClass('loading');
											$('#my-connections-list').show();
										}
										$('#your-connections-more-connections-button').html("See more bio connections").show();
									}
									
									
								</script>	
							
								
								
						
				
				
