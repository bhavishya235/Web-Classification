    <script type="text/javascript">
        $(function () {
            //Twitter bar
//            $('#open-tweet').hide();
//            $('#close-tweet').live('click', function (e) {
//                $(this).hide();
//                $('#open-tweet').show();
//                $('#tweet').addClass('closed');
//                e.preventDefault();
//            });
//            $('#open-tweet').live('click', function (e) {
//                $(this).hide();
//                $('#close-tweet').show();
//                $('#tweet').removeClass('closed');
//                e.preventDefault();
//            });

            //Show tweet
            // getTwitters('tweet-inner', {
            //     id: 'GymnasticsIre',
            //     count: 1,
            //     enableLinks: true,
            //     ignoreReplies: true,
            //     clearContents: true,
            //     template: '<span>%text%</span> <a href="http://twitter.com/%user_screen_name%/">FOLLOW</a>'
            // });

            //Video carousel
            $(".carousel .jCarouselLite").jCarouselLite({
                btnNext: ".video-carousel-next",
                btnPrev: ".video-carousel-prev",
                speed: 1000,
                circular: false,
                scroll: 1
            });

            $('.video-carousel-prev,.video-carousel-next').live('click', function (e) {
                e.preventDefault();
            });

            //Fading images
            if ($('#slideshow li').length > 1) {
                $('#slideshow').fadeSlideShow({
                    PlayPauseElement: false,
                    NextElement: false,
                    PrevElement: false,
                    ListElement: false
                });
            };

            //Logo slideshow
            var startSlide = 1;
            // Initialize Slides
            $('#slides').slides({
                preload: true,
                preloadImage: 'img/loading.gif',
                generatePagination: true,
                hoverPause: true,
                // Get the starting slide
                start: startSlide
            });

            //Countdown
            $('#countdown').countdown({ until: new Date(2012, 7 - 1, 27), format: 'D', layout:
				'<div id="timer_days" class="timer_numbers">{dnn}</div>'
            });

            function pad(number, length) {
                var str = '' + number;
                while (str.length < length) {
                    str = '0' + str;
                }

                return str;
            };

            var days = $("#timer_days").text();
            $("#days").text(pad(days, 3));

            //Wrap each character of date with span tag
            $("#days").children().andSelf().contents().each(function () {
                if (this.nodeType == 3) {
                    var $this = $(this);
                    $this.replaceWith($this.text().replace(/(\w)/g, "<span>$&</span>"));
                };
            });

			$("#video-carousel .carousel a").click(function(e) {
				e.preventDefault();
				var htmlStr = '<iframe id="video-homepage" src="'+ $(this).attr("href") +'" width="300" height="169" frameborder="0"></iframe>';
				$('#iframe').html(htmlStr);
			});
			


            $.getJSON("twitter.aspx", function (feeds) {
            var feedHTML = "";
            var displayCount = 1;
            for (var i = 0; i < displayCount; i++) {
                var tweet_name = feeds[i].user.name;
                var tweet_text = feeds[i].text;
                var tweet_time = feeds[i].created_at;
                var main_tweet = sort_links(tweet_text);
                var format_time = fix_time(tweet_time);

                $(main_tweet + " <span>" + format_time + "</span>").appendTo("#tweet-inner");

            }

            function sort_links(data) {
                data = data.replace(/((https?|s?ftp|ssh)\:\/\/[^"\s\<\>]*[^.,;'">\:\s\<\>\)\]\!])/g, function (url) {
                    return '<a href="' + url + '" >' + url + '</a>';
                });

                data = data.replace(/\B@([_a-z0-9]+)/ig, function (reply) {
                    return '<a href="http://twitter.com/' + reply.substring(1) + '" target="_blank">' + reply.charAt(0) + reply.substring(1) + '</a>';
                });
                data = data.replace(/\B#([_a-z0-9]+)/ig, function (reply) {
                    return '<a href="http://twitter.com/search?q=%23' + reply.substring(1) + '" target="_blank">' + reply.charAt(0) + reply.substring(1) + '</a>';
                });
                return data;
            }

            function fix_time(time_data) {
                var sort_time = time_data.split(" ");
                var fix_time = sort_time[3].split(":");
                sort_date = fix_time[0] + ":" + fix_time[1] + " " + sort_time[0] + " " + sort_time[2] + " " + sort_time[1] + " " + sort_time[5];
                return sort_date;
            }

        }).done(
            function () { console.log("complete"); }
        ).fail(
            function (e) { console.log("error:" + e.status); }
        );

        });
    </script>
    

