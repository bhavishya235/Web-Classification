<div class="hpContainer">

			<div id="rotatorContainer">
				
				<div class="hand" id="rotatorMainPicture"><script type="text/javascript">
$(document).ready(function () {
	for(var i = 1; i <= storyPositionCount; i++) {
		if ($.os.name == 'iphone' || $.os.name == 'ipad') {
			$("#rotatorThumbnail" + (i)).click(selectStoryEvent);
		} else {
			$("#rotatorThumbnail" + (i)).mouseover(selectStoryEvent);
			$("#rotatorThumbnail" + (i)).mouseout(startTimer);
			$("#rotatorThumbnail" + (i)).click(goToStory);
		}	
	}	
	$("#rotatorMainPicture").click(goToStory);
	$("#rotatorTitle").click(goToStory);
	$("#rotatorText").click(goToStory);	
	downloadRotatorData();
});
var storyPositionCount = 6;
var currentStoryPosition = 0;
var speedInSeconds = 5;
var updateFeedInSeconds = 90;
var timeout;
var imageCache = new Array();
function startTimer() {
	clearTimeout(timeout);
	timeout = setTimeout("selectNextStory()", speedInSeconds * 1000);
}
function downloadRotatorData() {
	$.ajax({
		type: "GET",
		url: "/fw/tools/rotatorFeed.phtml?type=json&count=" + storyPositionCount + "&tag=home page rotator",
		dataType: "json",
		success: function (data) {
			$.each(data, function (i, item) {
				var curDiv = $("#rotatorThumbnail" + (i + 1));
				curDiv.data('position', i + 1);
				curDiv.data('postID', item.PostID);
				curDiv.data('json', item);
				imageCache.push($('<img>').attr("src", '/images/post_images/' + item.ImageID + '.jpg'));
				setDivImage(curDiv, '/images/post_images/' + item.ImageID + '.jpg');
			});
			if (currentStoryPosition == 0) {
				selectNextStory();
			} else {
				currentStoryPosition = 0;
			}
			setTimeout("downloadRotatorData()", updateFeedInSeconds * 1000);
		},
		error: function (data) {
			alert("Error downloading stories.");
		}
	});
}
function setDivImage(div, imageURL) {
	if (div.children("img").length == 1) {
		div.children("img").attr("src", imageURL);
	} else {
		div.html('<img src="' + imageURL + '" width="' + div.width() + '" height="' + div.height() + '" />');
	}
}
function selectStoryEvent(event) {
	selectStory(event.currentTarget);
	clearTimeout(timeout);
}
function selectStory(div) {
	$(".rotatorThumbnail").removeClass('rotatorThumbnailHighlight')
	$(div).addClass('rotatorThumbnailHighlight');	
	var json = $(div).data('json');	
	setDivImage($("#rotatorMainPicture"), '/images/post_images/' + json.ImageID + '.jpg');
	$("#rotatorTitle").html(json.Title);
	$("#rotatorText").html(json.Preview);	
	$("#rotatorMainPicture").data('postID', json.PostID);
	$("#rotatorTitle").data('postID', json.PostID);
	$("#rotatorText").data('postID', json.PostID);	
	currentStoryPosition = $(div).data('position');
}
function selectNextStory() {
	var nextStoryPosition = (currentStoryPosition >= storyPositionCount) ? 1 : currentStoryPosition + 1;	
	selectStory($("#rotatorThumbnail" + nextStoryPosition));	
	startTimer();
}
function goToStory(event) {
	var div = event.currentTarget;	
	location.href = "/pages/post.html?PostID=" + $(div).data('postID') + "&prog=h";
}
</script></div>
				
				<div id="rotatorThumbnailColumn">
