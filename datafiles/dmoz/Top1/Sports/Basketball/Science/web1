function show_teammenu_effect(variable) { document.getElementById(variable).className = 'teammenuchosen'; }
function hide_teammenu_effect(variable) { document.getElementById(variable).className = 'teammenu'; }
function show_nbamenu_effect(variable) { document.getElementById(variable).className = 'nbamenuchosen'; }
function hide_nbamenu_effect(variable) { document.getElementById(variable).className = 'nbamenu'; }
function show_tableheadline_effect(variable) { document.getElementById(variable).className = 'tableheadlinechosen'; }
function hide_tableheadline_effect(variable) { document.getElementById(variable).className = 'tableheadline'; }
function show_form_display(variable) { document.getElementById(variable).style.display = "block"; document.getElementById(variable).style.visibility = "visible"; }
function hide_form_display(variable) { document.getElementById(variable).style.display = "none"; document.getElementById(variable).style.visibility = "hidden"; }
function show_form_effect(variable) { document.getElementById(variable).className = 'tableformeffect'; }
function hide_form_effect(variable) { document.getElementById(variable).className = 'submenu'; }
function show_div_effect(variable) { document.getElementById(variable).className = 'diveffect'; }
function hide_div_effect(variable) { document.getElementById(variable).className = 'divrows'; }
function show_form_hover(variable) { document.getElementById(variable).className = 'tableformhover'; }
function hide_form_hover(variable) { document.getElementById(variable).className = 'tableform'; }
function show_table_effect(variable) { document.getElementById(variable).className = 'tableeffect'; }
function hide_table_effect(variable) { document.getElementById(variable).className = 'statscontent'; }
function show_tableline_effect(variable) { document.getElementById(variable).className = 'teammenuchosen'; }
function hide_tableline_effect(variable) { document.getElementById(variable).className = 'tableheadline'; }
function show_nbamenu_hover(td,table,classname) { 
document.getElementById(td).className = classname; 
document.getElementById(table).style.display = "block"; document.getElementById(table).style.visibility = "visible"; }
function hide_nbamenu_hover(td,table,classname) { 
document.getElementById(td).className = classname; 
document.getElementById(table).style.display = "none"; document.getElementById(table).style.visibility = "hidden"; }
function show_teammenu_hover(td,table) { 
document.getElementById(td).className = 'teammenuchosen'; 
document.getElementById(table).style.display = "block"; document.getElementById(table).style.visibility = "visible"; }
function hide_teammenu_hover(td,table) { 
document.getElementById(td).className = 'teammenu'; 
document.getElementById(table).style.display = "none"; document.getElementById(table).style.visibility = "hidden"; }

function automatic1() {

// Create a variable url to contain the value of the 
// selected option from the the form named pulldown and variable selectname
var url = document.form1.selectname.options[document.form1.selectname.selectedIndex].value

// Re-direct the browser to the url value
window.location.href = url 

}

function automatic2() {

var url = document.form2.selectname.options[document.form2.selectname.selectedIndex].value
window.location.href = url 

}

function automatic3() {

var url = document.form3.selectname.options[document.form3.selectname.selectedIndex].value
window.location.href = url 

}

function automatic4() {

var url = document.form4.selectname.options[document.form4.selectname.selectedIndex].value
window.location.href = url 

}

function automatic5() {

var url = document.form5.selectname.options[document.form5.selectname.selectedIndex].value
window.location.href = url 

}

function toolexclude() {

var url = document.form1.exclude.options[document.form1.exclude.selectedIndex].value
window.location.href = url 

}

</script>
