		$(document).ready(function(){
			

			var isRefresh = ($('#sitiT1').length > 0);
			
			if( isRefresh && (window.cnnPageInfo_pageType == "section") ) { $('.cnnPage').addClass( BrowserDetect.browser.toLowerCase() + ' ' + BrowserDetect.OS.toLowerCase() ); }
			
			jQuery.fn.setVRTitle = function(){
			
				if( isRefresh ){
					var title = $(this[0]).find('H1').text();
					if ( $(this[0]).find('H1 > A').length > 0 ){
						title = $(this[0]).find('H1 > A').text();
					}
				} else {
					var title = $(this[0]).find('DIV.cnnTitle').text();
					if ( $(this[0]).find('DIV.cnnTitle > A').length > 0 ){
						title = $(this[0]).find('DIV.cnnTitle > A').text();
					}
				}
				
				return title.trim();
			};
			
			
