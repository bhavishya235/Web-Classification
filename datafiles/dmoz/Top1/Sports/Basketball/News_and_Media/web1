			if ((pagen=='article') && (this.pageType!='errorPage') && (this.sectionLevel2!='gallery') && (this.sectionLevel3!='gallery')) {
			this.prop12 = this.mainsection+':'+this.articlename;
			this.prop19 = 'article';
			this.prop40 = '|'+this.prop11;	
				}
				else {
			this.prop12 = '';
			this.prop19 = '';	
				}
				
			if (this.type=='print') {
			 this.prop18  = theurl.slice(7);
			   }
			   else {
			 this.prop18  ='';  
			   }	
				
			this.prop20 = '';
			this.prop36 = this.sectionLevel6;
		}
		var MyOmnitureVariables  = new OmnitureVariables();
		// ]]> --></script>
		
		
		<script type="text/javascript"><!-- // <![CDATA[
