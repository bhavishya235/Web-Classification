<script type="text/javascript">
try{
	
	if( cnnPage.isHomepage ){
			
			var watchnowcount = 0;
			$('#siWatchNow').ready(function(){
				if( watchnowcount == 0){
					watchnowcount += 1;
					$(this).hide();
