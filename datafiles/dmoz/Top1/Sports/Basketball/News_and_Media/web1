              var nextLi = 1;
              var listHeight = 0;
              var containerHeight = jQuery('#features').height();

              jQuery('#features .storyList li').each(function(){

                  listHeight+=jQuery(this).height()+20;

                  if( !(listHeight > containerHeight) ) {        
                      nextLi++; 
                  }                    
              })

              var position = jQuery('#latestNews li:eq('+nextLi+')').position();
              var nextPos  = position.top + (jQuery('#features .storyList li:eq(0)').height()+8 ) ;

              function scrollDown(container){ 
                  jQuery(container).scrollTo( { top:nextPos, left:0}, 500, {easing:'easeOutCubic'}); 
                  jQuery("#scrollDown").addClass("disabled-down");
                  jQuery("#scrollUp").removeClass("disabled-up");
              }

              function scrollUp(container){ 
                  jQuery(container).scrollTo( { top:0, left:0}, 500, {easing:'easeOutCubic'}  ); 
                  jQuery("#scrollUp").addClass("disabled-up");
                  jQuery("#scrollDown").removeClass("disabled-down");
              }

              jQuery("#scrollUp").click( function(){scrollUp('#newsContainer') });
              jQuery("#scrollDown").click( function(){scrollDown('#newsContainer') }); 
              jQuery("#scrollUpPhotos").click( function(){scrollUp('') });
              jQuery("#scrollDownPhotos").click( function(){scrollDown() });

              function initialize() {
  		        var feeds = [];

  		        var count 	= new google.feeds.Feed("http://counterkicks.com/feed/");
  		        count.site 	= '<img src="http://counterkicks.com/favicon.ico" alt="COUNTERKICKS" />  COUNTERKICKS';
  		        feeds[0] 	= count;

  				var draft 	= new google.feeds.Feed("http://www.nbadraft.net/rss.xml");
  		        draft.site 	= '<img src="http://www.nbadraft.net/favicon.ico" alt="NBADRAFT" />  NBADRAFT';
  		        feeds[1] 	= draft;

				var mixt 	= new google.feeds.Feed("http://hoopmixtape.com/feed/"); //new google.feeds.Feed("http://hoopmixtape.ning.com/profiles/blog/feed");
				mixt.site 	= '<img src="http://hoopmixtape.ning.com/favicon.ico">  HOOPMIXTAPE';
				feeds[2] 	= mixt;

  				var bil 	= new google.feeds.Feed("http://www.ballislife.com/feed/");
  		        bil.site 	= '<img src="http://www.ballislife.com/favicon.ico" alt="BALL IS LIFE" />  BALL IS LIFE';
  		        feeds[3] 	= bil;

				var tmr 	= new google.feeds.Feed("http://www.themarsreel.com/feed/");
				tmr.site 	= '<img src="http://www.themarsreel.com/wp-content/uploads/2012/06/avatar_fdba01400198_16.png">  TMR BASKETBALL';
				feeds[4] 	= tmr;

  		        var entryArray=[];
  		        var feedIndex=0;
                        var index_feeds = [];
                    
                   

  		    	jQuery(feeds).each(function(i){

  		    	    feeds[i].setNumEntries(1);    
  		            feeds[i].load(
  		                function(result){    
  		                    jQuery(result.feed.entries).each(function(x){
  		                        result.feed.entries[x].site = feeds[i].site;;
  		                        entryArray.push( result.feed.entries[x] );
  		                    });
                                    index_feeds[i] = i;
  		                    checkTest();
  		                    feedIndex++; 
  		                }
  		            );
  		    	})

  		    	function checkTest(){  

  		            if(feedIndex==feeds.length-1){
  		    	    	shuffle(index_feeds);
  		    	    	// Loop entries and output data
  		    	    	for(i in index_feeds){
                                    //var pos = index_feeds[i];
                                    var li = jQuery('<li />');
  		    	    	    var span = jQuery('<span />').html(entryArray[i].site);
  		    	    	    var a  = jQuery('<a />')
  		    	    	    a.attr("href",entryArray[i].link);
  		    	    	    a.html(entryArray[i].title);
  		    	    	    li.append(span, a);
  		    	    	    jQuery("#h360feed").append(li);
  		    			}
  		    	    }
  		    	}
  		    }

  		    google.setOnLoadCallback(initialize);

          });
      </script>
