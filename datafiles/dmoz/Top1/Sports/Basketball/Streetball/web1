<script type="text/javascript">
	$.fn.equalHeight = function() {
		var height = 0,
			reset = $.browser.msie ? "1%" : "auto";
		
		return this
			.css("height", reset)
			.each(function() {
				height = Math.max(height, this.offsetHeight);
			})
			.css("height", height)
			.each(function() {
					var h = this.offsetHeight;
					if (h > height) {
							$(this).css("height", height - (h - height));
					};
			});
	};
</script>
</head>
