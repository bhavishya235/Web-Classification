	var addToHomeConfig = {
		returningVisitor: true,		// Show the message only to returning visitors (ie: don't show it the first time)
		expire: 20160,				// Show the message only once every 7 days
		touchIcon: true,
		autostart: false,
		message: "Support free enterprise. Don't tax your thumbs. Add one-touch access to AEI on your mobile."
	};
	
	function pageLoad () {
		if ( window.location.hash.match('mbl') ) return;

		addToHome.show();
		window.location.hash = '#mbl';
	}
	var isiPad = navigator.userAgent.match(/iPad/i) != null;
	var isiPhone = navigator.userAgent.match(/iPhone/i) != null;
	var isiPod = navigator.userAgent.match(/iPod/i) != null;
		
	if(isiPad || isiPhone || isiPod){
	  window.addEventListener('load', pageLoad, false);
	}
</script>
