        }

        if (e.attachEvent) {
            e.attachEvent('onload', iframeOnload);
        }
        else {
            e.addEventListener('load', iframeOnload, false);
        }

        sliderHolder.insertBefore(e, sliderHolder.firstChild);
    })();
}


</script>

