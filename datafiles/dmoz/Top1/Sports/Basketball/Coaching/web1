<table cellspacing="0" cellpadding="0" border="0" width="600">
<tr><td width="130" valign="top">

<div id="overDiv" style="position:absolute; visibility:hide; z-index: 1;"></div>

<script language="JavaScript"><!--

////////////////////////////////////////////////////////////////////////////////////
//  Based on overLIB 2.22
//  Portions copyrighted by Erik Bosrup (erik@bosrup.com), Dan Steinman,
//    Landon Bradshaw, and Gnowknayme.
//  Original version of overLib can be found at http://www.bosrup.com/web/overlib/
//  
//  *** Modifications from overLIB 2.22 ***
//  Removed the entire "CONFIGURATION" section of the script (variables removed are listed below)
//  
//  REMOVED FUNCTIONS: drs(), drc(), src(), dls(), dlc(), slc(), dcs(), dcc(), scc(), dts(), dtc(), trk()
//  ALTERED FUNCTIONS: stc(), disp(), mouseMove(), layerWrite(), moveTo()
//  ADDED FUNCTION: createHTML()
//  
//  REMOVED VARIABLES: fcolor, backcolor, textcolor, capcolor, closecolor, width, border, offsetx, offsety, dir, tr, d, text, title
//  ADDED VARIABLES: isMac, linkArray, textArray, popupColor, beginOuter, beginInner, grayColumn, firstSport, beginRow, beginFont, endFont, endRow, closingRow, endInner, endOuter
//  *** End the modifications listing ***
////////////////////////////////////////////////////////////////////////////////////

ns4 = (document.layers)? true:false
ie4 = (document.all)? true:false

isMac = (navigator.userAgent.indexOf('Mac')>0)


if (ie4) {
	if (navigator.userAgent.indexOf('MSIE 5')>0) {
		ie5 = true;
	} else {
		ie5 = false; }
} else {
	ie5 = false;
}

// these arrays MUST be the same length!! -- but minus the first sport & link (which are defined below)

var linkArray = new Array();
linkArray[0] = "/cricket/";
linkArray[1] = "/cycling/";
linkArray[2] = "/motorsports/world/";
linkArray[3] = "/rugby/";
linkArray[4] = "/wintersports/";
linkArray[5] = "/basketball/wnba/";
linkArray[6] = "";
linkArray[7] = "/world/asia/";
linkArray[8] = "/world/europe/";

var textArray = new Array();
textArray[0] = "cricket";
textArray[1] = "cycling";
textArray[2] = "formula one";
textArray[3] = "rugby";
textArray[4] = "winter sports";
textArray[5] = "wnba";
textArray[6] = "<hr width='99%'>";
textArray[7] = "Asia Sport";
textArray[8] = "Europe Sport";

var popupColor = "#3366ff";
var beginOuter = "<table bgcolor='" + popupColor + "' border='0' cellpadding='2' cellspacing='0' width='120'><tr><td>\n";
var beginInner = "<table bgcolor='" + popupColor + "' cellspacing='1' cellpadding='2' border='0' width='100%'>\n";
var grayColumn = "<tr><td bgcolor='gray' rowspan='" + ( linkArray.length + 1 ) + "'><font size='1' face='helvetica,arial'>&nbsp;</font></td>\n";
var firstSport = "    <td bgcolor='#000000' width='90%'><a href='/athletics/'><font size='2' face='helvetica,arial' color='#ffffff'><b> athletics</b></font></a></td></tr>";
var beginRow = "<tr><td bgcolor='#000000'>";
var beginFont = "<font size='2' face='helvetica,arial' color='#ffffff'><b>";
var endFont = "</b></font>";
var endRow = "</td></tr>\n";
var closingRow = "<tr><td bgcolor='#000000' colspan='2' align='right'><a href='#' onClick='cClick(); return false;' id='PCL'><font size='1' face='helvetica,arial' color='#ffffff'><b>Close Menu</b></font></a></td></tr>\n";
var endInner = "</table>\n";
var endOuter = "</td></tr></table>\n";

var x = 0;
var y = 0;
var snow = 0;
var sw = 0;
var cnt = 0;
if ( (ns4) || (ie4) ) {
	if (ns4) over = document.overDiv
	if (ie4) over = overDiv.style
	document.onmousemove = mouseMove
	if (ns4) document.captureEvents(Event.MOUSEMOVE)
}

function nd() {
	if ( cnt >= 1 ) { sw = 0 };
	if ( (ns4) || (ie4) ) {
		if ( sw == 0 ) {
			snow = 0;
			hideObject(over);
		} else {
			cnt++;
		}
	}
}

function stc() {
	sw = 1;
	cnt = 0;
	txt = createHTML();
	layerWrite(txt);
	disp();
	snow = 0;
}

function disp() {
	if ( (ns4) || (ie4) ) {
		if (snow == 0) 	{
			moveTo(over,x,y);
			showObject(over);
			snow = 1;
		}
	}

}

function mouseMove(e) {
	if (ns4) {x=e.pageX; y=e.pageY;}
	if (ie4) {x=event.x; y=event.y;}
	if (ie5) {x=event.x+document.body.scrollLeft; y=event.y+document.body.scrollTop;}
	if (snow) {
		moveTo(over,x,y);
	}
}

function cClick() {
	hideObject(over);
	sw=0;
}

function layerWrite(txt) {
	if (ns4) {
		var lyr = document.overDiv.document
		lyr.write(txt)
		lyr.close()
	}
	else if (ie4) document.all["overDiv"].innerHTML = txt
}

function showObject(obj) {
	if (ns4) obj.visibility = "show"
	else if (ie4) obj.visibility = "visible"
}

function hideObject(obj) {
	if (ns4) obj.visibility = "hide"
	else if (ie4) obj.visibility = "hidden"
}

function moveTo(obj,xL,yL) {
	approxLineHeight = 20
	if (isMac) {
		approxLineHeight -= 5
		if (ie5) {
			approxLineHeight += 2
		}
	}
	obj.left = xL - 20
	obj.top = yL - (approxLineHeight * (textArray.length+2))
}

function createHTML() {
	returnString = new String();
	returnString = beginOuter + beginInner + grayColumn + firstSport;
	for( var i = 0; i < textArray.length; i++ ) {
		returnString += beginRow;
		if (linkArray[i] != '') {
		  returnString += "<a href='" + linkArray[i] + "'>" + beginFont + textArray[i] + endFont + "</a>";
		} else {
		  returnString += beginFont + textArray[i] + endFont;
		}
		returnString += endRow;
	}
	returnString += closingRow + endInner + endOuter;
	return returnString;
}
// -->
</script>

<table bgcolor="black" border="0" cellpadding="2" cellspacing="0" width="120"><tr><td>
<table bgcolor="black" cellspacing="1" cellpadding="2" border="0" width="100%">
<tr><td><font size="1" face="helvetica,arial">&nbsp;</font></td>
	<td bgcolor="#cc0000" colspan="2"><a href="/"><font size="2" face="helvetica,arial" color="white"><b>U.S.&nbsp;SPORTS</b></font></a></td></tr>
<tr><td bgcolor="#3366FF" rowspan="15"><font size="1" face="helvetica,arial">&nbsp;</font></td>
