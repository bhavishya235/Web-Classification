    //var l_sSrc = '';
    $(window).load(function(){
        //xAEI.fixListingBrokenImages();
        $('.category-image a img').each(function(){
            var l_sSrc = $(this).attr('src');
            l_sSrc = l_sSrc.replace('http://beta.aei.org', '');
            l_sSrc = l_sSrc.replace('http://www.aei.org', '');
            l_sSrc = l_sSrc.replace('http://aei.org', '');
            if(l_sSrc == '/files/_item_footer.jpg'){
                $(this).parents('.category-image').hide();
            }
        });

    });
</script>



