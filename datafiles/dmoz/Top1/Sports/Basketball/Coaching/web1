	<!--/frags/encyclopedia/encyclopediaLeftNav.html SSI include--> <noindex>
<div class=leftNavDiv>
<table cellpadding=0 border=0 cellspacing=0>
<!--<tr><td colspan=3><img src="/media/ency/i-rowa1.gif"></td></tr>-->
<tr><td background="/media/ency/i-rowb1.gif"><img src="/media/ency/i-rowb1.gif"></td>
	<td class=leftNav width=124 valign=top style="padding:1px;">
<!---------------START------------------->
<div style="margin-bottom:5px;">
<table cellpadding=0 border=0 cellspacing=0 width="121">
<tr>
	<td align="center">	
	<form name="historyNav" style="margin:0px;font-size:11px;">
<div align="center">
Browse through NBA Encyclopedia by first selecting the section and then selecting the year you want to see.</div>
<div style="margin:0px;" align="center">
<select name="StatsSection" onchange="javascript:checkYear()" style="font-family:Arial;font-size:11px;font-style:normal;width:90px;">
<option value="">Pick Section</option>
<option value="season">Recaps</option>
<option value="standings">Standings</option>
<option value="playoffs">Playoff Results</option>
<option value="finals">Finals Recaps</option>
<option value="awards">Award Winners</option>
<option value="points">Stats Leaders</option>
<option value="points">  --Points</option>
<option value="assists">  --Assists</option>
<option value="rebounds">  --Rebounds</option>
<option value="steals">  --Steals</option>
<option value="blocks">  --Blocks</option>
<option value="fgp">  --Field Goal %</option>
<option value="ftp">  --Free Throw %</option>
<option value="3pfg">  --3pt FG %</option></select>
</div>

<script language="JavaScript1.2">

	var isNav = null;
    var isIE = null;
	var URL = document.location + " ";
	var URLarray= URL.split('/');
	var	section = URLarray[URLarray.length-2];
	var	year = URLarray[URLarray.length-1].substring(0,4);
	if (section=="history"){year = 2009;}
	//function DetectBrowser() {
		if (parseInt(navigator.appVersion) >= 4) {
			if (navigator.appName == "Netscape") {
	 	  	 isNav = true;
			}
			else {
		    	isIE = true;
								
			}
		}
	//}
			
	function checkYear(){
		if (document.historyNav.StatsSection.options[document.historyNav.StatsSection.selectedIndex].value != ''){
	     	var section = document.historyNav.StatsSection.options[document.historyNav.StatsSection.selectedIndex].value;}
		if (document.historyNav.StatsYear.options[document.historyNav.StatsYear.selectedIndex].value != ''){
	     	var year = document.historyNav.StatsYear.options[document.historyNav.StatsYear.selectedIndex].value;}
		if (section == "rebounds") setYears(1950);
		if ((section == "blocks") || (section == "steals")) setYears(1973);
		if (section == "3pfg") setYears(1979);
		if ((section != "rebounds") && (section != "3pfg") && (section != "blocks") && (section != "steals")) setYears(1946);
		
	}
	
	function GoToPage() {
		section = URLarray[URLarray.length-2];
        	if (section=="history"){year = 2001;section="points"}
		year = URLarray[URLarray.length-1].substring(0,4);

		if (document.historyNav.StatsYear.options[document.historyNav.StatsYear.selectedIndex].value == '' && document.historyNav.StatsSection.options[document.historyNav.StatsSection.selectedIndex].value == ''){
			alert ("Please select a section and a year.");
			return false;
			}

		if (document.historyNav.StatsYear.options[document.historyNav.StatsYear.selectedIndex].value == ''){
			alert ("Please select a year.");
			return false;
			}
		

		if (document.historyNav.StatsSection.options[document.historyNav.StatsSection.selectedIndex].value != ''){
	       var section = document.historyNav.StatsSection.options[document.historyNav.StatsSection.selectedIndex].value;}
			else{alert("Please select a section.");
			return false;
			}
				
		if (document.historyNav.StatsYear.options[document.historyNav.StatsYear.selectedIndex].value != ''){
			var year = document.historyNav.StatsYear.options[document.historyNav.StatsYear.selectedIndex].value.substring(0,4);}
		if ((section == "rebounds")  && (year < 1950)) year = 1950;
		if (((section == "blocks") || (section == "steals")) && (year < 1973)) year = 1973;
		if ((section == "3pfg")  && (year < 1979)) year = 1979;
			
		//var re = new RegExp();
		//re.compile(joy[joy.length-2]);joy[joy.length-2]
		filename = year.toString() + (parseInt(year)+1).toString() + '.html'
		path= '/history/' + section + '/' + filename;
		//temp.replace(re,section);
		if(isNaN(year)||year == ''){
			alert("Please select a year.");
		}else{
			document.location = path
		//javascript:if( options[selectedIndex].value != '') document.location = options[selectedIndex].value;
		}
	}
	
	function setYears(startYear){
		document.historyNav.StatsYear.options.length = 0;
		 if(isNav == true){
		 	var oOption  = new Option("Select A Year","");
				var selLength = document.historyNav.StatsYear.length;
				document.historyNav.StatsYear.options[selLength] = oOption;
			}
		else{
		var oOption = document.createElement("option");
         oOption.text = 'Select Year';
         oOption.value = '';
         document.historyNav.StatsYear.add(oOption);
		
		}
		 
		for (var x = startYear; x < 2009;x++){
			optStartYear=x;
			optEndYear=x+1;
			if ( isNav == true ) {
				var oOption  = new Option(optStartYear+'-'+optEndYear,optStartYear);
				var selLength = document.historyNav.StatsYear.length;
				document.historyNav.StatsYear.options[selLength] = oOption;
	        }
	        else {
	            var oOption = document.createElement("option");
	            oOption.text = optStartYear+'-'+optEndYear;
	            oOption.value = optStartYear;
	            document.historyNav.StatsYear.add(oOption);
			}
		}
		document.historyNav.StatsYear.options.selectedIndex = 0;
	}
</script>
<select name="StatsYear" style="font-family:Arial;font-size:11px;font-style:normal;width:90px;">
<option value="" selected>Select a Year
</select>
<script language="JavaScript1.2">
	if (section == "rebounds")  setYears(1951);
	if ((section == "blocks") || (section == "steals")) setYears(1974);
	if (section == "3pfg") setYears(1979);
	if ((section != "rebounds") && (section != "3pfg") && (section != "blocks") && (section != "steals")) setYears(1946);
	 
</script>

<input type="Button" name="go" onclick="javascript:GoToPage()" value="GO!" align="center">
	</form>
