			var HPimages          = new Array();
			var HPimagesalt       = new Array();
			var HPimageslink      = new Array();
			var HPimagesnewwindow = new Array();
			var HPimageIndex = 0;
			var ImageID = new Number( 0 );
			function PreloadImages() {
HPimagesnewwindow[0] = '0';
				HPimages[0] = new Image();
				HPimages[0].src = "/images/d/426/Vinny_Mike_Tim_and_Mascot.jpg";
				HPimagesalt[0]  = "";
				HPimageslink[0] = "";
HPimagesnewwindow[1] = '0';
				HPimages[1] = new Image();
				HPimages[1].src = "/images/d/426/Special_Olympics_Norwich_2011.jpg";
				HPimagesalt[1]  = "";
				HPimageslink[1] = "";
HPimagesnewwindow[2] = '0';
				HPimages[2] = new Image();
				HPimages[2].src = "/images/d/426/refs4542.jpg";
				HPimagesalt[2]  = "";
				HPimageslink[2] = "";
HPimagesnewwindow[3] = '0';
				HPimages[3] = new Image();
				HPimages[3].src = "/images/d/426/IMG_1068.jpg";
				HPimagesalt[3]  = "";
				HPimageslink[3] = "";
				var FirstImg = new Number( 0 );
				FirstImg = setTimeout( "CycleImages()" , 3000 );
			}

			function PopUpLoc( URL ) {
				var newWindow = window.open( URL , 'extlink' , "screenX=10,left=10,screenY=10,top=10,width=800,height=800,scrollbars=yes,resizable=yes" , false );
				newWindow.focus();
			}

			function CycleImages() {
				var BigPic = document.getElementById( 'bigpic' );
				HPimageIndex++;
				if( HPimageIndex == HPimages.length ) {
					HPimageIndex = 0;
				}
				BigPic.src = HPimages[ HPimageIndex ].src;
				BigPic.alt = HPimagesalt[ HPimageIndex ];
				if( HPimageslink[ HPimageIndex ] ) {
					BigPic.className = "ImgLink";
					if( HPimagesnewwindow[ HPimageIndex ] == "1" ) {
						BigPic.onclick = function() { PopUpLoc( HPimageslink[ HPimageIndex ] ); }
					} else {
						BigPic.onclick = function() { window.location=HPimageslink[ HPimageIndex ]; }
					}
				} else {
					BigPic.className = "ImgNoLink";
					BigPic.onclick = function() { return; }
				}
				ImageID = setTimeout( "CycleImages()" , 4000 );
			}
		</script>
	<script language="javascript">
		function PopSub() {
			var subWindow = window.open( "/cgi-bin/p/awtp-subscriber-form.cgi?d=iaabo-105" , "subscriber" , "screenX=50,left=50,screenY=50,top=50,width=450,height=230,scrollbars=yes,resizable=yes" , false );
			subWindow.focus();
		}
	</script>

