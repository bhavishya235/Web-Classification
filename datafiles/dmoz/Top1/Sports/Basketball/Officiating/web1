<script>

    if( typeof getCookie('textSize') === 'undefined' || getCookie('textSize') === null){
        //do nothing
    }else{
        var get_text_size = getCookie('textSize');
        change_text_size(get_text_size);
    }


$('.text_size a').each(function(){
   $(this).click(function(){
       change_text_size($(this).text())
   });
});

function change_text_size(t_size){
    $('.text_size li').each(function(){
        if($(this).text() == t_size){
            $(this).addClass('selected');
        }else{
            $(this).attr('class','');
        }
    });

      if(t_size == 'Standard'){
        $('body').css('font-size','62.5%').removeAttr('class');
        SetCookie('textSize','Normal','','/');
     }

     if(t_size == 'Medium'){
        $('body').css('font-size','90%').attr('class','medium_text');
        SetCookie('textSize','Medium','','/');
     }

     if(t_size == 'Large'){
        $('body').css('font-size','125%').attr('class','large_text');
        SetCookie('textSize','Large','','/');
     }

 }

</script>

	
