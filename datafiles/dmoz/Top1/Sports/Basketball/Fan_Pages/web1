function img_act(imgName) {
        if (version == "enable") {
        imgOn = eval(imgName + "on.src");
        document [imgName].src = imgOn;
        }
}

function img_inact(imgName) {
        if (version == "enable") {
        imgOff = eval(imgName + "off.src");
        document [imgName].src = imgOff;
        }
}


function img_swap(imgName) {
        if (version == "enable") {
        imgOn = eval(imgName + "http://www.cara.com/on.src");
        document.a1.src = imgOn;
        }
}

function img_inswap(imgName) {
        if (version == "enable") {
        imgOff = eval(imgName + "http://www.cara.com/off.src");
        document.a1.src = imgOff;
        }
}

