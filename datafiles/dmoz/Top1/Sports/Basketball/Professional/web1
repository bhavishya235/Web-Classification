<script language="javascript">

function Toggle(index)
{
	var divs = $(".contentContainer");
	var div = $(".contentContainer-" + index);
	var tabs = $(".titleContainer a");
	var tab = $(".titleContainer-" + index + " a");
	
	divs.hide();
	div.show();
	tabs.removeClass("Selected");
	tab.addClass("Selected");
}

</script>

