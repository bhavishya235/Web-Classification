                        var isOpened = !Jst_ctl00_ctl00_ctl00_P1_P1_P1_M1_IsClosed[key];
						
                        Jst_ctl00_ctl00_ctl00_P1_P1_P1_M1_IsClosed[key] = isOpened;

						if (isOpened)
						{
							Js_Makc2_Element_Hide(id_Image_Hide);
							Js_Makc2_Element_Show(id_Image_Show);
							Js_Makc2_Element_Hide(id_Items);
