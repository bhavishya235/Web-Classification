    $(document).ready(function () {
        $(".StatsLeadersTopics a").click(function (event) {
            event.preventDefault();
            $(".StatsLeadersTopics a").attr("class","");
            $(this).attr("class","selected");
            $("#stats-leaders-content > div").hide();
            $("#stats-leaders-content .content-" + $(this).attr("data-topic")).show();
           
        });
    });

</script>
