    $(function() {
        //drop down navigation
        $('#navigation .menu>li').hover(function() {
            var liWidth = $(this).width();
            if($(this).find('ul.sub')) {
                $(this).find('ul.sub').show();
            }
        }, function() {
            if($(this).find('ul.sub')) {
                $(this).find('ul.sub').hide();
            }
        });
    });
</script>
