						function fn_submit()
						{
							var objVar = new getVars();
														objVar.fieldN[0] = "email";
							objVar.fieldV[0] = "Email Address";
							objVar.fieldT[0] = "text";
							objVar.fieldI[0] = "email";
							
														objVar.fieldN[1] = "password";
							objVar.fieldV[1] = "Password";
							objVar.fieldT[1] = "text";
							objVar.fieldI[1] = "password";
								if(checkEmpty('login',objVar))
							{
								//document.login.submit();
								return true;
							}
							else
								return false;
						}
						
						function fn_clear(id)
						{
							if(id==1)
							{
								if(document.login.email.value=="Enter Your Email Id")
									document.login.email.value="";
								
							}
							if(id==2)
							{
								if(document.login.password.value=="password")
									document.login.password.value="";
								
							}
						}
						
						function fn_clear_blur(id)
						{
							if(id==1)
							{
								if(document.login.email.value=="")
									document.login.email.value="Enter Your Email Id";
								
							}
							if(id==2)
							{
								if(document.login.password.value=="")
									document.login.password.value="password";
								
							}
						}					
	
