<script type="text/javascript">
    function loadHomePageAlert(urlString) {
        $.ajax({
            url:urlString,
            success:function(data, textStatus){
                $('#alert-placeholder').html(data);
            },
            error:function(XMLHttpRequest, textStatus, errorThrown){}
        });
    }

    $(document).ready(function() {
        var frontPageFeatureCarouselSettings = {
            'playerSettings': {
                'width': '669',
                'height': '376'
            },
            'mediaDataMap': mediaDataMap
        };
        $('#fpfCarousel').frontPageFeatureCarousel(frontPageFeatureCarouselSettings);

        var url = "/layoutParts/alerts?homepage=true";
        loadHomePageAlert(url);
    });
</script>
