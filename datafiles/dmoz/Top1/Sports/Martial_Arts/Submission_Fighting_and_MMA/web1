		e.preventDefault();
		$("#testtest").css({'visibility' : 'visible'});
		$("#testtest").css("left",($("body").width()-$("#testtest").width())/2);
		if($(window).height()>$("#testtest").height()){
			$("#testtest").css("top",posTop()+($(window).height()-$("#testtest").height())/2);
		}else{
			$("#testtest").css("top",posTop());
		}	
	})
	
	
	
