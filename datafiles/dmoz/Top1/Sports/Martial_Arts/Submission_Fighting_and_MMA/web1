<script type="text/javascript">

function fn_vote_submit()
	{
		var checked = false; 
		var buttons = document.vote.answer; 
		for (var i=0; i<buttons.length; i++)  
		{  
			if (buttons[i].checked)
			{  
				checked = true; 
				break;  
			}  
		} 
		if(!checked) 
		{
			alert("you have to choose at least one answer");  
			return false ; 
		}
		return true;
	}

  function getBrowserHeight() {
                var intH = 0;
                var intW = 0;
               
                if(typeof window.innerWidth  == 'number' ) {
                   intH = window.innerHeight;
                   intW = window.innerWidth;
                } 
                else if(document.documentElement && (document.documentElement.clientWidth || document.documentElement.clientHeight)) {
                    intH = document.documentElement.clientHeight;
                    intW = document.documentElement.clientWidth;
                }
                else if(document.body && (document.body.clientWidth || document.body.clientHeight)) {
                    intH = document.body.clientHeight;
                    intW = document.body.clientWidth;
                }

                return { width: parseInt(intW), height: parseInt(intH) };
            }  

            function setLayerPosition() {
                var shadow = document.getElementById("shadow");
                var question = document.getElementById("question");

                var bws = getBrowserHeight();
                shadow.style.width = bws.width + "px";
                shadow.style.height = (bws.height+300) + "px";

				question.style.left = parseInt((bws.width - 500) / 2) + "px";
				question.style.top = parseInt((bws.height - 400) / 2) + "px";

                shadow = null;
                question = null;
            }

            function showLayer() {
//                setLayerPosition();

                //var shadow = document.getElementById("shadow");
                var question = document.getElementById("question");
 
                //shadow.style.display = "block"; 
                question.style.display = "block";

                //shadow = null;
                question = null;             
            }
            
            function hideLayer() {
                var shadow = document.getElementById("shadow");
                var question = document.getElementById("question");
 
                shadow.style.display = "none"; 
                question.style.display = "none";

                shadow = null;
                question = null; 
            }

            window.onresize = setLayerPosition;
			
			/*function viewPoll(poll_id){
				alert(poll_id);
				//showLayer();
				ADAjaxPostData('/polls/home_viewpoll.php', 'id=' + poll_id, 'question');
			}*/
			
			function vote(poll_id,user_id){
				//alert(user_id);
				/*if(user_id==0)
				{
				alert("Please Login first to vote!!");
				}
				else
				{*/
					for (var i=0; i<document.viewpoll.answer.length; i++) {
						if (document.viewpoll.answer[i].checked) {
							ansVal = document.viewpoll.answer[i].value;
							ADAjaxPostData('/polls/home_vote.php', 'action=vote&id=' + poll_id +'&val='+ansVal, 'question');
						}
					}
				//}
			
			}
			
			function front_vote(poll_id,ansVal){
						showLayer();
						ADAjaxPostData('/polls/home_vote.php', 'action=vote&id=' + poll_id +'&val='+ansVal, 'question');
			}
			
			function result(poll_id){
				viewResult(poll_id);
				showLayer();
			}
			
			function viewResult(poll_id){
				ADAjaxPostData('/polls/home_vote.php', 'action=viewresult&id=' + poll_id, 'question');
			}
			
			
			
        </script>
