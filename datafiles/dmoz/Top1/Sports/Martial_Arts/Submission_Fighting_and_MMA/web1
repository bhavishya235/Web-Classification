 <script language="javascript">

 function chMd1(id)

 {

	if (id == "money_line1_1")

		document.getElementById("money_line1_2").checked = false;

	else

		document.getElementById("money_line1_1").checked = false;

	document.forms['myform'].bet_amount[0].disabled=false;

	//document.forms['myform'].bet_amount[1].disabled=true;

	//document.forms['myform'].bet_amount[2].disabled=true;

	//document.forms['myform'].bet_amount[3].disabled=true;

	//document.forms['myform'].bet_amount[1].value="";

	//document.forms['myform'].bet_amount[2].value="";

	//document.forms['myform'].bet_amount[3].value="";	

	document.forms['myform'].bet_amount[0].value="0";

	document.forms['myform'].bet_amount[0].focus();

	document.forms['myform'].bet_amount[0].select();

	calculate(document.forms['myform']);

 }

 function chMd2(id)

 {

	if (id == "money_line2_1")

		document.getElementById("money_line2_2").checked = false;

	else

		document.getElementById("money_line2_1").checked = false;

	//document.forms['myform'].bet_amount[0].disabled=true;

	document.forms['myform'].bet_amount[1].disabled=false;

	//document.forms['myform'].bet_amount[2].disabled=true;

	//document.forms['myform'].bet_amount[3].disabled=true;

	//document.forms['myform'].bet_amount[0].value="";

	//document.forms['myform'].bet_amount[2].value="";

	//document.forms['myform'].bet_amount[3].value="";	

	document.forms['myform'].bet_amount[1].value="0";	

	document.forms['myform'].bet_amount[1].focus();	

	document.forms['myform'].bet_amount[1].select();

	calculate(document.forms['myform']);

 }

 function chMd3(id)

 {

 	if (id == "money_line3_1")

		document.getElementById("money_line3_2").checked = false;

	else

		document.getElementById("money_line3_1").checked = false;

	//document.forms['myform'].bet_amount[0].disabled=true;

	//document.forms['myform'].bet_amount[1].disabled=true;

	document.forms['myform'].bet_amount[2].disabled=false;

	//document.forms['myform'].bet_amount[3].disabled=true;

	//document.forms['myform'].bet_amount[0].value="";

	//document.forms['myform'].bet_amount[1].value="";

	//document.forms['myform'].bet_amount[3].value="";	

	document.forms['myform'].bet_amount[2].value="0";	

	document.forms['myform'].bet_amount[2].focus();

	document.forms['myform'].bet_amount[2].select();

	calculate(document.forms['myform']);

 }

 function chMd4(id)

 {

	if (id == "money_line4_1")

		document.getElementById("money_line4_2").checked = false;

	else

		document.getElementById("money_line4_1").checked = false;

	//document.forms['myform'].bet_amount[0].disabled=true;

	//document.forms['myform'].bet_amount[1].disabled=true;

	//document.forms['myform'].bet_amount[2].disabled=true;

	document.forms['myform'].bet_amount[3].disabled=false;

	//document.forms['myform'].bet_amount[0].value="";

	//document.forms['myform'].bet_amount[1].value="";

	//document.forms['myform'].bet_amount[2].value="";

	document.forms['myform'].bet_amount[3].value="0";

	document.forms['myform'].bet_amount[3].focus();

	document.forms['myform'].bet_amount[3].select();

	calculate(document.forms['myform']);

 }

function calculate(t)

{

for (var i=0; i < t.money_line1.length; i++)

   {

   if (t.money_line1[i].checked)

      {

      var rad_val1 = t.money_line1[i].value;	  

      }

   }

for (var i=0; i < t.money_line2.length; i++)

   {

   if (t.money_line2[i].checked)

      {

      var rad_val2 = t.money_line2[i].value;	  

      }

   }

for (var i=0; i < t.money_line3.length; i++)

   {

   if (t.money_line3[i].checked)

      {

      var rad_val3 = t.money_line3[i].value;	  

      }

   }

for (var i=0; i < t.money_line4.length; i++)

   {

   if (t.money_line4[i].checked)

      {

      var rad_val4 = t.money_line4[i].value;	  

      }

   }   

   var rad2_val=new Array();

for (var i=0; i < t.bet_amount.length; i++)

   {

   if (t.bet_amount[i].value !="")

      {      

		rad2_val[i] = t.bet_amount[i].value;

      }

	else{

		rad2_val[i] = 0;

	}

   }   

   //alert (rad_val + rad2_val);

	var profit1 = adding(rad_val1,rad2_val[0]); 

	var profit2 = adding(rad_val2,rad2_val[1]); 

	var profit3 = adding(rad_val3,rad2_val[2]);

	var profit4 = adding(rad_val4,rad2_val[3]);

	t_profit = Number(profit1) + Number(profit2) + Number(profit3) + Number(profit4);

	//alert(profit);

	

	t_profit1 = t_profit.toFixed(4).replace(/\-/g, "");

	document.getElementById('result').innerHTML = t_profit1;

	//return false;	

}

function adding(rad_val, rad2_val){

	if(rad_val > 0)

	{

		//var ml = t.money_line.value;

		var ml = rad_val;

		var mlp = ml/100;

		var profit = mlp*rad2_val;

		var num = new Number(profit);		

		profit = num.toPrecision(5);

		return profit;

	}

	if(rad_val < 0)

	{

		//var ml = t.money_line.value;

		var ml = rad_val;

		var mlp = 100/ml;

		var profit = mlp*rad2_val;

		var num = new Number(profit);		

		profit = num.toPrecision(5);

		return profit;

	}

	return 0;

}

