		function DefaultText(txt, evt) {
			if (txt.value.length == 0 && evt.type == "blur") {
				txt.value = "Search the site here"; //grey
				txt.style.color = "#BFC0BD";
			}

			if (txt.value == "Search the site here" && evt.type == "focus") {
				txt.value = "";//Black
				txt.style.color = "black";
			}
		}

</script>
	
