                //  PerfectMind.Layout.DynamicTabs.resizeTabsBar();
                $(function () {
                    var mianWrapper = $("#main-wrapper");
                    var footerHeight = $("#footer-wrapper:visible").outerHeight();
                    function adjustFootterToBottom() {
                        var contentHeight = $(this).height() - footerHeight;
                        mianWrapper.css("min-height", contentHeight);
                    }
                    if ($("#footer-wrapper").length > 0) {
                        adjustFootterToBottom();
                        $("#footer-wrapper").css('visibility', 'visible');
                        $(window).resize(function () {
                            adjustFootterToBottom();
                        });
                    }
                });
            </script>
        
