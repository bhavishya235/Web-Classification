td_7 = "Enquiries";
url_7 = "enquiries.php";
target_7 = "_self";
img_7 = "menus/images/doc_qmark.gif";

function showPopup() {
	try {
		if((readCookie("popup_counter") == "") || (readCookie("popup_counter") != "1")) {
			showPage("http://www.gki.org.uk/gasshuku/index.php", 640, 510, "yes");
			createCookie("popup_counter", "1", 0.01389);
		}
	} catch(e) {
		alert(e.message);
	}
}

window.onload = function() {
	//showPopup();
}


</script>
</head>
