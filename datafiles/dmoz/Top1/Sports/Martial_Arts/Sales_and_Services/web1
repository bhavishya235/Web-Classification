<style type="text/css">
    .roswellGrays
    {
        color: #000000;
    }
</style>

<script type="text/javascript">
function clearBox(item)
{
    if(item.value=="user name"){
        $(item).removeClass("roswellGrays");
        item.value = "";
    }
}
function populateBox(item)
{
    if(item.value.length==0)
    {
        $(item).addClass("roswellGrays");
        item.value="user name";
    }
}
function sendSubmit(e)
{
    if(e.keyCode == 13)
         {
             if (!window.event) {
                e.preventDefault();
                __doPostBack('dnn$dnnSignin$btnLogin','');
             }            
            return false;
        }
        return true;
}

</script>

<div id="UserInfo" class="fieldItem">
    <h2>
