                <script type="text/javascript">
//<![CDATA[

  function showPageLoader() {
        
        if($('#pageLoaderWrap').length==1 && $('#pageLoaderWrap:visible').length==0){
        if ($('#gridRecords').length == 1) {                           
            $('#gridRecords').append($('#pageLoaderWrap'));

          //  $('#pageLoaderWrap').siblings().addClass("visibilityHidden");
            $('#gridRecords').addClass("positionRelative");
            $('#pageLoaderWrap').fadeIn(1600, "linear");
        }
        }
    }

    function hidePageLoader() {

        if($('#pageLoaderWrap').length==1 && $('#pageLoaderWrap:visible').length==1){
        if ($('#gridRecords').length == 1) {  
            $('#gridRecords').removeClass("positionRelative");
           // $('#pageLoaderWrap').siblings().removeClass("visibilityHidden");
           // $('#pageLoaderWrap').hide();
            $('#pageLoaderWrap').fadeOut(1600, "linear");
        }
        }
    }

    $(function() {
        $("#btnNav").bind("contextmenu",function (event) {            
           return false;       
        });

        var morebuttonMenuButtonSelectorId = $(".pm-button-more").parent().parent().prop('id');
         
        $(".pm-button-menu-item").on('click',function(event) {
         showPageLoader();
            event.stopPropagation();
            var menu = $("#" + morebuttonMenuButtonSelectorId).data("tMenu");
            if (menu != null && menu != undefined)
            {
                var items = $("li", menu.element); //jQuery collection of <li> elements
                menu.close(items);
            }
        });

        $("#multipleDeleteButton").click(function () {
            if (!$('#multipleDeleteButton').is('.disabled')) {
                //if (!$('#multipleDeleteNoDisplay')) $('#multipleDeleteNoDisplay').click();
                $('#multipleDeleteNoDisplay').click();
            }
        });
         
        var buttonLink =  function(linkId, onBegin) {
            $(linkId).pmAjaxLink(
            {
                loadingElementId: "customButtonRunning-" + linkId,
                updateTargetId: "customButtonNotification",
                onBegin: onBegin,
               onFailure: hidePageLoader,   
               onComplete: hidePageLoader,              
                type: "POST"                
            }).bind("contextmenu", function(event) {
                if($(this).is(".disabled")) {
                    event.preventDefault();
                    event.stopPropagation();
                }
            });
        };     
        $("#customButtonsContainerDiv .t-group").width('150px');
        
                 $(".pm-button").click(function () {
                    if (!($(this).hasClass("disabled")))
                    {
                        $(this).addClass("disabled");
                         //wizard-wrapper
                        $(this).addClass("ajax-loading");
                        showPageLoader();
                    }
                 });
                 $(".pm-button-menu-item").click(function () {
                    $(this).addClass("disabled");
                    $('.t-animation-container').hide();
                    hidePageLoader();
                 })
          
    
    });

    
   
    

//]]>
                </script>

