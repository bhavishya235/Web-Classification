function kickOffChat(){
var w = 640, h = 480;
if (document.all || document.layers) {
   w = screen.availWidth;
   h = screen.availHeight;
}
var leftPos = (w-640)/2, topPos = (h-480)/2;
xlaALSwindow=window.open("http://chat.masuccess.com/UserPreChat.aspx?ref=&d=&u=&bypass=","ALSRoom","toolbar=0,location=0,status=0,menubar=0,scrollbars=1,resizable=1,width=640,height=480,top=" + topPos + ",left=" + leftPos);
xlaALSwindow.focus();

}

</script>

 



