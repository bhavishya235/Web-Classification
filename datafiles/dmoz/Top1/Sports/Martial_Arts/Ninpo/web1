		function contactformscript() {
				jQuery('#contactConfirm').hide();
				jQuery.post('http://www.bujinkanmadison.com/wp-content/plugins/easy-speak-widget-contact-form/thanks.php', jQuery("#easyForm").serialize(),
				function(output){
				jQuery('#contactConfirm').html(output).fadeIn(1000);
			});
