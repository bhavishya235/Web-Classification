    epost("eike", "kujaw", "de", "Bujinkan Darmstadt"); 
  // Ende --> 
  </script>

    <p>
    Bitte &bdquo;Bujinkan Darmstadt&rdquo; als Betreff angeben,<br>
    damit die Nachricht nicht ungelesen gel&ouml;scht wird.

    <hr width="50%" align="left" noshade>

  </div>

</body>
