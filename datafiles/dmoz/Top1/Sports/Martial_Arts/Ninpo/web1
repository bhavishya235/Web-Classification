                document.getElementById("af-header-1905934038").className = "af-header af-quirksMode";
            }
            if (document.getElementById("af-footer-1905934038")) {
                document.getElementById("af-footer-1905934038").className = "af-footer af-quirksMode";
            }
        }
    })();
    -->
