                            <script type="text/javascript">
                                var player=(function(){ 
                                return {
                                    updateClipInfo:function(clipObj){
                                    
                                    $('#now-playing-clip-title').html(clipObj.episodeTitle);

                                    function hms(sec){
                                        var   hr = parseInt(sec/(60*60),10)
                                        , mi = parseInt(sec/60,10)- (hr*60)
                                        , se = sec%60;
                                            return [hr,mi,se]
                                            .join(':')
                                            .replace(/\b\d\b/g,
                                                function(a){ 
                                            return Number(a)===0 ? '00' : a<10? '0'+a : a; 
                                            }
                                        )
                                        .replace(/^00:/,'');
                                        }
                                        $("#now-playing-clip-duration").html(hms(clipObj.duration));
                                        $("#now-playing-clip-description").html(clipObj.videoCaption);
                                        $("#now-playing-href").attr("href", clipObj.fullyQualifiedURL);
                                    }
                                }
                                })();
                            </script>
                                
