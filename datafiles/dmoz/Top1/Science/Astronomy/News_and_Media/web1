function hideForOverlay()
{
hideTagType("select");
hideTagType("object");
hideTagType("embed");
hideTagType("iframe");
hideHighZIndex(20000);
if ($('dart_rect')) {
Element.setStyle($('dart_rect'), { visibility: "hidden" });
