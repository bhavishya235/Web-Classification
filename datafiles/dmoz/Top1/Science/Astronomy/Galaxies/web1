<script type="text/Javascript"> 
       
        var iplayerImgSrcTemplate = "http://ichef.bbci.co.uk/programmeimages/clip/{clipPid}_640_360.jpg";
       
	var RESOURCE_CLIPS_URI = "/science/space/universe/clips/sights/galaxies.json";
