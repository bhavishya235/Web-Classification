$(function() {
 $('.explorePhotosModImg').each(function() {
    $(this).load(function() {
    	var imgH = $('.explorePhotosModImg').height();
    	var vSpace = imgH/2;
    	$('.exploreVideoOverlay').css('top', vSpace + 'px');
    });
 });
});

