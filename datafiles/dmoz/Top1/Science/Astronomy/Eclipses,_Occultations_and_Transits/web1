    <script language="JAVASCRIPT" type="TEXT/JAVASCRIPT"><!-- Hide script

	now = new Date
	thisYr = now.getYear()
	if (thisYr < 1900)  {
	    thisYr = thisYr + 1900
	}

	eclipse = new Date(2015,2,20)
	
	function dayToDays(inTime)  {
	 return (Math.floor(inTime.getTime()/(1000*60*60*24)))
	}
	
	function daysTill(inDate)  {
	  return dayToDays(inDate) - dayToDays(now)
	}
// End hiding script -->
	</script>
