// on function

function imgAct(imgName) {

	if (document.images) {

		document.images[imgName].src = "rollovers/" + imgName + "_on.jpg";

	}

}

// off function

function imgInact(imgName) {

	if (document.images){

		document.images[imgName].src = "rollovers/" + imgName + "_off.jpg";

	}

