<script>
	(function ($) {
	// VERTICALLY ALIGN FUNCTION
	$.fn.vAlign = function(height) {
		/*if(height==16)
		{
			$(this).css('top', "12px");
			//alert(-4*mult);
		}
		if(height==32)
		{
			$(this).css('top', "8px");
		}
