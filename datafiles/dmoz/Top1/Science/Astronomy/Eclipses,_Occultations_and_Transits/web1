            (isMobile.iOS() && 
                ((! navigator.userAgent.match(/AppleWebKit/))
                    || navigator.userAgent.match('CriOS')
                    || navigator.userAgent.match('Mercury')
                    || navigator.userAgent.match('Opera')
                )
            )
        ) {
            $.smartbanner({
                title: 'HowStuffWorks App',
                author: 'HowStuffWorks.com',
                icon: 'http://static.ddmcdn.com/en-us/skins/homepage/hsw-ad-logo.png',
                force: varforce,
                daysHidden: 0,
                daysReminder: 0
            });
    }
