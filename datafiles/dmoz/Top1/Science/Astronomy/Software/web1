		errorMessage = confirmEmailField(formToBeChecked.email)
		if(errorMessage.length > 0){
			alert(errorMessage);
			return false;
		}
		errorMessage = confirmFieldNotEmpty(formToBeChecked.password,"your password.")
		if(errorMessage.length > 0){
			alert(errorMessage);
			return false;
		}
		
		return true;		
	}
	
