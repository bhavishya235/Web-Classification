realmedia_setup.jsp wxf
This file creates the necessary javascript so the realmedia banners work with either
journal homepages or TOC's.

OAS SETUP begin
-->
<script language="JavaScript" type="text/javascript">
   	OAS_url      = 'http://oas.iop.org/RealMedia/ads/';
       
        
        
        
            OAS_listpos  = 'Middle,x11,x12,x13,x14';
        
       

	//C Hayes agreed to leaving hardcoded to make testing easier
	//OAS_sitepage = window.location.host;
	OAS_sitepage = 'iopscience.iop.org';
	OAS_version  = 10;
	var journal = '0004-637X';
</script>


    
    
    
        <script language="JavaScript" type="text/javascript">
    	    OAS_query    = "?iopscience_pagetype=journal_home&iopscience_issn=" + journal;
	    </script>
    
    


<script language="JavaScript1.1" type="text/javascript">

    OAS_version = 11;
    if (navigator.userAgent.indexOf('Mozilla/3') != -1)
    	OAS_version = 10;

    if (OAS_version >= 11)
    	Realmedia.OAS_SCRIPT(OAS_url,OAS_sitepage,OAS_listpos,OAS_query);

</script>

<script language="JavaScript" type="text/javascript">
   document.write('');
     function OAS_AD(pos) {
       if (OAS_version >= 11 && typeof(OAS_RICH!='undefined'))
         OAS_RICH(pos);
       else
         Realmedia.OAS_NORMAL(pos,OAS_url,OAS_sitepage,OAS_listpos,OAS_query);
     }
</script>
        <script language="JavaScript" type="text/javascript">
            OAS_AD('x11');
            OAS_AD('x12');
            OAS_AD('x13');
            OAS_AD('x14');
        </script>
