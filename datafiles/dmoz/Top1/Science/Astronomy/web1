												<input id="myRating3" name="myRating" value="3" type="radio" class="auto-submit-star {split:4} star-rating-on-unrated" disabled='disabled' />
												<input id="myRating4" name="myRating" value="4" type="radio" class="auto-submit-star {split:4} star-rating-on-unrated" disabled='disabled' />
												<input id="myRating5" name="myRating" value="5" type="radio" class="auto-submit-star {split:4} star-rating-on-unrated" checked='checked' disabled='disabled' />
					    </div>

    <div class="averageRating">
        <p>
			Average:
			<span class="avgRate"><strong><span class="avgRateNumber">5</span></strong>/5</span>
		</p>
    </div>  
</div>

<script>
	$j(function() {
		$j('.auto-submit-star').rating({
			focus: function(value,link){
				$j('.star-rating-on-unrated').addClass('star-rating-off-unrated');
				$j('.star-rating-on-unrated').removeClass('star-rating-on-unrated');
			},
			blur: function(value,link){
				$j('.star-rating-off-unrated').addClass('star-rating-on-unrated');
				$j('.star-rating-off-unrated').removeClass('star-rating-off-unrated');
			},
			callback: function(value,link){
				$j.post('/',{
					__ajax: 'Rating',
					'myRating': value,
					'contentId': '1252',
					'contentType': 'site'
				}, function(data) {
					$j('.avgRateNumber').html(data.avgRating);
					$j('.star-rating-off-unrated').removeClass('star-rating-off-unrated');
					$j('.star-rating-on-unrated').removeClass('star-rating-on-unrated');
				}, 'json');
			}
		});
	});
</script><div class="sideModContainer">
