(function($){
 $.fn.jspost = function(options) {
    
  var defaults = {
   method: "post",
   action: "#"
  };
  var options = $.extend(defaults, options);
  var n=Math.floor(Math.random()*1000000000);
  var f = $("<form>").attr({"id":"jsPost"+n,"name":"jsPost"+n});
  f.attr({"action":options.action,"method":options.method});
  f.css("display","none");


  this.find("input,textarea,select,hidden").not("[name='submit']").not("[type='checkbox']").not("[type='radio']").each(function(i, item) {
        obj = $(this);
        var el = $("<input type=\"hidden\">").attr({'name':$(item,obj).attr("name"),'value':$(item,obj).val()});
        f.append(el);
    });

	this.find("input[type='checkbox']").each(function(i, item) {
        obj = $(this);
        var el = $("<input type=\"hidden\">").attr({'name':$(item,obj).attr("name"),'value':$(item,obj).is(':checked')});
        f.append(el);
    });
	
	this.find("input[type='radio']").each(function(i, item) {
        obj = $(this);
        var el = $("<input type=\"hidden\">").attr({'name':$(item,obj).attr("name"),'value':$(item,obj).is(':checked').val()});
        f.append(el);
    });
 
  f.append($("<input type=\"submit\">").attr({"id":"mys"+n,"name":"submit","value":"submit"}));
  f.appendTo(document.body);
 	try { 
		__sfga();
	}
	catch(err) { 
	}
  $("#mys"+n).click();
 };
})(jQuery);
</script>

