<script language="javascript" type="text/javascript">
    //<![CDATA[
    
	function SearchForm(){
		this.txtKeyword				= getObj("ctl00_HeaderPlaceholder1_ctl00_SearchForm1_srchKeywords");
	}
	
	function ctl00_HeaderPlaceholder1_ctl00_SearchForm1_testSearch(id) {
        var txtKeywords = getObj('ctl00_HeaderPlaceholder1_ctl00_SearchForm1_srchKeywords');
        if (txtKeywords.value == "") {
            alert('The Keyword(s) field cannot be blank.');
        } else if (txtKeywords.value == "enter keyword(s)") {
            alert('Please enter Keyword(s) in the search field.');
        } else {
            var search = "/search.htm"; 
			if(search.indexOf("?") > 0){
				search += "&Keywords=" + escape(txtKeywords.value);
			}else{
				search += "?Keywords=" + escape(txtKeywords.value);
			}
            window.location = search;
        }
        return false;
    }
    
    function ctl00_HeaderPlaceholder1_ctl00_SearchForm1_searchFocus(item){
        if(item.value=='enter keyword(s)'){item.value='';}
    }
    
    function ctl00_HeaderPlaceholder1_ctl00_SearchForm1_searchBlur(item){
        if(item.value==''){item.value='enter keyword(s)';}
    }
    
    function ctl00_HeaderPlaceholder1_ctl00_SearchForm1_searchPopulate(){
        var txtKeywords = getObj("ctl00_HeaderPlaceholder1_ctl00_SearchForm1_srchKeywords");
        var keywords = "";

        if(txtKeywords.value.length == 0){
            txtKeywords.value = keywords;
        }
    }

    addPageLoad_Handler(ctl00_HeaderPlaceholder1_ctl00_SearchForm1_searchPopulate);
    //]]>
</script>
