<script type="text/javascript">
var defaultText = 'Search our website';
$(document).ready(function() {
	var count=$('.rotate-wrap div.engage-content').length;
	var i=1;
	$('.rotate-wrap div.engage-content').each(function() {
		if (i <= count) {
			$(this).addClass('campaign-block'+i);
			i=i+1;
		}
	});
	$(".campaign-block5 a").attr("target","_blank");
	
	$('#searchtext').focus(function(){
		if($(this).val()==this.defaultValue) 
			$(this).val('');
	});
	$('#searchtext').blur(function(){
		if($(this).val().length == 0) 
			$(this).val(this.defaultValue);
	});
	$('#searchtext').keypress(function(e){
		if((e.which && e.which == 13) || (e.keyCode && e.keyCode == 13)){
			$('#btnSearchSubmit').click();
			return false;
		}
		else
			return true;
	});
	$('#btnSearchSubmit').click(function(){
		var search = $('#searchtext').val();
		if(search.length == 0 || search == defaultText)
			alert('Please enter a valid search term')
		else
			location.href='/SearchResults/tabid/124/Default.aspx?Search='+ search;
		return false;
	});
});

</script>

