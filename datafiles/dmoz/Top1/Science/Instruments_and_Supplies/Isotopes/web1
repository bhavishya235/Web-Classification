<script language="JavaScript" type="text/JavaScript">
	monName = new Array ("January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December");
	// run function after the page loads:
	window.onload = init;

	function init() {
   		// Check for DOM support:
  		 var el = document.getElementById('curdate');
  		 if (el) {
     		 // add current date, but check first if already there, for Mac IE
     		 if (! el.hasChildNodes()) {
				  var theDate = new Date;
				  var txt = document.createTextNode(monName[theDate.getMonth()] + " " + theDate.getDate() + ", " + theDate.getFullYear());
				  el.appendChild(txt);
			}
  	 	}
	}
</script>
