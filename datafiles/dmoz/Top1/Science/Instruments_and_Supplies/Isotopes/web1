<script language="javascript" type="text/javascript">
    function mobile() {
        var href = "/default.aspx";
        window.location.href = href;        
    }

    function addMtoLinks() {
        var aTags = document.getElementsByTagName("a");
		var theForm = getFormTag();
        var localPath = "http://" + window.location.pathname.split('/')[2];
        
        for (var i = 0; i < aTags.length; i++) {
            
            if (aTags[i].href.indexOf(localPath) == 0  ||(aTags[i].href.indexOf(location.href.substring(0,location.href.lastIndexOf('/')+1)) == 0 || aTags[i].href.charAt(0) == "/") && aTags[i].href.toLowerCase().indexOf("/sitecm/i/") != 0 && aTags[i].href.toLowerCase().indexOf("/docs/") != 0 ) {
                if (aTags[i].href.indexOf("?") > 0) {
                    aTags[i].href += "&M=1";
                } else {
                    aTags[i].href += "?M=1";
                }
            }
        }

		if(theForm.action.indexOf("?") > 0){
			theForm.action += "&M=1";
		}else{
			theForm.action += "?M=1";
		}

    }
    
    
</script>
<a href="javascript:mobile();" id="mobilelink" class="mobile"></a>



