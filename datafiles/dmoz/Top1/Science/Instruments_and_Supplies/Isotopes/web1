<script language="javascript" type="text/javascript">
	// Bucket preload images...
	var b2image = new Image();
		b2image.src = "images/new-home/bucket2_over.png";
	var b3image = new Image();
		b3image.src = "images/new-home/bucket3_over.png";
	// End preload.
	
	var hideTimer;
	var hideTarget;
	var activeBucket;
	
	function showBucket(index)
	{
		cancelHide();
		
		for (var i = 1; i <= 3; i++)
		{
			if (i != index) { resetBucket(i); }
		}
		
		var b = document.getElementById("bucket" + index);
		if (b)
		{
			b.style.backgroundColor = "#1a325d";
			b.style.backgroundImage = "none";
			if (index < 3)
			{
				var s = document.getElementById("bucket" + (index + 1));
				if (s)
				{
					s.style.backgroundImage = "none";
				}
			}
			var i = document.getElementById("bimg" + index);
			if (i)
			{
				i.src = "images/new-home/bucket" + index + "_over.png";
			}
			
			var p = document.getElementById("bucket" + index + "popup");
			if (p)
			{
				p.style.visibility = "visible";
			}
			
		}
	}
	function hideBucket(index)
	{
		hideTarget = index;
		hideTimer = window.setTimeout(resetBucket, 500);
	}
	function resetBucket(index)
	{
		index = index || hideTarget;
		if (index)
		{
			var b = document.getElementById("bucket" + index);
			if (b)
			{
				b.style.background = "none";
				b.style.backgroundImage = "url(images/new-home/dots-vertical-long.gif)";
				b.style.backgroundRepeat = "no-repeat";
				b.style.backgroundPosition = "0 30px";
				
				if (index < 3)
				{
					var s = document.getElementById("bucket" + (index + 1));
					if (s)
					{
						s.style.backgroundImage = "url(images/new-home/dots-vertical-long.gif)";
					}
				}
				var i = document.getElementById("bimg" + index);
				if (i)
				{
					i.src = "images/new-home/bucket" + index + ".png";
				}
				var p = document.getElementById("bucket" + index + "popup");
				if (p)
				{
					p.style.visibility = "hidden";
				}
				
			}
		}
	}
	function cancelHide()
	{
		window.clearTimeout(hideTimer);
	}
</script>
<script language="javascript" type="text/javascript">
