jQuery(function() {
	jQuery('.tab .tab-container').hide(0);
 
	jQuery('.tab .tab-nav li a').click(function() {
		var container = jQuery(this).attr('href');
		
		if(jQuery(this).parent().hasClass('current')) {
			return false;
		}
		
		jQuery('.tab .tab-container').hide(0);
		jQuery(container).fadeIn(500);
		jQuery('.tab .tab-nav li').removeClass('current');
		jQuery(this).parent().addClass('current');
		
		return false;
	});
	
