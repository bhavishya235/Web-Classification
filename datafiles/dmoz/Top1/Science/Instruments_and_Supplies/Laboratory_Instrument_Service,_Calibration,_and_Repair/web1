	function mijosearchsubmit(){
		var form = document.getElementById("mijosearchModule-127");
		var moquery = document.getElementById("qr-127").value.length;
		var text = document.getElementById("qr-127").value;
		var f = "search...";
		
		if(text.length < 3 || text == f ){
			return false;
		}

		if (moquery >= "3"  ) {
            form.submit();
		} 
		else {
			alert("Please, make sure that your query length is higher than 3 characters.");
			return false;
		}
	}

    
</script>

