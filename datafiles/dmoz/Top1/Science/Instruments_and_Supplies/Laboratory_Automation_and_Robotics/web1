<script type="text/javascript">
		$('#content').hide();
		function show_popup(url, popuptitle) {
			$('#content').show();
			var $loading = $('<img src="/images/loader.gif" alt="loading" class="loading">');
			if (url) {
				var $dialog = $('<div></div>')
				.append($loading.clone());
				$dialog
					.load(url + ' #popup')
					.dialog({
						title: popuptitle,
						width: 780,
						height: 500,
						zIndex: 10,
						open: function(event, ui) {
							if ($.browser.webkit || $.browser.msie) {
								$('#content').hide();
							}
						},
						close: function(event, ui) {
							if ($.browser.webkit || $.browser.msie) {
								$('#content').show();
								$('#content').focus();
							}
						}
					});

			} else { ;
				$('#popup_specs td a').each(function() {
					var $dialog = $('<div></div>')
					.append($loading.clone());
					var $link = $(this).one('click', function() {
						$dialog
							.load($link.attr('href') + ' #popup')
							.dialog({
								title: $link.attr('title'),
								width: 780,
								height: 500,
								zIndex: 10,
								open: function(event, ui) {
									if ($.browser.webkit || $.browser.msie) {
										$('#content').hide();
									}
								},
								close: function(event, ui) {
									if ($.browser.webkit || $.browser.msie) {
										$('#content').show();
										$('#content').focus();
									}
								}
							});

						$link.click(function() {
							$dialog.dialog('open');
							if ($.browser.webkit || $.browser.msie) {
								$('#content').hide();
							}
						});
					});
				});
			}
		}
	$(document).ready(function() {
		show_popup()
	});
</script>
