function toggleChild(node)
{
var parent = node.parentNode;
if(!parent) // largely, a test for 'old' browsers
return true
var tmp = parent.getElementsByTagName("div");
var child = tmp[0]
if(!child)
return true
var displayValue = child.style.display
if(displayValue == "none")
child.style.display = "block";
else
child.style.display = "none";
if (child != s2008111212092858)
s2008111212092858.style.display = "none";
return true;
}

</script>
