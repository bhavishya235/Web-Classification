function SubMenuOver(thisitem){
//if submenu not active change color on mouseover
	if(thisitem.className!='smenuactive'){
	thisitem.className='smenuover';
	}
}
function SubMenuOut(thisitem){
// if submenu not active change color on mouseover
	if(thisitem.className!='smenuactive'){
		thisitem.className='smenu';
	}
