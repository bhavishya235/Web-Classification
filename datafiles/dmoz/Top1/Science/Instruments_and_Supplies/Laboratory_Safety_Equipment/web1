				history.back(1);
				return false;
			});
			
			 $("#selproducts").change(function() {
			  var target= $(this).val();
			  var path = "http://s-curve.com/index.php";
			  window.location= path + "/products/view/" + target;
			  
			});
			
			$(".subclasses select").change(function() {
				window.location = "http://s-curve.com/index.php/dispensers/by_product" + $(this).val();
			});
			
			$("#menu a.title").toggle(
			    function () {
					$(this).next("ul").slideDown();
				},
				function () {
					$(this).next("ul").slideUp();
				}
			);
			
			
				
				$("#menu a.title:first").trigger('click');
						
			
 			 //$('#checkout').ketchup();
			 
			 $('input[name=same]:radio').change(function() {
				var val = $(this).val();
				if (val=="no"){
					$("#shipping").slideDown();
				}else{
					$("#shipping").slideUp();
				}
			});
			
		$("#front_text p:first").addClass('firstline');	
		 $("#front_text p:last").addClass('lastline');
		 
