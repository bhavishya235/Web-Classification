	</div>
</div></div><div class = 'single-tab-wrapper' id = 'bing-maps-widget-tab2'><script type="text/javascript">
  var map = null;

  
	function GetWi8893Map()
	{
		var locs = [];
		var mapOptions ={
			credentials: "Ag3uNSgaR61QvjTSy6Z4xw0xJUpoyr7NB_c_MZ8TaumLvModnyW78_1TUXN9KT1f",
			center: new Microsoft.Maps.Location(35.976681,-84.229538),
			zoom: 15,
			disableZooming:true,
			disablePanning:true,
			enableSearchLogo:false,
			enableClickableLogo:false,
			mapTypeId: Microsoft.Maps.MapTypeId.road,
			showDashboard: false
		}
		var map = new Microsoft.Maps.Map(document.getElementById("myW8893Map"), mapOptions);
		var center = map.getCenter();
		var pin = new Microsoft.Maps.Pushpin(center, {icon:"/templates/source/creator/ltr/bing maps/default/gfx/pointer.png", height:43, width:40}); 
		var location = new Microsoft.Maps.Location(35.976681,-84.229538);

		map.entities.push(pin);
		locs.push(location);
		
		// additional addresses latitude and longitude arrays
		var additionalLatArray = [];
		var additionalLongArray = [];
		
		location = null;
		
		// go over the additional addresses and create a new pin for each one
		for (var i=0;i<additionalLatArray.length;i++)
		{
			location = new Microsoft.Maps.Location(additionalLatArray[i],additionalLongArray[i]);
			pin = new Microsoft.Maps.Pushpin(location, {icon:"/templates/source/creator/ltr/bing maps/default/gfx/pointer.png", height:43, width:40}); 
			map.entities.push(pin);	
			locs.push(location);
		}
		
		// if there are multiple locations, set the best view to show them all
		if(locs.length > 1)
		{
			var bestview = Microsoft.Maps.LocationRect.fromLocations(locs);
			map.setView({bounds:bestview });		
		}
	}	

	function disposeMap() 
	{
		map.dispose();
	}
 </script>

