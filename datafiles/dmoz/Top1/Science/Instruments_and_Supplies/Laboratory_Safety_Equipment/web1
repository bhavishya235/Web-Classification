<script type="text/javascript">

	// get querystring as an array split on "&"
var querystring = location.search.replace( '?', '' ).split( '&' );
// declare object
var queryObj = {};
// loop through each name-value pair and populate object
for ( var i=0; i<querystring.length; i++ ) {
      // get name and value
      var name = querystring[i].split('=')[0];
      var value = querystring[i].split('=')[1];
      // populate object
      queryObj[name] = value;
}

	if (location.pathname.indexOf("-s/") != -1 || location.pathname.indexOf("_s/") != -1)
	{
		var lstlnkval = $('.pages_available_text a').last().text();
		var curpage=1;	
		if(queryObj["page"]!=undefined)
			var curpage = queryObj["page"];
		var nxtparam;
		if(curpage > lstlnkval)
			nxtparam = curpage;
		else
			nxtparam = parseInt(curpage) + 1 > parseInt(lstlnkval) ? parseInt(lstlnkval) : parseInt(curpage) + 1;
		var prevparam = curpage > 1 ? parseInt(curpage)-1 : 1;
		
		//$('.pages_available_text').empty();	
		
		if(curpage > 1)
			$('.pages_available_text').append("&nbsp;<a href='javascript:void(0);' id='prev'><img src='/v/vspfiles/templates/CP-Green/images/buttons/Previous.gif' align='absmiddle' border='0'></a>&nbsp;");
		//else
		//	$('.pages_available_text').append("&nbsp;Previous&nbsp;");
		if(curpage != nxtparam)	
			$('.pages_available_text').append("&nbsp;<a href='javascript:void(0);' id='next'><img src='/v/vspfiles/templates/CP-Green/images/buttons/Next.gif' align='absmiddle' border='0'></a>&nbsp;");
		//else
		//	$('.pages_available_text').append("&nbsp;Next&nbsp;");
		$('.pages_available_text #prev').bind("click", function()
		{
			Add_Search_Param('page',prevparam);
			Refine();
		});
		$('.pages_available_text #next').bind("click", function()
		{
			Add_Search_Param('page',nxtparam);
			Refine();
		});
	}

	
</script>

