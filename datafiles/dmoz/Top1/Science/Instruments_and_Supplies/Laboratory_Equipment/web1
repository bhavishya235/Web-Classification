<script type="text/javascript">
    //Selects drop down item based on selection or mouseover
    function setSearchSelectedText(source, eventArgs) {  
        var obj = "ctl00_ctl00_ctl00_pageContent_headerControl_siteSearch_txtSearchBox";
        var submitButton = "ctl00_ctl00_ctl00_pageContent_headerControl_siteSearch_btnSubmit";
        document.getElementById(obj).value = eventArgs.get_value();
        OASetAutoSuggestedSearchResult(document.getElementById(obj).value);
        onMouseClick(submitButton, '#searchBox');
    }   
</script>

