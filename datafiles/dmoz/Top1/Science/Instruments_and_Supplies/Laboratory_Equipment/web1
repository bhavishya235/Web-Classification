<script type="text/javascript" language="javascript">
var slideArray = new Array()
slideArray[0]= "<h1>Welcome to Krüss</h1>";
slideArray[1]= "<h1>Willkommen bei Krüss</h1>";
slideArray[2]= "<h1>Bienvenidos a Krüss</h1>";
slideArray[3]= "<h1>Bienvenue chez Krüss</h1>";
function firstSlide()
{
document.getElementById('div_display').innerHTML=slideArray[0];
setTimeout("secondSlide()",2000);
}
function secondSlide()
{
document.getElementById('div_display').innerHTML=slideArray[1];
setTimeout("thirdSlide()",2000);
}
function thirdSlide()
{
document.getElementById('div_display').innerHTML=slideArray[2];
setTimeout("fourthSlide()",2000);
}
function fourthSlide()
{
document.getElementById('div_display').innerHTML=slideArray[3];
setTimeout("firstSlide()",2000);
}
</script>

