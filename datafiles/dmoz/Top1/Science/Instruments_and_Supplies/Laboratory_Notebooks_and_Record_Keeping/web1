<!-- Velaro Weblink Code -->
<script type="text/javascript">
	function get_velaro_src() {
		var vnt='yes';
		var vrt='no';
		var vt=1;
		var pt='http';
		var qs=escape(window.location.search);
		var ti=new Date();
		if(location.href.substr(0,5).toLowerCase()=='https')
			pt='https';
		if (document.cookie.length>0) {
			var cookieLength = 14;
			if (document.cookie.indexOf('velaront5882=')!=-1)
				vnt='no';
			cs=document.cookie.indexOf('velaroret5882=');
			if (cs!=-1) {
				vrt='yes';
				cs=cs + cookieLength;
				c_end=document.cookie.indexOf(';',cs);
				if (c_end==-1)
					c_end=document.cookie.length;
				vt = parseInt(unescape(document.cookie.substring(cs,c_end)));
				if(vnt=='yes')
					vt++;
			}
		}
		var ed = new Date();
		ed.setHours(23);
		ed.setMinutes(59);
		ed.setSeconds(59);
		document.cookie='velaront1=yes;expires='+ed.toGMTString();
		ed.setFullYear(ed.getFullYear()+1);
		var la='';
		if (navigator.appName == 'Netscape')
			la=navigator.language; else la=navigator.systemLanguage;
		document.cookie='velaroret5882='+vt+';expires='+ed.toGMTString();
		var pn = '&amp;pn='+location.pathname;
		var rm = escape(window.document.referrer);
		var pm = window.document.URL.replace(/&/g,"*");
		return pt+'://service.velaro.com/lf/monitor2.aspx?siteid=5882&amp;secure=yes&amp;newtoday='+vnt+'&amp;returning='+vrt+'&amp;vt='+vt +
			'&amp;qs='+qs+'&amp;ti='+ti.getTime()+'&amp;tz='+ti.getTimezoneOffset()+'&amp;an='+escape(navigator.appName)+'&amp;co='+escape(navigator.cookieEnabled) +
			'&amp;la='+escape(la)+'&amp;pl='+escape(navigator.platform)+'&amp;pal='+screen.colorDepth+'&amp;sw='+escape(screen.width+'x'+screen.height) +
			pn + '&amp;je='+navigator.javaEnabled()+'&amp;origin=' + rm+'&amp;pa='+pm;
	}

	window.addEvent('domready', function () {
		var e=window.document.createElement('script');
		e.setAttribute('src',get_velaro_src());
		window.document.body.appendChild(e);
	});

</script>
<!-- End Velaro Weblink Code -->

