        function showMask(){
            $("#maskPop").css({"display":"block", "opacity":0});
            $("#maskPop").animate({"opacity":1}, 600);
        }

        $("#closemaskPop").click(function(e){
            $("#maskPop").animate({"opacity":0}, 300, function(){
                $("#maskPop").css({"display":"none"});
                $("#popUpContent").html('<div id="videoPop"></div>')
            });return false;
        });

