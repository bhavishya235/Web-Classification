jQuery(document).ready(function() {
  // XML should be on the same domain!
  jQuery.get('http://www.totallyintegratedautomation.com/states.xml', function(d){
    jQuery("#eventscontainer").append("<ul></ul>");
    jQuery(d).find('item').each(function(){      // Node is labeled "item"
      var jQueryevent = jQuery(this); 
      var title = jQueryevent.find('title').text();
      var link = jQueryevent.find('url').text();
      var description = jQueryevent.find('description').text();
      var html = '<li><a href="'+ link +'" target="_blank">' + title + '</li>';   //Adding an <li> tag per loop
      jQuery("#eventscontainer ul").append(jQuery(html));
    });
  });
});
</script>
