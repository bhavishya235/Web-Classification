<script type="text/javascript">

$(window).load(function(){

  function getViewportSize() {
    if ($('body').innerWidth() <= 767) {
      return 'mobile';
    }
    else {
      return 'desktop';
    }
  }

  var currBreakpoint = getViewportSize();
  var didResize  = true; // default's to true so it's always analysed on first load
  var raw_slider = $(".expert-slider").html(); // grab the unaltered HTML and store it
  var raw_slider2 = $(".feature-slider").html(); // grab the unaltered HTML and store it
  var raw_slider3 = $(".pubs-slider").html(); // grab the unaltered HTML and store it

  function enableCarousels() {
    currBreakpoint = getViewportSize();

    $('.sidebar-left .menu-block-2 ul').slideUp();
    // $(".primary-nav").css("left","-100%");
    // remove the old flexslider (which has attached event handlers and adjusted DOM nodes)
    $(".expert-slider").remove();
    $(".feature-slider").remove();
    $(".pubs-slider").remove();

    // now re-insert clean mark-up so flexslider can run on it properly
    $(".panels-expert").append("<div class='expert-slider'></div>");
    $(".expert-slider").html(raw_slider);
    $(".panels-feature").append("<div class='feature-slider'></div>");
    $(".feature-slider").html(raw_slider2);
    $(".panels-pubs").append("<div class='pubs-slider'></div>");
    $(".pubs-slider").html(raw_slider3);

    // Slider with one slide
    $('.expert-slider').flexslider({
      selector: ".slides > .expert-row",
      animation: "fade",
      animationLoop: true,
      slideshow: false
    });
    $('.expert-slider .slides .col').css("width","33.333%");

    // Slider with one slide
    $('.feature-slider').flexslider({
      selector: ".slides > .feature-row",
      animation: "fade",
      animationLoop: true,
      slideshow: false
    });
    $('.feature-slider .slides a').css("width","49.5%");

    // Slider with one slide
    $('.pubs-slider').flexslider({
      selector: ".slides > .pub-row",
      animation: "fade",
      animationLoop: true,
      slideshow: false
    });

    equalheight('.panels-feature .box');
    equalheight('.panels-expert .expert');
    equalheight('.footer-menu .section-name');

    var allPanels = $('#nav ul.menu ul.menu').hide();

    $('.menu-block-1 > ul.menu > li > a').click(function() {
      event.preventDefault();
      allPanels.slideUp();
      $(this).next().slideDown();
    });

  }

  function disableCarousels() {
    currBreakpoint = getViewportSize();

    $('.sidebar-left .menu-block-2 ul').slideDown();
    $('.mobile-search').css('display', '');
    $(".primary-nav").css("left","0px");
    // remove the old flexslider (which has attached event handlers and adjusted DOM nodes)
    $(".expert-slider").remove();
    $(".feature-slider").remove();
    $(".pubs-slider").remove();

    // now re-insert clean mark-up so flexslider can run on it properly
    $(".panels-expert").append("<div class='expert-slider'></div>");
    $(".expert-slider").html(raw_slider);
    $(".panels-feature").append("<div class='feature-slider'></div>");
    $(".feature-slider").html(raw_slider2);
    $(".panels-pubs").append("<div class='pubs-slider'></div>");
    $(".pubs-slider").html(raw_slider3);

    $('#nav .content > ul.menu > li > a').unbind();
    $('#nav ul.menu ul.menu').removeAttr("style");

  }

  // on window resize, set the didResize to true
  $(window).resize(function() {
    didResize = true;
  });

  // every 1/4 second, check if the browser was resized
  // we throttled this because some browsers fire the resize even continuously during resize
  // that causes excessive processing, this helps limit that
  setInterval(function() {
    if(didResize) {
      didResize = false;

      // if the new breakpoint is different to the old one, do some stuff
      var windowWidth = $('body').innerWidth();
      //alert(windowWidth);
      //alert(currBreakpoint);

      if (windowWidth <= 767 && currBreakpoint == 'desktop') {
        enableCarousels();
      }
      else if(windowWidth >= 768 && currBreakpoint == 'mobile') {
        disableCarousels();
      }
    }
    equalheight('.panels-feature .box');
    equalheight('.panels-expert .expert');
    equalheight('.footer-menu .section-name');
  }, 125); // lower numbers create more, frequent visible re-renderings

  if($('body').innerWidth() <= 767) {
    enableCarousels();

  }
}); // $(document).ready

$(function(){
  //SyntaxHighlighter.all();
});

$(window).load(function(){

  $('.issues-slider').flexslider({
    animation: "slide",
    animationLoop: true,
    slideshow: false,
    itemWidth: 170,
    itemMargin: 8,
    minItems: 3
  });

  // Slide Animation
  $('.home-carousel').flexslider({
    animation: "slide",
    controlNav: "thumbnails",
    slideshow: true
  });

  // Fade Animation
  // $('.home-carousel').flexslider({
  //   animation: "fade",
  //   controlNav: "thumbnails",
  //   slideshow: true,
  //   start: function(slider) {
  //     slider.slides.slice(1).hide();
  //   },
  //   after: function(slider) {
  //     $('.home-carousel > .slides > .flex-active-slide').show();
  //   }
  // });

  equalheight('.panels-feature .box');
  equalheight('.panels-expert .expert');
  equalheight('.footer-menu .section-name');
  $( ".tabs" ).tabs();


  $(".mobile-menu-toggle").on('click', function() {
    if(! $(this).hasClass('footer-trigger')) {
      event.preventDefault();
    }
  //alert('test');
  var $righty = $('.primary-nav');
  $righty.animate({
    right: parseInt($righty.css('right'),10) == 0 ?
    -$righty.outerWidth() :
    0
  });
});
  $(".mobile-search-toggle").on('click', function() {
    event.preventDefault();
    $('.mobile-search').slideToggle();

  });

 });

$(function(){
  //$('select.styled').customSelect();
  //$('select.form-select').customSelect();
});

$(document).ready(function() {
    $('.staff-directory-container .views-table').dataTable( {
        "bPaginate": false,
        "bLengthChange": false,
        "bFilter": true,
        "bSort": true,
        "bInfo": false,
        "bAutoWidth": false,
        "oLanguage": {
         "sSearch": "FILTER RECORDS (name, email, phone):"
       }
    } );

    $('#block-menu-block-2 h2').click(function() {
      var htmlStr = $(this).html();
      if (htmlStr == "show navigation") {
        $(this).html("hide navigation");
        $(this).addClass("accordion-open");
      }
      else {
        $(this).html("show navigation");
        $(this).removeClass("accordion-open");
      }
      $('#block-menu-block-2 ul').slideToggle();
      return false;
    });

} );

</script>
