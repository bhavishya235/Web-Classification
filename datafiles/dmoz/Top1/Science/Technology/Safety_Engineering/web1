<script type="text/javascript">

$(document).ready(function(){

  var se = $('#google-cse-searchbox-form-1 .form-text');

  se.data('defaultValue','Search');

  // Set the initial default value
  se.val(se.data('defaultValue'));

  se.focus(function(){

    var cval = $(this).val();
    var dval = se.data('defaultValue');

    if(cval == dval){
      $(this).val('');
    }

  });

  se.blur(function(){

    var cval = $(this).val();
    var dval = se.data('defaultValue');

    if(cval == ''){
      $(this).val(dval);
    }

  });

});

</script>

