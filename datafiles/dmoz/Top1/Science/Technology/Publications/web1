	<script type="text/javascript">
	var obj_leftcolumn_standard_4 = {
	  setTCValues : function() {
	   var tcTagStr = '';
	   var tcTagArr = tcTagStr.split("/");
	   if (tcTagArr != null && tcTagArr != "") {
	    for(var i=1;i<=3;i++) {
	     var tcTag = (i <= tcTagArr.length) ? tcTagArr[i-1] : "";
	     $.cookie('TC'+i, tcTag, { path : '/' });
	    }  
	   } 
	  }    
	};
	$(document).ready(function(){
	  $('#leftcolumn_standard_4 a').click(function(e) {
	     obj_leftcolumn_standard_4.setTCValues();
	    });  
	});
	</script>
	 
	
