<!-- END: DIV ID="HEADING" --><script language="JavaScript" type="text/javascript">
// the following requires the getDirectoryName() in /js/master_new.js - this will wrap the nav in div with unique IDs based on the 1st and 2nd directories for CSS purposes - to highlight the Main and Sub Nav
var var_FirstDirName = getDirectoryName(1);
var var_SecondDirName = getDirectoryName(2);
if (var_FirstDirName.match('asp')){
		var_FirstDirName = '';
	}

	//following sets acceptions to which nav item is selected
    else if(var_FirstDirName == 'whitepapers'){ //selects education nav and whitepaper sub-nav when first directory = whitepaper
        var_FirstDirName = 'education';
        var_SecondDirName = 'whitepapers';
    }
    else if(var_FirstDirName == 'webinars'){ //selects education nav and webinars sub-nav when first directory = webinar
        var_FirstDirName = 'education';
        var_SecondDirName = 'webinars';
    }
	/* Check if Vote on Photo Contest Entries page (09/26/12) */
	else if(var_SecondDirName == 'entries-vote'){ 
        var_FirstDirName = 'multimedia';
        var_SecondDirName = 'photocontestvote';
    }
	/* Check if on Photo Contest Archives landing page (01/14/2013) */
	else if(var_SecondDirName == 'archives'){ 
        var_FirstDirName = 'multimedia';
        var_SecondDirName = 'photocontestarchives';
    }
	else if(var_FirstDirName == 'photocontest'){ 
        var_FirstDirName = 'multimedia';
        var_SecondDirName = 'photocontest';
    }
	else if(var_FirstDirName == 'community'){ 
        var_FirstDirName = 'opinions';
        var_SecondDirName = 'community';
    }
		
	if (var_SecondDirName.match('asp')){
		var_SecondDirName = '';
	}
 	
document.write('<div id="'+var_FirstDirName+'Dir">'); // MAIN NAV - wraps a div with the first directory's name in it 
document.write('<div id="'+var_SecondDirName+'SubDir">');// SUB NAV - wraps a div with the second directory's name in it

document.write('<style type="text/css"><!--');// start CSS
document.write('#'+var_FirstDirName+'Dir #'+var_FirstDirName+'li a.top {background-color: #FFFFFF; color: #000000;}');// highlight main tab
document.write('#'+var_SecondDirName+'SubDir #'+var_SecondDirName+'li a {text-decoration:underline;}');// underline sub nav
document.write('#'+var_FirstDirName+'Dir #'+var_FirstDirName+'li ul{ display: block;z-index:20;}');// show sub nav while in section
document.write('--></style>');// End CSS

</script>


<!-- PRODUCTION NOTE: each new <li> that is created MUST have an ID that is equal to that directory's name (lower case) + 'li' (one word) -->

