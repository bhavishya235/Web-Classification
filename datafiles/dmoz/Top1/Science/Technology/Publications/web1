 <script type="text/javascript">
 var pwSiteCatalystHelper = new PWFE.SiteCatalystHelper();
 var contentType = "generic";
  
 s.pageType="";
 s.pageName=(contentType == 'wrapper' ? document.title : "FE_Home");
 
 s.server="";
 s.channel="";
 s.prop1="";
 s.prop2="";
 s.prop3="FireEngineeringcom: Fire Service News &amp Firefighter Training, Rescue, more";
 s.prop5=(contentType == 'wrapper' ? "" :contentType);
 if (contentType == 'wrapper') {
     s.prop6 = "";
     s.prop7 = s.prop6 + "/" + s.pageName;
