         if (beaconcodeArray[i] != "") {
           if (criteriaArray[i] == 0) {
             // Only render beacon code for return visitors
             if ("Repeat" == s.getNewRepeat()) {
               jQuery("head").append(beaconcodeArray[i]);                 
             }     
           }
           else if (criteriaArray[i] == 1) {
             // Only render beacon code for product viewers
             if ("product-releases item" == s.prop5 || "products item" == s.prop5) {
               jQuery("head").append(beaconcodeArray[i]);
             }   
           }
           else if (criteriaArray[i] == 2) {
             // Render beacon code for all visitors
             jQuery("head").append(beaconcodeArray[i]);
           } 
         } 
      }
