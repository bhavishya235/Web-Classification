<script type="text/javascript">
	function lightbox(title, width, height, url)
		{
			$('#lightbox_overlay').css('display','block');
			$('#div_lightbox_top').html(title);
			//$('#iframe_lightbox_content').css('width', width);
			//$('#iframe_lightbox_content').css('height', height);
			//$('#iframe_lightbox_content').attr('src', url);
			$('#td_lightbox_content').css('width', width);
			$('#td_lightbox_content').css('height', height);
			$('#td_lightbox_content').css('background-image', "url('" + url + "')");
		}
	
	function hideLightboxOverlay()
		{
			$('#lightbox_overlay').css('display','none');
			//$('#iframe_lightbox_content').attr('src','');
			$('#td_lightbox_content').html('');
		}
</script>

