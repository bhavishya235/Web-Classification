	var obj_farcolumn_standard_3 = {
	  setTCValues : function() {
	   var tcTagStr = '';
	   var tcTagArr = tcTagStr.split("/");
	   if (tcTagArr != null && tcTagArr != "") {
	    for(var i=1;i<=3;i++) {
	     var tcTag = (i <= tcTagArr.length) ? tcTagArr[i-1] : "";
	     $.cookie('TC'+i, tcTag, { path : '/' });
	    }  
	   } 
	  }    
	};
	$(document).ready(function(){
	  $('#farcolumn_standard_3 a').click(function(e) {
	     obj_farcolumn_standard_3.setTCValues();
	    });  
	});
	</script>
	 
	
