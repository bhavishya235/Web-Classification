<TITLE>The OTA Legacy</TITLE>
</HEAD>
<FRAMESET  FRAMEBORDER=NO BORDER=0 COLS="120,*">
   <FRAME SRC="ns20/legacy_i.html"
      NAME="legacy_index"
      MARGINHEIGHT=1
      MARGINWIDTH=1
      SCROLLING="no"
      NORESIZE>
      
   <FRAMESET FRAMEBORDER=NO BORDER=0 ROWS="60,*">
