<script type="text/javascript">
	//var board = 1;
	//var bOld=1;
	//var bNew=2;
	var board = 0;
	var bOld=0;
	var bNew=1;
	var t=setTimeout("nextBoard()",15000);
	
	$(document).ready(function(){
		$("#flagshipLeft").fadeTo("slow", 0.1); 
		$("#flagshipRight").fadeTo("slow", 0.1); 

		$("#flagship").hover(function(){
			$("#flagshipLeft").fadeTo("slow", 1);
			$("#flagshipRight").fadeTo("slow", 1); 
		},function(){
   			$("#flagshipLeft").fadeTo("slow", 0.1);
   			$("#flagshipRight").fadeTo("slow", 0.1);
		});
		//swfobject.registerObject("flashLogo");
		

	});	
	
	function nextBoard(){
		clearTimeout(t);
		bOld=board;
		if (board<6){
			board++;
		} else {
			//board=1;
			board=0;
		}
		bNew=board;
		$("#billboard" + bOld).fadeOut("slow", function(){
			$("#billboard" + bNew).fadeIn("slow",function(){return false;} );
			return false;
		});
		t=setTimeout("nextBoard()",15000);
	}
	function prevBoard(){
		clearTimeout(t);
		bOld=board;
		if (board>0){
		//if (board>1){
			board--;
		} else {
			board=6;
		}
		bNew=board;
		$("#billboard" + bOld).fadeOut("slow", function(){
			$("#billboard" + bNew).fadeIn("slow");
		});
		t=setTimeout("nextBoard()",15000);
	}
		
</script>


