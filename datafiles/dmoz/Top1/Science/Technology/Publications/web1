<script type="text/javascript">
function obj(field){
	return document.getElementById(field);
}

function trim(stringToTrim) {
	return stringToTrim.replace(/^\s+|\s+$/g,"");
}

function checkemail(str){
	var filter=/^([\w-]+(?:\.[\w-]+)*)@((?:[\w-]+\.)*\w[\w-]{0,66})\.([a-z]{2,6}(?:\.[a-z]{2})?)$/i
	if (str.length > 99){
		return false;
	}
	if (filter.test(str)){
		testresults=true
	} else {
		testresults=false
	}
	return (testresults)
}

function toggleElement(el){
	if(document.getElementById(el).style.display==""){
		document.getElementById(el).style.display="block";
		document.getElementById('collapse-' + el).innerHTML="-";
		return;
	}
	if(document.getElementById(el).style.display=="none"){
		document.getElementById(el).style.display="block";
		document.getElementById('collapse-' + el).innerHTML="-";
		return;
	}
	if(document.getElementById(el).style.display=="block"){
		document.getElementById(el).style.display="none";
		document.getElementById('collapse-' + el).innerHTML="+";
		return;
	}
}
function forgotPassword(){
	document.getElementById('passwordboxes').style.display="block";
	document.getElementById('loginboxes').style.display="none";
	document.getElementById('registerboxes').style.display="none";
}

function showLogin(){
	document.getElementById('passwordboxes').style.display="none";
	document.getElementById('loginboxes').style.display="block";
	document.getElementById('registerboxes').style.display="none";
}

function showRegister(){
	document.getElementById('passwordboxes').style.display="none";
	document.getElementById('loginboxes').style.display="none";
	document.getElementById('registerboxes').style.display="block";
}

function validateUsername(fld) {
	obj('registrationMsg').innerHTML = '';
    var error = "";
    var illegalChars = /\W/; // allow letters, numbers, and underscores
 
    if (fld == "") {
        error = "Username required.";
    } else if ((fld.length < 6) || (fld.length > 25)) {
        error = "Username needs to be between 5 and 25 characters long.";
    } else if (illegalChars.test(fld)) {
        error = "Username contains illegal characters, only letters and numbers allowed.";
    } else {
        error = "ok"
    }
    return error;
}

function validateName(fld) {
	obj('registrationMsg').innerHTML = '';
    var error = "";
    var illegalChars = /\W/; // allow letters, numbers, and underscores
    if (fld == "") {
        error = "Name required.";
    } else if ((fld.length < 5) || (fld.length > 100)) {
        error = "Name needs to be between 5 and 100 characters long.";
    } else {
        error = "ok"
    }
    return error;
}

function requestPassword(){
	if (checkemail(obj('new_email').value)){
		obj('requestPasswordMsg').innerHTML = 'Requesting new password.<br/>Waiting for server response.';
		if (window.XMLHttpRequest){
			var http = new XMLHttpRequest();
		} else {
		  var http = new ActiveXObject("Microsoft.XMLHTTP");
		}
		var url = "lostpassword.php";
		var params = "email=" + obj('new_email').value;
		http.open("POST", url, true);
		http.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
		http.setRequestHeader("Content-length", params.length);
		http.setRequestHeader("Connection", "close");
		http.onreadystatechange = function() {
			if(http.readyState == 4 && http.status == 200) {
				obj('new_email').value = '';
				obj('requestPasswordMsg').innerHTML =  http.responseText ;
			}
		}
		http.send(params);
	} else {
		obj('requestPasswordMsg').innerHTML = 'Invalid email address.';
	}

}

function doRegistration(){
	if(validateName(obj('register_name').value) == 'ok'){
		if(validateUsername(obj('register_login').value) == 'ok'){	
			if (checkemail(obj('register_email').value)){
				if(obj('register_company').value!=''){
					if(obj('register_phone').value!=''){
						if(obj('register_position').value!=''){
							obj('registrationMsg').innerHTML = 'Processing registration.<br/>Waiting for server response.';
							if (window.XMLHttpRequest){
								var http = new XMLHttpRequest();
							} else {
							  var http = new ActiveXObject("Microsoft.XMLHTTP");
							}
							var url = "register.php";
							var params = "email=" + obj('register_email').value;
							params += "&name=" + obj('register_name').value;
							params += "&username=" + obj('register_login').value;
							params += "&company=" + obj('register_company').value;
							params += "&position=" + obj('register_position').value;
							params += "&phone=" + obj('register_phone').value;
							if (obj('canContact').checked){
								params += "&contact=no";
							}
							http.open("POST", url, true);
							http.setRequestHeader("Content-type", "application/x-www-form-urlencoded");
							http.setRequestHeader("Content-length", params.length);
							http.setRequestHeader("Connection", "close");
							http.onreadystatechange = function() {
								if(http.readyState == 4 && http.status == 200) {
									obj('registrationMsg').innerHTML =  http.responseText ;
								}
							}
							http.send(params);
						} else {
							obj('registrationMsg').innerHTML = 'Position required.';
						}		
					} else {
						obj('registrationMsg').innerHTML = 'Phone number required.';
					}
				} else {
					obj('registrationMsg').innerHTML = 'Company information required.';
				}			
			} else {
				obj('registrationMsg').innerHTML = 'Invalid email address.';
			}
		} else {
			obj('registrationMsg').innerHTML = validateUsername(obj('register_name').value);
		}		 		
	} else {
		obj('registrationMsg').innerHTML = validateName(obj('register_name').value);
	}			




}

function grayOut(vis, options) {
  // Pass true to gray out screen, false to ungray
  // options are optional.  This is a JSON object with the following (optional) properties
  // opacity:0-100         // Lower number = less grayout higher = more of a blackout 
  // zindex: #             // HTML elements with a higher zindex appear on top of the gray out
  // bgcolor: (#xxxxxx)    // Standard RGB Hex color code
  // grayOut(true, {'zindex':'50', 'bgcolor':'#0000FF', 'opacity':'70'});
  // Because options is JSON opacity/zindex/bgcolor are all optional and can appear
  // in any order.  Pass only the properties you need to set.
  var options = options || {}; 
  var zindex = options.zindex || 50;
  var opacity = options.opacity || 70;
  var opaque = (opacity / 100);
  var bgcolor = options.bgcolor || '#000000';
  var dark=document.getElementById('darkenScreenObject');
  if (!dark) {
    // The dark layer doesn't exist, it's never been created.  So we'll
    // create it here and apply some basic styles.
    // If you are getting errors in IE see: http://support.microsoft.com/default.aspx/kb/927917
    var tbody = document.getElementsByTagName("body")[0];
    var tnode = document.createElement('div');           // Create the layer.
        tnode.style.position='absolute';                 // Position absolutely
        tnode.style.top='0px';                           // In the top
        tnode.style.left='0px';                          // Left corner of the page
        tnode.style.overflow='hidden';                   // Try to avoid making scroll bars            
        tnode.style.display='none';                      // Start out Hidden
        tnode.id='darkenScreenObject';                   // Name it so we can find it later
    tbody.appendChild(tnode);                            // Add it to the web page
    dark=document.getElementById('darkenScreenObject');  // Get the object.
  }
  if (vis) {
    // Calculate the page width and height 
    if( document.body && ( document.body.scrollWidth || document.body.scrollHeight ) ) {
        var pageWidth = document.body.scrollWidth+'px';
        var pageHeight = document.body.scrollHeight+'px';
    } else if( document.body.offsetWidth ) {
      var pageWidth = document.body.offsetWidth+'px';
      var pageHeight = document.body.offsetHeight+'px';
    } else {
       var pageWidth='100%';
       var pageHeight='100%';
    }   
    //set the shader to cover the entire page and make it visible.
    dark.style.opacity=opaque;                      
    dark.style.MozOpacity=opaque;                   
    dark.style.filter='alpha(opacity='+opacity+')'; 
    dark.style.zIndex=zindex;        
    dark.style.backgroundColor=bgcolor;  
    dark.style.width= pageWidth;
    dark.style.height= pageHeight;
    dark.style.display='block';                          
  } else {
     dark.style.display='none';
  }
}

function clickclear(thisfield, defaulttext) {
	if (thisfield.value == defaulttext) {
		thisfield.value = "";
	}
}
function clickrecall(thisfield, defaulttext) {
	if (thisfield.value == "") {
		thisfield.value = defaulttext;
	}
}

function checkMailList(){
	if(obj('mailName').value == 'Your Name'){
		obj('mailListError').style.display='block';
		return false;
	} else if(obj('mailEmail').value == 'Your Email') {
		obj('mailListError').style.display='block';
		return false;		
	} else if(checkemail(obj('mailEmail').value)==false) {
		obj('mailListEmailError').style.display='block';
		return false;		
	} else if(obj('mailPhone').value == 'Your Phone') {
		obj('mailListError').style.display='block';
		return false;
	} else if(obj('mailCompany').value == 'Your Company') {
		obj('mailListError').style.display='block';
		return false;
	} else if(obj('mailPosition').value == 'Your Position') {
		obj('mailListError').style.display='block';
		return false;
	} else {
		return true;
	}

}


function startPopups(){
	obj('flagship').style.height="543px";
	setTimeout('popup1()',1000);
}
function popup1(){
	$('#otn').fadeIn(500, function(){setTimeout('popup2()',3000);});
}
function popup2(){
	$('#otn2').fadeIn(500, function(){setTimeout('popup3()',3000);});
}
function popup3(){
	$('#pdh').fadeIn(500, function(){setTimeout('popup4()',3000);});
}
function popup4(){
	$('#fc').fadeIn(500, function(){setTimeout('popup5()',3000);});
}
function popup5(){
	$('#fc2').fadeIn(500, function(){setTimeout('popup6()',3000);});
}
function popup6(){
	$('#cfp').fadeIn(500, function(){setTimeout('popup7()',3000);});
}
function popup7(){
	$('#guy').fadeIn(500, function(){setTimeout('popup8()',3000);});
}
function popup8(){
	$('#roomWhatElse').fadeIn(500, function(){setTimeout('fadeRoom()',12000);});
}
function fadeRoom(){
	$('#room').fadeOut(1000, function(){});
	$('#flagship').animate({height: '400'}, 1000, function() {});
}


</script>


