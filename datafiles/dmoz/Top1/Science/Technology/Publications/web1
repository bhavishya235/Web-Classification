	// MULTIMEDIA TABS settings
	var objTabsObject3 = new Array();
	objTabsObject3[0] = new TabObject('syndicateNewsTab1', 'syndicateNewsContent1');
	objTabsObject3[1] = new TabObject('syndicateNewsTab2', 'syndicateNewsContent2');
	
	
	function selectTab3OnLoad() {
	var randNbr = Math.floor(objTabsObject3.length*Math.random());
	setDisplay(randNbr, objTabsObject3);
	}
	
	// on window load
	addLoadEvent(selectTab3OnLoad);
