	<script language="javascript">
	  // MULTIMEDIA TABS settings
	  var objTabsObject2 = new Array();
	  objTabsObject2[0] = new TabObject('multiMediasTab1', 'multiMediasContent1');
	  objTabsObject2[1] = new TabObject('multiMediasTab2', 'multiMediasContent2');
	  objTabsObject2[2] = new TabObject('multiMediasTab3', 'multiMediasContent3');
	  
	  function selectTab2OnLoad() {
	    var randNbr = Math.floor(objTabsObject2.length*Math.random());
		setDisplay(randNbr, objTabsObject2);
	  }
	  
	  // on window load
	  addLoadEvent(selectTab2OnLoad);
	</script>
