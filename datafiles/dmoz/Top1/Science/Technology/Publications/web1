	                    //In the event the header variable isn't present (like if we are rendering the component by itself)
	                    if ( typeof adCacheBust == 'undefined' ) {
	                        adCacheBust = new String(Math.random());
	                    }

						updateScreenContext();

	                    if ( screenContext == 'desktop' ){
	                        document.write('<scr' + 'ipt type="text/javascript" src="http://ads.pennnet.com/RealMedia/ads/adstream_jx.ads/' + OAS_sitepage + '/1' + adCacheBust.substring (2,11) + '@' + OAS_locId + '"></scr' + 'ipt>');
                        }
                    }
                // -->
                </script>
                
        </div>
          

</div>
</div><div class="section"><div class="new"></div>
</div>
</div>

