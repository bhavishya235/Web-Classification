<script language="javascript">
    /*  SUBMIT VALIDATOR CODE */
    var reconPass = document.getElementById('SessionValidator_t_pass');
    var recon = document.getElementById('SessionValidator_reconnectDiv');
    var ValidatorErrorPrefix = 'We\'re sorry, the web-server is temporarily unavailable. A website update may be in progress or an error may have occurred. \nDo not navigate away from this page - doing so may result in the loss of your changes.\nPlease allow a few minutes and then try submitting again.\nIf this issue does not resolve, please contact your website administrator, Steve at us-tech@gim.net or  -> Error code: ';
    var validatorXMLurl = 'isSessionAlive.aspx?SessionID=b6438499-8410-4bd3-ad7c-4865795d5464&xmlguid=266783c9-643f-411a-836e-5c299b1007cb';
    var validatorXMLurlCont = 'continueSession.aspx?SessionID=b6438499-8410-4bd3-ad7c-4865795d5464&xmlguid=266783c9-643f-411a-836e-5c299b1007cb';
    var xhr;

    var LastClickedUniqueID = 'none';


    function checkSession(UniqueID) {
        __doPostBack(UniqueID, '');
    }

    function checkSessionBroken(UniqueID) {

        try { showLoadingPanel(); } catch (e) { }

        LastClickedUniqueID = UniqueID;
        try {
            xhr = new ActiveXObject("Microsoft.XMLHTTP");    // Trying Internet Explorer 
        }
        catch (e)    // Failed 
        {
            xhr = new XMLHttpRequest();    // Other browsers.
        }

        xhr.onreadystatechange = function () {
            if (xhr.readyState == 4) {
                if (xhr.status == 200) {
                    var resText = xhr.responseText.split(",");
                    if (parseInt(resText[0]) > 0) {
                        __doPostBack(LastClickedUniqueID, '');
                    }
                    else {
                        if (parseInt(resText[0]) == 0) {

                            recon.style.height = parseInt(document.body.offsetHeight) + parseInt(document.body.scrollTop);
                            recon.style.width = parseInt(document.body.offsetWidth) + parseInt(document.body.scrollLeft);
                            recon.style.top = 0;
                            recon.style.left = 0;
                            recon.style.display = 'block';
                            reconPass.focus();
                            try { hideLoadingPanel(); } catch (e) { alert(e); }
                            return false;
                        } else {
                            alert(resText[1]);
                            try { hideLoadingPanel(); } catch (e) { alert(e); }
                            return false;
                        }
                    }
                }
                else {
                    //document.ajax.dyn="Error code " + xhr.status;
                    if (xhr.status != 0) {
                        alert(ValidatorErrorPrefix + xhr.status);
                        try { hideLoadingPanel(); } catch (e) { }
                        return false;
                    }
                }
            }
        };

        var ie = (document.all) ? 1 : 0;
        if (ie) {
            xhr.open('Post', pref + validatorXMLurl, true);
        } else {
            xhr.open('Get', pref + validatorXMLurl, true);
        }
        xhr.send(null);
    }

    function continueSession() {
        return true;
    }
    function continueSessionBroken() {
        try {
            xhr = new ActiveXObject("Microsoft.XMLHTTP");    // Trying Internet Explorer 
        }
        catch (e)    // Failed 
        {
            xhr = new XMLHttpRequest();    // Other browsers.
        }

        xhr.onreadystatechange = function () {
            if (xhr.readyState == 4) {
                if (xhr.status == 200) {
                    var resText = xhr.responseText.split(",");
                    if (parseInt(resText[0]) > 0) {
                        __doPostBack(LastClickedUniqueID, '');
                        return false;
                    }
                    else {
                        alert(resText[1]);
                        return false;
                    }
                }
                else {
                    //document.ajax.dyn="Error code " + xhr.status;
                    alert(ValidatorErrorPrefix + xhr.status);
                    return false;
                }
            }
        };
        var passInput = document.getElementById(reconPass);
        var ie = (document.all) ? 1 : 0;
        if (ie) {
            xhr.open('Post', validatorXMLurlCont + '&Pass=' + passInput.value, true);
        } else {
            xhr.open('Get', validatorXMLurlCont + '&Pass=' + passInput.value, true);
        }
        xhr.send(null);
    }

    /*  END of SUBMIT VALIDATOR CODE  */
</script>

