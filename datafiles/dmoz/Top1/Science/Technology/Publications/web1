  <script type="text/javascript">
function slide() {
    // slides up or down and sets the new image source
    if($('#extendedfooterlinks').hasClass("invisible")) {
        $('#extendedfooterlinks').slideDown('slow');
        $('#extendedfooterlinks').removeClass("invisible");
        $('#opencloseImg').attr("src", "http://electroiq.com/wp-content/themes/twentytwelve/images/closeimage.jpg");
    } else {
        $('#extendedfooterlinks').slideUp('slow');
        $('#extendedfooterlinks').addClass("invisible");
        $('#opencloseImg').attr("src", "http://electroiq.com/wp-content/themes/twentytwelve/images/openimage.jpg");
    }
}
</script>
