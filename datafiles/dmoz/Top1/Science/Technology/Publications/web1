<script language="JavaScript" type="text/javascript">
		
		var var_sitePageInfo = 'enr.construction.com'; //this will be default for pages not within any specific directory
		var var_listposInfo = 'Top2,Middle,Left';
		
		
		// the script below will add the first & second directory to the default "var_sitePageInfo" above
				
		if ((getDirectoryName(1) != '') && (getDirectoryName(1).match('asp') == null)){
			var_sitePageInfo = var_sitePageInfo+'/'+getDirectoryName(1); //if there is a first directory it will be added to "var_sitePageInfo"
				if ((getDirectoryName(2).match('asp') == null) && (getDirectoryName(2) != '') ){
					var_sitePageInfo = var_sitePageInfo+'/'+getDirectoryName(2); //if there is a second directory it will be added to the end of  "var_sitePageInfo" after the first directory

				}
		
		} 
		//
		else if ((getDirectoryName(1) == '') || (getDirectoryName(1).match('default'))){
			var_sitePageInfo = var_sitePageInfo+'/home'; //site page 
			var_listposInfo = 'Top2,TopRight,TopLeft,Middle,Left';//for homepage only
		}
		//alert(var_listposInfo);
		//alert(var_sitePageInfo);
	
</script>

