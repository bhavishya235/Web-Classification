function mmLoadMenus() {
  if (window.mm_menu_0827224029_0) return;

window.mm_menu_0827224029_0 = new Menu("root",97,16,"Arial, Helvetica, sans-serif",10,"#000000","#ffffff","#6699CC","#000000","left","middle",3,0,1000,-5,7,true,true,true,0,true,true);

  mm_menu_0827224029_0.addMenuItem("About&nbsp;Us","location='about/index.htm'");

   mm_menu_0827224029_0.hideOnMouseOut=true;

   mm_menu_0827224029_0.bgColor='#ffffff';

   mm_menu_0827224029_0.menuBorder=1;

   mm_menu_0827224029_0.menuLiteBgColor='#ffffff';

   mm_menu_0827224029_0.menuBorderBgColor='#000000';

window.mm_menu_0827224030_0 = new Menu("root",148,16,"Arial, Helvetica, sans-serif",10,"#000000","#ffffff","#6699CC","#000000","left","middle",3,0,1000,-5,7,true,true,true,0,true,true);

  mm_menu_0827224030_0.addMenuItem("Cyrospace&nbsp;Network","location='network/index.htm'");

  mm_menu_0827224030_0.addMenuItem("Join&nbsp;Our&nbsp;Network","location='network/join.htm'");

   mm_menu_0827224030_0.hideOnMouseOut=true;

   mm_menu_0827224030_0.bgColor='#FFFFFF';

   mm_menu_0827224030_0.menuBorder=1;

   mm_menu_0827224030_0.menuLiteBgColor='#FFFFFF';

   mm_menu_0827224030_0.menuBorderBgColor='#000000';

   mm_menu_0903101539_0.hideOnMouseOut=true;

