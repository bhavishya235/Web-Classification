	<script type="text/javascript">
function getSearchResults()
{
	//Builds the search results Url
	var searchText = document.getElementById('txtSearch').value;
	if(searchText.length > 1 )
	{
		var url = '/Searchresults/SearchText=' + searchText;
		var ddlSection = document.getElementById('ddlSection');
		if(ddlSection)
		{
			url = url + '/Section=' + ddlSection.value;
		}
		window.location = url;
	}
	else
	{
		document.getElementById('divSearchError').innerText = 'Please search on a valid word or phrase';
	}
}

function whichKey(e) 
{		
	//Returns the key code for different browsers
	if ( window.navigator.appName.toLowerCase().indexOf("microsoft") > -1) 
	{
		//If the enter code
		if(window.event.keyCode == 13)
		{
			if (window.event) 
			{
				var s = document.getElementById('txtSearch');		
				if(s)
				{
					if( s.value.length > 1)
					{
						window.event.returnValue = false;
						getSearchResults();
					}
					else
					{
						window.event.returnValue = false;
					}
				}
			}
		}
	}
	else
	{
		//If the enter code
		if(e.keyCode == 13)
		{
			var searchText = document.getElementById('txtSearch').value;		
			if(searchText.length > 1)
			{
				e.returnValue = false;
				getSearchResults();
			}
			else
			{
				e.returnValue = false;
			}
		}
	}
}


//Bind onkeydown event to whickKey function
if ( typeof document.addEventListener != "undefined" )
	document.addEventListener( "keydown", whichKey, false );
else if ( typeof window.attachEvent != "undefined" ) {
	document.attachEvent( "onkeydown", whichKey );
}

</script>


		
		
