var IS_IE = $.browser.msie;
var IEdebug = false;
if (IS_IE && IEdebug) {
    $('body').append("<div id='IEconsole' style='position:absolute; z-index: 1000; top:580px; right:0; width:200px; background:#EECCCC;'>IE Debug Console -v " + IEdebug + "<\/div>");
}
this.echo = function (msg, clear) {
    var curContent = $('#IEconsole').html() + "<br />";
    if (clear) {
        curContent = '';
    }
    $('#IEconsole').html(curContent + msg);
}
