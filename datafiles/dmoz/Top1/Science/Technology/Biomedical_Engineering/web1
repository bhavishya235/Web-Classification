function playSlider() {
	if(!$('#slider_list li.active').next().length) {
		delete slideInt;
	} else {
		var nextSlide = $('#slider_list li.active').next();
		$('#slider_list li.active').removeClass('active').hide();
		$(nextSlide).addClass('active').show();
	}
	
}
