  <meta name="keywords"
 content="Parkinson's Disease, Parkinsons, tremor, gait, dyskinesia, sport injuries, human movement, pallidotomy, neurology, neurosurgery, rehabilitation, range of motion, physical therapy, deep brain stimulation, gyroscopes, solid state gyroscope">
  <meta name="Microsoft Border" content="none">
  <meta name="ProgId" content="FrontPage.Editor.Document">
