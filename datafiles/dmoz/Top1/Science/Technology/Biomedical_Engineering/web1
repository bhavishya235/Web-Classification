     <td width="49%" bgcolor="#006699" align="center">
      <strong>
      <font face="verdana, geneva, helvetica" size="2" color="#eeeeee">
      Agenda
      </font>
      </strong>
     </td>

     <td width="2%" bgcolor="#EEEEEE">
      <br />
     </td>

     <td width="49%" bgcolor="#006699" align="center">
      <strong>
      <font face="verdana, geneva, helvetica" size="2" color="#eeeeee">
      Calendar
      </font>
      </strong>
