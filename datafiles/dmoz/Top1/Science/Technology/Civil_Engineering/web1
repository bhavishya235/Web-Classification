ul.links li {padding:0 0.3em;}

.view-events-block .panel-2col div {padding-bottom:0px !important;}

.event-nodeapi div label {font-weight: bold;}

.view-data-node-view {float:right; margin-bottom: 12px;}

.panel-flexible h2.title {font-size: 1.333em; border-bottom:1px solid #DAD9DA; margin:0.7em 0;;}

.panel-flexible h2 {font-size: 1em;}

div.links ul li {list-style-type:none !important; float: right !important; margin-bottom:10px;}

.pager {clear:both;
text-align:center; margin-top:12px;}

.pager a, .pager strong.pager-current {padding:0.5em;}

.panel-panel.panel-col-last div.links {clear: right;}

.panel-pane div.content p a:last-child {float: none !important; padding: 0;}

.panel-col-last .panel-pane .item-list ul li a {float: none; padding; 0;}

.panel-col-last .panel-pane .item-list ul {padding: 0;}

.filefield-icon {float: right; margin-top:0.4em; margin-right: 0;}

.field .field-label, .field .field-label-inline, .field .field-label-inline-first {float: right;}

.filefield-item a {max-width: 250px; text-align:right;}

a.event_calendar {display: none;}

.panel-col-last .panel-pane .item-list ul li {list-style: none; margin-left: 0; margin-top: 0; margin-right: 0; margin-bottom: 10px;}

.panel-pane {overflow: hidden;}

.event-nodeapi {display: none;}

div.news_and_notices img {float: left; margin-right: 18px; display: inline; max-width:250px;}

.field-field-external-video {padding-top: 10px;}

div#uc-content div.primary span.left {float: none; margin: 0;}

div#uc-content div.primary span.inline-left {float: none; margin: 0;}

.rounded_corner {margin-top: -16px; margin-bottom: 16px;}

.panel-flexible .news_and_notices.teaser h2 {display: none;}

.news_and_notices.teaser .content {margin-top: 8px; margin-bottom: 8px;}

.news_and_notices.teaser:first-child {border-top:none;}
