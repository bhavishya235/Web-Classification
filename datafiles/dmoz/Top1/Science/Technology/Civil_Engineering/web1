	function validate(fn){
		if(fn.userID.value.indexOf('@')+2>=fn.userID.value.lastIndexOf('.') || fn.userID.value.indexOf('@')<1 || fn.userID.value.lastIndexOf('.')+2>=fn.userID.value.length){
			alert('Your user ID is your e-mail address');
			fn.userID.focus();
			return false;
		}
		if(fn.pwd.value==''){
			alert('Please enter your password');
			fn.pwd.focus();
			return false;
		}
	}
</script>
