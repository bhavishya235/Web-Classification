<script type="application/javascript">
function checkPlaceHolder(obj, isFocused){
	if (isFocused){
		if (obj.value=="Search..."){
			obj.value = "";
			obj.style.cssText="color:#000000";
		}
	}else{
		if (obj.value==""){
			obj.value = "Search...";
			obj.style.cssText="color:#999999";
		}
	}
}

function onSearchFocus(){
	var inp = document.getElementById("find");
	if (inp.value=="Search..."){
		var part = inp.createTextRange();
    	part.move("character", 0);
	    part.select();
	}
}
</script>
