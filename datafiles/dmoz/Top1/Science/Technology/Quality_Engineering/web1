<p class="nomarginbottom"><em class="bold nobold bot-icon-text">Connect on Twitter</em></p>

<div class="quote-l nomargintop"><div class="quote-r"><em class="nobold" id="footer-twitter"></em></div></div>

<a href="http://twitter.com/isixsigma" class="readon bot-readon" target="_blank"><span>Follow @iSixSigma</span></a>
<script>
  
    // set your twitter id
    var user = 'isixsigma';
      
    // using jquery built in get json method with twitter api, return only one result
    jQuery.getJSON('http://www.isixsigma.com/twitter/twitter.php', function(data)      {
          
        // result returned
        var tweet = data[0].text;
      
        // process links and reply
        tweet = tweet.replace(/(\b(https?|ftp|file):\/\/[-A-Z0-9+&@#\/%?=~_|!:,.;]*[-A-Z0-9+&@#\/%=~_|])/ig, function(url) {
            return '<a href="'+url+'">'+url+'</a>';
        }).replace(/B@([_a-z0-9]+)/ig, function(reply) {
            return  reply.charAt(0)+'<a href="http://twitter.com/'+reply.substring(1)+'">'+reply.substring(1)+'</a>';
        });
      
        // output the result
        jQuery("#footer-twitter").html(tweet);
    }); 
</script></div>
