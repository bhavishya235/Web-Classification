ntop_lines[85] = ['ntopline0086', '', 0, 0, 8, 'http://www.aws.org/w/a/membership/index.html?id=I824iwmy', '', 'Membership', '', '', 'MEMBER', ''];
var ntop_anchor_down = 1;
var ntop_link_prepend = '';
var ntop_link_target = '';
var ntop_last_level = 1;
var ntop_link_class = 'submenu';
var ntop_link_class_open = 'submenu';
var ntop_link_class_closed = 'submenu';
var ntop_link_style = '';
var ntop_link_style_open = '';
var ntop_link_style_closed = '';
var ntop_submenu_image_right = '/img/sm_right.gif';
var ntop_submenu_image_left = '';

// CLIP HERE
// If you want to move these functions to the HEAD
	function ntop_menu_link (idx) {

		if( ntop_browserType() == "other" )
			return;

		var l = ntop_lines[ idx ];

		if(l == undefined) {
			alert("Bad idx=" + idx + ", no line there.");
			return;
		}

		var mouseo = '';

		var out = '<tr><td id="' + l[0] + 'left"';
		if(l[ntop_MV_CHILDREN] > 0) {
			baseid = l[0];
			mouseo_beg = ' onMouseOver="ntop_mousein(this,';
			mouseo_beg += l[ntop_MV_LEVEL] + ',';
			mouseo_end = ')"';
			out += mouseo_beg + l[0] + mouseo_end;
		}

		out += '>';

		if(ntop_submenu_image_left && l[ntop_MV_CHILDREN] > 0) {
			if(ntop_submenu_image_left.substr(0,1) == '<')
				out += ntop_submenu_image_left;
			else
				out += '<img src="' + ntop_submenu_image_left + '" border="0">';
		}
		out += '</td><td><div';
		
		if(l[ntop_MV_CHILDREN] > 0) {
			out += ' id="' + l[0] + '"' + mouseo_beg + "''" + mouseo_end;
		}
		out += '>';
		var tstyle = ntop_link_style;
		var tclass = ntop_link_class;
		var ttarget = ntop_link_target;
		var tprepend = ntop_link_prepend;
		if(l[ntop_PAGE]) {
			out = out + '<a href="' + tprepend + l[ ntop_PAGE ] + '"';
			if(tclass)
				out = out + ' class="' + tclass + '"';
			if(tstyle)
				out = out + ' style="' + tstyle + '"';
			if(ttarget)
				out = out + ' target="' + ttarget + '"';
			if(l[ntop_DESCRIPTION])
				out = out + ' title="' + l[ ntop_DESCRIPTION ] + '"';
			out = out + '>';
			out = out + l[ ntop_NAME ] + '</a>';
		}
		else {
			out = out + l[ ntop_NAME ];
		}
	// alert("build idx=" + idx + " into: " + out);

		out += '</div></td><td id="' + l[0] + 'right"';

		if(l[ntop_MV_CHILDREN] > 0) {
			out += mouseo_beg + l[0] + mouseo_end;
		}

		out += '>';
		if(ntop_submenu_image_right && l[ntop_MV_CHILDREN] > 0) {
			if(ntop_submenu_image_right.substr(0,1) == '<')
				out += ntop_submenu_image_right;
			else
				out += '<img src="' + ntop_submenu_image_right + '" border="0">';
		}
		out += '</td></tr>';

		return out;
	}

	function ntop_mousein (obj,level,otherid) {
		if( ntop_browserType() == "other" )
			return;

		if(otherid != '' && otherid != undefined)
			obj = document.getElementById(otherid);

		if(level == undefined) 
			level = 0;
		level++;

		var divname = ntop_mydiv + level;

		var fod = document.getElementById( divname );
		if(fod == undefined) {
			return;
		}
		fod.style.display = 'none';
		clearTimeout( ntop_timeoutCode );
		ntop_timeoutCode = -1;

		var html = '<table cellpadding="0" cellspacing="0" border="0">';

		var idx = -1;
		var digid = obj.id;
		digid = digid.replace(/^ntop_/, '');
		for(var j = 0; j < ntop_lines.length; j++) {
			if(ntop_lines[j][0] == digid) {
				idx = j;
				break;
			}
		}

		if(idx < 0) 
			return;
	
		var l = ntop_lines[idx];
		var currentlevel = l[ntop_MV_LEVEL];
		if(currentlevel == undefined)
			currentlevel = 0;

		ntop_menuClear(currentlevel);
		if(l[ntop_MV_CHILDREN] < 1) 
			return;

		var x = ntop_getRightX( obj, currentlevel ) + 1;
		var y = ntop_getTopX( obj, currentlevel );
		var menu = fod.style;
		menu.left = x + "px";
		menu.top = y + "px";
		menu.display = 'block';

		var i;
		for( i = idx + 1; ; i++ )
		{
			var l = ntop_lines[i];
// alert("running link for level=" + l[ntop_MV_LEVEL] + ", line=" + l);
			if(l == undefined || l[ntop_MV_LEVEL] < level)
				break;
			if(l[ntop_MV_LEVEL] == level)
				html += ntop_menu_link(i);
		}
		html += '</table>';
		fod.innerHTML = html;
	}

	function ntop_getRightX( obj, level )
	{
		if( ntop_browserType() == "other" )
			return;
		var pos = 0;
		var n = 0;
		var x = obj.offsetParent;
		if(x == undefined) 
			x = obj;
		while(x.offsetParent != undefined) {
			n += x.offsetLeft;
			x = x.offsetParent;
		}
		pos = n + obj.offsetLeft;
		if(ntop_anchor_down != 1 || level > 0)
			pos += obj.offsetWidth;
		return pos;
	}

	function ntop_getTopX( obj, level )
	{
		if( ntop_browserType() == "other" )
			return;

		var pos = 0;
		var n = 0;
		var x = obj;
		while(x.offsetParent != undefined) {
			n += x.offsetParent.offsetTop;
			x = x.offsetParent;
		}
		pos = n + obj.offsetTop;
		if(ntop_anchor_down && level == 0)
			pos += obj.offsetHeight;
		return pos;
	}
	
	function ntop_mouseout( obj, level )
	{
		if( ntop_browserType() == "other" )
			return;

		if(level == undefined) 
			level = 0;
		level++;
		ntop_timeoutCode = setTimeout( "ntop_menuClear();", 1000 );
	}

	function ntop_menuClear(level)
	{
		if( ntop_browserType() == "other" )
			return;

		if (level == undefined)
			level = 0;
		level++;
		for( var i = level; i <= ntop_last_level; i++) {
			var thisdiv = ntop_mydiv + i;
			var fod = document.getElementById( thisdiv );
			if(fod != undefined)
				fod.style.display = 'none';
		}
		clearTimeout( ntop_timeoutCode );
		ntop_timeoutCode = -1;
	}

	function ntop_menuBusy()
	{
		if( ntop_browserType() == "other" )
			return;

		clearTimeout( ntop_timeoutCode );
		ntop_timeoutCode = -1;
	}

	var ntop_clientType = "unknown";

	function ntop_browserType()
	{
		if( ntop_clientType != "unknown"  )
			return ntop_clientType;
	
		ntop_clientType = "other";
		if (document.all) {
			if( document.getElementById )
		  		ntop_clientType = "ie";
		}
		else if (document.layers) {
		}
		else if (document.getElementById) {
			ntop_clientType = "ns6";
		}
		else
		{
		}

		return ntop_clientType;
	}

// END CLIP
</script>
