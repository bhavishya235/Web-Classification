			var suggested_location = "Point Hope, AK";
			var suggested_location_gacid = "a82149";
			var location_fixed = "false";
			function setup_location_suggestion() {
				if (suggested_location.length && suggested_location_gacid.length && location_fixed == 'false') {
					$('#edit-location').val(suggested_location);
					$('#edit-gacid').val(suggested_location_gacid);
				}
			}
			$(document).ready(function () {
				setup_location_suggestion();
				$("#edit-keyword")[0].focus();
				$("button#modal-form-submit").click(function (e) {
					var options = [];
					options['at'] = $("#alertToken").val();
					options['source'] = "recent_home";
					options['search_args_display'] = $("#alertStrArgs").val();
					options['verified'] = 1;
					timestampId = $("#alertTimestamp").val();
					$("#alertLastEmail").val($("#alertEmail").val());

					var callbacks = [];
					callbacks['pre'] = 'alertPreModal';
					callbacks['onSuccess'] = 'alertOnSuccessModal';
					callbacks['onError'] = 'alertOnErrorModal';
					jobalert_add($("#alertStrArgs").val(), $("#alertEmail").val(), options, callbacks);
				});
			});

			function build_alert_request(timestamp) {
				email = $("#alertLastEmail").val();
				output = '<p style="color:#777;margin-bottom:15px">Be the first to know when ' + recentSearches[timestamp].keyword + ' jobs in ' + recentSearches[timestamp].locationStr + ' are posted.</p>';
				output += '<form id="modal-form">My email: <input type="text" name="alertEmail" id="alertEmail" value="' + email + '"/>';
				output += '<input type="hidden" name="alertKeyword" id="alertKeyword"/>';
				output += '<input type="hidden" name="alertLocation" id="alertLocation"/>';
				output += '<input type="hidden" name="alertStrArgs" id="alertStrArgs"/>';
				output += '<input type="hidden" name="alertToken" id="alertToken"/>';
				output += '<input type="hidden" name="alertTimestamp" id="alertTimestamp"/></form>';
				$("#alertModalBody").html(output);
				$("#alertKeyword").val(recentSearches[timestamp].keyword);
				$("#alertLocation").val(recentSearches[timestamp].location);
				$("#alertStrArgs").val(recentSearches[timestamp].strArgs);
				$("#alertToken").val(recentSearches[timestamp].at);
				$("#alertTimestamp").val(timestamp);
				$("#alertEmail").keypress(function(event) {
					if ( event.which == 13 ) {
						event.preventDefault();
						$("button#modal-form-submit").click();
					}
				});
				$('#modal-form-submit').show();
			}
		</script>
