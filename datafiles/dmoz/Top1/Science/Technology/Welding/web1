<script type="text/javascript">
function ClearSearchBox() {
	YAHOO.util.Dom.get('searchquery').value = '';
}

function ClearFooterEmailBox() {
	YAHOO.util.Dom.get('mw-footer-newsletters-email').value = '';
}

YAHOO.util.Event.addListener('mw-footer-newsletters-email','focus', ClearFooterEmailBox);

YAHOO.util.Event.addListener('searchquery','focus', ClearSearchBox);
</script>

