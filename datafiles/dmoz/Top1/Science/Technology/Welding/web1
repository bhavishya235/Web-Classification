background: url('/site-files/weldingdesign.com/files/uploads/2013/04/richie-bros-equipment-one.png') no-repeat left top; }
.auctBlockCarouselStyle .auctSloganArea { background: #969494; color: #fff; font-family: Arial,"Helvetica Neue",Helvetica,sans-serif; text-align: center; text-transform: uppercase; margin: 0; padding: 2px 5px; font-size: .7857em; line-height: 15px; font-weight: bold; }
.auctBlockCarouselStyle .carouselV2 { width: 300px; position: relative; margin: 1em 0;}
.pm-cont-sidebar .auctBlockCarouselStyle .carouselV2 { width: 225px; }
.auctBlockCarouselStyle .carouselV2 .carouselSlides { overflow: hidden; width: 179px; margin: 0 auto; }
.auctBlockCarouselStyle .carouselV2 .carouselSlides ul { position: relative; *position:auto; left: 0; top: 0; list-style: none; margin: 0; padding: 0; width: 900px;}
.auctBlockCarouselStyle .carouselSlides li { width: 179px; color: #999; font-size: .8571em; float: left; }
.auctBlockCarouselStyle .carouselSlides .auctionList { width: 179px; overflow: hidden; }
.auctBlockCarouselStyle .carouselSlides .auctionList li a { display: block; clear: both; font-weight: bold; }
.auctBlockCarouselStyle .carouselSlides .auctionList li img { width: 177px; height: 132px; border: 1px solid #999; margin: 0 0 .5em; }
.auctBlockCarouselStyle a.carouselPrev, a.carouselNext { display: block; overflow: hidden; width: 14px; height: 0; padding-top:27px; outline: 0; }
.auctBlockCarouselStyle a.carouselPrev { background: url('/site-files/weldingdesign.com/files/uploads/2013/01/arrow_auction_prev.png') left top no-repeat; position: absolute; left:35px; top:30%; }
.auctBlockCarouselStyle a.carouselNext { background: url('/site-files/weldingdesign.com/files/uploads/2013/01/arrow_auction_next.png') left top no-repeat; position: absolute; right:34px; top: 30%; }
.pm-cont-sidebar .auctBlockCarouselStyle a.carouselPrev { left: 3px; }
.pm-cont-sidebar .auctBlockCarouselStyle a.carouselNext { right: 3px; }	</style>
<script type="text/javascript">
$(document).ready(function() {
	//rotation speed and timer
	var speed = 5000;
	var run = setInterval('rotate()', speed);
	//grab the width and calculate left value
	var item_width = $('.carouselSlides li').outerWidth();
	var left_value = item_width * (-1);
    //move the last item before first item, just in case user click prev button
	$('.carouselSlides li:first').before($('.carouselSlides li:last'));
	//set the default item to the correct position
	$('.carouselSlides ul').css({'left' : left_value});
    //if user clicked on prev button
	$('.carouselPrev').click(function() {
		//get the right position
		var left_indent = parseInt($('.carouselSlides ul').css('left')) + item_width;
		//slide the item
		$('.carouselSlides ul:not(:animated)').animate({'left' : left_indent}, 200,function(){
            //move the last item and put it as first item
			$('.carouselSlides li:first').before($('.carouselSlides li:last'));
			//set the default item to correct position
			$('.carouselSlides ul').css({'left' : left_value});
		});
		//cancel the link behavior
		return false;
	});
    //if user clicked on next button
	$('.carouselNext').click(function() {
		//get the right position
		var left_indent = parseInt($('.carouselSlides ul').css('left')) - item_width;
		//slide the item
		$('.carouselSlides ul:not(:animated)').animate({'left' : left_indent}, 200, function () {
            //move the first item and put it as last item
			$('.carouselSlides li:last').after($('.carouselSlides li:first'));
			//set the default item to correct position
			$('.carouselSlides ul').css({'left' : left_value});
		});
		//cancel the link behavior
		return false;
	});
	//if mouse hover, pause the auto rotation, otherwise rotate it
	$('.carouselSlides').hover(

		function() {
			clearInterval(run);
		},
		function() {
			run = setInterval('rotate()', speed);
		}
	);
});
//a simple function to click next link
//a timer will call this function, and the rotation will begin
function rotate() {
	var item_width = $('.carouselSlides li').outerWidth();
	var left_value = item_width * (-1);
	var left_indent = parseInt($('.carouselSlides ul').css('left')) - item_width;
	//slide the item
	$('.carouselSlides ul:not(:animated)').animate({'left' : left_indent}, 200, function () {
		//move the first item and put it as last item
		$('.carouselSlides li:last').after($('.carouselSlides li:first'));
		//set the default item to correct position
		$('.carouselSlides ul').css({'left' : left_value});
	});
}
</script></p>
<p>
	&nbsp;</p>
