                    <script language="javascript">
          function ShowHorizontalHomeMenu(displayObj,offsetWidth,offsetTop) {

          CloseHomeMenu();

          document.getElementById('' + displayObj + '').style.display = "block";
          document.getElementById('' + displayObj + '').style.marginLeft = offsetWidth + "px";
          }

          function CloseHorizontalHomeMenu(displayObj) {
          document.getElementById('' + displayObj + '').style.display = "none";

          }

          function ShowVerticalHomeMenu(displayObj) {
          document.getElementById('' + displayObj + '').style.display = "block";
          document.getElementById('' + displayObj + '').parentNode.parentNode.style.display = "block"

          document.getElementById('' + displayObj + '').style.left = document.getElementById('' + displayObj + '').parentNode.parentNode.offsetWidth + "px";
          document.getElementById('' + displayObj + '').style.top = document.getElementById('' + displayObj + '').parentNode.offsetTop + "px";
          }

          function CloseVerticalHomeMenu(displayObj) {
          document.getElementById('' + displayObj + '').style.display = "none";
          }

          function CloseHomeMenu() {
          document.getElementById("item_homemenuforcontent").style.display = "none";
          document.getElementById("item_homemenuwhy").style.display = "none";
          document.getElementById("item_homemenucourses").style.display = "none";
          document.getElementById("item_homecertication").style.display = "none";


          document.getElementById("item_homemilitary").style.display = "none";
          document.getElementById("item_homegetjob").style.display = "none";
          document.getElementById("item_homebookstore").style.display = "none";
          document.getElementById("item_homemenuDownLoads").style.display = "none";
          document.getElementById("item_homegeneralinfo").style.display = "none";

          }
          document.onclick = CloseHomeMenu;

        </script><div class="text_GetStarted"><a href="#" onmouseover="ShowHorizontalHomeMenu('item_homemenuforcontent',130)"></a></div>
