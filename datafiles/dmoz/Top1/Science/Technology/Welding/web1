<!-- / views-view.tpl.php--></div></div><div class="homepage-features-block"><div class="features-block-col1"><h5 class="block-title">Features</h5>
<!--views-view.tpl.php-->
<div class="view view-landing-page-content view view-landing-page-content view-id-landing_page_content view-display-id-block view-dom-id-10"> 
  <!-- views-view-unformatted.tpl.php -->
  <div class="views-row views-row-1 views-row-odd views-row-first"><div id="node-8381" class="node node-article node-article-teaser node-teaser clearfix clearfix">
            <div class="thumbnail">
        <a href="/equipment-amp-automation/10-tips-semi-automatic-mig-savings"><img alt="" src="//weldingdesign.com/site-files/weldingdesign.com/files/imagecache/thumb_img/uploads/2013/12/wd10moneysavingstips-bernard.jpg" title="" height="101" width="180" class="imagecache imagecache-thumb_img imagecache-default imagecache-thumb_img_default" /></a>      </div>
        <div class="teaser-content">
      <div class="teaser-meta">
        <div class="created">Dec. 24, 2013</div>
        <div class="type">Article</div>
                <div class="source-name"></div>
