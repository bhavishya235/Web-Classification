              <!-- ********* START SPOTLIGHT ********* -->
 
<div class="spotlight">
 
    <script type="text/javascript">
		$(document).ready(function() {
 
			var currentPosition = 0;
			var slideWidth = $( "div.slide" ).first().width();
			$('div.slide').css('width', slideWidth );
			var slides = $('.slide');
			var numberOfSlides = slides.length;
			var slideShowInterval;
			var speed = 8000;
			//Assign a timer, so it will run periodically
			slideShowInterval = setInterval(changePosition, speed);
			slides.wrapAll('<div id="slidesHolder"></div>')
			slides.css({ 'float' : 'left' });
 
			$('#slidesHolder').css('width', slideWidth * numberOfSlides);
 
			$('.nav').bind('click', function() {
				//determine new position
				currentPosition  = $(this).attr('data-slide');
				newPostion(currentPosition);
			});
 
			function newPostion(currentPosition){
				clearInterval(slideShowInterval);
				slideShowInterval = setInterval(changePosition, speed);
				$('#slidesHolder').animate({'marginLeft' : slideWidth*(-currentPosition)}, 1000);
			}
 
			function changePosition() {
				if(currentPosition == numberOfSlides - 1) {
					// currentPosition = 0;
					// moveSlide();
				} else {
					currentPosition++;
					$('#slidesHolder').animate({'marginLeft' : slideWidth*(-currentPosition)}, 1000);
				}
			}
		
			$(window).resize(function(){
				currentPosition = 0;
				slideWidth = $("div#slideshowWindow").width();
				$('div.slide').css('width', slideWidth );
				$('#slidesHolder').css('width', slideWidth * numberOfSlides);
				moveSlide();
			})
 
		});
 
 
	</script>
 
	<div id="slideshow">
	     <div id="slideshowWindow">
	     	<!-- START LOOP -->
			<div class="slide">
				<div class="text blue">
					<div class="strap">Spotlight</div>
					<h2><a href="/en/chemistry/study-chemistry/undergraduate/">Undergraduate Studies</a></h2>
					<p class="summary">Are you considering a degree in chemistry?</p>
					<p class="more"><a href="/en/chemistry/study-chemistry/undergraduate/">Read more</a><p>
				</div><!--/text-->
				<div class="slideshow_view">
                                  <img src="/en/media/academic/chemistry/images/spotlight_undergrads2.jpg" alt="Undergraduate Studies" />
				</div>
				<ol class="sliderNav">
					<!-- START LOOP -->
					<li class="nav sel" data-slide="0"><a href="#">1</a></li>
                                  	<li class="nav" data-slide="1"><a href="#">2</a></li>
                                  	<li class="nav" data-slide="2"><a href="#">3</a></li>
					<!-- END LOOP -->
				</ol>
			</div><!--/slide-->
                        <div class="slide">
				<div class="text blue">
					<div class="strap">Spotlight</div>
