var count = 0;
function ChangeImage(){
count = count + 1;
if (count == 20) { count = 1; }
blendimage('p_lt_zoneTopBanner_RotatingBannerControl_pnlRotatingAdvert', 'p_lt_zoneTopBanner_RotatingBannerControl_imgAdvert', _images[count].src,1000);
var t = setTimeout("ChangeImage()", 6000);
t = null;
}
_images = new Array(19);
pic1 = new Image(430,150);
