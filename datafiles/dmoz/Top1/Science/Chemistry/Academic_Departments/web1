    <script type='text/javascript'>
    var pkBaseURL = (('https:' == document.location.protocol) ? 'https://webtrack.uni-marburg.de/' : 'http://webtrack.uni-marburg.de/');
    document.write(unescape('%3Cscript src="' + pkBaseURL + 'piwik.js" type="text/javascript"%3E%3C/script%3E'));
    </script>
    <script type='text/javascript'>
      try {
        var piwikTracker;
    
        piwikTracker = Piwik.getTracker(pkBaseURL + 'piwik.php', 15);
        piwikTracker.setVisitorCookieTimeout('604800');
        piwikTracker.trackPageView();
        piwikTracker.enableLinkTracking();
    
    
        piwikTracker = Piwik.getTracker(pkBaseURL + 'piwik.php', 1);
        piwikTracker.setVisitorCookieTimeout('604800');
        piwikTracker.trackPageView();
        piwikTracker.enableLinkTracking();
    
      } catch( err ) {}
    </script>
