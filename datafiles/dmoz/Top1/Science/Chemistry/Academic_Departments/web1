		<script type="text/javascript">
			q=window.location.search;
			if(q!=""&&q!="/") {
				qmark=q.indexOf('?');
				amp=q.indexOf('&');
				if(amp==-1) {
					// q=q.substr(qmark+3);
					Q=q.substr(qmark+3);
					T = "website"
				} else {
					Q=q.substr(qmark+3,amp-3);
					t=q.substr(amp+1);
					eq=t.indexOf('=');
					T=t.substr(eq+1);
				}		
				Q=Q.replace('+',' ');
				document.getElementById('gsc-i-id1').value=Q;
				for(i=0; i<document.uccsearchform.searchFilter.length; i++) {
					if (document.uccsearchform.searchFilter[i].checked) {
						document.uccsearchform.searchFilter[i].checked='';
					}
					if(document.uccsearchform.searchFilter[i].value==T) {
						document.uccsearchform.searchFilter[i].checked='checked';
					}
				}
			}
		</script>
	</form>
