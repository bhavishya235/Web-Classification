<script>function cambiar(j)
			{
				if(j==4)
					j=0;
				
				if(titulo[j]!='undefined' && titulo[j]!="")
				 {
					
				document.getElementById("image").innerHTML=imag[j];
				document.getElementById("titulo").innerHTML=titulo[j];
				document.getElementById("descripcion").innerHTML=descripcion[j];
				 }
				 else
				 {
				 	j=j+1;
				    cambiar(j);
				 }	
			
				j=j+1;
				var h=setTimeout("cambiar("+j+")", 5000);
			}
			
var t=setTimeout("cambiar(1)", 5000);

			
			
			
</script>
