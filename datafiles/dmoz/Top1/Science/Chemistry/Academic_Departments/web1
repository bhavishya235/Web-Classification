		<dt><span class="au_news_events_weekday">Thu</span> <span class="au_news_events_date">22</span> <span class="au_news_events_month">May</span></dt>
		<dd><span class="au_news_events_time">15:00-16:00</span> | <span class="au_news_events_place">1514-213 Auditorium I, Institut for Kemi, Langelandsgade 140, 8000 Aarhus C </span><br />
			<a href="http://chem.au.dk/en/research/dissemination-of-knowledge/chemistry-seminar-lectures-2014-2015/" target="_top" title="">Chemistry Seminar Lectures: Prof. Amitabha Chattopadhyay (Host: Birgit Schiøtt og Thomas Vosegaard)</a><br /></dd>
	
		<dt><span class="au_news_events_weekday">Thu</span> <span class="au_news_events_date">20</span> <span class="au_news_events_month">Nov</span></dt>
		<dd><span class="au_news_events_time">15:15-16:00</span> | <span class="au_news_events_place">1514-213 Auditorium I, Department of Chemistry, Langelandsgade 140, 8000 Aarhus C </span><br />
			<a href="http://chem.au.dk/en/research/dissemination-of-knowledge/chemistry-seminar-lectures-2014-2015/" target="_top" title="">Chemistry Seminar Lectures: Prof. Dr. Christian Hertweck, Leibniz-Institute for Natural Product Research and Infection Biology (HKI) (Host: Thomas B. Poulsen)</a><br /></dd>
	  end -->
</div></div>									<div class="column last"><a id="c244992"></a><div id="c244991" class="csc-default"><div class="csc-header csc-header-n1"><h1 class="csc-firstHeader">Shortcuts</h1></div></div><a id="c244994"></a><div id="c244993" class="csc-default"><p class="au_button" style="width: 156px; background-color: #009ee0;">
