  <!-- Image-Portlet-Box -->

  
    <script type='text/javascript'>;
      jq.preloadImages = function() {
          var a = (typeof arguments[0] == 'object')? arguments[0] : arguments;
          for(var i = a.length -1; i >= 0; i--) {
            jq("<img>").attr("src", a[i]);
          }
      };

      jq.preloadImages(['http://www.uni-marburg.de/fb15/portletimage.2010-11-10.1502264725/image_preview']);

      jq(document).ready(function () { 
        var delay = 8000;
        var random = true;
        var sum = '1';
        var start = 0;
        if (random) start = Math.floor(Math.random()*(sum));

        function isInteger(s) {
          return (s.toString().search(/^-?[0-9]+$/) == 0);
        }
        var maxHeight = 0;
        
          
           

            var img = new Image(); img.src = 'http://www.uni-marburg.de/fb15/portletimage.2010-11-10.1502264725/image_preview';
            var cssWidth = jq("#imageportlet_content").css("width");
            cssWidth = parseInt(cssWidth.substring(0, cssWidth.indexOf("px")));

            var link = 'http://www.uni-marburg.de/fb15/aktuelles/news/2010/alias.2010-10-28.1937566620';
            var index = '0';
            var idname= 'portlet_image_'+index;
            var description = 'Der Fachbereich gehört zur Spitzengruppe im CHE ExcellenceRanking';
            var height_o = 110;
            var width_o = 110;
            
            jq("#imageportlet_content").append("<div id="+idname+" />");
            jq('#'+idname).append(img);
            if(description.length > 0) {
                jq('#'+idname).append('<p>'+description+'</p>');
            }

            if(!isInteger(cssWidth))
                cssWidth = 168;

            if (width_o <= cssWidth){
                if ( width_o >= 2/3 * cssWidth )
                    jq("#imageportlet_content").width(width_o);
                else
                    jq("#imageportlet_content").width(parseInt(2/3 * cssWidth));

                cssWidth = width_o;
            }

            jq('#'+idname+' img').width(cssWidth);

            var newHeight = height_o;
            if (width_o > cssWidth)
              newHeight = parseInt((cssWidth / width_o) * height_o);
            
            var link_url = "<a href=".concat(link);
            link_url = link_url.concat(" />");

            jq('#'+idname+' img').wrap(link_url);
            if(description.length > 0){
                jq('#'+idname+' p').wrap(link_url);
            }

            totalHeight = newHeight + jq('#'+idname+' p').height() + 20;
            if(totalHeight > maxHeight) {
                maxHeight = totalHeight;
            }

            jq('#'+idname+' img').height(newHeight);
            jq('#'+idname).height(totalHeight);
            jq("#imageportlet_content").height(jq('#'+idname).height());

            var cssObj = { 'position':'absolute','top':'0px' };
            jq('#'+idname).css(cssObj);
            if (index == start)
              jq('#'+idname).show();
            else
              jq('#'+idname).hide();
               
           
          
        

        jq("#imageportlet_content").height(maxHeight);

        function fade(position){
          jq('#portlet_image_'+position).fadeOut("fast");
          jq('#portlet_image_'+position+' p').fadeOut("fast");
          position = (position+1) % sum;
          jq('#portlet_image_'+position).fadeIn("slow");
          jq('#portlet_image_'+position+' p').fadeIn("slow");
          //jq("#imageportlet_content").height(jq('#portlet_image_'+position).height());
          jq("#imageportlet_content").width(jq('#portlet_image_'+position + ' img').first().width());
          setTimeout( function(){ fade(position) }, delay);
        }
        
        if (sum > 1){
            setTimeout( function(){ fade(start) }, delay);
        }
      });

    </script>
  

  <div id="portlet-imageportlet">
