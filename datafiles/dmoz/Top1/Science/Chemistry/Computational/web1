	  var serverurl = 'http://www.schrodinger.com';

			function ShowPopup() {
				/*IEfix();*/
				document.getElementById('popup').style.display = 'block';
				document.getElementById('shadow').style.display = 'block';
				/*var ua = navigator.userAgent.toLowerCase();
				if (ua.indexOf("safari") != -1) {
					if (document.getElementById('popup').offsetHeight+211+eval(window.pageYOffset) > document.getElementById('main').offsetHeight) {
					document.getElementById('shadow').style.height = document.getElementById('popup').offsetHeight+eval(window.pageYOffset)+260+'px';
					}
				} else
					if (document.getElementById('popup').offsetHeight+211+eval(document.documentElement.scrollTop) > document.getElementById('main').offsetHeight) {
					document.getElementById('shadow').style.height = document.getElementById('popup').offsetHeight+eval(document.documentElement.scrollTop)+260+'px';
				}*/
				if(document.getElementById('pbox_center_center')!=null)text_width=document.getElementById('pbox_center_center').offsetWidth;
				if(document.getElementById('pbox_text')!=null)document.getElementById('pbox_text').style.width = (text_width >= 333) ? text_width -70 + 'px' : '263px';
				var docRef = document;
				var docElRef = docRef.documentElement;
				var bodyRef = docRef.body;
				docRef.getElementById('shadow').style.height = (bodyRef.scrollHeight < docElRef.scrollHeight) ? docElRef.scrollHeight + 'px' : bodyRef.scrollHeight + 'px';

			}

function show_seminar(id)
{
			var request = $.ajax({
				type: 'POST',
				url: 'http://www.schrodinger.com/index.php',
				data: {mode: 'showseminar', id: id}
			});

			request.done(function(transport) {
				if(transport) {
				   $('#popupcontentblock').html(transport);
				   ShowPopup();
				   var ua = navigator.userAgent.toLowerCase();
				   if (ua.indexOf("safari") != -1) { document.getElementById('popup').style.top = eval(window.pageYOffset) + 211 + 'px'; } else
				   { document.getElementById('popup').style.top = eval(document.documentElement.scrollTop) + 211 + 'px';}
				} else {
					show_error_msg(1);
				}
			});

			request.fail(function(jqXHR, textStatus) {
			  show_error_msg(1);
			});
}


			function HidePopup() {
				document.getElementById('shadow').style.display = 'none';
				document.getElementById('popup').style.display = 'none';
			}
