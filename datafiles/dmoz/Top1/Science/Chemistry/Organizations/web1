	mapping.push({scVar:"eVar1",cqVar:"pagedata.omniSubSection1",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"channel",cqVar:"pagedata.omniChannel",resourceType:"acs/components/pages/contentpage"});
    for (var i=0; i<mapping.length; i++){
		var m = mapping[i];
		if (!options || (options.componentPath == m.resourceType)) {
			CQ_Analytics.Sitecatalyst.setEvar(m.scVar,m.cqVar);
		}
    }
}

CQ_Analytics.CCM.addListener("storesinitialize", function(e) {
    var lte = s.linkTrackEvents;
