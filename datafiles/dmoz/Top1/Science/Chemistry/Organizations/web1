<script type="text/javascript" src="/etc/clientlibs/foundation/sitecatalyst/plugins.js"></script>

<script type="text/javascript">
<!--
/* You may give each page an identifying name, server, and channel on
the next lines. */

CQ_Analytics.Sitecatalyst.frameworkComponents = ['foundation/components/page','acs/components/pages/contentpage'];
/**
 * Sets SiteCatalyst variables accordingly to mapped components. If <code>options</code> 
 * object is provided only variables matching the options.componentPath are set.
 *
 * @param {Object} options Parameter object from CQ_Analytics.record() call. Optional.
 */
CQ_Analytics.Sitecatalyst.updateEvars = function(options) {
    var mapping = [];
	mapping.push({scVar:"eVar5",cqVar:"pagedata.title",resourceType:"foundation/components/page"});
	mapping.push({scVar:"prop41",cqVar:"pagedata.path",resourceType:"foundation/components/page"});
	mapping.push({scVar:"prop5",cqVar:"pagedata.title",resourceType:"foundation/components/page"});
	mapping.push({scVar:"pageName",cqVar:"pagedata.omniPageName",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"eVar2",cqVar:"pagedata.omniSubSection2",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"eVar3",cqVar:"pagedata.omniSubSection3",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"prop2",cqVar:"pagedata.omniSubSection2",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"eVar26",cqVar:"pagedata.omniDomain",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"prop1",cqVar:"pagedata.omniSubSection1",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"eVar27",cqVar:"pagedata.omniChannel",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"eVar11",cqVar:"pagedata.omniPageName",resourceType:"acs/components/pages/contentpage"});
	mapping.push({scVar:"eVar6",cqVar:"pagedata.omniContentId",resourceType:"acs/components/pages/contentpage"});
