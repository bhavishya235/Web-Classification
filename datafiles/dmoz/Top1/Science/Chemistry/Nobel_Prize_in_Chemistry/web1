  <script src="/prog/js/foundation/foundation.js"></script>
  
  <script src="/prog/js/foundation/foundation.alerts.js"></script>
  
  <script src="/prog/js/foundation/foundation.clearing.js"></script>
  
  <script src="/prog/js/foundation/foundation.cookie.js"></script>
  
  <script src="/prog/js/foundation/foundation.dropdown.js"></script>
  
