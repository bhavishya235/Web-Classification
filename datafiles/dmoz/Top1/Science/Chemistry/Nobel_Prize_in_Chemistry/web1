 <!--googleon: index--><div class="storytitle">
      <h1>'Obsessive Genius,' The Life of Marie Curie</h1>
   <input type="hidden" id="title4212857" value="'Obsessive Genius,' The Life of Marie Curie"></input>
   <input type="hidden" id="modelShortUrl4212857" value="http://www.npr.org/templates/story/story.php?storyId=4212857"></input>
   <input type="hidden" id="modelFullUrl4212857" value="http://www.npr.org/templates/story/story.php?storyId=4212857"></input>
</div>

<!-- END CLASS="STORYTITLE" -->
<div id="story-meta">
      <div class="dateblock">
            <time datetime="2004-12-10"><span class="date">December 10, 2004</span><span class="time">12:00 AM</span></time>
   </div>
</div>

<!-- END ID="STORY-META" -->
