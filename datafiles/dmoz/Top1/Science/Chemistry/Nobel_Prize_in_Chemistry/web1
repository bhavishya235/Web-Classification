<div class="row">
    <div class="large-12 columns">
        <div id="basic-create-list">
            <input type="hidden" id="hidden-the-year" value="1954">
            <input type="hidden" id="hidden-the-prize" value="c">
            <select id="goto-prize-change-prize" style="font-size: 0.813em; width: 61%; display: inline; margin-right: 3%;">
