  <script>
  	var left_height, middle_height, right_height, highest;
	
    $(document).foundation();
	
	var footer_visible = false;
	var filter_year = 2013;
	
	create_list_form_toggled = false;
	create_list_form_visible = false;

function toggle_create_list_form() {
	
	if(create_list_form_visible) {
		
		$('#create-list-container-yo').css('display', 'none');
		create_list_form_visible = false;
	}
	else {
		
		$('#create-list-container-yo').css('display', 'inline');
		create_list_form_visible = true;
	}
	
	if(!create_list_form_toggled) {
		
		//var left_height = $('#nobel-left-col').height();
		var left_height = $('#create-list-container-yo').height();
		left_height = Math.round(left_height);
		left_height += 200;
		var middle_height = $('#nobel-middle-col').height();
		middle_height = Math.round(middle_height);
		var right_height = $('#nobel-right-col').height();
		right_height = Math.round(right_height);
			
		// Calculate the height of the containers and make it look right
		highest = left_height;
		
		if(middle_height > highest) {
			highest = middle_height;
		}
		
		if(right_height > highest) {
			highest = right_height;
		}
		
		var highest_mid = highest+20;
		$('#nobel-left-col').height(highest);
		$('#nobel-middle-col').height(highest_mid);
		$('#nobel-right-col').height(highest);
		
		create_list_form_toggled = false;
	}
}

$('document').ready(function() {
	
	//checkVersion();
	
	
	setTimeout(function() {
		
		var left_height = $('#nobel-left-col').height();
		var left_height = Math.round(left_height);
		var middle_height = $('#nobel-middle-col').height();
		var middle_height = Math.round(middle_height);
		var right_height = $('#nobel-right-col').height();
		var right_height = Math.round(right_height);
		
		if($(window).width() > 780) {
			
			// Calculate the height of the containers and make it look right
			highest = left_height;
			
			if(middle_height > highest) {
				highest = middle_height;
			}
			
			if(right_height > highest) {
				highest = right_height;
			}
			
			var highest_mid = highest+20;
			$('#nobel-left-col').height(highest);
			$('#nobel-middle-col').height(highest_mid);
			$('#nobel-right-col').height(highest);
		}
	}, 1000);
	
	
	/* Left sidebar filter */
	$('#left-arrow').click(function() {
		
		if(filter_year > 1901) {
			filter_year--;
			$('#year-input').val(filter_year);
		}
	});
	
	$('#right-arrow').click(function() {
		
		if(filter_year < 2013) {
			filter_year++;
			$('#year-input').val(filter_year);
		}
	});
	
	
	$('.footer-top-link').click(function() {
		
		if(!footer_visible) {
			show_footer();
		}
		else {
			hide_footer();
		}
	});
	
	$('.footer-top-link').click(function(event) {
		event.preventDefault();
	});
	
	// Left column filter dropdown
	$('#left-col-filter-dropdown').change(function() {
	});
	
	/* Goto other biography up-and-down-year */
	var selected_goto_year = 2013;
	selected_goto_year = $('#hidden-the-year').val();
	var url;
	var selected_goto_prize;
	
	// Dropdown
	$('#goto-prize-change-prize').change(function() {
		selected_goto_prize = $(this).find(":selected").val();
		
		if(selected_goto_prize == "economic-sciences" && selected_goto_year<1969) {
			url = 'http://www.nobelprize.org/nobel_prizes/economic-sciences/laureates/1969/';
		}
		else {
			url = 'http://www.nobelprize.org';
			url += '/nobel_prizes/'+selected_goto_prize+'/laureates/'+selected_goto_year+'/';
		}
			
		location.href = url;
	});
	
	// Button tap down
	$('#goto-prize-tap-down').click(function() {
		if(parseInt(selected_goto_year) > 1901) {
			selected_goto_prize = $('#goto-prize-change-prize').find(":selected").val();
			selected_goto_year--;
			
			if(selected_goto_prize == "economic-sciences" && selected_goto_year<1969) {
				url = 'http://www.nobelprize.org/nobel_prizes/economic-sciences/laureates/1969/';
			}
			else {
				url = 'http://www.nobelprize.org';
				url += '/nobel_prizes/'+selected_goto_prize+'/laureates/'+selected_goto_year+'/';
			}
			
			location.href = url;
		}
	});
	
	// Button tap up
	$('#goto-prize-tap-up').click(function() {
		if(parseInt(selected_goto_year) < 2013) {
			selected_goto_prize = $('#goto-prize-change-prize').find(":selected").val();
			selected_goto_year++;
			
			if(selected_goto_prize == "economic-sciences" && selected_goto_year<1969) {
				url = 'http://www.nobelprize.org/nobel_prizes/economic-sciences/laureates/1969/';
			}
			else {
				//url = 'http://www.nobelprize.org';
				url = 'http://www.nobelprize.org';
				url += '/nobel_prizes/'+selected_goto_prize+'/laureates/'+selected_goto_year+'/';
			}
			
			location.href = url;
		}
	});
	
	$('#goto-prize-change-year').keyup(function() {
		
		if($(this).val().length == 4) {
			selected_goto_year = $('#goto-prize-change-year').val();
			selected_goto_prize = $('#goto-prize-change-prize').find(":selected").val();
			
			if(selected_goto_prize == "economic-sciences" && selected_goto_year<1969) {
				url = 'http://'+www.nobelprize.org+'/nobel_prizes/economic-sciences/laureates/1969/';
			}
			else {
				url = 'http://www.nobelprize.org';
				url += '/nobel_prizes/'+selected_goto_prize+'/laureates/'+selected_goto_year+'/';
			}
			
			location.href = url;
		}
	});
});


// Show the footer
function show_footer() {
	
	$('#footer-container').animate({
		bottom: '0'
	}, 500, function() {
	});
	
	//$('#footer-top-link-first').css('background-image', 'url(/images/foundation/arrow-down.png)');
	$('#big-footer-link').attr('src', '/images/foundation/arrow-down.png');
	
	footer_visible = true;
}

// Hide the footer
function hide_footer() {
	
	$('#footer-container').animate({
		bottom: '-215px'
	}, 500, function() {
	});
	
	//$('#footer-top-link-first').css('background-image', 'url(/images/foundation/arrow-right.png)');
	$('#big-footer-link').attr('src', '/images/foundation/arrow-right.png');
	
	footer_visible = false;
}


// POLL
poll_stats = [];
poll_stats_percent = [];
alt_ids = [];
total_votes = 0;
num_alts = 0;
$(document).ready(function() {
	
	$(".poll-vote-button").click(function(event){
		
		event.preventDefault();
		//# Get the poll ID and selected alternative ID
		var poll_id = $(this).parent().attr('data-poll');
		var stapel_color = $(this).parent().attr('data-stapel-color');
		
		var selected_alt_id = 0;
		
		//# Get the selected alternative ID
		var alt_counter = 0;
		$('.poll-'+poll_id+'-alt').each(function() {
			
			alt_ids[alt_counter] = $(this).attr('data-alt-id');
			
			if($(this).is(':checked')) {
				
				selected_alt_id = $(this).attr('data-alt-id');
			}
			
			
			alt_counter++;
			num_alts++;
		});
		
		$.ajax({
			type: "POST",
			url: "http://www.nobelprize.org/prog/php/poll/api.php",
			data: {poll_id: poll_id,
					alt_id: selected_alt_id},
			dataType: "json",
			beforeSend:function(){
				//alert("sending");
			},
			error: function(jqXHR, textStatus, errorThrown){
				//alert("Error: "+errorThrown);
			},
			success: function(data){
				
				$.each(data, function(index, element) {
					
					//self.questions[i]['id'] = element.question_id;
					poll_stats[element.id] = [];
					poll_stats[element.id]['votes'] = parseInt(element.votes);
					total_votes += parseInt(element.votes);
				});

				//# Calculate percent % stats
				var percent_per_vote = 100/total_votes;
				
				var tot_percent = 0;
				
				for(var per_counter = 0; per_counter < num_alts; per_counter++) {
					
					var curr_alt_id = alt_ids[per_counter];
					poll_stats[curr_alt_id]['percent'] = parseInt(poll_stats[curr_alt_id]['votes'] * percent_per_vote);
					
					tot_percent += poll_stats[curr_alt_id]['percent'];
				}
				
				//# Show results to the user
				$('#poll-button-space-'+poll_id).html('<p style="padding-left: 5px;">Number of votes: '+total_votes+'</p>');
				$('#poll-button-space-'+poll_id).css('padding-left', '10px');
				$('#poll-button-space-'+poll_id).css('padding-right', '10px');
				
				//# Loop through the alt ids and show the percentage stats for it
				if($('#poll-style-'+poll_id).val() == '25') {
					
					for(var insta = 0; insta < num_alts; insta++) {
	
						var curraltid = alt_ids[insta];
						var curraltpercent = poll_stats[curraltid]['percent'];
						
						if(curraltpercent >= 15) {
						
							$('#poll-alt-container-'+curraltid).html('<div style="width: 95%; margin: 0 auto;"><p style="margin: 0;">'+$('#poll-alt-text-'+curraltid).val()+'</p></div>');
							$('#poll-alt-container-'+curraltid).append('<div style="margin: 0 auto; width: 95%; background-color: '+stapel_color+';"><div style="width: '+curraltpercent+'%; background-color: #94bfa1;"><p style="padding-top: 3px; padding-bottom: 3px; color: #ffffff; margin: 0 0 0 5px;">'+curraltpercent+'%</p></div></div>');
						}
						else {
							
							$('#poll-alt-container-'+curraltid).html('<div style="width: 95%; margin: 0 auto;"><p style="margin: 0;">'+$('#poll-alt-text-'+curraltid).val()+'</p></div>');
							$('#poll-alt-container-'+curraltid).append('<div style="margin: 0 auto; width: 95%; background-color: '+stapel_color+';"><div style="width: '+curraltpercent+'%; background-color: #94bfa1;"><p style="padding-top: 3px; padding-bottom: 3px; color: #333333; margin: 0 0 0 5px;">'+curraltpercent+'%</p></div></div>');
						}
					}
				}
				
				if($('#poll-style-'+poll_id).val() == '50') {
					
					for(var insta = 0; insta < num_alts; insta++) {
	
						var curraltid = alt_ids[insta];
						var curraltpercent = poll_stats[curraltid]['percent'];
						
						if(curraltpercent >= 15) {
							
							$('#poll-alt-container-'+curraltid).html('<div style="margin: 0 auto; margin-bottom: 10px; width: 95%;"><div style="border-radius: 3px; display: inline-block; width: 50%; background-color: '+stapel_color+';"><div style="border-top-left-radius: 3px; border-bottom-left-radius: 3px; width: '+curraltpercent+'%; background-color: #94bfa1;"><p style="padding-left: 5px; padding-top: 3px; padding-bottom: 3px; color: #ffffff; margin: 0 0 0 5px;">'+curraltpercent+'%</p></div></div><div style="display: inline-block; width: 50%;"><p style="margin: 0 0 0 10px;">'+$('#poll-alt-text-'+curraltid).val()+'</p></div></div>');
						}
						else {
						
							$('#poll-alt-container-'+curraltid).html('<div style="margin: 0 auto; margin-bottom: 10px; width: 95%;"><div style="border-radius: 3px; display: inline-block; width: 50%; background-color: '+stapel_color+';"><div style="border-top-left-radius: 3px; border-bottom-left-radius: 3px; width: '+curraltpercent+'%; background-color: #94bfa1;"><p style="padding-left: 5px; padding-top: 3px; padding-bottom: 3px; color: #333333; margin: 0 0 0 5px;">'+curraltpercent+'%</p></div></div><div style="display: inline-block; width: 50%;"><p style="margin: 0 0 0 10px;">'+$('#poll-alt-text-'+curraltid).val()+'</p></div></div>');
						}
					}
				}
			}
		});
	});
});



  </script>


</body></html>

