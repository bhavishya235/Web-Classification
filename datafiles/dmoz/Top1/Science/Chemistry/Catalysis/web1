<script type="text/javascript" language="javascript">

function loadpage()
{
	aboutclick();
	setTimeout("runSlideShow()",3000);
	setTimeout("runSlideShow2()",6000);
	setTimeout("runSlideShow3()",9000);
	setTimeout("runSlideShow4()",3000);
}

var mapVar = 0;
function grandFunctionRailroad()
{
	mapVar = 1;
	loadarrow('content/generalinformationpage.htm');
	hp = document.getElementById("marqueecontainer");
}

function loadmainnav(tag)
{
	hp = document.getElementById("marqueecontainer");
	bc = document.getElementById("brandingcontainer");
	bc.style.visibility = "Visible";
	cleararrows();
	checkbutton(tag);
	
	return dw_loadExternal('content/home.htm');
}

function loadawardsnav(tag)
{
	hp = document.getElementById("marqueecontainer");
	bc = document.getElementById("brandingcontainer");
	bc.style.visibility = "Visible";
	cleararrows();
	checkbutton(tag);
	
	return dw_loadExternal('content/awardspage.htm');
}

function loadmap()
{
	mapVar = 0;
	cleararrows();
	HidePopup();
	dw_loadExternal('content/googlemap.htm');
	HidePopup();
	setTimeout('initialize()', 1500);
	HidePopup();
}

function initialize()
{
  if (GBrowserIsCompatible())
  {
    var map = new GMap2(document.getElementById("map_canvas"));
	map.setCenter(new GLatLng(36.6001,  -121.89505), 15);
	var mapTypeControl = new GMapTypeControl();
	var topRight = new GControlPosition(G_ANCHOR_TOP_RIGHT, new GSize(10,10));
	var bottomRight = new GControlPosition(G_ANCHOR_TOP_RIGHT, new GSize(10,10));
	var point = new GLatLng(36.6001,  -121.89505 );
	map.addOverlay(new GMarker(point));
	map.addControl(new GMapTypeControl(), topRight);
	map.addControl(new GSmallMapControl());
	map.openInfoWindow(map.getCenter(), document.createTextNode("350 Calle Principal   Monterey, CA 93940"));
  }
}

function loadconferencenav(tag)
{
	mapVar = 0;
	cleararrows();
	checkbutton(tag);
	hp = document.getElementById("marqueecontainer");
	hp.style.display = "none";
	bc = document.getElementById("brandingcontainer");
	bc.style.visibility = "hidden";
	return dw_loadExternal('content/conferenceopeningpage.htm');
}

function loadhomenav(tag)
{
	mapVar = 0;
	cleararrows();
	checkbutton(tag);
	hp = document.getElementById("marqueecontainer");
	hp.style.display = "none";
	return dw_loadExternal('content/home.htm');
}

function loadarrow(element)
{
	mapVar = 1;
	hp = document.getElementById("marqueecontainer");
	cleararrows();
	showarrow(element);
	return dw_loadExternal(element);
}

function loadSponsorExhibitor(element)
{
	return dw_loadExternal(element);
}

function ShowPopup(attrib)
{
	if (mapVar == 1)
	{
	hp = document.getElementById("mappopup");
	frame = document.getElementById("contentcontainer");
	frame.style.visibility = "hidden";
	hp.style.display = "block";
	hp.style.top = attrib.offsetTop + 260 + 'px';
	hp.style.left = attrib.offsetLeft + 280 + 'px';
	}
}

function HidePopup()
{
	hp = document.getElementById("mappopup");
	frame = document.getElementById("contentcontainer");
	frame.style.visibility = "Visible";
	hp.style.display = "none";
}

</script>
</head>
