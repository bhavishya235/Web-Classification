<script src="http://maps.google.com/maps?file=api&amp;v=2&amp;key=ABQIAAAAOH0QluDIE2XhCOE7zBjcpBQcl83zOUY9GEVmycQeUzYPTdwDXhRZTR315vhdvGuXL38Rxk2plk-UXw" type="text/javascript"></script>
<script src="sitescript.js">
