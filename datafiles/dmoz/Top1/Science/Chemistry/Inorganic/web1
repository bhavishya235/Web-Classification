<script type="text/javascript">
ibmweb.config.set({
siteid:'www',
greeting:{enabled:false},
signin:{enabled:false},
footermenu:{enabled:true}
});

var edit_server = 'resweb.watson.ibm.com';

function editProfile() {
	base = 'http://'+edit_server+'/researcher/';
	document.location = base + 'edit_profile.php'
}
</script>

