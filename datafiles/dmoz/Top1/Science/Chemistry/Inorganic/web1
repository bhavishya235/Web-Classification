<link href="res_v17.css" rel="stylesheet" type="text/css"></link>


<script type="text/javascript">

function doEdit() {
	window.open('http://'+edit_server+'/researcher/edit_project.php?area=3&id=4245', 'Edit', 'toolbar=0,location=0,dependent=1,scrollbars=1,innerWidth=950,innerHeight=820');
}

function doJoin() {
	window.open('http://'+edit_server+'/researcher/edit_profile.php?join=4245', '', '');
}

var resEditCookie = dojo.cookie("res-cwacheck");			// set by cwacheck.php if successful log on
var group_id = "4245";
var group_name = "STM Image Gallery";
var group_type = "3";
var group_publish = "1";
var ignore_publish = "";

function inProject(str) {
	var table = 'researchers';
	var where = uriEncode64('email="'+str.toLowerCase()+'"');
	var url = "select_items.php?table="+table+"&where="+where;
	var resRec = ajaxSimpleJSQuery(url);
	var researcher_id = resRec[1][0];
	table = 'researcher_group_entries';
	where = uriEncode64('researcher_id='+researcher_id);
	url = "select_items.php?table="+table+"&where="+where;
	var researcher_groups = ajaxSimpleJSQuery(url);
	if (researcher_groups.length > 1) {
		for (i = 1; i < researcher_groups.length; i++) {
			if (researcher_groups[i][2] == group_id)
				return true;
			}
		}
	return false;
}

function pageLoaded() {
	if (group_name == '') {
		location.replace('404.html');
		return;
		}
	if (group_publish == 0)
		document.getElementById('not_externalized').innerHTML = '<div class="res-alert">Page not available.</div><br/>';
	var img = new Image();
	img.src = "http://"+edit_server+"/researcher/images/a_black.png";
	img.onload = function() {
		internalImageLoaded();
		}
}

function internalImageLoaded() {
	if (group_publish == 0) {
		if (ignore_publish == 'y')
			showInternalItems();
		else
			location.replace('http://'+edit_server+'/researcher/view_project.php?id='+group_id+'&i=y');
		}
	else
		showInternalItems();

}

function showInternalItems() {
	if (group_publish == 0)
		document.getElementById('not_externalized').innerHTML = '<div class="res-alert">Page not externalized. Check Allow External Page Views in the page editor to externalize.</div><br/>';
	document.getElementById('last_updated').innerHTML = 'last updated ' + '07/02/2013';
	if (resEditCookie) {
		if (!inProject(resEditCookie))
			showJoinButton = true;
		else {
			document.getElementById('join_group').innerHTML = '<input name="edit" type="button" value="Edit" onClick="doEdit()">';
			}
		}
	else
		showJoinButton = true;
	if (showJoinButton)
		document.getElementById('join_group').innerHTML = '<input name="join" type="button" value="Join" onClick="doJoin()">';
}

</script>

