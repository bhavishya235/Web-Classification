      <div id="content">

        <article id="content-main" class="span-six top">


          <h1>WE BUILD THE FUTURE</h1>
              
          <ol class="news-list news-list-stack global">
          
          

	<li class="news">
		 		    
  		<div class="carousel slide no-caption" data-controller="carousel"  >
    	<div class="carousel-inner">
     	      			<div class="item active ">
      				      				
      				      				 <img src="http://www.petrochemistry.eu/images/bicycle.jpg">
        			        			        			        				<figcaption>&nbsp;</figcaption>
        			      			</div>
      			     	      			<div class="item  ">
      				      				
      				      				 <img src="http://www.petrochemistry.eu/images/red-scultpture.jpg">
        			        			        			        				<figcaption>&nbsp;</figcaption>
        			      			</div>
      			     	      			<div class="item  ">
      				      				
      				      				 <img src="http://www.petrochemistry.eu/images/ceiling.jpg">
        			        			        			        				<figcaption>&nbsp;</figcaption>
        			      			</div>
      			     	      			<div class="item  ">
      				      				
