<script type="text/javascript">
	//This function controls the visibility of the main menu
	var menuVisible = false;
	function ToggleMenu(){
		menuVisible = !menuVisible;
		if (menuVisible){
			document.getElementById("scetimenu").style.visibility = "hidden";
			document.getElementById("scetimenu2").style.visibility = "visible";
		}
		else {
			document.getElementById("scetimenu").style.visibility = "visible";
			document.getElementById("scetimenu2").style.visibility = "hidden";
		}
	}
</script>

<script type="text/javascript">
	//This function controls the visibility of the main menu
	var menuVisible = false;
	function ToggleMenu2(){
		menuVisible = !menuVisible;
		if (menuVisible){
			document.getElementById("scetimenu3").style.visibility = "hidden";
			document.getElementById("scetimenu4").style.visibility = "visible";
		}
		else {
			document.getElementById("scetimenu3").style.visibility = "visible";
			document.getElementById("scetimenu4").style.visibility = "hidden";
		}
	}
</script>



	
