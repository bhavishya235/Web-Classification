<script language="JavaScript">

function isNullString(sString){
	var sStr, i, iStart;
	sStr = sString.split("");
	iStart = -1 ;
	for (i = 0 ; i < sStr.length ; i++)	{if (sStr[i] != " " && sStr[i] != "") {iStart = i; break;}}
	return (iStart == -1);
}

