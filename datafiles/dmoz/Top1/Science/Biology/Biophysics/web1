            <h2 style="padding-left: 1em;">Search BMRB</h2>
            <!--
  search form
  requires jquery
  -->
<script type="text/javascript">
$( document ).ready( function() {

    function fill_dbnames_select() {
        var html = '';
        var codes = ( function() {
        var rc = null;
        $.ajax( {
            "async" : false,
            "global" : false,
            "url" : "/software/lib/list_pgdb_column.php",
            "data" : { "tag" : "Entity_db_link__Database_code" },
            "success" : function( data ) { rc = data; },
            "dataType" : "json"
        } );
        return rc;
        } )();
        var len = codes.length;
        if( len > 0 ) {
            for( var i = 0; i < len; i++ ) {
                var code = codes[i];
                if( code === "PDB" ) continue;
                html += '<option value="' + code + '">' + code + "</option>";
            }
            $( "#dbname" ).append( html );
        }
    }
    fill_dbnames_select();

} );
</script>
