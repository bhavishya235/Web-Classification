		var allcookies = document.cookie;
		var pos = allcookies.indexOf("CustKey=");
		var value = null;
		if (pos != -1) {
			var start = pos + 8;
			var end = allcookies.indexOf(";", start);
				if (end == -1) end = allcookies.length;
				value = allcookies.substring(start, end);
				}
	
		if (value == null)
 
		
			{
				show('logindiv');
				hide('logoutdiv');
			}
			else
