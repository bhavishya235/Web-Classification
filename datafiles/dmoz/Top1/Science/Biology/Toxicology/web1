<script type="text/javascript">
function get_random(rand)
{
	if (!rand)
		return 0;

	var today = new Date()
	var seed = today.getSeconds()
    var ranNum= Math.floor(Math.random(seed)*6)
    return ranNum;
}
</script>

<script type="text/javascript">
$(document).ready(function() {
$(function() {
	$('#729 img').show();
	$('#729').cycle({
    fx:      'fade',
	speed: 2500,
    timeout:  4000,
    easing:  'backinout',
	startingSlide: get_random(1)
	});
});
});
</script>
