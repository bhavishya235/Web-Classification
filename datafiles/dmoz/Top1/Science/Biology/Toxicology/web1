<span class="Login"><script type="text/javascript">var loginform = '<a href="http://www.acmt.net/cgi/page.cgi/member_main.html">Member Login</a>';
var id_cookie = get_cookie('ACMTID');
if (id_cookie) {
    var id = id_cookie.split(':');
    document.write('Logged in as: <a href="http://www.acmt.net/cgi/page.cgi/member_main.html">' + id[0] + '</a> (<a href="/cgi/logout.cgi" class="Logout">Log out</a>)');
}
else {
    document.write(loginform);
}
function get_cookie(name) {
    var key = name + "=";
    var cookies = document.cookie.split(';');
    for(var i=0;i < cookies.length;i++) {
        var cookie = cookies[i];
        while (cookie.charAt(0)==' ') cookie = cookie.substring(1,cookie.length);
        if (cookie.indexOf(key) == 0) return cookie.substring(key.length,cookie.length);
    }
    return null;
}
</script>
