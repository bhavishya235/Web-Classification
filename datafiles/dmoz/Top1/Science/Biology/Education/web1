var real_location = "http://www.madsci.org/";

function framethis () {

        if (top.location) {
                    if (self != top) top.location = self.location;
                    } else {
                           if (parent.location) {
                              if (parent.location != real_location) parent.location = real_location;
                    }
        }
}
//-->
