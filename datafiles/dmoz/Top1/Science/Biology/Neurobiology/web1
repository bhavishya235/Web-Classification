<!-- Hide JavaScript from JavaScript-Impaired Browsers
function checkVersion() {
	if(navigator.appVersion.charAt(0)>= 3)
		return true;
	else
		return false;
}

ROs = new Array(6);
var preloadFlag = false;

function hilite(imgsrc, imgname){
	if(checkVersion())
	{	if (preloadFlag == true)
		{	document.images[imgname].src=imgsrc;
		}
	}
}

function newWindow(videoLink)
{	videoWindow = window.open(videoLink,'videoWin', 'width=400,height=300,toolbar=no,location=no,status=yes,menubar=no,resizeable=no,scrollbar=no')
	videoWindow.focus()
}

function gameWindow(game)
{	gameWin = window.open(game,'gameW', 'width=470,height=400,toolbar=no,location=no,status=yes,menubar=no,resizeable=no,scrollbar=no')
	gameWin.focus()
}

//--- stop Hiding-->
</script>
</head>
