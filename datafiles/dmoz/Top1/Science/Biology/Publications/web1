      <script type="text/javascript" src="http://code.jquery.com/jquery-1.10.1.min.js"></script>
<script type="text/javascript" src="http://cdnjs.cloudflare.com/ajax/libs/gsap/1.10.1/TweenMax.min.js"></script>

<script type="text/javascript">
$(document).ready(function() {
    var loop = -1;
    var t1 = new TimelineMax({repeat: loop});
    var t2 = new TimelineMax({repeat: loop});

    // ANIMATION
    t1.from($('#slide1'), 1, {opacity: 0, delay: .5})
      .from($('#caption1'), .5, {opacity: 0, delay: 1}, 'slide1')
      .from($('#slide2'), 1, {opacity: 0, delay: 7})
      .from($('#caption2'), .5, {opacity: 0, delay: .5}, 'slide2')
      .from($('#slide3'), 1, {opacity: 0, delay: 7})
      .from($('#caption3'), .5, {opacity: 0, delay: .5}, 'slide3')
      .to($('#caption3'), 7); // delay before loop;

    t2.to($('#nav-button-1 a'), 1.4, {css:{className:'+=active-slide'}, delay: .1}, 'button1')
      .to($('#nav-button-1 a'), .1, {css:{className:'-=active-slide'}, delay: 7.4})
      .to($('#nav-button-2 a'), 1.4, {css:{className:'+=active-slide'}, delay: .1}, 'button2')
      .to($('#nav-button-2 a'), .1, {css:{className:'-=active-slide'}, delay: 7.4})
      .to($('#nav-button-3 a'), 1.4, {css:{className:'+=active-slide'}, delay: .1}, 'button3')
      .to($('#nav-button-3 a'), .1, {css:{className:'-=active-slide'}, delay: 7.4})
      .to($('#nav-button-3'), 7); // delay before loop;

    // HOVER CONTROLS
    $('#splash').hover(
        function(){
            $('#slide-controls').fadeIn();
        },
        function(){
            $('#slide-controls').fadeOut();
        }
    );

    $('#slide-controls a').css('opacity', '.5');
    $('#slide-controls li').hover(
        function(){
            $(this).fadeTo('fast', 1);
        },
        function(){
            $(this).fadeTo('fast', .5);
        }
    );

    // CHAPTERS
    $('#nav-button-1 a').click(function(){
        t1.seek('slide1');
        t2.seek('button1');
    });
    $('#nav-button-2 a').click(function(){
        t1.seek('slide2');
        t2.seek('button2');
    });
    $('#nav-button-3 a').click(function(){
        t1.seek('slide3');
        t2.seek('button3');
    });
});

</script>

