//<![CDATA[
    $(document).ready(function () {
        var searchDefaultText = "Search";

        $('#tbSearchTerm')
            .val(searchDefaultText)
            .focus(function () {
                if ($(this).val() == searchDefaultText) {
                    $(this).val("");
                }
            })
            .blur(function () {
                if ($.trim($(this).val()) == "") {
                    $(this).val(searchDefaultText);
                }
            });

        $('#btnSearchSubmit').click(function (e) {
            if ($('#tbSearchTerm').val() == searchDefaultText) {
                e.preventDefault();
                return false;
            }
        });
    });
//]]>
</script>
   
