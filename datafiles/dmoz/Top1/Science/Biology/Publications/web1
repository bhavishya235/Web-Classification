		 <script>
		jQuery(document).ready(function() {
			if(jQuery(".msg.survey").length > 0) {

				var surveycookie = jQuery(".msg.survey").attr("id");

				if(site.allCookies.getItem(surveycookie) == null) { 
					var surveyendday = "28" ;
					var surveyendmonth = "1" ;

					// current year
					var dateobj = new Date();
					var yeartoexpire = dateobj.getUTCFullYear(); 

					var surveyurl = "https://www.surveymonkey.com/s/M2X3ZYJ ";

					var surveycookieexpirydate = new Date(yeartoexpire, surveyendmonth, surveyendday, 0, 0, 0, 0); 

					// display the survey
					jQuery(".msg.survey").show();

					jQuery(".msg-close-img, .survey-button a").click(function() {
						site.allCookies.setItem(surveycookie, "closed", surveycookieexpirydate);
					});
				} else {
					jQuery(".msg.survey").detach();
				}
			}
		});

		</script>

	


