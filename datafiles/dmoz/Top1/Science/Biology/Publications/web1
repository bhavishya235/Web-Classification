<script type="text/html" id="quotes_by_search_template">


	<% if (heading == "") { %>
		    <div class="rounded " id="quotes_by_search">
<div class="wrap-inner ">
        <h2>Latest <%= entries[0].type %></h2>    <div class="module gray  inner">
<div class="module-inner padded-inner">
	<% } else { %>
		    <div class="rounded " id="quotes_by_search">
<div class="wrap-inner ">
        <%= heading %>    <div class="module gray  inner">
<div class="module-inner padded-inner">
	<% } %>

<% for ( var i = 0; i < entries.length; i++ ) { %>
        <div class="panel-feed">
			<p class="header">
			<strong><a href="<%= entries[i].articleUrl%>" class="article_title"><%= entries[i].bibliograhyTitle %></a></strong>
			<% if(entries[i].isOpenAccess == "true") {%>
				&nbsp;<a href="http://www.biomedcentral.com/about/access"><img class="openaccess-img" src="/images/icons/open-access.gif" alt="Open Access" title="Open Access"/></a>
			 <%}%>
			<%if((entries[i].isOpenAccess)=="false" && entries[i].isFree == "true"){%>
				&nbsp;<a href="/about/access"><img class="free-img" src="/images/icons/free.gif" alt="Free" title="Free"/></a>
			<% } %>
			<% if(entries[i].isOpenAccess == "true" && (entries[i].isFree == "false")) {%>
				&nbsp;<a href="/about/access"><img class="subscription-img" src="/images/icons/subscription.gif" alt="Subscription" title="Subscription"/></a>
			<% } %>
			<% if(entries[i].isOpenAccess == "false" && (entries[i].isFree == "false")) {%>
				&nbsp;<a href="/about/access"><img class="subscription-img" src="/images/icons/subscription.gif" alt="Subscription" title="Subscription"/></a>
			<% } %>
			<% if(entries[i].isHighlyAccessed == "true"){%>
				&nbsp;<a href="http://www.biomedcentral.com/about/mostviewed"><img class="highlyaccessed-img" src="/images/icons/highly-accessed.gif" alt="Highly Accessed" title="Highly Accessed"/></a>
			<% } %>
			</p>
			<p class="authors"><%= entries[i].authorNames %></p>
			<p class="citation"><%= entries[i].longCitation %></p>

		<p class="nav">
       <span class="left">
        <a href="<%= entries[i].abstractPath %>" class="abstract-link">Abstract</a> |
        <%if (entries[i].status == "LIVE"){%>
            <a href="<%= entries[i].articleUrl %>" class="fulltext-link">Full text</a> |
        <%}%>
         <a href="/content/pdf/<%=entries[i].arxId %>.pdf" class="pdf-link"><% if(entries[i].status=="QC"){%>Provisional PDF <% }else {%>PDF<%}%></a>
         </span>
		</p>
    </div>
<% } %>
	<% if(params.viewMoreLinkSearch) { %>
	<p class="pager left"><a id="view-more" href=" <%= params.viewMoreLinkSearch %>"><% if(params.viewMoreTextSearch) { %><%= params.viewMoreTextSearch %><% } else { %>View more<% } %></a></p>
	<% }  %> 
    </div>
</div>
        </div>
</div>

</script>



