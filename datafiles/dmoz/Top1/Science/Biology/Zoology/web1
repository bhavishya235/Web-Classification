	nav[28].src="images/function.2.gif"


function display(onoff,img) 
{ 
 	if (onoff !=0) 
	{
 		document.getElementsByName(img)[0].src=nav[onoff].src
	}
	
} // end function
// Stop hiding from old browsers -->
</SCRIPT>
	
