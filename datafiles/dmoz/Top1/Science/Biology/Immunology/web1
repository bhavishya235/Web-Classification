		function resizecolumn(sourceid,targetid) {
		//alert(document.getElementById(sourceid).offsetHeight+'px');
		if(document.getElementById(targetid) && document.getElementById(sourceid)) {
		document.getElementById(targetid).style.height = document.getElementById(sourceid).offsetHeight+ 'px';
			}
