	<!-- Redirect vanderbilthealth search options -->
	<script type="text/javascript">
		$(document).ready( function() {
			$("#searchform select").change( function() {
				var action = $(this).val() == "sub" ? "/root/vumc.php" : "/vanderbilthealth/main/search";
				if ($(this).val() == "sub" ) { 
					$("<input>").attr({
						type: "hidden",
						name: "doctemplate",
						value: "edu-search"
					}).appendTo("#searchform");

					$("<input>").attr({
						type: "hidden",
						name: "site",
						value: "'.$site.'"
					}).appendTo("#searchform");

				} else { $("#searchform input[type='hidden']").remove(); }

				$("#searchform").attr("action", action);
			});
		});
	</script>

	<!-- Feedback form script -->
	
		<script type="text/javascript">
			$(document).ready( function() {
				$('.feedback').click(function() {

					var page_title = 'Department of Pathology, Microbiology, and Immunology';
					var master_email = 'andries.zijlstra@vanderbilt.edu';
					var feedback_site = $(this).data('site_feedback');

					

					$('#feedback_dialog').load('feedback.php', function(){
						$('#feedback_dialog').dialog({
							title: 'Feedback',
					        resizable: true,
					        autoOpen:true,
					        modal: true,
					        hide: 'fade',
					        width:435,
					        height:375,
					        open: function() {

								$('.page_title_area').text(page_title);
								$('#master_email').val(master_email);

							    $('#vh_feedback_button').click(function() {
							    	$('#feedback_buttons').hide();
							    	$('#email').hide();
									$('#feedback_form').show();
									// $('#feedback_dialog').dialog({height:450});
								});

							    $('#cancel_feedback').click(function() {
									$('#feedback_dialog').dialog('close');
							    });

					        	if ( feedback_site == true ) {
									$('#vh_feedback_button').click();
								}

					        }
						});

					    $('#form1').submit(function(e) {

							e.preventDefault();
							e.stopPropagation();

							if ( $('#comments').val() == '' ) {
								alert('Please type in your comments and then sumbit the form.');
								return false;
							}

							else if ( $('#email').val() != '' ) {
								//this field left blank intentionaly
								return false;
							}

							else {
								form_info = $(this).serialize();

								$.post('/root/feedback_submit.php', form_info, 
								function(data) {
									$('#feedback_form').html(data.notice);
								}, 'json' );

								return false;	
							}							
						});

					});

				});

			});
		</script>

		
