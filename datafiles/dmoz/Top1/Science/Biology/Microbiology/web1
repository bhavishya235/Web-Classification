<script>
	function submitAreas ()
	{
		var inputValue = document.getElementById("mod_search_searchword").value;

		if(inputValue == 'Keywords' || inputValue == '')
			return false;

		var areaList = document.getElementById('mod_search_type').value.split('|'); 
		var form = document.getElementById('form-modSearch');

		for (x=0; x<areaList.length; x++)
		{
			input = document.createElement('input');
			input.setAttribute("type", "hidden");
			input.setAttribute("name", "areas[]");
			input.setAttribute("value", areaList[x]);
			document.getElementById("form-modSearch").appendChild(input);
		} // for

		return true;
	} // submitAreas
</script>

<div class="mod-search clearfix">
	<form onSubmit="return submitAreas();" class="jForm form-modSearch" name="form-modSearch" id="form-modSearch" method="post" action="index.php"> 
		<button onclick="this.form.searchword.focus();" class="button" type="submit">GO</button> 
		<select id="mod_search_type" name="arealist" class="inputbox field select"> 
			<option value="content|jlibrary|news|video|images|resources|collections|tags">All Content</option> 
			<option value="news|tags">News</option> 
			<option value="video|tags">Video</option> 
			<option value="images|tags">Images</option> 
			<option value="resources|tags">Resources</option> 
			<option value="collections|tags">My Collections</option> 
			<option value="submissions|tags">Submissions</option> 
			<option value="tags">Tags</option> 
		</select>
		<input onfocus="if(this.value=='Keywords') this.value='';" onblur="if(this.value=='') this.value='Keywords';" value="Keywords" size="20" class="field text" alt="GO" maxlength="100" id="mod_search_searchword" name="searchword" type="text" />
		<div class="search">Search</div>
		<input value="search" name="task" type="hidden" /> 
		<input value="com_search" name="option" type="hidden" /> 
	</form>
</div></div>
