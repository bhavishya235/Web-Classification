	function openBrow(pageUrl,winw,winh) {
		var mywin = null;
		if( pageUrl != "" ) {
		   mywin = window.open(pageUrl,"newwin","toolbar=no,menubar=no,status=no,location=yes,scrollbars=yes,resizable=yes");
		   mywin.resizeTo(winw,winh);
		   mywin.focus();
		}
}
</script>

<script>
$('form.validate').submit(function() {
    var errors = [];
    $('.required', this).each(function(idx, elem) {
        var e = $(this);
        if(!$.trim(e.val())) {
            var label = $('label[for=' + e.attr('id') + ']').text();
            e.css('background-color', '#fcc');
            errors.push(label);
        }
        else {
            e.css('background-color', '#fff');
        }
    });
    if(errors.length > 0) {
        alert('Please make sure you fill in ' + errors.join(' and ') + ' before submitting.');
        return false;
    }
    return true;
});
</script>

