$(function() {
	
	<!-- Submenu >> Start -->
	$('.js .sidebar .menu-main-menu-container a').wrapInner('<span>');
	$('.js .sidebar .menu-main-menu-container ul li a').hover(
		function () {													  
			$(this).animate({
				paddingLeft: '10px'
			}, 250);
			return false
		},					
		function () {						
			$(this).animate({
				paddingLeft: '0px'
			}, 250);
			return false
		}
	);
	<!-- Submenu >> End -->

	<!-- Content Toggle >> Start -->
	$('.toggle-handle.with-icon').prepend('<span class="icon"></span>');
	
	$('.toggle-content').each(function(key, value) {
		$(value).data('getHeight', $(value).height());
	});
	
	$('.toggle-content').css({height : '0', opacity: '0'});
	$('.toggle-handle').toggle(function () {
		$(this).next('.toggle-content').animate({height: $(this).next(".toggle-content").data("getHeight"), opacity: '1.0'}, 250);
		$(this).children('.show').css({display: 'none'});									 
		$(this).children('.hide').css({display: 'block'});									 
		$(this).children('.icon').css({backgroundPosition: 'bottom left'});									 
	},					
	function () {	
		$(this).next('.toggle-content').animate({height: '0px',opacity: '0.0'}, 250);
		$(this).children('.show').css({display: 'block'});									 
		$(this).children('.hide').css({display: 'none'});									 
		$(this).children('.icon').css({backgroundPosition: 'top left'});									 
	});
	<!-- Content Toggle >> End -->


	<!-- Tabs >> Start -->
	$("ul.tabs").tabs("> .pane");
	<!-- Tabs >> End -->


	<!-- Links on Facebook Template >> Start -->
	$('.page-template-page-facebook-php a').attr('target', '_parent');
	<!-- Links on Facebook Template >> End -->
	
	
	
	<!-- ??? >> Start -->
	//$('.js .browse').css({display: 'block'});
	<!-- ??? >> End -->

	

	<!-- fancyBox >> Start -->
	$('.gallery a').addClass('fancybox hover');	
	//$('.fancybox img').wrap('<span>');	
//	$('.hover img').hover(
//		function () {													  
//			$(this).animate({
//				opacity: '0.5'
//			}, 250);
//		},					
//		function () {	
//			$(this).animate({
//				opacity: '1.0'
//			}, 250);
//		}
//	);

	$(".fancybox").fancybox({
		transitionIn: 'elastic',
		transitionOut: 'elastic'
	});
	
	$(".fancybox-text-trigger").fancybox({
		titleShow: false,
		overlayColor: '#333',
		overlayOpacity: '0.5'
	});
	
	$(".fancybox-iframe").fancybox({
		width: '75%',
		height: '75%',
		autoScale: false,
		transitionIn: 'none',
		transitionOut: 'none',
		type: 'iframe'
	});
	<!-- fancyBox >> End -->

	<!-- Edit Link >> Start -->
		<!-- Edit Link >> End -->
	
	<!-- Tooltips >> Start -->
	// add tooltips to the insocial links
	$('#body .main .author-info .insocial-links a').addClass('tooltip-trigger');

	$('.tooltip-trigger').tooltip({
		tipClass: 'tooltip-viewer',
		relative: false,
		effect: 'fade',
		offset: [-10, 0],
		opacity: '0.9',
		delay: '0'
	});	
	<!-- Tooltips >> End -->

	<!-- Hovercards >> Start -->
	$('.hovercard-trigger').tooltip({
		tipClass: 'hovercard-viewer',
		relative: true,
		effect: 'fade',
		offset: [-10, 0],
		opacity: '1.0',
		delay: '200'
	});
	<!-- Hovercards >> End -->

});


	

jQuery(function($) {

	<!-- FORMS >> Start -->
	$(".inlined").next('.text').each(function (type) {
	 
		$(this).focus(function () {
			$(this).prev(".inlined").addClass("focus");
		});
	 
		$(this).keypress(function () {
			$(this).prev(".inlined").addClass("has-text").removeClass("focus");
		});
	 
		$(this).blur(function () {
			if($(this).val() == "") {
				$(this).prev(".inlined").removeClass("has-text").removeClass("focus");
			}
		});
	});
	
	$('form').submit(function() {
		var hasError = false;
		$(this).find('.required').each(function() {
			if(jQuery.trim($(this).val()) == '') {
				var labelText = $(this).prev('label').text().replace(' *','');
				$(this).parent().append('<span class="not-valid">'+labelText+' is a required field.</span>');
				$('.not-valid').hover(function() {
					$(this).fadeOut(500);
				});
				hasError = true;
			}
		});
		if(hasError) {
			return false;
		}
	});
	
	$('textarea').prettyComments();
	<!-- FORMS >> End -->
	
	
	$(".cb-enable").click(function(){
		var parent = $(this).parents('.switch.bar.checkbox');
		$('.cb-disable',parent).removeClass('selected');
		$(this).addClass('selected');
		$('input.checkbox',parent).attr('checked', true);
	});
	$(".cb-disable").click(function(){
		var parent = $(this).parents('.switch.bar.checkbox');
		$('.cb-enable',parent).removeClass('selected');
		$(this).addClass('selected');
		$('input.checkbox',parent).attr('checked', false);
	});
	
	
	
	
	
				
	<!-- Page Scroll >> Start -->
	$('.page.scroller').click(function(){
		$('body').scrollTo({top: '0', left: '0'} , 1000 );
	});

	$.localScroll({
		target: 'body',
		axis: 'y',
		queue: true,
		duration: '1000',
		hash: true,
		margin: true,
		offset: {top: '-40'}
	});
	<!-- Page Scroller >> End -->

	jQuery('#testplayer').youTubeEmbed("http://www.youtube.com/watch?v=0NcJ_63z-mA");
	

//	$('form').submit(function(){
//		$('#player').youTubeEmbed($('#url').val());
//		$('#url').val('');
//		return false;
//	});

});

</script> 


