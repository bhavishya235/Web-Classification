<div class="imagedroite">
<div class="bordureHaut"></div><div class="bordureGauche"><div class="bordureDroite"><div class="contenu">
<script language="javascript" type="text/javascript">
$(function() {
var date_du_jour = new Date();
var mois = date_du_jour.getMonth();
var annee = date_du_jour.getFullYear();
mois = mois +1;
var min = new Date(annee, mois-2, 1);
var max = new Date(annee, mois+2, 0);
var jourMin  = min.getDate();
var moisMin  = min.getMonth();
var anneeMin = min.getFullYear();
var jourMax  = max.getDate();
var moisMax  = max.getMonth();
var anneeMax = max.getFullYear();
$("#datepickerContainer").datepicker($.datepicker.regional['FR']);
$("#datepickerContainer").datepicker('option', 'minDate', new Date(anneeMin, moisMin, jourMin));
$("#datepickerContainer").datepicker('option', 'maxDate', new Date(anneeMax, moisMax, jourMax));
});
</script>
<script language="javascript" type="text/javascript">
function goToPlan() {
var action = document.getElementById("frm").action;
//$("#datepickerContainer").datepicker('option', {dateFormat: 'dd-mm-yy'});
var re = /\//g;
var dateToSubmit  = document.getElementById("datepickerInput").value.replace(re, "-");
var startOptionsWith = "";
if( action.substring(action.length-1) != "/" ) {
startOptionsWith = "/";
}
var sURL = action + startOptionsWith + "date/" + dateToSubmit + "/postDate/true/vue/agenda";
window.location.href = sURL;
}
</script>
<div id="bgAgendaBlack" class="bgAgendaBlack">
