    Event.observe(document.body, 'load', function(e) {
        var pad = $("top_header").height() + 'px 0px 0px 0px';
        $("left_nav").setStyle({ padding: pad });

        var HeaderImg = $("HeaderImage");
        if (HeaderImg != null)
            if (HeaderImg.getWidth() > 780)
            HeaderImg.setStyle({ width: '780px' });

        var tableHeader = $("TableHeader");
        var leftTab = $("LeftTab-selected");
        var verticalBar = $("vertical_bar");
        if (tableHeader != null) // not printer friendly page
        {
            if (leftTab != null) {
                if (verticalBar.getWidth() > 1)
                    $("masterTable").setStyle({ width: '1006px' });
                else
                    $("masterTable").setStyle({ width: '1004px' });
            }
            else {
                if (verticalBar.getWidth() > 1)
                    $("masterTable").setStyle({ width: '852px' });
                else
                    $("masterTable").setStyle({ width: '850px' });
            }

        }
    }, false);
</script>
