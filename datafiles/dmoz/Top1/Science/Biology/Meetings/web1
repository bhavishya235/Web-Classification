<script type="text/javascript">
	function startTicker() {
		ticker = document.getElementById('marq');
		tickerContent = document.getElementById('marqContent');

		ticker.origWidth = tickerContent.offsetWidth;

		while (tickerContent.offsetWidth < ticker.offsetWidth) {
			tickerContent.innerHTML += tickerContent.innerHTML;
		}

		tickerContent.innerHTML += tickerContent.innerHTML;

		scrollTicker();
	}
	function scrollTicker() {
		ticker = document.getElementById('marq');
		if (!ticker.pauzed) ticker.pauzed = 'N';

		if (ticker.pauzed == 'N') {
			ticker.scrollLeft += 1;
			if (ticker.scrollLeft >= ticker.origWidth) ticker.scrollLeft = 0;
		}

		setTimeout('scrollTicker()', 30);
	}
	function eventAdd(aobject, aeventstring, afunction) {
		if(window.addEventListener){ // Mozilla, Netscape, Firefox
			aobject.addEventListener(aeventstring, afunction, false);
		} else { // IE
			aobject.attachEvent('on' + aeventstring, afunction);
		}
	}
	function loading() {
	}
	eventAdd(window, 'load', startTicker);
</script>

