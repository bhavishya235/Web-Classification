<script type="text/javascript">
	n = 18; // number of hidden layers
	function show(a){
		for(i=1;i<=n;i++){
			document.getElementById('subnav_'+i).style.display = 'none';
		}
		document.getElementById('subnav_'+a).style.display = 'block';
	}
</script>

<script type="text/javascript">
	n = 18; // number of hidden layers
	function hideDivs(){
		for(i=1;i<=n;i++){
			document.getElementById('subnav_'+i).style.display = 'none';
		}
	}
</script>


