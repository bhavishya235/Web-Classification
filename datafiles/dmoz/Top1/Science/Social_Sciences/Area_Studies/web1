<script type='text/javascript'>
function getPageMeta() {
    var modDate = "Friday, October 04, 2013";
    modDate = modDate.replace(/,/g, "");
    modDate = modDate.split(" ");
    return parseInt(modDate[2]) + " " + modDate[1] + " " + modDate[3];
} 
</script>

