<script language="javascript">
  function checksearch(aform)
  {
    if (bEnteredText)
    {
      with (aform)
      {
        if (sSearch.value.length<3)
        {
          alert("You need to enter at least 3 characters to perform a search");
          return false;
        }
      }
      return true;
    }
    else
    {
      alert("You must specify your search criteria");
      return false;
    }
  }

  var bEnteredText=false;

  function testEnteredText(iObject)
  {
    if (!bEnteredText)
    {
      iObject.value="";
      bEnteredText=true;
    }
  }
</script>
