  <link href="/css/style.css" rel="stylesheet" type="text/css" media="screen, print" />
  <link href="http://cms.ku.dk/css/units/samf.css" rel="stylesheet" type="text/css" media="screen, print" />

<script type="text/javascript">//<!--
var textsize_cookie_domain = '.ku.dk';
var vdoc_lang = 'en';
//--></script>
<script src="http://cms.ku.dk/scripts_js/functions.js" type="text/javascript"></script>
<script src="http://cms.ku.dk/system/cookies.js" type="text/javascript"></script>


  <link href="/css/print.css" rel="stylesheet" type="text/css" media="print" />

    <meta name="Author" content="webmaster" />
    <meta name="Creator" content="webmaster" />
