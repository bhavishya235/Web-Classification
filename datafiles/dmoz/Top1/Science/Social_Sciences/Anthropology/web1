<script type="text/javascript">
  (function() {
    var cx = '012837232987443630560:d_umb_mqpn4';
    var gcse = document.createElement('script');
    gcse.type = 'text/javascript';
    gcse.async = true;
    gcse.src = (document.location.protocol == 'https:' ? 'https:' : 'http:') +
    '//www.google.com/cse/cse.js?cx=' + cx;
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(gcse, s);
    })();
  
  $(document).ready(function () {
    $(".search").on("click", ".gsc-search-button-v2", function (e) {
      e.preventDefault();
      
      var strSearchType = $("input[name=searchOn]:checked").val();
      var strSearchTerm = null;
      
      if(strSearchType == "people")
        strSearchTerm = $("#txtPeopleSearch").val();
      else
        strSearchTerm = $("#gsc-i-id1").val();
      
      var strPeopleSearchUrl = "https://webapp.cortland.edu/DirectorySearch/?facnme=" + strSearchTerm;
      var strSiteSearchUrl = "http://www.google.com/cse?cx=012837232987443630560%3Ad_umb_mqpn4&q=academic%20calendar&oq=a&gs_l=partner.1.0.0l10.1807.1807.0.3912.1.1.0.0.0.0.158.158.0j1.1.0.gsnos%2Cn%3D13.1.0.0.0jj1..1ac.1.OrerGi58eWo#gsc.tab=0&gsc.page=1&gsc.q=" + strSearchTerm;
      
      if(strSearchType == "people"){
        window.location = strPeopleSearchUrl;
      } else {
        window.location = strSiteSearchUrl;
      }
    });
    
    $(".search").on("keypress", "#gsc-i-id1", function (e) {
      $(this).css("margin-top", "-13px");
    });
    
    $(".search").on("keypress", "#txtPeopleSearch", function (e) {
      if(e.keyCode == 13){
        $(".gsc-search-button-v2").click();
      }
    });
    
    $(".search").on("blur", "#gsc-i-id1", function () {
      if($(this).val() == "" || $(this).val() == null) {
        $(this).css("margin-top", "0px");
      }
    });
    
    $(".search").on("click", "a[name=searchOn]", function () {
      var radioId = $(this).prev().attr("id");

      if(radioId == "StyledRadiobox0"){
        $("#gsc-i-id1").next().remove();
        $("#gsc-i-id1").removeClass("InputHidden"); 
      } else {
        $("#gsc-i-id1").addClass("InputHidden");
        $("#gsc-i-id1").val("");
        $("#gsc-i-id1").after('<input autocomplete="off" type="text" size="10" class="gsc-input" name="search" title="search" id="txtPeopleSearch" style="width: 100%; padding: 0px; border: none; margin: 0px; height: auto; outline: none; background-color: rgb(255, 255, 255); background-position: 0% 50%; background-repeat: no-repeat no-repeat; font-size: 12px;" dir="ltr" spellcheck="false">');
      }
    });
    
    $(".search").on("click", ".radioLabel", function () {
      var radioId = $(this).prev().children("input")[0].getAttribute("id");

      if(radioId == "StyledRadiobox0"){
        $("#gsc-i-id1").next().remove();
        $("#gsc-i-id1").removeClass("InputHidden");
      } else {
        $("#gsc-i-id1").addClass("InputHidden");
        $("#gsc-i-id1").val("");
        $("#gsc-i-id1").after('<input autocomplete="off" type="text" size="10" class="gsc-input" name="search" title="search" id="txtPeopleSearch" style="width: 100%; padding: 0px; border: none; margin: 0px; height: auto; outline: none; background-color: rgb(255, 255, 255); background-position: 0% 50%; background-repeat: no-repeat no-repeat; font-size: 12px;" dir="ltr" spellcheck="false">');
      }
    });
  });
</script>

