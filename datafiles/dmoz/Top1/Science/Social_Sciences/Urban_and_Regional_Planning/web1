        // actually, should be refactored into an object rather than just a global var.                         
        var ibmPageActionWindowIDs = 

                               { "wps.Selfcare": "7_BA9NEP341GDFE0IA6VI8FG0002"};


        
        var wpsBaseUrl = "/wps";
function getImagePath(/*String*/relPath) {
                return "/wps/themes/html/"+com.ibm.portal.aggregation.PORTAL_AGGREGATOR.page.getTheme()+"/"+relPath; 
        };
        if ( typeof( ibm ) == "undefined" ) { ibm = {} }
        if ( typeof( ibm.portal ) == "undefined" ) { ibm.portal = {} }  
        ibm.portal.ThemeImages = new function( ) {
                this.dotGifPath = "/wps/images/dot.gif";
                this.clearPixelPath = "about:blank";
                this.contextLoadingPath = "/PpWeb20Theme/themes/html/PpWeb20Theme/colors/default/context_loading.gif";
