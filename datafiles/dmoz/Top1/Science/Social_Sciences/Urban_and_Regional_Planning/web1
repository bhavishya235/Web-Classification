var alreadyClearedDefaultInput = false;

function clearDefaultInput() {
  if (alreadyClearedDefaultInput) {
    return false;
  } else {
    alreadyClearedDefaultInput = true;
    document.getElementById('search').value="";
    return false;
  }
}

