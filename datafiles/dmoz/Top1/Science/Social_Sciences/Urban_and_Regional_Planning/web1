<script type="text/javascript">
var advertUrls= new Array();
</script>
<p style="display: none" id="advertImageContainer">
<img src="/wps/wcm/connect/e8de5a8041a871ffa2d9fe7a7a96a0c0/adjacent_300_ani.gif?MOD=AJPERES&amp;CACHEID=e8de5a8041a871ffa2d9fe7a7a96a0c0" border="0" alt="Advert for Adjacent Planning and Building Control Today" name="Advert for Adjacent Planning and Building Control Today" width="300" height="250"  />
<script type="text/javascript">
advertUrls[advertUrls.length] = 'http://www.adjacentgovernment.co.uk/category/pbc-editions/';
</script>
<img src="/wps/wcm/connect/f0b2b280430074f6bcd2be929b9b67de/wunda_300_ani.gif?MOD=AJPERES&amp;CACHEID=f0b2b280430074f6bcd2be929b9b67de" border="0" alt="Advert for Wunda" width="300" height="250"  />
<script type="text/javascript">
advertUrls[advertUrls.length] = 'http://www.wundafloorheating.co.uk';
</script>
</p>
 

<div id='pp-ad-container'></div>

<script type="text/javascript">
var currentAdvertUrl = "";

// Set the adverts for the page
function setAdverts(){
   if(document.getElementById('advertImageContainer') != null) {
       var e = document.getElementById('advertImageContainer');
       var x = e.getElementsByTagName("img");
       var randomNum = (Math.floor((Math.random()* (x.length))));

       document.getElementById("ppAdvertImage").src = x[randomNum].src;
       // Set the advert URL
       currentAdvertUrl = advertUrls[randomNum];
       NTPT_PGEXTRA="lc=http://www.planningportal.gov.uk/portlets/advertisements%3Frefadv%3D"+currentAdvertUrl+"&#38;amp;lang=en";
       ntptEventTag("");
   }
}

function doLink(url){
	window.open(url);
}

var adCont =document.getElementById('pp-ad-container'); 
adCont.innerHTML='<a href="javascript:doLink(currentAdvertUrl)"  rel="nofollow"><center><img alt="Advert" id="ppAdvertImage" name="pageAdvert"/></center></a>';


// Call the set adverts now we have all components on the page
setAdverts();
</script>

<noscript>
<p><a href="http://www.planningportal.gov.uk/general/aboutus/commercial/" target="_blank"><center><img src="/wps/wcm/connect/d90d1d004714ddc1ad1dedbdde2c2877/pp_advert_homepage1.gif?MOD=AJPERES&amp;CACHEID=d90d1d004714ddc1ad1dedbdde2c2877" border="0" alt="Find out about commercial opportunitiess on the Planning Portal" width="300" height="250"  /></center></a>

<img src="/images/ntpagetag.gif?lc=http://www.planningportal.gov.uk/portlets/advertisement/&amp;lang=en" height="1" width="1" alt="" />

</p>
</noscript>



         	         


	</div>
