// Extracts the Visitor ID from the Google Analytics cookie
_goog_id = function(){
  var id, a, c = document.cookie.split('; ');
  for(i in c){
    a = c[i].split('=');
    if(a[0]=='__utma'){id = a[1].split('.')[1];}
  }
  return id || 'unknown';
};

// UoL
_gaq.push(['_setAccount', 'UA-409240-1']);
_gaq.push(['_setDomainName', 'le.ac.uk']);
_gaq.push(['_trackPageview']);
_gaq.push(function(){
  _gaq.push(['_setCustomVar',5,'VisitorID',_goog_id(),1]);
  document.getElementById('google_visitor_id').value = _goog_id();
});

// NI
