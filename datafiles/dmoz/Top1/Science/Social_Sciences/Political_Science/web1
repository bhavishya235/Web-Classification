			<script type="text/javascript">$(document).ready(function(){$('.event-name').each(function(){var $this=$(this);$a=$this.children('a');if($.trim($a.attr('href'))===''){var eventName=$a.text();$this.html(eventName);var location=$.trim($this.nextAll('span.event-location:first').text());if(location.length>1){$this.nextAll('span.event-location:first').show();}}});});</script><script language="JavaScript" type="text/javascript">$(document).ready(function(){$('span.people').each(function(){var people=$(this).text().split("; ");var totalSpeakers=people.length;var speakers="";for(var i in people){var person=people[i].split(": ");if(person[0]=="Speaker"){var parts=person[1].split("# ");if(parts.length==1){speakers=speakers+person[1];}else{speakers=speakers+parts[0]+"("+parts[1]+")";}
if(i<(totalSpeakers-1)){speakers+="; ";}}else if(person[0]=="Chair"){var chair="";var parts=person[1].split("# ");if(parts.length==1){chair=person[1];}else{chair=parts[0]+"("+parts[1]+")";}}}
if(typeof(chair)!='undefined'){speakers=speakers+"; Chair: "+chair;}
$(this).text(speakers);});if($('#homeeventsbox p').length>0){}else{$('#homeeventsbox h2').after('<p>We are currently organising more events. In the meantime why not check out some of our recent <a href="http://www.aog.ed.ac.uk/news_and_events">past events</a></p>');}
var uniqueEvents={};$('.homeEvent').each(function(){var html=$(this).html();var $eventType=$(this).find('.event-type');if($.trim($eventType.text())===":"){$eventType.remove();}
if(uniqueEvents[html]){$(this).remove();}else{uniqueEvents[html]=true;}});});</script>
