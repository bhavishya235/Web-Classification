<script type="text/javascript">
$(function(){
$("#query").live("blur", function(){
	var default_value = $(this).attr("rel");
	if ($(this).val() == ""){
		$(this).val(default_value);
	}
}).live("focus", function(){
	var default_value = $(this).attr("rel");
	if ($(this).val() == default_value){
		$(this).val("");
	}
});});
</script>

