<script type="text/javascript">

//<![CDATA[

var _gaq = _gaq || [];
	_gaq.push(['_setAccount', 'UA-37934249-1']);
	_gaq.push(['_trackPageview']);

	(function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
	})();  

$(document).ready(function(){
	$(".hide").hide();
	$("#main1").show();
	});

jQuery(function(){
	jQuery('ul.sf-menu').superfish();
	});
		
function submitform(){
	if(myform.search.value.length == 0){
	window.location = "http://www.plymouth.ac.uk/search";
	return false;
	}

	document.getElementById('myform').submit();
	}

$(document).ready(function(){
    $(".clicklink").click(function(){
	window.location=$(this).find("a").attr("href"); return false;
	});
	});

function button1(div1, div2){
	$(".hide").stop( true, false );
	$(div1).fadeOut(300, function() {
	$(div2).fadeIn(300);
	$(".hide").css({ opacity: 1 });
	});
	}		

function slidez(){
	$("#main1").delay(10000).fadeOut(300, function() {
	$("#main2").fadeIn(300, function() {
	$("#main2").delay(10000).fadeOut(300, function() {
	$("#main3").fadeIn(300, function() {
	$("#main3").delay(10000).fadeOut(300, function() {
	$("#main1").fadeIn(300);
	slidez();
	});
	});
	});
	});
	});
	}

$(document).ready(slidez);

$(document).keydown(function (evt) {
    if ((evt.keyCode == 37 || evt.keyCode == 51 || evt.keyCode == 99) && $('#main1').is(':visible')){
      button1('#main1','#main3');
    }
	if ((evt.keyCode == 37 || evt.keyCode == 49 || evt.keyCode == 97) && $('#main2').is(':visible')){
      button1('#main2','#main1');
    }
	if ((evt.keyCode == 37 || evt.keyCode == 50 || evt.keyCode == 98) && $('#main3').is(':visible')){
      button1('#main3','#main2');
    }
	if ((evt.keyCode == 39 || evt.keyCode == 50 || evt.keyCode == 98) && $('#main1').is(':visible')){
      button1('#main1','#main2');
    }
	if ((evt.keyCode == 39 || evt.keyCode == 51 || evt.keyCode == 99) && $('#main2').is(':visible')){
      button1('#main2','#main3');
    }
	if ((evt.keyCode == 39 || evt.keyCode == 49 || evt.keyCode == 97) && $('#main3').is(':visible')){
      button1('#main3','#main1');
    }
	});
