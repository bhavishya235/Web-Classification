		function recordOutboundLink(link, category) {
    try {
        _gaq.push(['_trackEvent', category, link.href]);
        setTimeout('document.location = "' + link.href + '"', 100)
        return true;
    } catch (err) { }
}
		(function($){$(function(){
$('a[href^="http://bham.hobsons.co.uk"]').click(function (e) {
    var success = recordOutboundLink(this, 'Outbound Links');
    if(success){
        e.preventDefault();
    }
})
$('a[href^="http://dres.bham.ac.uk"]').click(function (e) {
    var success = recordOutboundLink(this, 'Outbound Links');
    if(success){
        e.preventDefault();
    }
})
$('a[href^="https://pga.bham.ac.uk"]').click(function (e) {
    var success = recordOutboundLink(this, 'Outbound Links');
    if(success){
        e.preventDefault();
    }
})
