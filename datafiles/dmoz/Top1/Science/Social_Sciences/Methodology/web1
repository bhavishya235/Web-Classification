            var $LOG = YAHOO.Smb.Asteroids.Logger.Log;
        </script><title>Global Association for Research Methods and Data Science - Home</title></head><body class="lo_layout1"><div id="body"><div id="doc" class=""><div xmlns="" id="hd"><style>
