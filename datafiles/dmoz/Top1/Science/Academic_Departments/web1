<script language="JavaScript1.2">
function printpage(){
	var icerik = document.getElementById('icerik').innerHTML;
	document.body.innerHTML = '<br> URL : ' + window.location + ' :: ' + createdate() + ' Tarihinde alÄ±ndÄ± <br><hr>' + icerik + '<hr><br>Copyright Â© 2010 - Ä°stanbul Ãœniversitesi Enformatik BÃ¶lÃ¼mÃ¼';
	var printlink = document.getElementById('print');
	printlink.innerHTML = '<a href="'+window.location+'" onclick="printpage();return false;">Geri DÃ¶n</a>';
	window.print();
	return false;
}

function makeArray() {
	for (i = 0; i<makeArray.arguments.length; i++)
	this[i + 1] = makeArray.arguments[i];
}

function checkTime(i){
	if (i<10){
	  i="0" + i;
	}
	return i;
}

function createdate(){
	var months = new makeArray('Ocak','Åžubat','Mart','Nisan','MayÄ±s','Haziran', 'Temmuz','AÄŸustos','EylÃ¼l','Ekim','KasÄ±m','AralÄ±k');
	var date = new Date();
	var day = date.getDate();
	var month = date.getMonth() + 1;
	var yy = date.getYear();
	var year = (yy < 1000) ? yy + 1900 : yy;
	var h=checkTime(date.getHours());
	var m=checkTime(date.getMinutes());
	var s=checkTime(date.getSeconds());
	return day + " " + months[month] + " " + year + " - " +h+":"+m+":"+s;
}
</script>
