<script type="text/javascript" >
    function clickButton(e, buttonid) {
        var evt = e ? e : window.event;
        var bt = document.getElementById(buttonid);
        var myText = document.getElementById('SiteSearch1_txtbxSiteSeach');

        if (bt) {
            if (evt.keyCode == 13) {

                bt.click();
                window.location = "http://www.napier.ac.uk/Search/Results.aspx?k=" + myText.value;
                return false;
            }
        }
    }
</script>

