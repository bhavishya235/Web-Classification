                fx: 'fade',
                speed: 'fast',
                timeout: 0,
                pager: '#nav',
                next: '#next',
                prev: '#prev',
                before: onBefore,
                after: onAfter
            });

            function onBefore()
            {
                $('#vidfooter').html($(this).attr('title'));
            }
            function onAfter()
            {
                $('#vidfooter').html($(this).attr('title'));
            }


            $('#nav a:last').css('border-right', '1px dotted #FFFFFF');

            var ie55 = (navigator.appName == "Microsoft Internet Explorer" && parseInt(navigator.appVersion) == 4 && navigator.appVersion.indexOf("MSIE 5.5") != -1);
