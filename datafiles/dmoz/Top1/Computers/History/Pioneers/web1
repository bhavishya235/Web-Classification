                before: revBefore,
                after: revAfter
            });
            function revBefore()
            {
                //alert($(this).height());
                $('#reviews').css('visibility', 'visible');
                $(this).css('visibility', 'visible');
                //$('#reviews').height($(this).height());
            }
            function revAfter()
            {
                //alert($(this).height());
                $('#reviews').css('visibility', 'visible');
                $(this).css('visibility', 'visible');
                //$('#reviews').height($(this).height());
            }
        });
