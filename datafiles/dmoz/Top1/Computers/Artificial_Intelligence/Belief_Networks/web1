    <script type="text/javascript" src="http://ajax.aspnetcdn.com/ajax/jQuery/jquery-1.7.2.min.js"></script>
    <script type="text/javascript">
        var catalog = {
            init: function () {
                $(".symbol").click(function (e) {
                    $(this).text(($(this).text() == "+") ? "-" : "+");
                    $(this).nextAll().eq(1).toggle();
                });
                $(".showing span").click(function () {
                    $("input[name='ps']").val($(this).text());
                    $("input[name='p']").val(1);
                    catalog.go();
                });
                $(".selSort").change(function () {
                    $("input[name='sb']").val($(this).val());
                    $("input[name='p']").val(1);
                    catalog.go();
                });
                $(".pn").click(function () {
                    $("input[name='p']").val($(this).text());
                    catalog.go();
                });
                $("#leftCol .name").click(function () {
                    var a = $("input[name='t']").val().toLowerCase().split(","), t = $(this).children("span:first-child").text().toLowerCase(), index = $.inArray(t, a);
                    if (index == -1)
                        (a[0] == "") ? a[0] = t : a.push(t);
                    else {
                        a.splice(index, 1);
                        if (t != 'videos') {
                            $(this).next().find(".sel span:first-child").each(function () {
                                catalog.toggleArray($("input[name='sf']"), $(this).text());
                            });
                        }
                        else {
                            $(this).next().find(".sel span.vid").each(function () {
                                catalog.toggleArray($("input[name='vr']"), $(this).text());
                            });
                        }
                    }
                    $("input[name='t']").val(a.join(","));
                    $("input[name='p']").val(1);
                    catalog.go();
                });
                $(".subMenu span:first-child").click(function () {
                    var a = $("input[name='sf']").val().toLowerCase().split(","), t = $(this).text().toLowerCase(), index = $.inArray(t, a);
                    if (index == -1)
                        (a[0] == "") ? a[0] = t : a.push(t);
                    else
                        a.splice(index, 1);
                    $("input[name='sf']").val(a.join(","));
                    var p = $(this).parent().parent().prev();
                    if (!p.hasClass("sel"))
                        p.click();
                    else
                        catalog.go();
                });
                $(".subMenu.video span:first-child").unbind("click").click(function () {
                    var a = $("input[name='vr']").val().toLowerCase().split(","), t = $(this).children("span").text(), index = $.inArray(t, a);
                    if (index == -1)
                        (a[0] == "") ? a[0] = t : a.push(t);
                    else
                        a.splice(index, 1);
                    $("input[name='vr']").val(a.join(","));
                    var m = $(this).parent().parent().prev();
                    if (!m.hasClass("sel"))
                        m.click();
                    else {
                        $("input[name='p']").val(1);
                        catalog.go();
                    }
                });
                $("#leftCol .related").click(function () {
                    var a = $("input[name='r']").val().toLowerCase().split(","), t = $(this).children("span:first-child").children("span").text(), index = $.inArray(t, a);
                    if (index == -1)
                        (a[0] == "") ? a[0] = t : a.push(t);
                    else
                        a.splice(index, 1);
                    $("input[name='r']").val(a.join(","));
                    $("input[name='p']").val(1);
                    catalog.go();
                });
                $("#leftCol .raRelated").click(function () {
                    var a = $("input[name='ra']").val().toLowerCase().split(","), t = $(this).children("span:first-child").children("span").text(), index = $.inArray(t, a);
                    if (index == -1)
                        (a[0] == "") ? a[0] = t : a.push(t);
                    else
                        a.splice(index, 1);
                    $("input[name='ra']").val(a.join(","));
                    $("input[name='p']").val(1);
                    catalog.go();
                });
                $(".next").click(function () { catalog.pageDelta(1); });
                $(".previous").click(function () { catalog.pageDelta(-1); });
                $("#dpData .related .type").click(function () {
                    var t = $(this);
                    var c = t.siblings(".sel");
                    c.removeClass("sel");
                    if (c.text() != "")
                        t.siblings("." + c.text()).toggle();
                    t.toggleClass("sel");
                    t.siblings("." + t.text()).toggle();
                });
                $(".more").click(function () {
                    var t = $(this);
                    t.prevUntil(".picked").toggle();
                    t.text((t.text() == "More...") ? "Less..." : "More...");
                });
                $("#goMag").click(function () {
                    var v = $("#searchTextBox").val();
                    v = v.replace(/\u2018|\u2019|\u201A/g, "'").replace(/\u201c|\u201d|\u201e/g, '"');
                    if (v != "Keyword") {
                        $("input[name='s']").val(escape(v));
                        $("input[name='sb']").val("bm");
                    }
                    else {
                        $("input[name='s']").val("");
                        $("input[name='sb']").val("no");
                    }
                    $("input[name='p']").val(1);
                    catalog.go();
                });
                $("#searchTextBox").focus(function () {
                    if ($(this).val() == "Keyword") {
                        $(this).val("");
                        $(this).addClass("sel");
                    }
                });
                $("#searchTextBox").blur(function () {
                    if ($(this).val() == "") {
                        $(this).val("Keyword");
                        $("input[name='s']").val("");
                        $(this).removeClass("sel");
                        $("input[name='sb']").val("no");
                    }
                });
                $("#searchTextBox").keyup(function (event) {
                    if (event.keyCode == 13)
                        $("#goMag").click();
                });
                $("#dpData").css("min-height", ($("#leftCol").height() - $("#rightCol .shade").eq(1).height() - 9) + "px");
            },
            pageDelta: function (d) {
                var i = $("input[name='p']"), n = parseInt(i.val()) + d;
                if (n < 1) { return; }
                i.val(n);
                catalog.go();
            },
            go: function () {
                $("form").submit();
            },
            toggleArray: function (i, t) {
                var a = i.val().toLowerCase().split(","), t = t.toLowerCase(), index = $.inArray(t, a);
                if (index == -1)
                    (a[0] == "") ? a[0] = t : a.push(t);
                else
                    a.splice(index, 1);
                i.val(a.join(","));
            }
        };

        $(document).ready(function () { catalog.init(); });
    </script>
