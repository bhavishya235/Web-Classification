          <td><img name="publications_n_people_" src="graphics/publications_n_people_.gif" width="96" height="14" border="0"></td>
        </tr>
        <tr> 
          <td><a href="index.jsp?realpage=people" onMouseOut="MM_swapImgRestore()" onMouseOver="MM_swapImage('publications_n_people_','','graphics/people_above_over.gif','people_normal','','graphics/people_over.gif','people_n_conferences_n','','graphics/people_below_over.gif',1)"><img name="people_normal" src="graphics/people_normal.gif" width="96" height="37" border="0"></a></td>
