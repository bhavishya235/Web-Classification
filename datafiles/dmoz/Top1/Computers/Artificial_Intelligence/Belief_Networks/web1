  <tr>
    <td align="center" valign="top">
      <br> 
     <table width = 160 border="0" cellpadding="0" cellspacing="0" align="left">
         <!-- fwtable fwsrc="Button Normal State.png" fwbase="Button Normal.gif" fwstyle="Dreamweaver" fwdocid = "742308039" fwnested="0" -->
        <tr> 
          <td rowspan="15"><img src="graphics/spacer.gif" width="31" height="1"></td>
          <td><a href="index.jsp" onMouseOut="MM_swapImgRestore()" onMouseOver="MM_swapImage('home1_normal','','graphics/home_over.gif','home_n_publications_no','','graphics/home_below_over.gif',1)"><img name="home1_normal" src="graphics/home1_normal.gif" width="96" height="52" border="0"></a></td>
