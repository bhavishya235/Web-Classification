ons a{color:#fff;font-weight:bold;text-decoration:none;}div.icons ul{list-style-type:none;margin:0;padding:0;overflow:hidden;}div.icons li{float:left;}div.icons a:link, div.icons a:visited{display:block;width:15px;height:15px;padding:0px;margin:5px;text-align:center;}div.icons .google{background:#abcfff;}div.icons .google:hover{background:#4d90fe;}div.icons .bing{background:#e96;}div.icons .bing:hover{background:#ffa500;}div.icons .yahoo{background:#c299e6;}div.icons .yahoo:hover{background:#7b0099;}div.icons a.twitter{background:#c0deed;}div.icons .twitter:hover{background:#099;}div.icons a.facebook{background:#69f;}div.icons .facebook:hover{background:#039;}div.icons a.linkedin{background:#a6a6a6;}div.icons .linkedin:hover{background:#0077b5;}div.icons table a{color:#357ec7;text-decoration:none;font-weight:normal;}div.icons table a:link, table div.icons a:visited{display:inline;padding:0px;margin:0px;}.bbottom{border-bottom:1px solid #fcc;}.submit-url{font-size:0.8em;font-weight:bold;}.big{font-size:1.2em;}.small{font-size:1.05em;}.smaller{font-size:0.9em;}div.news p{padding:1em;border-top:1px solid #fcc;background:#f2f2f2;}.date{color:#00853f;}</style>
