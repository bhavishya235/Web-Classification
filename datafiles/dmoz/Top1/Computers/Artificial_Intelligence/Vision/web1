<script type="text/javascript">
    $(function() {
		/**
			* the menu
			*/
		var $menu = $('#cmm_menu');
				
		/**
			* for each list element,
			* we show the submenu when hovering and
			* expand the span element (title) to 510px
			*/
		$menu.children('li').each(function(){
			var $this = $(this);
			var $span = $this.children('span');
			$span.data('width',$span.width());
					
			$this.bind('mouseenter',function(){
				$menu.find('.cmm_submenu_0, .cmm_submenu_1, .cmm_submenu_2, .cmm_submenu_3, .cmm_submenu_4, .cmm_submenu_5, .cmm_submenu_6, .cmm_submenu_7').stop(true,true).hide();
				$span.stop().animate({'width':$span.data('width')+'px'},25,function(){
					$this.find('.cmm_submenu_0, .cmm_submenu_1, .cmm_submenu_2, .cmm_submenu_3, .cmm_submenu_4, .cmm_submenu_5, .cmm_submenu_6, .cmm_submenu_7').slideDown(25);
							
				});
			}).bind('mouseleave',function(){
				$this.find('.cmm_submenu_0, .cmm_submenu_1, .cmm_submenu_2, .cmm_submenu_3, .cmm_submenu_4, .cmm_submenu_5, .cmm_submenu_6, .cmm_submenu_7').slideUp(25);
				$span.stop().animate({'width':$span.data('width')+'px'},25);
						
			});
		});
    });
</script>

