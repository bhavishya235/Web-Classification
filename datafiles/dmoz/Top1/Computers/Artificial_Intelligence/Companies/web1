	<script type="text/javascript">
	
	 var image = document.getElementById("HeaderEventImage");
	 var images = ["AirportHeaderBg.png", "TornadoHeaderBg.png","FloodHeaderBg.png", "SchoolHeaderBg.png", "FireHeaderBg.png"];
	 var numImages = images.length;
	 var index = numImages;
	 var changer;
	 
	 switchHeaderImage();
	 
	function switchHeaderImage(){
		if (index < numImages-1){
			index ++;
		}else{
			index = 0;
		}
		image.src = "/Images/" + images[index];
		changer = setTimeout(switchHeaderImage, 5000);
	}
	
	</script>
	
