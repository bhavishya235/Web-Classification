					<script type="text/javascript">
						
						function startTicker(){
							var tickerDiv = document.getElementById("ticker");
							tickerDiv.style.display = "";
							
							var tickerImg = document.getElementById("tickerImage");
							tickerImg.src = "";
							tickerImg.src = "/Images/ticker2.gif";
						}
						
						function startPopupAlert(){
							var popupAlertDiv = document.getElementById("popupAlert");
							popupAlertDiv.style.display = "";
						}
						
						function startVoipAlert(){
							var voipAlertDiv = document.getElementById("voipAlert");
							voipAlertDiv.style.display = "";
						}
						
						function startGalaxyAlert(){
							var galaxyAlertDiv = document.getElementById("galaxyAlert");
							galaxyAlertDiv.style.display = "";
						}
						
						var randomnumber=Math.floor(Math.random()*2);
						//alert(randomnumber);
							
						//if (randomnumber > 0){
							window.setTimeout("startPopupAlert()", 2000);
						//}else{
							window.setTimeout("startTicker()", 2050);
						//}
							window.setTimeout("startVoipAlert()", 2100);
							window.setTimeout("startGalaxyAlert()", 2150);
					</script>

