function tjcLoginClick()
{
toggleLogin('PopUpLogin');
	 var pos = YAHOO.util.Dom.getXY('tjcLogin');
	 pos[0] -= 170;
	 pos[1] += 25;
	 YAHOO.util.Dom.setXY('PopUpLogin', pos);
}
