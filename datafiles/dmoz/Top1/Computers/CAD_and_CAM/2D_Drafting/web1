 function checkEmail(myForm) {
		if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(subs.contact_email.value)){
		return (true)
		}
		alert("That email address seems to be invalid.")
	return (false)
	}
</script>
