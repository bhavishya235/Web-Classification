                     var teasersJson={"defaultTeaser":{"country":"US","teaserPath":"/content/campaigns/autodesk/meganav-products/united-states"},"teasers":[{"country":"AR","teaserPath":""},{"country":"AU","teaserPath":""},{"country":"AT","teaserPath":""},{"country":"BE","teaserPath":""},{"country":"BO","teaserPath":""},{"country":"BR","teaserPath":""},{"country":"BG","teaserPath":""},{"country":"CA","teaserPath":""},{"country":"CL","teaserPath":""},{"country":"CO","teaserPath":""},{"country":"CR","teaserPath":""},{"country":"HR","teaserPath":""},{"country":"CZ","teaserPath":""},{"country":"DK","teaserPath":""},{"country":"EC","teaserPath":""},{"country":"EG","teaserPath":""},{"country":"FR","teaserPath":""},{"country":"DE","teaserPath":""},{"country":"GR","teaserPath":""},{"country":"HK","teaserPath":""},{"country":"HU","teaserPath":""},{"country":"IN","teaserPath":""},{"country":"ID","teaserPath":""},{"country":"IE","teaserPath":""},{"country":"IL","teaserPath":""},{"country":"IT","teaserPath":""},{"country":"JP","teaserPath":""},{"country":"KZ","teaserPath":""},{"country":"KR","teaserPath":""},{"country":"KW","teaserPath":""},{"country":"LB","teaserPath":""},{"country":"LT","teaserPath":""},{"country":"MY","teaserPath":""},{"country":"YT","teaserPath":""},{"country":"MX","teaserPath":""},{"country":"MA","teaserPath":""},{"country":"NL","teaserPath":""},{"country":"NZ","teaserPath":""},{"country":"NO","teaserPath":""},{"country":"OM","teaserPath":""},{"country":"PA","teaserPath":""},{"country":"PY","teaserPath":""},{"country":"PE","teaserPath":""},{"country":"PH","teaserPath":""},{"country":"PL","teaserPath":""},{"country":"PT","teaserPath":""},{"country":"QA","teaserPath":""},{"country":"RO","teaserPath":""},{"country":"RU","teaserPath":""},{"country":"SA","teaserPath":""},{"country":"SG","teaserPath":""},{"country":"SK","teaserPath":""},{"country":"SI","teaserPath":""},{"country":"ZA","teaserPath":""},{"country":"ES","teaserPath":""},{"country":"LK","teaserPath":