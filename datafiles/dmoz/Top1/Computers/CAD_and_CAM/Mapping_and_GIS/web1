<script language="JavaScript" type="text/javascript"><!--
/* You may give each page an identifying name, server, and channel on
    the next lines. */
    var url = document.location.pathname.toLowerCase().replace(/%20/g, "+"); // this is the file structure + the file name
 
    var host = window.location.hostname.toLowerCase();
    var lastChar ="";
    var actualPageName="";
    
    if (url) {
       var temp = s.split(url, '/');//temp[0] will be ""

        if (temp.length > 0) {
        
        
    
        //for localhost and dev cms site
            if ((host == "localhost" || host =="dev-www.bentley.com" || host =="grabk35340ext.bentley.com") && (url.indexOf("bentleywebsite") != -1 || url.indexOf("dev-www.bentley.com") != -1)) {
            
                s.prop2 = "bentley";//top channel
                            
                if (temp[1] && temp[2]) {
                    lastItem = temp[temp.length -1]
                    if(temp[1].indexOf("dev-www.bentley.com") != -1){
                        s.prop1 = temp[2]; //region
                    }
                    else{
                        var langcookie = "currentlang";
                        var thiscookie = document.cookie.indexOf(langcookie);
                            if(thiscookie != -1)
                            {
                                var c_start=thiscookie+langcookie.length+1;
                                
                                var region =document.cookie.substring(c_start,c_start+5);
                                s.prop1 = region.toLowerCase(); //region
                            }
                    }
 
                    
                    s.channel = temp[2];

                    if (url.indexOf(".htm") == -1 && url.indexOf(".aspx") == -1){
                        var postingUrl = "/en-us/index.htm";
                        var urlWithExtension = url + ".htm";
                        
                        if(postingUrl != urlWithExtension){ //compare posting url to document.url. if same, don't need actualpagename
                            actualPageName = "index";       
                        }
                    }
                    else {
                        actualPageName = temp[temp.length - 1];
                        temp.splice(temp.length - 1, 1); //so pagename is not part of the channel hierarchy

                        var dotInd = actualPageName.lastIndexOf('.');
                        if (dotInd > 0) {
                            actualPageName = actualPageName.substring(0, dotInd);
                        }

                        if (url.indexOf("/tools/library.aspx") != -1) {
                        
                            var taggedcontentId = "";
                            var eventId = "";
                            
                            if(taggedcontentId != null && taggedcontentId != "")
                            {
                                actualPageName += " id=" + taggedcontentId;
                            }
                            else if(eventId != null && eventId != "")
                            {
                                actualPageName += " event=" + eventId;
                            }
                        }
                    }
                    
                                       
                    if (temp[3]) {
                        s.prop3 = temp[3];

                        if (temp[4]) {
                            s.prop4 = temp[4];

                            if (temp[5]) {
                                s.prop5 = temp[5];
                                if (temp[6]) {
                                    s.prop6 = temp[6];
                                    if (temp[7]) {
                                        s.prop7 = temp[7];
                                    }
                                    else {
                                        s.prop7 = s.prop6;
                                    }
                                }
                                else {
                                    s.prop6 = s.prop5;
                                    s.prop7 = s.prop5;
                                }
                            }
                            else {
                                s.prop5 = s.prop4;
                                s.prop6 = s.prop4;
                                s.prop7 = s.prop4;
                            }
                        }
                        else {
                            s.prop4 = s.prop3;
                            s.prop5 = s.prop3;
                            s.prop6 = s.prop3;
                            s.prop7 = s.prop3;
                        }
                    }
                    else{
                        s.prop3 = s.channel;
                        s.prop4 = s.channel;
                        s.prop5 = s.channel;
                        s.prop6 = s.channel;
                        s.prop7 = s.channel;
                    }

                    var i = 2;
                    var pagen = "";
                    for (i = 2; i < temp.length; i++) {
                        pagen += temp[i];
                        if (i != temp.length - 1) {
                            pagen += ":";
                        }
                    }
                    if(actualPageName != ""){
                        pagen += ":" + actualPageName;
                        }
                    s.pageName = pagen;
                    
                }
            }
            else if(host == "qa-www.bentley.com" || host == "www.bentley.com") {
                s.prop2 = "bentley";//top channel
                       
                if (temp.length == 2) {//has to be region root www.bentley.com/en-us/, etc.
                    s.prop1 = temp[1]; //region
                    s.prop3 = "home";
                    s.prop4 = "home";
                    s.prop5 = "home";
                    s.prop6 = "home";
                    s.prop7 = "home";
                    s.channel = "home";

                    if (url.indexOf(".htm") == -1 && url.indexOf(".aspx") == -1) {
                        var postingUrl = "/en-us/index.htm";
                        var urlWithExtension = url + ".htm";
                        
                        if(postingUrl != urlWithExtension){
                            actualPageName = "index";       
                        }
                    }
                    s.pageName = s.prop1 + ":" + actualPageName;
                }
                else {
                    if (url.indexOf(".htm") == -1 && url.indexOf(".aspx") == -1) {//if no extension, get posting URL
                        var postingUrl = "/en-us/index.htm";
                        var urlWithExtension = url + ".htm";
                        
                        if(postingUrl != urlWithExtension){ //compare posting url to document.url. if same, don't need actualpagename
                            actualPageName = "index";       
                        }
                    }
                    else {
                        actualPageName = temp[temp.length - 1];
                        temp.splice(temp.length - 1, 1);

                        var dotInd = actualPageName.lastIndexOf('.');
                        if (dotInd > 0) {
                            actualPageName = actualPageName.substring(0, dotInd);
                        }
                        
                        if (url.indexOf("/tools/library.aspx") != -1) {
                            var taggedcontentId = "";
                            var eventId = "";
                            
                            if(taggedcontentId != null && taggedcontentId != "")
                            {
                                actualPageName += " id=" + taggedcontentId;
                            }
                            else if(eventId != null && eventId != "")
                            {
                                actualPageName += " event=" + eventId;
                            }
                        }
                    }
                    
                    if (temp[1] && temp[2]) {
                        if(temp[1].indexOf("bentleywebsite") != -1){
                            var langcookie = "currentlang";
                            var thiscookie = document.cookie.indexOf(langcookie);
                            if (thiscookie != -1) {
                                var c_start = thiscookie + langcookie.length + 1;

                                var region = document.cookie.substring(c_start, c_start + 5);
                                s.prop1 = region.toLowerCase(); //region
                            }
                        }
                        else{
                             s.prop1 = temp[1]; //region
                        } 
                        
                        s.channel = temp[2];
                                           
                        if (temp[3]) {
                            s.prop3 = temp[3];

                            if (temp[4]) {
                                s.prop4 = temp[4];

                                if (temp[5]) {
                                    s.prop5 = temp[5];
                                    if (temp[6]) {
                                        s.prop6 = temp[6];
                                        if (temp[7]) {
                                            s.prop7 = temp[7];
                                        }
                                        else {
                                            s.prop7 = s.prop6;
                                        }
                                    }
                                    else {
                                        s.prop6 = s.prop5;
                                        s.prop7 = s.prop5;
                                    }
                                }
                                else {
                                    s.prop5 = s.prop4;
                                    s.prop6 = s.prop4;
                                    s.prop7 = s.prop4;
                                }
                            }
                            else {
                                s.prop4 = s.prop3;
                                s.prop5 = s.prop3;
                                s.prop6 = s.prop3;
                                s.prop7 = s.prop3;
                            }
                        }
                        else{
                            s.prop3 = s.channel;
                            s.prop4 = s.channel;
                            s.prop5 = s.channel;
                            s.prop6 = s.channel;
                            s.prop7 = s.channel;
                        }

                        var i = 1;
                        var pagen = "";
                        for (i = 1; i < temp.length; i++) {
                            pagen += temp[i];
                            if (i != temp.length - 1) {
                                pagen += ":";
                            }
                        }
                        if(actualPageName != ""){
                                pagen += ":" + actualPageName;
                            }
                        s.pageName = pagen;
                    }
                }
            }
            var templateName = "homepage2010";
            if( templateName != ""){
            
                if(templateName.toLowerCase() !="registration" && templateName.toLowerCase() !="webcast" && templateName.toLowerCase() !="confirmation" && templateName.toLowerCase() !="confirmation_nonav"){
                    CallBentleySiteCatalystWebService(ServiceCallbackToSendImageRequestToSiteCatalyst);
                }
            }
            else if(actualPageName != "library" && actualPageName.indexOf("library id=") == -1 && actualPageName.indexOf("library event=") == -1 && actualPageName != "sales_contact" && actualPageName != "sales_thank_you" && actualPageName !="academic_contact" && actualPageName !="bao_contact" && actualPageName !="apdemo" && actualPageName !="contact" && actualPageName !="emailverification" && url.indexOf("/search/results.aspx")== -1){
                CallBentleySiteCatalystWebService(ServiceCallbackToSendImageRequestToSiteCatalyst);
            }
             
					    
        }
    }

/************* DO NOT ALTER ANYTHING BELOW THIS LINE ! **************/
//--></script>
