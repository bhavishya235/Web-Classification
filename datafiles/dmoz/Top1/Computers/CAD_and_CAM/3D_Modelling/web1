            <!-- NEWSLETTER -->
            <script>

  $(document).ready(function(){
    $("#mc-embedded-subscribe-form").submit(signupUserForPromoEmails)
  });

  function signupUserForPromoEmails()
  {
    var $email = encodeURIComponent($("#mce-EMAIL").val());
    var $data = "email=" + $email
      + "&confirmation=footer"
      + "&tracking_type=footer"
      + "&tracking_location="+document.location.pathname;

    $request = $.ajax({
      type:"POST",
      async:true,
      url: '/register/email-signup',
      data: $data,
      dataType: 'json',
      success: onPromoEmailSignupDone
    });

    return false;
  }

  function onPromoEmailSignupDone(data)
  {
    if(data['response_status'] == "failure")
    {
      $("#mce-FormError").text(data['response_text']);
      $("#mce-FormSuccess").hide();
      $("#mce-FormError").show();
    }
    else
    {
      $("#mce-FormSuccess").text(data['confirmation_content']);
      $("#mce-FormError").hide();
      $("#mce-FormSuccess").show();
    }
  }


</script>


