<script type="text/javascript">//<![CDATA[
function FWImage_Indigo(chain,msg)
{
	if (chain==1)
		FWShowHideLayerFade("Ariv2",msg,msg);
	else 
		FWShowHideLayerFade("Ariv2",!msg,msg);
	if (chain==2)
		FWShowHideLayerFade("LCatsTent",msg,msg);
	else 
		FWShowHideLayerFade("LCatsTent",!msg,msg);
	if (chain==3)
		FWShowHideLayerFade("MBubbleTent",msg,msg);
	else 
		FWShowHideLayerFade("MBubbleTent",!msg,msg);
	if (chain==4)
		FWShowHideLayerFade("GPictOnSails",msg,msg);
	else 
		FWShowHideLayerFade("GPictOnSails",!msg,msg);
	if (chain==5)
		FWShowHideLayerFade("HImageUnfold",msg,msg);
	else 
		FWShowHideLayerFade("HImageUnfold",!msg,msg);
	if (chain==6)
		FWShowHideLayerFade("Rmodels",msg,msg);
	else 
		FWShowHideLayerFade("Rmodels",!msg,msg);
	if (chain==7)
		FWShowHideLayerFade("TPeterPlate",msg,msg);
	else 
		FWShowHideLayerFade("TPeterPlate",!msg,msg);
	if (chain==8)
		FWShowHideLayerFade("Sindustrial",msg,msg);
	else 
		FWShowHideLayerFade("Sindustrial",!msg,msg);
}
window.FT_Indigo=new Object;
window.FT_Indigo['0']=FWImage_Indigo;
//]]></script></body>
