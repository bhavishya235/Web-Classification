  <div id="footer-social-newsletter">
    <form id="mc-embedded-subscribe-form" name="mc-embedded-subscribe-form" class="validate newsletter-form">
      <h5>Get 3D Printed Inspiration</h5>
      <p class="bottom">Weekly top designs, exclusive offers, tips, and tricks.</p>
      <div class="newsletter-text-field">

        <input type="text" value="" name="EMAIL" class="input" id="mce-EMAIL" placeholder="Email address..." required="" autocomplete="off" style="width:160px;">

        <input type="submit" value="Sign Up" name="subscribe" id="mc-embedded-subscribe" class="secondary-button-inset">
        <div id="mce-FormError" class="text-error" style="display:none"></div>
        <div id="mce-FormSuccess"  class="text-success" style="display:none;"></div>
      </div>
    </form>
  </div>
</div>


