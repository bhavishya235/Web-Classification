<script language="javascript" type="text/javascript">

   var i=0;
   var adsNum = 0 + 1;
   var finished=false;
   var paused=false;
   var running=false; 
   
   var fade = 0;
	function fade_in()
	{
		if(fade != 100)
		{
		fade += 5;
		document.getElementById('adContainer').style.MozOpacity = fade/100;
		} else if (fade == 100)  {
			fade=0;
			return;
		}
		setTimeout("fade_in()", 100)
	}

   function loadPics()
   {  
	  ads=new Array();
	  	  ads[0]="GeoMine v2.3 delivers enhanced Dynamic Anisotropy!";
	  	  
	  
	  fcolor=new Array();
	  	  fcolor[0]="#ffffff";
	  	  
      fsize=new Array();
	  	  fsize[0]="18px;line-height:21px";
	  	  
	  bgs=new Array();
	  	  bgs[0]="#3399ff";
	  	  
	  links=new Array();
	  	  links[0]="http://www.threedify.com/en/news-articles/275-threedify-geomodelerv-23-released";
	  	  
	  wins=new Array();
	  	  wins[0]="_self";
	  	  
	  startSlide();
   }
   

   function startSlide()
   {
	  
	  document.getElementById('adContent').innerHTML= ads[i];
	  document.getElementById('linkage').setAttribute("href",links[i]);
	  document.getElementById('linkage').setAttribute("target",wins[i]);
	  document.getElementById('adContent').setAttribute("style","color:" + fcolor[i] + ";font-weight:normal;font-size:" + fsize[i] + ";padding:5px;font-family: Arial, Helvetica, sans-serif;");
	  
	  

	   
	  
	  document.getElementById('adTable').setAttribute("style","background-color:" + bgs[i] + ";border-radius: 15px;-moz-border-radius: 15px;height:50px;overflow:hidden;-moz-box-shadow: 3px 2px 2px gray;-webkit-box-shadow: 3px 2px 2px gray;box-shadow: 3px 2px 2px gray;");
	 
	 <!--[if IE]> document.getElementById('adTable').style.setAttribute("background-color","#FF0000"); <![endif]-->
	 
	  
	  
      running=true;
      if(navigator.appVersion.indexOf("MSIE") == -1)
      {
         interval = setInterval(FFSlideshow,7500);
      }
      else
      {
         interval = setInterval(slideshow,7500);
      }
	  i++;
	  
   }
   function slideshow()
   {
      if (i<adsNum)
      {
         document.getElementById('adContainer').filters[0].Apply();
		 document.getElementById('adContent').innerHTML= ads[i];
		 document.getElementById('adContent').setAttribute("style","color:" + fcolor[i] + ";font-weight:normal;font-size:" + fsize[i] + ";padding:5px;font-family: Arial, Helvetica, sans-serif;");
		 
		  
		 
		 document.getElementById('adTable').setAttribute("style","background-color:" + bgs[i] + ";border-radius: 15px;-moz-border-radius: 15px;height:50px;overflow:hidden;-moz-box-shadow: 3px 2px 2px gray;-webkit-box-shadow: 3px 2px 2px gray;box-shadow: 3px 2px 2px gray;");
		 
		 document.getElementById('cell').style.setAttribute("background-color","#00FF00");
		 
		 
		 document.getElementById('linkage').setAttribute("href",links[i]);
		 document.getElementById('linkage').setAttribute("target",wins[i]);
         document.getElementById('adContainer').filters[0].Play();
		 i++;
      }
      else if (i==adsNum && finished==false)
      {
         clearInterval(interval);
		 i=0;
		 startSlide();
      }
      else
      {
         document.getElementById('adContainer').filters[0].Apply();
         finished=false;
         document.getElementById('adContainer').filters[0].Play();
      }
   }
   function FFSlideshow()
   {
      if (i<adsNum)
      {
		 document.getElementById('adContent').innerHTML= ads[i];
		 document.getElementById('adContent').setAttribute("style","color:" + fcolor[i] + ";font-weight:normal;font-size:" + fsize[i] + ";padding:5px;font-family: Arial, Helvetica, sans-serif;");
		 document.getElementById('adTable').setAttribute("style","background-color:" + bgs[i] + ";border-radius: 15px;-moz-border-radius: 15px;height:50px;overflow:hidden;-moz-box-shadow: 3px 2px 2px gray;-webkit-box-shadow: 3px 2px 2px gray;box-shadow: 3px 2px 2px gray;");
		 

		 
		 
		 
		 document.getElementById('linkage').setAttribute("href",links[i]);
		 document.getElementById('linkage').setAttribute("target",wins[i]);
		 fade_in();
		 i++
		 
      }
      else if (i==adsNum && finished==false)
      {
         clearInterval(interval);
		 i=0;
		 startSlide();
      }
      else
      {
         i=0;
         finished=false;
      }
   }
   window.onload = loadPics;
</script>

