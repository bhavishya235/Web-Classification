		    <script type="text/javascript">
		        function ValidateRequestTrial() {
		            var error = "";
		            var form = document.getElementById('trial_form');
		            if (form.first_name.value == "") {
		                error += "- First Name is required field.\n"
		            }
		            if (form.last_name.value == "") {
		                error += "- Last Name is required field.\n"
		            }
		            if (form.email.value == "") {
		                error += "- Email is required field.\n"
		            }
		            if (validateEmail(form.email.value) == false) {
		                error += "- Email has incorrect format.\n"
		            }
		            if (form.company.value == "") {
		                error += "- Company/School is required field.\n";
		            }
		            if (form.industry.value == "") {
		                error += "- Industry is required field.\n";
		            }
		            if (form.country.value == "") {
		                error += "- Country is required field.\n";
		            }
		            if (form.state.value == "") {
		                error += "- State/Province is required field.\n";
		            }
		            if (form.phone.value == "") {
		                error += "- Phone is required field.\n";
		            }
		            if (document.getElementById("t_00N40000001bbx9").value == "") {
		                error += "- 'Subscribe to solidThinking Newsletter?' is required field.\n";
		            }
		            if (document.getElementById("t_00N40000001bc1A").value == "") {
		                error += "- Product is required field.\n";
		            }
		            if (document.getElementById("t_00N40000001bbyJ").value == "") {
		                error += "- Operating System is required field.\n";
		            }
		            if (document.getElementById("00N40000002ntRX").value == "") {
		                error += "- Primary CAD Provider is required field.\n";
		            }

		            function isHTML(str) {
		                var a = document.createElement('div');
		                a.innerHTML = str;
		                for (var c = a.childNodes, i = c.length; i--; ) {
		                    if (c[i].nodeType == 1) return true;
		                }
		                return false;
		            }

		            // check for validation errors
		            if (error != "") {
		                alert(error);
		                return false;
		            } else if (IsSpam(form)) {
		                return false;
		            } else {
		                return true;
		            }

		        }
		        function validateEmail(email) {
		            var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
		            return re.test(email);
		        } 
            </script>
		</form>
	</div><!-- end #trial -->
	
    
