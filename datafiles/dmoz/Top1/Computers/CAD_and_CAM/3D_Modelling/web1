    <!-- SalesForce and Google Analytics integration http://www.e-nor.com/blog/google-analytics/integrate-salesforce-and-google-analytics -->
    <script type="text/javascript">
        var z = _uGC(document.cookie, '__utmz=', ';');
        var source = _uGC(z, 'utmcsr=', '|');
        var medium = _uGC(z, 'utmcmd=', '|');
        var term = _uGC(z, 'utmctr=', '|');
        var content = _uGC(z, 'utmcct=', '|');
        var campaign = _uGC(z, 'utmccn=', '|');
        var gclid = _uGC(z, 'utmgclid=', '|');
        if (gclid != "-") {
            source = 'google';
            medium = 'cpc';
        }

        var csegment = _uGC(document.cookie, '__utmv=', ';');
        if (csegment != '-') {
            var csegmentex = /[1-9]*?\.(.*)/;
            csegment = csegment.match(csegmentex);
            csegment = csegment[1];

        } else {
            csegment = '';
        }

        function _uGC(l, n, s) {
            if (!l || l == "" || !n || n == "" || !s || s == "") return "-";
            var i, i2, i3, c = "-";
            i = l.indexOf(n);
            i3 = n.indexOf("=") + 1;
            if (i > -1) {
                i2 = l.indexOf(s, i); if (i2 < 0) { i2 = l.length; }
                c = l.substring((i + i3), i2);
            }
            return c;
        }

        document.short_trial_form.txt_medium.value = medium; /* Campaign_Medium */
        document.short_trial_form.txt_source.value = source; /* Campaign_Source */
        document.short_trial_form.txt_campaign_name.value = campaign; /* Campaign_CampaignName */
        document.short_trial_form.txt_term.value = term; /* Campaign_Term */
        document.short_trial_form.txt_content.value = content; /* Campaign_Content */

        function IsSpam(form) {

            if (form.first_name.value == form.last_name.value) {
                return true;
            }
            if (form.company.value == form.title.value) {
                return true;
            }

            if (isHTML(form.description.value)) {
                return true;
            }

            if (form.link.value != "") {
                return true;
            }

            if (form.info.value != "") {
                return true;
            }
            return false;
        }
</script>
