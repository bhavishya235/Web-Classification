<script type="text/javascript">
var userlocationCookie="";
    $(function() {
        var overlay         = $('#pbl-alert-overlay').first();
        var target          = $('#adsk-global-control').first();
        var geolocation     = common.cookie.getCookie('geolocation/address/countryCode');
       if(geolocation==='undefined' || geolocation == undefined){
            geolocation     = common.cookie.getClientCookie('userlocation');
            if(geolocation==='undefined' || geolocation == undefined || geolocation==""){
                common.geolocation.initiateGeoLocation();
                PBLAlertOverlay = adsk.general.PBLAlertOverlay( overlay, target);
            }else{
            	//If the country is already exist in the cookie, then set the site-selector url to the WorldWide tab.
                $('#adsk-global-control').attr('href', '/content/autodesk/global/en/site-selector.html');
            }
        }else{
            common.cookie.setClientCookie('userlocation',geolocation,365);
        }
        
    });
    userlocationCookie=common.cookie.getClientCookie('userlocation');
</script>

