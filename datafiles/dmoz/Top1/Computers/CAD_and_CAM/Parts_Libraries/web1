			},
	doDisableMenu: function(name)
	{
		var sub_menu_item = $$('.'+ name)[0];
		if(sub_menu_item !== null && sub_menu_item !== undefined)
		{
			sub_menu_item.parentNode.style.display = 'none';
		}
	},
