			function submitSiteSearch()
			{
				with (document.frmSiteSearch)
				{
					keyword.value = keyword.value.trim()
					
					if (keyword.value.length > 0)
					{
						var intIndex = 0;
						var blnValueFound = false;
						
						while ((! blnValueFound) && (intIndex < rdbSearchSection.length))
						{
							if (rdbSearchSection[intIndex].checked)
							{
								action = rdbSearchSection[intIndex].value + ".asp";
								blnValueFound = true;
							}
							
							intIndex++;
						}
						
						if (blnValueFound)
						{
							submit();
						}
					}
					else
					{
						alert("Please enter a brief description of the symbol you are looking for.");
						keyword.focus();
					}
				}
			}
		</script>
