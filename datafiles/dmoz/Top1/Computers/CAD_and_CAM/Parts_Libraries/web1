<script type="text/javascript">
		//$("body").ActivDebug();	
        $(document).ready(function(){

            $(".flagPrinc").click(function(e){
                e.stopPropagation();  
                if($(".flagsSecond").css("display") == "none"){
                    $(".flagsSecond").show("normal");
                    $(".flagsSecond").css("left",$(this).offset().left);
                    $(".flagsSecond").css("top",$(this).offset().top+$(this).height());
                }else{
                    $(".flagsSecond").hide("fast");
                }
            });
            
            $(".flSecond1").hover(function(){
                $(this).addClass("flSecondHover");
            },function(){
                $(this).removeClass("flSecondHover");
            });
            
            $(".flSecond2").hover(function(){
                $(this).addClass("flSecondHover");
            },function(){
                $(this).removeClass("flSecondHover");
            }); 
	
            $(document).click(function(e){
               var cn = e.target.className;
               $(".flagsSecond").hide("fast");
            });			 
        }); 
</script>		

