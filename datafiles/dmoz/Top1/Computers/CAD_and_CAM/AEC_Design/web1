<script language="javascript">

	function runSearch()
	{
	
		var search = document.frmSearch.txtSearchInput.value;
		document.frmSearch.txtSearchInput.focus();
		
		if( search == "" ) return false;
	
		if( search == "Search CADdetails" ) return false;

		if( search.length < 2 )
		{
			alert("Search must contain 2 or more characters.")
			return false;
		}
		if( search == "and" || search == "AND" || search == "And" || search == "and " )
		{
			alert("Invalid search phrase. Please be more specific.")
			return false;
		}
	
		while(search!=search.replace(" and ", " "))
			search=search.replace(" and ", "______");

		while(search!=search.replace("+", "___plus___"))
			search=search.replace("+", "___plus___");
			
		document.frmSearch.search.value = search;
		document.frmSearch.txtSearchInput.value = "";
		
		return true;
		
	}
	
	function searchTop10(strSearchTerm)
	{
		if (strSearchTerm != "")
		{
			document.frmSearch.search.value = strSearchTerm;
			document.frmSearch.txtSearchInput.value = "";
			document.frmSearch.submit();
		}
	}
	
	function quickLink(strQuickLinkURL)
	{
		if (strQuickLinkURL != "")
		{
			document.frmQuickLinks.txtQuickLink.value = strQuickLinkURL;
			document.frmQuickLinks.submit();
		}
	}
	
	function logout()
	{
		document.frmLogout.submit();
	}
	
	function adjustCommunity(strNewCommunity)
	{
		document.frmCommunity.txtNewCommunity.value = strNewCommunity;
		document.frmCommunity.submit();
	}
	function showHideDiv(obj)
	{
		if (obj.style.display == "none") { obj.style.display = ""; }
		else { obj.style.display = "none"; }
	}

	function showHideExpandTable(image, obj)
	{
		showHideDiv(obj);

		// Change the image.
		if (image.src == "http://www.caddetails.com/2004/images/arrow_up.gif")
			image.src = "http://www.caddetails.com/2004/images/arrow_down.gif";
		else
			image.src = "http://www.caddetails.com/2004/images/arrow_up.gif";

	}
</script>
<SCRIPT LANGUAGE='JAVASCRIPT' TYPE='TEXT/JAVASCRIPT'>
 <!--
/****************************************************
     AUTHOR: WWW.CGISCRIPT.NET, LLC
     URL: http://www.cgiscript.net
     Use the code for FREE but leave this message intact.
     Download your FREE CGI/Perl Scripts today!
     ( http://www.cgiscript.net/scripts.htm )
****************************************************/
var win=null;
function NewWindow(mypage,myname,w,h,pos,infocus){
if(pos=="random"){myleft=(screen.width)?Math.floor(Math.random()*(screen.width-w)):100;mytop=(screen.height)?Math.floor(Math.random()*((screen.height-h)-75)):100;}
if(pos=="center"){myleft=(screen.width)?(screen.width-w)/2:100;mytop=(screen.height)?(screen.height-h)/2:100;}
else if((pos!='center' && pos!="random") || pos==null){myleft=0;mytop=20}
settings="width=" + w + ",height=" + h + ",top=" + mytop + ",left=" + myleft + ",scrollbars=yes,location=no,directories=no,status=no,menubar=no,toolbar=no,resizable=yes";win=window.open(mypage,myname,settings);
win.focus();}
// -->
</SCRIPT>
