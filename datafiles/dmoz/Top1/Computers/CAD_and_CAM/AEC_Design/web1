function PlayIt(anObj){
		// check if method if available 
		if (anObj.controls.isAvailable('Play'))
        // start playback
        anObj.controls.play();
		document.getElementById("vidImage").style.display = 'none';
		document.getElementById("video").style.display = 'block';
}

function StopIt(anObj) {
		// check if method if available 
		if (anObj.controls.isAvailable('Stop'))
		// stop playback
		anObj.controls.stop();
		document.getElementById("video").style.display = 'none';
		document.getElementById("vidImage").style.display = 'block'; 
		}
		
function PauseIt(anObj) {
