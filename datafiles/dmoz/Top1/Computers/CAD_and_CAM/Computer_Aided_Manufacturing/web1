		<td valign="top" align="center" width="100%" height="100%">


<!-- page content section tables -->

<!-- cnc controls banner table -->
<table width="100%" border="0" cellpadding="0" cellspacing="0" id="cnccontrols_banner">
    <tr>
        <td valign="middle" align="center" ><a href="http://www.cnccontrols.com/"><img src="images/banner5.gif" alt="Click Here To Visit Our PC Based CNC COntrols Website"/></a><br />
          <a href="http://www.cnccontrols.com/"><img src="images/banner6.gif" alt="Click Here To Visit Our PC Based CNC COntrols Website"/></a><h4 align="center" style="font-family:Verdana, Arial, Helvetica, sans-serif; font-size:12pt; color:#0000FF;">PC Based CNC controls, CNC Controls for the retrofit and CNC Controller OEM market</h4></td>
