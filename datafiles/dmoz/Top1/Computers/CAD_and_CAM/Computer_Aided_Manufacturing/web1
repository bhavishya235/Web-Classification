				jQuery(".builtByArea").hide();
				jQuery(".viewAll").hide();
				jQuery("#toggleSpace").html('<img id="toggleLink" src="http://www.emastercam.com/wp-content/plugins/twitter/arrow_down.gif" alt="down" title="down"/>');

				// toggles the slickbox on clicking the noted link
				jQuery("#toggleLink").click(function()
				{
					if(jQuery(".tweetSpace").is(":visible"))
					{
						jQuery(".viewAll").toggle(400);
						jQuery(".builtByArea").toggle(400);
						jQuery(".tweetSpace").toggle(400);
						jQuery("#toggleLink").attr("src", "http://www.emastercam.com/wp-content/plugins/twitter/arrow_down.gif");
						jQuery("#toggleLink").attr("alt", "down");
						jQuery("#toggleLink").attr("title", "down");
					}
					else
					{
						jQuery(".viewAll").toggle(400);
						jQuery(".builtByArea").toggle(400);
						jQuery(".tweetSpace").toggle(400);
						jQuery("#toggleLink").attr("src", "http://www.emastercam.com/wp-content/plugins/twitter/arrow_up.gif");
						jQuery("#toggleLink").attr("alt", "up");
						jQuery("#toggleLink").attr("title", "up");
					}
					return false;
				});

			});

			</script>
