<script type="text/javascript">function returntohome()
{
    window.location='/';
}

function removeSearchBackgroundImage()
{
    document.getElementById('mce-EMAIL').style.backgroundImage='none';
}

function removeSearchBackgroundImageHome()
{
    document.getElementById('mce-EMAIL-home').style.backgroundImage='none';
}

function removeSearchBackgroundImageContact()
{
    document.getElementById('mce-EMAIL-contact-page').style.backgroundImage='none';
}

// ROLLOVER MENUS

var timeout	=750;
var closetimer	= 0;
var menupanel	= 0;

function InitializeMenus()
{
    setSubmenuPositions();
}

//Set the positions of the submenus
function setSubmenuPositions()
{
    var anchordivid = '';    
    var submenudivid = '';
    var sourceoffset = 0;

    for(i=0;i<=20;i++)
    {
        anchordivid = document.getElementById('topmenuitem' + i);
        if (anchordivid)
        {
            sourceoffset = getOffset(anchordivid).left;

            submenudivid = document.getElementById('rollsubmenu' + i);
            if(submenudivid)
            {
                submenudivid.style.left = sourceoffset + 10;
            }  
        }
    }
}

//Get position of parent
function getOffset( el ) 
{
    var _x = 0;
    var _y = 0;
    
    while( el && !isNaN( el.offsetLeft ) && !isNaN( el.offsetTop ) ) 
    {
        _x += el.offsetLeft - el.scrollLeft;
        _y += el.offsetTop - el.scrollTop;

        if (el.id=='topmenu-outside')
        {
            break;
        }

        el = el.offsetParent;
    }

    return { top: _y, left: _x };
}

// open hidden layer
function mopen(id)
{	
	// cancel close timer
	abandonmenutimer();

	// close old layer
	if(menupanel) menupanel.style.visibility = 'hidden';

	// get new layer and show it
	menupanel = document.getElementById(id);
	if(menupanel) menupanel.style.visibility = 'visible';
}
// close showed layer
function mclose()
{
	if(menupanel) menupanel.style.visibility = 'hidden';
}
// go close timer
function setmenutimer()
{
	closetimer = window.setTimeout(mclose, timeout);
}
// cancel close timer
function abandonmenutimer()
{
	if(closetimer)
	{
		window.clearTimeout(closetimer);
		closetimer = null;
	}
}
// close layer when click-out
document.onclick = mclose; </script><style>
