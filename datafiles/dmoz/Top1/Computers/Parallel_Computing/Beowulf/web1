<script type="text/javascript">
(function($)  {

    $('div.tabwrap').hide();
    $('div.nav-tabs ul li:first').addClass('active').show();
    $('div.tabwrap:first').show();

    $('div.nav-tabs ul li').click(function() {
        $('div.nav-tabs ul li').removeClass('active');
        $(this).addClass('active');
        $('div.tabwrap').hide();
        var activeTab = $(this).find('a').attr('href'); 
        $(activeTab).show();
        return false;
    });

  //Dropdown Products 
  $('#navbar li.products').hover(function() {
    $('#dropdown div.products').show();
  }, function() {
    $('#dropdown div.products').hide();
  });

  $('#dropdown div.products').hover(function() {
    $(this).show();
    $('#navbar li.products').addClass('current');
  }, function() {
    $(this).hide();
    $('#navbar li.products').removeClass('current');
  });

  //Dropdown Services
  $('#navbar li.services').hover(function() {
    $('#dropdown div.services').show();
  }, function() {
    $('#dropdown div.services').hide();
  });

  $('#dropdown div.services').hover(function() {
    $(this).show();
    $('#navbar li.services').addClass('current');
  }, function() {
    $(this).hide();
    $('#navbar li.services').removeClass('current');
  });

  //Dropdown Solutions 
  $('#navbar li.solutions').hover(function() {
    $('#dropdown div.solutions').show();
  }, function() {
    $('#dropdown div.solutions').hide();
  });

  $('#dropdown div.solutions').hover(function() {
    $(this).show();
    $('#navbar li.solutions').addClass('current');
  }, function() {
    $(this).hide();
    $('#navbar li.solutions').removeClass('current');
  });

  //Dropdown Support
  $('#navbar li.support').hover(function() {
    $('#dropdown div.support').show();
  }, function() {
    $('#dropdown div.support').hide();
  });

  $('#dropdown div.support').hover(function() {
    $(this).show();
    $('#navbar li.support').addClass('current');
  }, function() {
    $(this).hide();
    $('#navbar li.support').removeClass('current');
  });

  //Dropdown Company
  $('#navbar li.company').hover(function() {
    $('#dropdown div.company').show();
  }, function() {
    $('#dropdown div.company').hide();
  });

  $('#dropdown div.company').hover(function() {
    $(this).show();
    $('#navbar li.company').addClass('current');
  }, function() {
    $(this).hide();
    $('#navbar li.company').removeClass('current');
  });

  $('#dropdown .dropouter').append('<span class="arrow"></span>');


  $('.colorbox').colorbox();

	$('#icebreaker_quote').live('click', function() {
        var ibs = [];
		$('input[type=checkbox]').each(function () {
			if (this.checked) {
				console.log($(this).val()); 
				ibs.push($(this).val());
			}
		});
		if (ibs.length == 0) {
			alert('Please select at least one model.');
		} else {
			var url = "/penguin/sales_help.php?action=quote&cfgid=icebreaker&models=" + encodeURIComponent(ibs.join(' and '));
			popUp(url,'600','600');
		}
		return false;
	});

  $('li.nolink').each(function()  {
    var item = $(this).find('span').html();
    var new_item = "<a class='upcase' href='show_menu'>" + item + "</a>";
    $(this).find('span').html(new_item);
  });

  $('a[href="show_menu"]').live('click', function()  {
    // switch class from collapsed to expanded
    return false;
  });

	$('a.sales_help').click(function()  {
		var cfgid = $('#cfgid').val();
		if (cfgid)  {
			var url = "/penguin/sales_help.php?action=quote&cfgid=" 
                        + encodeURIComponent(cfgid);
		}  else  {
			var url = $(this).attr('href');
			if (!url)
				url = "/penguin/sales_help.php";
		}
		popUp(url,'600','600'); 
		return false;
	});

	$('#calculator_container input').keyup(function()  {
		return update_odc_calc();
	});

	$('#odc_education a').live('click', function()  {
		$('#odc_standard').html('<a href="">Standard</a>');
		$('#odc_standard').removeClass('odc_selected');
		$('#odc_education').html('Education');
		$('#odc_education').addClass('odc_selected');
		return update_odc_calc();
	});
	$('#odc_standard a').live('click', function()  {
		$('#odc_standard').html('Standard</a>');
		$('#odc_standard').addClass('odc_selected');
		$('#odc_education').html('<a href="">Education</a>');
		$('#odc_education').removeClass('odc_selected');
		return update_odc_calc();
	});

	function update_odc_calc()  {
		$('#odc_message').hide();
		var num_cores = $('#odc_num_cores').val(); 
		var hours     = $('#odc_hours').val();
		var stor      = $('#odc_storage').val();
		var days      = $('#odc_days').val();

		// validate input
		if (!is_numeric(num_cores) || (num_cores < 0) ||
			!is_numeric(hours) || (hours < 0) ||
			!is_numeric(days) || (days < 0) ||
			!is_numeric(stor) || (stor < 0))  {
			$('#odc_message').html('Only 0 or positive integers please!');
			$('#odc_message').show();
			return false;
		}
		//num_cores = Math.ceil(num_cores);
		//stor      = Math.ceil(stor);
	
		if ($('#odc_standard').hasClass('odc_selected'))  {
			var ch_rate = 0.10;
			var sto_rate = 0.10;
		}  else  {
			var ch_rate = 0.07;
			var sto_rate = 0.07;
		} 
		var total = (num_cores * hours * ch_rate) + ((stor * sto_rate) / 30 * days);
		$('#odc_total').html('$' + round_number(total, 2).toFixed(2)); 
		return false;
	}

	$('.colorbox').colorbox();

	if ($('#block-menu-menu-products').length)  {
		var current_page = $(location).attr('pathname');

		var arch_array = [
			'/Products/RackmountedServers/RelionESeries',
			'/Products/RackmountedServers/Relion',
			'/Products/RackmountedServers/Altus'
			];
		var function_array = [
			'/Products/HPCComputeServers',
			'/Products/HPCClusterManagementServers',
			'/Products/ServerFarms',
			'/Products/GPUComputing'
			];

		var arch_expand = 0;
		var func_expand = 0;
		if ($.inArray(current_page, arch_array) >= 0)  {
			// show ARCH menu expanded
			arch_expand = 1;
		}  else if ($.inArray(current_page, function_array) >= 0)  {
			// show FUNC menu expanded
			func_expand = 1;
		}  else if (current_page == '/Products/RackmountedServers')  {
			arch_expand = 1;
			func_expand = 1;
		}  else  {
			// no change
		}

		if (arch_expand)  {
			var item = $('#block-menu-menu-products ul.menu > li').first();	
			if (item.hasClass('collapsed'))  {
				item.addClass('expanded active-trail');
				item.removeClass('collapsed');
				item.children('a').first().addClass('active-trail');
				item.append('<ul class="menu"> <li class="first leaf"> <a title="" href="/Products/RackmountedServers/Relion">Relion (Intel) Servers</a> </li> <li class="last leaf"> <a title="" href="/Products/RackmountedServers/Altus">Altus (AMD) Servers</a> </li> </ul>');
			}
		}

		if (func_expand)  {
			var item = $('#block-menu-menu-products ul.menu > li:nth-child(2)').first();	
			if (item.hasClass('collapsed'))  {
				item.addClass('expanded active-trail');
				item.removeClass('collapsed');
				item.children('a').first().addClass('active-trail');
				item.append('<ul class="menu"> <li class="first leaf"><a href="/Products/HPCComputeServers" title="">HPC Compute Servers</a></li> <li class="leaf"><a href="/Products/HPCClusterManagementServers" title="">Application Servers &amp; HPC Cluster Management Servers</a></li> <li class="leaf"><a href="/Products/ServerFarms" title="">Server Farms</a></li> <li class="last leaf"><a href="/Products/GPUComputing" title="">GPU Computing</a></li> </ul>');

			}
		}
	}

})(jQuery);

function popUp(url,w,h)  {
		var winl = (screen.width - w) / 2;
		var wint = (screen.height - h) / 2;
		var param = "toolbar=0,location=0,top=" + wint +",left=" +winl+ ",directories=0,status=1,menubar=0,scrollbars=1,resizable=0,width=" + w + ",height=" + h;
		floater = window.open(url, "PopUp", param);
		floater.focus();
}

function is_numeric(n)  {
		return !isNaN(parseFloat(n)) && isFinite(n);
}

function round_number(num, dec) {
		return Math.round(num * Math.pow(10, dec)) / Math.pow(10, dec);
}
