<script type="text/javascript">

jQuery(document).ready(function() {

  jQuery('#slideshow div.slides').cycle({
	fx:     'scrollHorz',
	timeout: 10000,
    speed: 1200,
	pager:  '#pager'
  });

  jQuery('#home-boxes div.upper').hover(
    function() {
      var bg = jQuery(this).css('background-image');
      var new_bg = bg.replace('_gradient', '');
      jQuery(this).css('background-image', new_bg);
    },
    function() {
      var bg = jQuery(this).css('background-image');
      var new_bg = bg.replace('.png', '_gradient.png');
      jQuery(this).css('background-image', new_bg);
    }
  );

});

</script>

