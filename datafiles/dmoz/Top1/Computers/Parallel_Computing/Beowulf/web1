<AREA COORDS="116,410,297,484" HREF="/cgi-bin/chekov.cgi" TARGET="_top">
<AREA COORDS="316,410,497,484" HREF="/cgi-bin/riker.cgi" TARGET="_top">
<AREA COORDS="476,323,657,402" HREF="/cgi-bin/uhura.cgi" TARGET="_top">
<AREA COORDS="476,244,657,323" HREF="/cgi-bin/picard.cgi" TARGET="_top">
