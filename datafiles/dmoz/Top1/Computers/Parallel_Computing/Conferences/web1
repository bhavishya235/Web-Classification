<script language="javascript">

var images = new Array();
var timeouts = new Array();
var links = new Array();
var bannernames = new Array();
var index = 1;


images[0]="";timeouts[0]=4;links[0]="";bannernames[0]="";
 
function rotateImage()
{
  $('#advert').fadeOut('fast', function()
  {
    $(this).attr('src', images[index]);
	
	$('#adlink').attr('href', links[index]);
	
	$('#adlink').attr('onClick', "_gaq.push(['_trackEvent', 'Banner_Navbar_2014', 'Click','"+ bannernames[index]+"',1.00,true]);");
	
	setTimeout (rotateImage, timeouts[index]*1000);
 
    $(this).fadeIn('fast', function()
    {
      if (index == images.length-1)
      {
        index = 0;
      }
      else
      {
        index++;
      }
    });
  });
  
  
  
}

$(document).ready(function()
{
  //setInterval (rotateImage, 2500);
  setTimeout (rotateImage, timeouts[0]*1000);
});
</script>

