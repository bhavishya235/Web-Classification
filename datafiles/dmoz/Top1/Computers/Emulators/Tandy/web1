<script type="text/javascript">
    /*global Handlebars*/
    jQuery(function($) {
        var source = $('#jobwidget-template').html();
        var template = Handlebars.compile(source);

        Handlebars.registerHelper('stripe', function(array, display_count, random, fn) {
            var buffer = "";
            var max_iterations = Math.min(array.length,display_count);
            for (var i = 0, len = array.length; i < max_iterations; i++) {
                var item = null;
                if (random === true) {
                    var ri = Math.floor((Math.random()*array.length)+1);
                    item = array[ri];
                } else {
                    item = array[i];
                }
                if (item) {
                    item.css_class = (i % 2 === 0 ? 'even' : 'odd');
                }
                buffer += fn.fn(item);
            }
            return buffer;
        });

        function load_jobBoard() {
            $.ajax({
                url:'/jobs/json',
                success: function(data) {
                    $('#job-listing').html(template(data));
                },
                dataType: 'json'
            });
        }
        load_jobBoard();
    });
</script>

<script type="text/x-handlebars-template" id="jobwidget-template">
    
    {{#stripe data.jobs 5 true}}
    <li class="item {{css_class}}">
    <span class="job-link"><a href="{{ this.url }}">{{ this.title }}</a></span>
        <span class="job-details">{{ this.company}} - {{ this.location}}</span>
    </li>
    {{/stripe}}
    
</script>

