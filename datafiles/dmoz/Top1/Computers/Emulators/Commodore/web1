                {{#ifTagged "release"}}
                  {{ cap this.verb }}
                {{else}}
                  {{ icon this.actor.activity_extras.icon_url }}
                  {{ activity_obj this.actor }}
                  {{ this.verb }}
                {{/ifTagged}}
                {{ activity_obj this.obj }}
                {{#if this.target.activity_name}} on {{ activity_obj this.target }}{{/if}}
            </h4>
            {{#if this.obj.activity_extras.summary}}
