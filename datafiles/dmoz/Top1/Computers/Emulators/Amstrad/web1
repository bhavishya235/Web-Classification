        <section id="project-description">
            <header>
                <h2>Description</h2>
            </header>
            <p id="description" itemprop="description">An emulator of the Amstrad CPC 8bit home computer range.</p>
            <p>
                <a id="homepage" class="pspbtn" href="http://www.caprice32.cybercube.com" title="Caprice32 Web Site">Caprice32 Web Site</a>
            </p>
        </section>

