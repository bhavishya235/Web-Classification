function installSearchEngine(name, ext) {
 if (window.external && ("AddSearchProvider" in window.external)) {
   // Firefox 2 and IE 7, OpenSearch
   window.external.AddSearchProvider("http://stardot.org.uk/searchplugins/" + name + ".xml");
 } else if (window.sidebar && ("addSearchEngine" in window.sidebar)) {
   // Firefox <= 1.5, Sherlock
   window.sidebar.addSearchEngine("http://stardot.org.uk/searchplugins/" + name + ".src",
                                  "http://stardot.org.uk/searchplugins/" + name + "." + ext,
                                  "Search Plugin", "");
 } else {
   // No search engine support (IE 6, Opera, etc).
   alert("No search engine support");
 }
}
</script>
