              <tr> 
                <td colspan="6" class="bgwhite" height="2"> <table width="96%" border="0" height="8" align="right" cellpadding="0" class="bgwhite">
                    <tr> 
                      <td class="bgwhite" height="2"> <script>
						var totallinks=6
 
						var urls=new Array(totallinks)
						var descriptions=new Array(totallinks)
						var titles=new Array(totallinks)
 
							urls[0]="http://vision.info.bg/munch/default.htm"
							descriptions[0]=""
							titles[0]="Vision Present Eduard Munch"
							 
							urls[1]="http://vision.info.bg/sarayn/"
							descriptions[1]=""
							titles[1]="Vision Present Martiros Saryan"
							 
							urls[2]="http://vision.info.bg/ensor/default.htm"
							descriptions[2]=""
							titles[2]="Vision Present James Ensor"
							 
							urls[3]="http://vision.info.bg/magritte/default.htm"
							descriptions[3]=""
							titles[3]="Vision Present Rene Magritte"
							
							urls[4]="http://vision.info.bg/renoir/default.htm"
							descriptions[4]=""
							titles[4]="Vision Present Auguste Renoir - Impressionism"
							
							urls[5]="http://vision.info.bg/caillebotte/default.htm"
							descriptions[5]=""
							titles[5]="Vision Present Gustave Caillebotte"
 
					var MachoManRandomSavage=Math.floor(Math.random()*totallinks)
					</script> <script>
					var cont=descriptions[MachoManRandomSavage]+""+descriptions[MachoManRandomSavage]
					cont+="<br><a href='"+urls[MachoManRandomSavage]+"'>"
					cont+=titles[MachoManRandomSavage]+"</a><br>"
					document.write(cont)
					</script> </td>
                    </tr>
                  </table></td>
              </tr>
              <tr valign="bottom"> 
                <td colspan="6" class="bgwhite" height="1"><img src="images/spacer.gif" width="1" height="20"></td>
              </tr>
