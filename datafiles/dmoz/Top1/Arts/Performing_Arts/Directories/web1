<td bgcolor="#FFFFFF">
<div calss="margin-top:3pix">
<img src="common/img/spacer.gif" id="myimages" width="779" height="252" border="0" usemap="#Map" />
<script type="text/javascript">
  photo = new Array();
  var i=0,pn=0,tid;
  photo[pn++]="topimg/img2.jpg";
  photo[pn++]="topimg/img2.jpg";
  photo[pn++]="topimg/img5.jpg";
  photo[pn++]="topimg/img11.jpg";
  photo[pn++]="topimg/img14.jpg";
  photo[pn++]="topimg/img18.jpg";
  photo[pn++]="topimg/img4.jpg";
  photo[pn++]="topimg/img6.jpg";
  photo[pn++]="topimg/img11.jpg";
  photo[pn++]="topimg/img13.jpg";
  photo[pn++]="topimg/img7.jpg";
  photo[pn++]="topimg/img8.jpg";
  photo[pn++]="topimg/img0.jpg";
  hyouji();
  var n=photo.length;

function kaisi(){
  teisi();
  tugi();
  tid=setInterval("tugi();",4000);
}

function tugi(){
  if(i<n-1){
    i++;
    hyouji();
  } else {
    i=0;
    hyouji();
  }
}

function mae(){
  if(i>0){
    i--;
    hyouji();
  } else {
    i=n-1;
    hyouji();
  }
}

function teisi(){
  clearInterval(tid);
}

function hyouji(){
  document.getElementById('myimages').src=photo[i];
}
</script>
