<script type="text/javascript">
$(document).ready(function() 
{
	StartSlider();
	  
    $("#accordion").accordion();

    // Groups Form Fields...
    //
    UserInputEffects('#input_contact_first_name input',"First Name", false);
    UserInputEffects('#input_contact_last_name input',"Last Name", false);
    UserInputEffects('#input_contact_email input', "Email", false);
    UserInputEffects('#input_contact_phone input',"Phone", true);
    UserInputEffects("#input_contact_comment textarea", "Comment", false);


    //Community Form Fields...
    //
    UserInputEffects("#input_community_org_name input", "Organization Name:", false);
    UserInputEffects("#input_community_tax_exempt_num input", "501(c)(3) or tax exempt number:", false);
    UserInputEffects("#input_community_contact_person input", "Contact Person:", false);
    UserInputEffects("#input_community_phone input", "Telephone:", true);
    UserInputEffects("#input_community_fax input", "Fax:", true);
    UserInputEffects("#input_community_address input", "Address:", false);
    UserInputEffects("#input_community_city input", "City:", false);
    UserInputEffects("#input_community_state input", "State:", false);
    UserInputEffects("#input_community_zip input", "Zip:", false);
    UserInputEffects('#input_community_email input', "Email:", false);
    UserInputEffects('#input_community_children input', "Children:", true);
    UserInputEffects('#input_community_adult input', "Adults:", true);
    UserInputEffects('#input_community_purpose textarea', "Purpose", false);
    
    $('.meet_team_click_event').click(function()
    {
    	currentId = $(this).attr('id');
    	currentContentId = '#'+currentId+'_content';
    	$(this).stop();
	    if ($(this).hasClass('expandContent'))
	    {
		    
		    $(currentContentId).slideDown('fast', function()
				                                  {
		    									      $('#'+currentId).removeClass('expandContent');
		    	                                      $('#'+currentId).addClass('hideContent');
                                                  });
		    $('#'+currentId + ' .arrow img').attr('src', 'http://bigapplecircus.org.s3.amazonaws.com/img/arrow_up.png');
	    }
	    else
	    {
	    	$(currentContentId).slideUp('fast', function()
			    							    {
	    										    $('#'+currentId).removeClass('hideContent');
	    											$('#'+currentId).addClass('expandContent');
												});
	    	$('#'+currentId + ' .arrow img').attr('src', 'http://bigapplecircus.org.s3.amazonaws.com/img/arrow_down.png');
	    }
	});

    //for some reason the textarea box does not show default text when first loading so im forcing it to blur so our custom functions do it...
    //
	$('textarea').trigger('blur');
    
	// hide #back-top first
	$("#back-top").hide();
	
	// fade in #back-top
	$(function () {
		$(window).scroll(function () 
	    {
		    if ($(this).scrollTop() < 2930)
		    {	
				if ($(this).scrollTop() > 100) 
			    {
					$('#back-top').fadeIn();
				} else{
					$('#back-top').fadeOut();
				}
		    }
		    else
		    {
					$('#back-top').fadeOut();
		    }
		});

		// scroll body to 0px on click
		$('#back-top a').click(function () {
			$('body,html').animate({
				scrollTop: 0
			}, 800);
			return false;
		});
	});

    GoogleEvents();
	
});
</script>
