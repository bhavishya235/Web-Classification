		jQuery(document).ready(function($) {
			$(window).load(function() {
				$('#tb-slider-homepage-version2 .flexslider').flexslider({
					animation: "fade",
					// pauseOnHover: true - Removed because currently causes conflicts with pause/play functionality
										slideshowSpeed: 8000,
																				controlsContainer: ".slides-wrapper-homepage-version2",
					start: function(slider) {
        					        				$('#tb-slider-homepage-version2 .flex-direction-nav li:first-child').after('<li><a class="pause" href="#">Pause</a></li><li><a class="play" href="#" style="display:none">Play</a></li>');
	        				$('#tb-slider-homepage-version2 .pause').click(function(){
								slider.pause();
								$(this).hide();
								$('#tb-slider-homepage-version2 .play').show();
								return false;
							});
							$('#tb-slider-homepage-version2 .play').click(function(){
								slider.resume();
								$(this).hide();
								$('#tb-slider-homepage-version2 .pause').show();
								return false;
							});
							$('#tb-slider-homepage-version2 .flex-control-nav li, #tb-slider-homepage-version2 .flex-direction-nav li').click(function(){
								$('#tb-slider-homepage-version2 .pause').hide();
								$('#tb-slider-homepage-version2 .play').show();
							});
        				        				$('#tb-slider-homepage-version2 .image-link').click(function(){
        					$('#tb-slider-homepage-version2 .pause').hide();
        					$('#tb-slider-homepage-version2 .play').show();
        					slider.pause();
        				});
        			}
				}).parent().find('.tb-loader').fadeOut();
			});
		});
		</script>
				<div id="tb-slider-homepage-version2" class="slider-wrapper standard-slider-wrapper slider_has_mobile_fallback">
