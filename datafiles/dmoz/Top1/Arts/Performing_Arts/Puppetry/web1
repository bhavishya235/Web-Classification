<script type="text/javascript">

$(document).ready(function(){
  
  $("#ss_iim").hide();
  $("#ss_esnam").hide();
  $("#ss_stages").hide();
  $("#ss_residences").hide();
  $("#ss_cdoc").hide();
  $("#ss_service_edu").hide();
  $("#ss_collections").hide();
  $("#ss_editions").hide();
  $("#ss_menu").hide();
  $("#production").hide();
  $("#distribution").hide();
  $("#dossierArtistique").hide();

  $("#menu_ppal ul li.mnuTop:not(#menu_ppal ul li.mnuTop:only-child)").click(function () {

    if ($("#ss_menu").is(":hidden")) {
      $("#ss_menu").slideDown("slow");
      $(this).addClass("selected");
    } else if  ($(this).hasClass("selected"))  {  
      $("#ss_menu").slideUp("slow");
      $(this).removeClass("selected");
    } else {
      $("#menu_ppal ul li.mnuTop").removeClass("selected");
      $(this).addClass("selected");
    }  
    
  });

  $("li#tit_iim:not(.ici)").hover(function () {
		$("#iim").attr("src","http://www.marionnette.com/media/public/images/tit_iim_roll-fr.png");
  }, 
  function () {
    if ($("#tit_iim").hasClass("selected")) {
	  	$("#iim").attr("src","http://www.marionnette.com/media/public/images/tit_iim_roll-fr.png");
    } else {
			$("#iim").attr("src","http://www.marionnette.com/media/public/images/tit_iim-fr.png");
    };		
  });

  $("#tit_iim").click(function () {
    if ($("#tit_iim").hasClass("selected")) {
			$("#iim").attr("src","http://www.marionnette.com/media/public/images/tit_iim_roll-fr.png");
			if ($("#tit_cexp").hasClass("ici")) {
			$("#coll").attr("src","http://www.marionnette.com/media/public/images/tit_collections_roll-fr.png");
			} else {
			$("#coll").attr("src","http://www.marionnette.com/media/public/images/tit_collections-fr.png");	
			};
			if ($("#tit_edit").hasClass("ici")) {
			$("#edi").attr("src","http://www.marionnette.com/media/public/images/tit_editions_roll-fr.png");
			} else {
			$("#edi").attr("src","http://www.marionnette.com/media/public/images/tit_editions-fr.png");
			};
			if ($("#tit_sedu").hasClass("ici")) {
			$("#edu").attr("src","http://www.marionnette.com/media/public/images/tit_service_educatif_roll-fr.png");
			} else {
			$("#edu").attr("src","http://www.marionnette.com/media/public/images/tit_service_educatif-fr.png");
			};
			if ($("#tit_esnam").hasClass("ici")) {
			$("#esnam").attr("src","http://www.marionnette.com/media/public/images/tit_esnam_roll-fr.png");
			} else {
			$("#esnam").attr("src","http://www.marionnette.com/media/public/images/tit_esnam-fr.png");
			};
			if ($("#tit_cdoc").hasClass("ici")) {
			$("#doc").attr("src","http://www.marionnette.com/media/public/images/tit_centreDoc_roll-fr.png");
			} else {
			$("#doc").attr("src","http://www.marionnette.com/media/public/images/tit_centreDoc-fr.png");	
			};
			if ($("#tit_res").hasClass("ici")) {	
			$("#residence").attr("src","http://www.marionnette.com/media/public/images/tit_residences_roll-fr.png");
			} else {
			$("#residence").attr("src","http://www.marionnette.com/media/public/images/tit_residences-fr.png");
			};
			if ($("#tit_stages").hasClass("ici")) {
		    $("#stages").attr("src","http://www.marionnette.com/media/public/images/tit_stages_roll-fr.png");
			} else {
			$("#stages").attr("src","http://www.marionnette.com/media/public/images/tit_stages-fr.png");	
			};
	} else  {
			
			if ($("#tit_iim").hasClass("ici")) {
			$("#iim").attr("src","http://www.marionnette.com/media/public/images/tit_iim_roll-fr.png");
			} else {
			$("#iim").attr("src","http://www.marionnette.com/media/public/images/tit_iim-fr.png");
			};
			
};	
$("#ss_iim").fadeIn(1000); 
$("#ss_collections").hide();
$("#ss_editions").hide(); 
$("#ss_service_edu").hide(); 
$("#ss_cdoc").hide(); 
$("#ss_residences").hide(); 
$("#ss_stages").hide(); 
$("#ss_esnam").hide(); 
});


