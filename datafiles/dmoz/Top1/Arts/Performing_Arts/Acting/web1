<script language=javascript type='text/javascript'> 
 var numberOfDiv = 2; // Change the number of DIVs
  function showHideDiv(show) 
   {
    for(i=1;i<=numberOfDiv;i++)
   {				
    if(i==show)
     {
      document.getElementById('div'+show).style.visibility = 'visible';
      document.getElementById('div'+show).style.display = 'block';				
     }
      else
     {
      document.getElementById('div'+i).style.visibility = 'hidden';
      document.getElementById('div'+i).style.display = 'none';				
     }				
    }
	return false;
   } 
</script>
