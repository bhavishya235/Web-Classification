	function setDestination(sbox){
		var base_url = "http://www.actingbiz.com";
		var results_url = base_url +"/"+ sbox.value +  "/results.php" ;
		document.frmSearch.action = results_url;
		//alert(results_url);
		
	}
	
	function submitSearchForm(){
		var sbox = document.getElementById("search_type");
		setDestination(sbox);
		document.frmSearch.submit();
	}
	
	
	function priorSubmit(){
		var sbox = document.getElementById("search_type");
		setDestination(sbox);
	}
	
</script>

