	window.onload = preload_images;
	function preload_images(){
		MM_preloadImages('images/btn-design-collection-on.gif','images/btn-packages-pricing-on.gif','images/btn-faq-on.gif','images/btn-contact-on.gif','images/btn-account-login-on.gif','images/btn-about-us-on.gif')	
