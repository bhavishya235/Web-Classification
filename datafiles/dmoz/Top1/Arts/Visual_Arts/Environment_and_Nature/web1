                        function setbranding(name) {    
                            $(name).attr("placeholder","").addClass("branding");
                            if($(name).val()==""){
                                $(name).addClass("branding");
                            } else {
                                $(name).removeClass("branding");
                            }
                            $(name).focus(function(){
                                $(this).removeClass("branding");
                            });
                            $(name).blur(function(){
                                if($(this).val()==""){
                                    $(this).addClass("branding");   
                                }
                            });
                        }
                    });
