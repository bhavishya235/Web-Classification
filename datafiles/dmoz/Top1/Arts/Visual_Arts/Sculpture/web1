    <td rowspan="6" width="357"><img src="slice/top_background.gif" width="357" height="255" align="absbottom"></td>
  </tr>
  <tr> 
    <td colspan="3"><a href="stainedglass.html" onMouseOut="MM_swapImgRestore()" onMouseOver="MM_swapImage('Image12','','slice/stainedglassON.gif',1)"><img name="Image12" border="0" src="slice/stainedglassOFF.gif" width="193" height="23"></a><br><a href="paintings.html"><img src="images/paintingbutton.jpg"></a></td>
    <td colspan="3" rowspan="2">&nbsp;</td>
