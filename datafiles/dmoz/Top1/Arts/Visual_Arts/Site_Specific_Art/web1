Bild5.src = "flaggen/flagge_IT_60prozent.jpg";     /* Standard-Grafik 5 */
Bild5over = new Image();
Bild5over.src = "flaggen/flagge_IT_20x30.jpg"; /* fÃ¼nfte Highlight-Grafik */

/* usw. fuer alle weiteren zu benutzenden Grafiken */

function Bildwechsel (Bildnr, Bildobjekt) {
  window.document.images[Bildnr].src = Bildobjekt.src;
}
</script>

</head>
