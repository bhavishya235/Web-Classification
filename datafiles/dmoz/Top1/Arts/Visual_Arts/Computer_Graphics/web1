			<link rel="stylesheet" type="text/css" href="http://kandacewright.com/index.php?ak_action=aktt_css" />
			<script type="text/javascript" src="http://kandacewright.com/index.php?ak_action=aktt_js"></script>
		
<!-- Start Of Script Generated By WP-PageNavi 2.40 -->
<link rel="stylesheet" href="http://kandacewright.com/wp-content/plugins/wp-pagenavi/pagenavi-css.css" type="text/css" media="screen" />
