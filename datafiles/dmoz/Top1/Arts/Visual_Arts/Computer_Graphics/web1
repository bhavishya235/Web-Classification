	 $(document).ready(function() 
	 	{
	
	 setInterval(function(){check_size()},200);
	 
	 		

		});
		
		
		
	function login_error()
		{
		login_box();
		$('#login_error').fadeIn(); 
		$('#login').animate({height: '265'}, 400);						
		setTimeout("$('#login_error').fadeOut(); ", 3000);
		setTimeout("$('#login').animate({height: '205'}, 400);", 3000);
		}
		
	function file_error()	
		{
					if(document.getElementById("error_pop_box").style.display == "block")
						{
						document.getElementById("error_pop_box").style.display = "none";
						instructions.style.display = "none";
						}
					else 
						{
						document.getElementById("error_pop_box").style.display = "block";
						instructions.style.display = "none";
						}
		}

	 function removeImage(imageId, folderId) {
	 		
	 		 if (window.XMLHttpRequest)
                    {xmlhttp=new XMLHttpRequest();}

                xmlhttp.onreadystatechange=function()
                {
                    if (xmlhttp.readyState==4 && xmlhttp.status==200)
                        { 
                       // document.getElementById("folders").innerHTML= '';
                        //document.getElementById("folders").innerHTML=xmlhttp.responseText;
						//document.getElementById("spinner4").style.display = "none";
						
						document.getElementById("gallery").innerHTML = "";

						if (window.XMLHttpRequest)
							 {xmlhttp=new XMLHttpRequest();}
		 
						 xmlhttp.onreadystatechange=function()
						 {
							 if (xmlhttp.readyState==4 && xmlhttp.status==200)
								 { 
								 document.getElementById("gallery").innerHTML=xmlhttp.responseText;
								 
								 
									  if (window.XMLHttpRequest)
										   {xmlhttp=new XMLHttpRequest();}
					   
									   xmlhttp.onreadystatechange=function()
									   {
										   if (xmlhttp.readyState==4 && xmlhttp.status==200)
											   { 
											   document.getElementById("folders").innerHTML=xmlhttp.responseText;
					   		
					   							 document.getElementById("spinner").style.display = "none";

												 add_swipe();
												  add_sortable();
												  check_size();
											   }
									   }
									   
									   xmlhttp.open("GET","./themes/hyperion/get_folders.php",true);
									   xmlhttp.send();
											   
								 
								 
								
		 						
								 }
						 }
						 
						 xmlhttp.open("GET","./themes/hyperion/ajax_images.php?folder="+folderId+"&logged=yes",true);
						 xmlhttp.send();
			  
						check_size();	
						check_size(); // must call twice.

                        }
                }
                
				xmlhttp.open("GET","./mobile/remove_image.php?id="+imageId,true);
                xmlhttp.send();
                
                
            
                
	 
	 				
	    //window.location="/inksplat&remove2="+imageId+"&q=folder&id="+folderId;
		}
		
		
		 function add_swipe()
	 	{
	 		 	var myPhotoSwipe = $("#gallery a").photoSwipe({ preventSlideshow: false , captionAndToolbarOpacity: .8 , fadeInSpeed: 0 ,  fadeOutSpeed: 0 ,  enableMouseWheel: false , enableKeyboard: true, backButtonHideEnabled: false, captionAndToolbarShowEmptyCaptions: false, imageScaleMethod: "fitNoUpscale" });
				}	
	</script>
	
	</script>


	


