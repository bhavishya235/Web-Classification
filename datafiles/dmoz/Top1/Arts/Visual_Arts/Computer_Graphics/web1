<SCRIPT>
x = 2;
orgClipBottom = 305
orgClipTop = 0


function stopScroll() {
newsContentScroll = 0;
}	


		function moveUp(xSpeed) {
		orgClipBottom = orgClipBottom - xSpeed;
		orgClipTop = orgClipTop - xSpeed;
		newsLayer.style.clip ='rect(' + (orgClipTop) + ',225,' + (orgClipBottom) + ',0)';
		newsLayer.style.posTop  = newsLayer.style.posTop + xSpeed;
		if (newsContentScroll == 1) {
			eval("setTimeout('moveUp(" + xSpeed + ")', 100)");
			}
			
//	checkEdgesOfCopy()
		}	
		
	function moveDown(xSpeed) {
		//move text up
		orgClipBottom = orgClipBottom + xSpeed;
		orgClipTop = orgClipTop + xSpeed;
		newsLayer.style.clip ='rect(' + (orgClipTop) + ',225,' + (orgClipBottom) + ',0)';
		newsLayer.style.posTop  = newsLayer.style.posTop - xSpeed;
		if (newsContentScroll == 1) {
			eval("setTimeout('moveDown(" + xSpeed + ")', 100)");
			}
//	checkEdgesOfCopy()
		}	
	
 
newsLayer.style.clip ='rect(' + (orgClipTop) + ',225,' + (orgClipBottom) + ',0)';
newsLayer.style.visibility = "visible";



</SCRIPT>











