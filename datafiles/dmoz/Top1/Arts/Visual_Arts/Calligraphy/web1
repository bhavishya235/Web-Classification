<script type="text/javascript">
            $(document).ready(function() {
                // randomize images
                var cardImages = ["k:3allama.png","k:bashir.png","k:bismil.png","k:bilbirri.png","k:hamdu.png","k:i3tasimu.png","k:ilahi.png","k:jadilhum.png","k:jannatu.png","k:kursee.png","k:khalaqna.png","k:mahabba.png","k:ra3in.png","k:qawlun.png","k:rahmati.png","k:rutbatu.png","k:sadri.png","k:tawfiqi.png","k:walidayni.png","m:afaq.png","m:af3al.png","m:af3al2.png","m:allah-moh.png","m:aqranihi.png","m:damana.png","m:fadli.png","m:darajat.png","m:ghafer.png","m:hafith.png","m:ghalibun.png","m:hamdu2.png","m:hikmah.png","m:husna.png","m:ighfir.png","m:ista3anta.png","m:jama3a.png","m:jalalouh.png","m:madad.png","m:kun.png","m:miraat.png","m:ni3mati.png","m:samawat.png","m:qadeer.png","m:sirat.png","m:shakartum.png","m:taqabbal.png","m:tawfiqi3.png","m:tuHsuha.png","m:walilahi.png","m:ya3malu.png","m:yusr.png","m:da3wa.png","m:lateefon_1.jpg","m:31.png","m:bithikri.png","m:zakat.png","m:3ajibtou.png","m:bayyid.png","m:sa3a.png","h:yadu.png","h:tazawwadu.png","h:ta3allam.png","h:quwwat.png","h:ni3ma.png","h:khayr.png","h:ikhwa.png","h:hikma.png","h:ghafir.png","h:ahsan.png","h:3ayn.png","h:3adala.png","h:h41.png","h:h61.png","h:fayakun.png"];
                var imageIndex = 1;
                cardImages.sort(function() {
                    return 0.5 - Math.random()
                });
                // initial state
                $("#card-image").attr("src", "/static/gallery/slide/" + cardImages[0].substring(2));
                if (cardImages[0].substring(0, 1) == 'm') {
                    $("#card-href").attr("href", "/mokhtar-gallery/" +
                            cardImages[0].substring(2, cardImages[0].indexOf('.', -1)));
                }
                if (cardImages[0].substring(0, 1) == 'h') {
                    $("#card-href").attr("href", "/hurufiyyat-gallery/" +
                            cardImages[0].substring(2, cardImages[0].indexOf('.', -1)));
                }
                if (cardImages[0].substring(0, 1) == 'k') {
                    $("#card-href").attr("href", "/kamel-gallery/" +
                            cardImages[0].substring(2, cardImages[0].indexOf('.', -1)));
                }
                // refresh image every N seconds
                setInterval(function() {
                    $("#card-image").attr("src", "/static/gallery/slide/" + cardImages[imageIndex].substring(2));
                    if (cardImages[imageIndex].substring(0, 1) == 'm') {
                        $("#card-href").attr("href", "/mokhtar-gallery/" +
                                cardImages[imageIndex].substring(2, cardImages[imageIndex].indexOf('.', -1)));
                    }
                    if (cardImages[imageIndex].substring(0, 1) == 'h') {
                        $("#card-href").attr("href", "/hurufiyyat-gallery/" +
                                cardImages[imageIndex].substring(2, cardImages[imageIndex].indexOf('.', -1)));
                    }
                    if (cardImages[imageIndex].substring(0, 1) == 'k') {
                        $("#card-href").attr("href", "/kamel-gallery/" +
                                cardImages[imageIndex].substring(2, cardImages[imageIndex].indexOf('.', -1)));
                    }
                    if (imageIndex < (cardImages.length - 1)) {
                        imageIndex++;
                    } else {
                        imageIndex = 0;
                    }
                }, 10000);
            });
        </script>


