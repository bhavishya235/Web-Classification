	var cookieState = readCookie('tribeInfo');

	function writeHideLink() {
		var cookieState = readCookie('tribeInfo');
		if(cookieState == 'closed') {
			document.getElementById('showHideLink').innerHTML = 'show';
		} else {
			document.getElementById('showHideLink').innerHTML = 'hide';
		}		
	}

</script>

