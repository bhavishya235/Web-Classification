		<script type="text/javascript" src="http://ajax.googleapis.com/ajax/libs/swfobject/2.2/swfobject.js"></script>
        <div class="musicMargins">
          <div id="myContent">
            This text will be replaced by the flash music player.
          </div><br />
        </div>

		<script type="text/javascript">
		var flashvars = {
  autoPlay: "on",
  soundPath: "mp3s/1.mp3"
};
var params = {
  menu: "false"
};
var attributes = {
  id: "myDynamicContent",
  name: "myDynamicContent"
};

swfobject.embedSWF("musiccontroller.swf", "myContent", "26", "26", "9.0.0","expressInstall.swf", flashvars, params, attributes);

		</script>
	<!-- End Copy/Paste --></div>
