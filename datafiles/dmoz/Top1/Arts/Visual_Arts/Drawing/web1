function newImage(arg) {
	if (document.images) {
		rslt = new Image();
		rslt.src = arg;
		return rslt;
	}
}

function changeImages() {
	if (document.images && (preloadFlag == true)) {
		for (var i=0; i<changeImages.arguments.length; i+=2) {
			document[changeImages.arguments[i]].src = changeImages.arguments[i+1];
		}
	}
}

var preloadFlag = false;
function preloadImages() {
	if (document.images) {
		home_link_over = newImage("http://www.drawingsociety.com/images/home_link-over.gif");
		askamaster_link_over = newImage("http://www.drawingsociety.com/images/askamaster_link-over.gif");
		members_link_over = newImage("http://www.drawingsociety.com/images/members_link-over.gif");
		dscgallery_link_over = newImage("http://www.drawingsociety.com/images/dscgallery_link-over.gif");
		contact_link_over = newImage("http://www.drawingsociety.com/images/contact_link-over.gif");
		index_08_contact_link_over = newImage("http://www.drawingsociety.com/images/index_08-contact_link_over.gif");
		preloadFlag = true;
	}
}

