var mod_smartslider_121captions = sliderDojo.clone(captions);
sliderDojo.addOnLoad(sliderDojo, function(){
  var dojo = this;
  new OfflajnSliderSimpleHorizontal({
    node: dojo.byId('mod_smartslider_121'),
    rawcaptions: mod_smartslider_121captions,
    autoplay: 1,
    autoplayinterval: 5000,
    restartautoplay: 1,
    maineasing: dojo.fx.easing.cubicInOut,
    maininterval: 1200,
    mousescroll: 1,
    transition: 2  });
});
