	<div id="mod-fwg-carousel-down-button-1881771534" class="mod-fwg-carousel-down-button"></div>
</div>
<script type="text/javascript">
var mod_fwg_carousel_pos_1881771534 = 0;

function fwg_fade(elem, start, end, time) {
	elem.style.opacity = start;
	(function fwg_go() {
		if (start < end) {
			var opacity = elem.style.opacity * 1 + 0.1;
			elem.style.opacity = opacity;
			elem.style.filter = 'alpha(opacity=' + elem.style.opacity * 100 + ')';
			if (elem.style.opacity < end) setTimeout(fwg_go, 50);
		} else {
			var opacity = elem.style.opacity * 1 - 0.1;
			elem.style.opacity = opacity;
			elem.style.filter = 'alpha(opacity=' + elem.style.opacity * 100 + ')';
			if (elem.style.opacity > end) setTimeout(fwg_go, 50);
		}
	})();
}

window.addEvent('domready', function(){
	document.getElements('a.fwg-carousel-lightbox').cerabox({
		titleFormat: 'Image {number} / {total} {title}'
	});
	var mod_fwg_body = document.getElementById('mod-fwg-carousel-1881771534');
	var mod_fwg_frame = document.getElementById('mod-fwg-carousel-frame-1881771534');
	var mod_fwg_film = document.getElementById('mod-fwg-carousel-film-1881771534');

	var mod_fwg_up = document.getElementById('mod-fwg-carousel-up-button-1881771534');
	var mod_fwg_down = document.getElementById('mod-fwg-carousel-down-button-1881771534');

	mod_fwg_down.addEvent('click', function(ev) {
		var film_height = parseInt(mod_fwg_film.style.height) * -1;
		if (mod_fwg_carousel_pos_1881771534 > film_height) {
			mod_fwg_carousel_pos_1881771534 = Math.max(film_height, mod_fwg_carousel_pos_1881771534 - 128);
			(function fwg_go_up() {
				var top = mod_fwg_film.style.top.replace(/([\d.]+)(px|pt|em|%)/,'$1') * 1 - 20;
				mod_fwg_film.style.top = top+'px';
				if (top > mod_fwg_carousel_pos_1881771534) setTimeout(fwg_go_up, 10);
			})();
		}
	});

	mod_fwg_up.addEvent('click', function(ev) {
		if (mod_fwg_carousel_pos_1881771534 < 0) {
			mod_fwg_carousel_pos_1881771534 = Math.min(0, mod_fwg_carousel_pos_1881771534 + 128);
			(function fwg_go_down() {
				var top = mod_fwg_film.style.top.replace(/([\d.]+)(px|pt|em|%)/,'$1') * 1 + 20;
				mod_fwg_film.style.top = top+'px';
				if (top < mod_fwg_carousel_pos_1881771534) setTimeout(fwg_go_down, 10);
			})();
		}
	});

	var fwg_divs = mod_fwg_film.getElementsByTagName('div');
	for (var i = 0; i < fwg_divs.length; i++) {
		var src_images = fwg_divs[i].getElementsByTagName('img');
		if (src_images.length > 0) {
			fwg_divs[i].style.width = '164px';
			fwg_divs[i].style.height = '124px';
			src_images[0].style.position = 'absolute';
			src_images[0].style.top = '0';
			src_images[0].style.left = '0';
		}

		fwg_divs[i].addEvent('mouseover', function(ev) {
			var src_images = this.getElementsByTagName('img');
			if (src_images.length > 0) {
				src_images[0].style.zIndex = '10';
				src_images[0].style.left = '-4px';
				src_images[0].style.top = '-4px';
				src_images[0].style.width = '172px';
			}
			fwg_fade(this, 0.6, 1, 500);
		});
		fwg_divs[i].addEvent('mouseout', function(ev) {
			var src_images = this.getElementsByTagName('img');
			if (src_images.length > 0) {
				src_images[0].style.zIndex = '0';
				src_images[0].style.left = '0';
				src_images[0].style.top = '0';
				src_images[0].style.width = '164px';
			}
			fwg_fade(this, 1, 0.6, 500);
		});
	}
});

</script>		            </div>
                </div>
                           </div>
