<script language="JavaScript" type="text/JavaScript">
        function GlossaryDefinition(label,instance)
        {
         // AJAX call to get definition of "label" from glossary table
         if (window.XMLHttpRequest)
           {// code for IE7+, Firefox, Chrome, Opera, Safari
            xmlhttp=new XMLHttpRequest();
           }
         else
           {// code for IE6, IE5
           xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
           }

         xmlhttp.onreadystatechange=function()
           {
            if (xmlhttp.readyState==4 && xmlhttp.status==200)
             {
              infopop.childNodes[instance].innerHTML=xmlhttp.responseText;
             }
            }
         xmlhttp.open("GET","getGlossaryDefinition.php?term="+ escape(label),true);
         xmlhttp.send();

        }

        function GlossarySource(text)
        {

           //  window.alert(escape(text));
           //  popUpWin=window.open('/getGlossarySource.php?term=' + escape(text),'Glossary Source','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=,resizable=,copyhistory=no,left=500,top=200,width=500,height=200');
           // popUpWin.focus();
           //   window.alert(escape(text));
           //   location.replace('/glossary.php#' + escape(text));
           // window.location = '/glossary.php#' + escape(text.toLowerCase());
        }


function openReservationForm(docid)
{

  //    window.alert(docid);
    reservationURL = '/make_a_reservation_popup.php?docid=' + docid;
    params='width='+screen.width + ', height='+screen.height + ', top=0, left=0, fullscreen=no,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=,copyhistory=no';
    popUpWin=window.open(reservationURL,'ReservationForm',params);
    if (window.focus) {popUpWin.focus()}


}
function openGoogleCheckoutCart()
{
//alert("in openGoogleCheckoutCart");
var numberOfTickets = [];
var productsDiv = document.getElementById("GoogleCheckoutProducts");
var productDivs = productsDiv.getElementsByTagName("div");
var numberMatchString = new RegExp("^Number");
var descriptionMatchString = new RegExp("^PriceDescription");

var foundNonZeroQuantity = false;

outer_loop:
for (var i = 0;i<productDivs.length;i++) {
  if (numberMatchString.test(productDivs[i].id)) {
   for (var j = 0;j<productDivs[i].childNodes.length;j++) {
    if (numberMatchString.test(productDivs[i].childNodes[j].id)) {
     if (productDivs[i].childNodes[j].value != null && productDivs[i].childNodes[j].value > 0) {
      foundNonZeroQuantity = true;
      break outer_loop;
     }
    }
   }
  }
}
//alert("in openGoogleCheckoutCart; foundNonZeroQuantity is " + foundNonZeroQuantity);
if (!foundNonZeroQuantity) {
 alert("Please specify the number of tickets you wish to purchase");
 return false;
}
//alert("in openGoogleCheckoutCart; googlecart quantity is " + googlecart.getTotalQuantity());
// if Cart has already been filled, but we wind up back here due to
// form validation failure, don't "re-add" the tickets to the cart
// make sure cart is empty before adding  number of tickets specified in form
 googlecart.clear(); 

 // iterate through the products;
 // only call addItem if value of NumberXQuantity is > 0
 for (var i = 0;i<productDivs.length;i++) {
  if (numberMatchString.test(productDivs[i].id))
   {
   //alert(productDivs[i].id);
   for (var j = 0;j<productDivs[i].childNodes.length;j++) {
    // find the node with id NumberX (numberMatchString)
    if (numberMatchString.test(productDivs[i].childNodes[j].id))
    {
     if ( productDivs[i].childNodes[j].value != null && productDivs[i].childNodes[j].value != 0) {
     //alert(productDivs[i].childNodes[j].id);
     //alert(productDivs[i].childNodes[j].value);
     numberOfTickets[i] = productDivs[i].childNodes[j].value;
     googlecart.addItem(googlecart.makeItemFromNode(productDivs[i]),numberOfTickets[i]);
     }
    }
   }
   for (var j = 0;j<productDivs[i].childNodes.length;j++) {
    // find the node with id PriceDescriptionNumberX (descriptionMatchString)
    if (descriptionMatchString.test(productDivs[i].childNodes[j].id) && (numberOfTickets[i] > 0))
    {
      productDivs[i].childNodes[j].value = numberOfTickets[i];
     }
    }
   }

  }

document.reservationContactForm.onsubmit="return true";
document.reservationContactForm.action="sendReservationData.php";
document.reservationContactForm.submit();

}

function openTicketPurchaseDataForm(docid)
{

  // popUpWin.focus();
    reservationURL = '/make_a_ticket_purchase_popup.php?docid=' + docid;
    params='width='+screen.width + ', height='+screen.height + ', top=0, left=0, fullscreen=no,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=,copyhistory=no';
    popUpWin=window.open(reservationURL,'ReservationForm',params);
    if (window.focus) {popUpWin.focus()}
}

function openTicketSalesForm(docid)
{

  //   window.alert(escape(text));
    purchaseURL = '/make_a_reservation_popup_googleCheckout.php?docid=' + docid;
    params='width=700, height=600, top=0, left=0, fullscreen=no,toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=,copyhistory=no';
    popUpWin=window.open(purchaseURL,'PurchaseForm',params);

    if (window.focus) {popUpWin.focus()}

}

