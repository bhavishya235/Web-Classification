		function submitform(){
			if(document.search.onsubmit && !document.search.onsubmit()){
				$('div').scrollTop(0);
				return;
			}
			document.search.submit();
		}
		function submitform(){
			if(document.search-box.onsubmit && !document.search-box.onsubmit()){
				return;
			}
			document.search-box.submit();
		}
