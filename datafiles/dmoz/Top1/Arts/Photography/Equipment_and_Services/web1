      <tr>
        <td height="20" width="10" valign="middle" align="right"></td>
        <td height="20" valign="middle" align="left" nowrap width="80"><a href="../about/index.html"><font face="Trebuchet MS" color="#FFFFFF">About Me</font></a></td>
        <td height="20" width="10"></td>
      </tr>
      <tr>
        <td height="20" width="10" valign="middle" align="right"></td>
        <td height="20" valign="middle" align="left" nowrap width="80"></td>
        <td height="20" width="10"></td>
      </tr>
      <tr>
        <td height="20" width="10" valign="middle" align="right"></td>
        <td height="20" valign="middle" align="left" nowrap width="80"><font face="Trebuchet MS" color="#FFFFFF"><a href="mailto:sweixel@photoscene.com"><img src="../emailstamp2.gif" alt="Email Me!" border="0" WIDTH="55" HEIGHT="60"></a></font></td>
