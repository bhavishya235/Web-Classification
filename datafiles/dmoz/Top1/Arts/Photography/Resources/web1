<p class="datetime">
Tue February 25, 2014 02:15 PM<BR>
	<script language="JAVASCRIPT" type="text/javascript">

	
		var now = new Date;
	
		function isHoliday() {
			if (now.getMonth() == 1 && now.getDate() == 1) {
				// new years
				return 1;
			}
			if (now.getMonth() == 5 && now.getDate() == 29) {
				// memorial day
				return 1;
			}
			if (now.getMonth() == 7 && now.getDate() == 4) {
				// independence day
				return 1;
			}
			if (now.getMonth() == 9 && now.getDate() == 4) {
				// labor day
				return 1;
			}
			if (now.getMonth() == 11 && now.getDate() == 23) {
				// Thanksgiving
				return 1;
			}
			if (now.getMonth() == 11 && now.getDate() == 24) {
				// Thanksgiving
				return 1;
			}
			if (now.getMonth() == 12 && now.getDate() == 25) {
				// Christmas
				return 1;
			}
			return 0;
		}
	
		if (now.getDay() > 0 && now.getDay() < 6 &&
			  isHoliday() == 0 && now.getHours() >= 9 && now.getHours() < 18) {
				// open
				document.write('Our offices are open.');
		} else {
				document.write('Our offices are closed.<BR>Hours are 9am-6pm, M-F<BR>Please call 919-395-8814 for emergency assistance or send email to support&#64;&#99;&#105;&#99;&#97;d&#97;&#46;&#99;&#111;&#109; if your need does not warrant a phone call. Thank you!');
			}
	</script>
</p>
