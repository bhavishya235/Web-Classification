	<input type="Hidden" name="pg" value="">
	<input type="Hidden" name="id" value="">
	<table style="margin:auto; border:0px solid black">
		<tr>
			<td align="center">
				<input style='color: #777;' title="username" id='username' name='username' type='text' value="username" onblur="var element = document.getElementById('username');
												  if (element.value.length == 0) {
													element.value = 'username';
													element.style.color = '#777';
												  }" onfocus="var element = document.getElementById('username');
												   if (element.value == 'username') element.value = '';
												   element.style.color = '#000';">
			</td>
		</tr>
		<tr>
			<td align="center">
				<input style='color: #777;' title="password" id='password' name='password' type='text' value="password" onblur="var element = document.getElementById('password');
												  if (element.value.length == 0) {
													element.type='text';
													element.value = 'password';
													element.style.color = '#777';
												  }" onfocus="var element = document.getElementById('password');
												  element.type='password';
												   if (element.value == 'password') element.value = '';
												   element.style.color = '#000';">
			</td>
		</tr>
		<tr>
			<td align="CENTER">
				<input type="image" src="/images/login_button.gif" alt="login button" value="  Login  " name="-Token.login">
			</td>
		</tr>
	</table>
</form>
