    function entsub(ourform) 
    {
        if (window.event && window.event.keyCode == 13)
        {
            var oel = document.getElementById ("ctl00_RightMenu1_hdnRightMenuSubmit"); 
            oel.value="RightMenu";
        }
        else if (window.event.keyCode == 37)
            return false;
        else
            return true;
    }

