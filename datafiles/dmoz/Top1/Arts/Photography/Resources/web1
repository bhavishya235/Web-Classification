    scroll: 1,
    wrap: "circular",
    visible: 4,
    auto: 3,            
    itemFirstInCallback: {
        onAfterAnimation: function(carousel, item, idx, state) {
            $(item).children().css('border-left-color', 'transparent');
        }
    },
    itemFirstOutCallback: {
        onAfterAnimation: function(carousel, item, idx, state) {
