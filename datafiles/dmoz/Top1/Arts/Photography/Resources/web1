				};
				var segmentations = {"currency":"USD","country":"IN","language":"en"};
				var additional_segmentations = {};
				/* merge these two */
				for (var segmentation_key in additional_segmentations) {
					if (additional_segmentations.hasOwnProperty(segmentation_key)) {
						segmentations[segmentation_key] = additional_segmentations[segmentation_key];
					}
				}
				Absinthe.initialize({
					visit_id: 3582285126,
					visitor_id: 2479720410,
					external_account_id: null,
					segmentations: segmentations
				});
			}
		</script>


</head>



<body class="fixed_width home_out language_en chrome os_linux" >

