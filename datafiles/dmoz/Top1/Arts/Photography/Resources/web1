			defaultText: {
				user: "Username",
				pass: "Password"
			}
		});
		

	var ValidatorMessages = new function () {
		this.must_be_provided = "must be provided";
		this.avoid_special_characters = "Please avoid special characters &lt;, &gt;, &quot;, and &amp;";
		this.username_must_be_provided = "Username must be provided";
		this.username_exists = "Username already exists! Please choose another";
		this.agree_to_tos = "Please agree to our terms in order to continue.";
		this.username_no_spaces = "Username cannot contain any spaces";
		this.avoid_special_characters_amp_ok = "Please avoid special characters &lt;, &gt;, and &quot;";
		this.email_must_be_provided =  "Email Address must be provided";
		this.pass_must_be_provided = "Password must be provided";
		this.pass_too_short = "Password must be at least 4 letters/numbers long";
		this.username_password_match = "Password cannot contain your username. Please choose another password.";
		this.email_invalid = "Please enter a valid email address.";
		this.username_invalid_characters = "Username can only contain letters or numbers";
		this.email_exists = "E-mail address already exists!  Please login.";
	};

	document.getElementById('home_registration_form').onsubmit = function() {
		if (HomeFormValidator.validateForm()) { 
			this.submit();
			return true;
		}
		return false;
	};
	HomeFormValidator.init(ValidatorMessages);
	registerForms();
	 var headerPix = new Array("http://s4.picdn.net/images/lohp/hero-1.jpg",
		"http://s6.picdn.net/images/lohp/hero-2.jpg",
		"http://s6.picdn.net/images/lohp/hero-3.jpg",
		"http://s5.picdn.net/images/lohp/hero-4.jpg",
		"http://s2.picdn.net/images/lohp/hero-5.jpg",
		"http://s2.picdn.net/images/lohp/hero-6.jpg",
		"http://s3.picdn.net/images/lohp/hero-7.jpg",
		"http://s3.picdn.net/images/lohp/hero-8.jpg",
		"http://s6.picdn.net/images/lohp/hero-9.jpg",
		"http://s5.picdn.net/images/lohp/hero-10.jpg");
	var randomizer = Math.floor(Math.random() * headerPix.length);
	var $tabs = $$('#image-carousel .nav a');
	var $tray = $('carousel-tray');
	document.getElementById("search-shutterstock").style.backgroundImage = "url(" + headerPix[randomizer] + ")";
	window.carousel = new Carousel($tabs, $tray, { start: 2, duration: 800, speed: 20 }); 
	$('pass_register').observe('keyup', function(e){
		var password = document.getElementById('pass_register');
		var confirm = document.getElementById('pass_register_confirm');
		confirm.value = password.value;
	});

	})();
	</script>

<!-- lilb backend events -->

<script type="text/javascript">
(function(){

	function load_script(script, callback){
		var s = document.createElement('SCRIPT');
		s.type = 'text/javascript';
		s.async = true;
		if(s.readyState){
			s.onreadystatechange = function(){
				if(s.readyState == 'loaded' || s.readyState == 'complete'){
					s.onreadystatechange = null;
					callback();
				}
			}
		}else{
			s.onload = callback;
		}
		s.src=script;
		document.getElementsByTagName('head')[0].appendChild(s);
	}

	function load_lb2(){
		load_script("http://lilb2.shutterstock.com/lilbro.min.js?v=vp03&rev=1", function(){
			if(window.LilBro){
				var base = {"country":"IN","session_id":"7bbc4eda0f7cd262fc3b5255794483c8","variant_ids":"389=1379;158=572;212=815;221=886;542=1853;332=1160;230=958;115=466;218=872;430=1486;377=1334;454=1558;545=1862;551=1874;530=1787;485=1655","language":"en","sort_method":"popular","search_id":"13933557323822138641","thumb_size":"mosaic","ip_address":"103.246.106.9"};

				Ss.lilBro = new LilBro({
					element: document.getElementById('lil_brother'),
					server: 'lilb2.shutterstock.com',
					visit_id_cookie: 'visit_id',
					visitor_id_cookie: 'visitor_id',
					event_base: base,
					watch_focus: true
				});

				Ss.lilBro.write({
					event_type: 'page_load'
				});
				Ss.ResourceReady.ready('lilBro');
			}
		});
	}

	if(!window.JSON){
		load_script('/js/json2.js', load_lb2);
	}else{
		load_lb2();
	}
})();
</script>





<script type="text/javascript">
if (typeof Absinthe !== 'undefined') {
	Absinthe.loadBottom();
}
</script>

</div> <!-- closing div.lil_brother opened in header -->


</body>
</html>




