<script>
/*=============== Popup Request Catalog ===============*/
var pstatus = false;
var trackClosedW = false;
window.onbeforeunload = confirmExit;

function close_this_s(){
    $.colorbox.close();
    $("body").removeClass("pprc");
}

function confirmExit() {
    if(trackClosedW) {
        ga('send', 'event', 'Popup', 'Drop-off', '/photo-articles/archive/how-to-take-great-photos-of-holiday-lights');
    }
}

function cookie_close_act(){
    if(!pstatus){
        setCookieM("rcpopup", "u5l0nHu29", 15);
        // GA T - Close Event
        ga('send', 'event', 'Popup', 'Closed','/photo-articles/archive/how-to-take-great-photos-of-holiday-lights');
        trackClosedW = false;
    }
}

if ($(".popuprc")[0]){
        setTimeout(function () {
            $("body").addClass("pprc");
            $(".popuprc-click").trigger("click");
            $("#cboxClose").addClass("popuprc-click");
            // GA T - Opened
            ga('send', 'event', 'Popup', 'Opened', '/photo-articles/archive/how-to-take-great-photos-of-holiday-lights');
            trackClosedW = true;
        },10000);
}

$(".popuprc-click").colorbox({
    inline:true,
    width:"90%",
    maxWidth:"850",
    overlayClose: false,
    escKey:false,
    fixed: true
});
$(".popuprc-close").on("click", function(){
    cookie_close_act();
    $.colorbox.close();
});

// variable to hold request
var request;

/*
$(".popuprc-next").on("click", function(event){
*/
$("#request-catalog").submit(function(event){

    if($(this).validationEngine('validate')){

        // abort any pending request
        if (request) {
            request.abort();
        }
        // setup some local variables
        var $form = $(this);
        // lets select and cache all the fields
        var $inputs = $form.find("input, select, button, textarea");
        // serialize the data in the form
        var serializedData = $form.serialize();

        // lets disable the inputs for the duration of the ajax request
        $inputs.prop("disabled", true);

        // fire off the request to /form.php
        request = $.ajax({
            url: "/_process/popup/submit",
            type: "post",
            data: serializedData
        });

        // callback handler that will be called on success
        request.done(function (response, textStatus, jqXHR){
            // log a message to the console
            //console.log(response);
            //console.log("Hooray, it worked!");

            $(".popuprc-close").removeClass("popuprc-click");
            $(".pprc-close").on("click", function(){
                $.colorbox.close();
                $("body").removeClass("pprc");
            });
            // Popup status
            pstatus = true;
            setCookieM("rcpopup", "u5l0nHu29", 182);
            // GA T - Step 1
            ga('send', 'event', 'Popup', 'Step 1', $(".popup1 #course").val());
            trackClosedW = false;

            var i = response["i"];
            var s = response["s"];
            var firstName = $(".popup1 #first_name").val();
            var lastName = $(".popup1 #last_name").val();
            var email = $(".popup1 #email").val();
            var course = $(".popup1 #course").val();

            //console.log("First Name: "+ firstName + "     Last Name: " + lastName + "     Email: " +email +"     Course: " + course + "     Value of i: " + i + "     Value of s:" + s);

            var iframeSrc = "/_popup_request_catalog"+"?first_name="+firstName+"&last_name="+lastName+"&email="+email+"&course="+course+"&i="+i+"&s="+s ;
            $(".popup2 iframe").attr("src", iframeSrc);
            $(function(){
                parent.$.colorbox.resize({
                    //innerWidth:$("body").width(),
                    innerHeight:555
                });
            });
            $(".popup1").slideUp();
            $(".popup2").slideDown();

        });

        // callback handler that will be called on failure
        request.fail(function (jqXHR, textStatus, errorThrown){
            // log the error to the console
            console.error(
                "The following error occured: "+
                textStatus, errorThrown
            );
        });

        // callback handler that will be called regardless
        // if the request failed or succeeded
        request.always(function () {
            // reenable the inputs
            $inputs.prop("disabled", false);
        });

        // prevent default posting of form
        event.preventDefault();

    }//If Validation engine Valid




});
function setCookieM(c_name,c_value,c_expiredays) {
    var exdate=new Date();
    exdate.setDate(exdate.getDate()+c_expiredays);
    document.cookie=c_name+ "=" +escape(c_value)+
    ((c_expiredays==null) ? "" : ";expires="+exdate.toGMTString()+"; path=/");
}



</script>

</div>
	</body>
</html>



