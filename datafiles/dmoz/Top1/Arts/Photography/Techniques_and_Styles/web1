<script language="JavaScript">





var browser     = '';





var version     = '';





var entrance    = '';





var cond        = '';





// BROWSER





if (browser == ''){





if (navigator.appName.indexOf('Microsoft') != -1)





browser = 'IE'





else if (navigator.appName.indexOf('Netscape') != -1)





browser = 'Netscape'





else browser = 'NN';





}





// VERSION?





if (version == ''){





version= navigator.appVersion;





paren = version.indexOf('(');





whole_version = navigator.appVersion.substring(0,paren-1);





version         = parseInt(whole_version);





}





// BROWSER & VERSION





if (browser == 'IE' && version >= 4) entrance = 'yes';





if (browser == 'IE' && version < 4) entrance = 'no';





if (browser == 'Netscape' && version >= 2.02) entrance = 'yes';





if (browser == 'Netscape' && version < 2.02) entrance = 'no';





if (entrance=='yes'){





name0a=new Image;name0a.src="graphics/list1.png";





name0b=new Image;name0b.src="graphics/list1rd.png";





name0c=new Image;name0c.src="graphics/list1.png";





name1a=new Image;name1a.src="graphics/list2.png";





name1b=new Image;name1b.src="graphics/list2rd.png";





name1c=new Image;name1c.src="graphics/list2rd.png";





name2a=new Image;name2a.src="graphics/list3.png";





name2b=new Image;name2b.src="graphics/list3rd.png";





name2c=new Image;name2c.src="graphics/list3rd.png";





name3a=new Image;name3a.src="graphics/list4.png";





name3b=new Image;name3b.src="graphics/list4rd.png";





name3c=new Image;name3c.src="graphics/list4rd.png";





name4a=new Image;name4a.src="graphics/list5.png";





name4b=new Image;name4b.src="graphics/list5rd.png";





name4c=new Image;name4c.src="graphics/list5rd.png";





name5a=new Image;name5a.src="graphics/list6.png";





name5b=new Image;name5b.src="graphics/list6rd.png";





name5c=new Image;name5c.src="graphics/list6rd.png";





}





function lighten(imgName) {





if (entrance == 'yes'){





imgOn = eval(imgName + 'b.src');





document[imgName].src = imgOn;





}





}





function darken(imgName) {





if (entrance == 'yes'){





imgOff = eval(imgName + 'a.src');





document[imgName].src = imgOff;





}





}





function darken2(imgName) {





if (entrance == 'yes'){





imgClick = eval(imgName + 'c.src');





document[imgName].src = imgClick;





}





}











  </script>

