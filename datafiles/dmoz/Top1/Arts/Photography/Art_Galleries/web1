        <script type="text/javascript" >
    // Put multiple current exhibitions side by side.
    $(function() {
        var $list = $('.exhibitionList.regular');
        var $items = $list.find('li');
        if ($items.length > 1 ) {
            if ($items.length < 2) return;

            // Determine width
            var current_width = $list.width();
            var max_width = parseInt(current_width / 2) - 100;

            // Set up layout styles
            // items are floated, with every 3rd item on a new line.
            // A singled item at the end is centered on its own line.
            $items.width(max_width).css({
                'float': 'left'
            }).filter(':nth-child(odd)').css({
                'margin-right': 40
            }).end().filter(':last-child').each(function() {
                if ($items.length % 2 != 0 ) {
                    // center last child if odd number of items.
                    $(this).css({
                        'float': 'none', clear: 'both',
                         marginLeft: 'auto', marginRight: 'auto'
                    });
                }
            }).end().find('h2').css('font-size', '18px').end().find('a > img').each(function() {
                // Here we resize the images to fit the scaled layout.
                var src = $(this).attr('src');
                src = src.replace(/w=[^&]+/, 'w=' + max_width);
                var preload = new Image();
                var $img = $(this);
                preload.onload = function() {
                    var w = this.width;
                    //if (w > max_width) return;
                    $img.attr('src', src);
                }
                preload.src = src;                
            });

            // List styles.
            $(window).load(function(){ $list.width(max_width*2 + 70); })
        }
    });
    </script>
    
