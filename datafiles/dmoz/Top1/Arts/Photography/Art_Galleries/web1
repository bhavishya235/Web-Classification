function makeCaps(a) {

  g=a.split(' ');for (l=0;l<g.length;l++) g[l]=g[l].toUpperCase().slice(0,1)+g[l].slice(1);

  return g.join(" ");

}

function getLoc(c) {

  var d="";if (c>0) for (k=0;k<c;k++) d=d+"../"; return d;

}

