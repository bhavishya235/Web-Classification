		<!--[if IE]>
		<script type="text/javascript">
			function fixOutOfMemoryError() {
				__flash_unloadHandler = function() {};
				__flash_savedUnloadHandler = function() {};
			}
			window.attachEvent("onbeforeunload", fixOutOfMemoryError);
