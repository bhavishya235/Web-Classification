<SCRIPT language="Javascript" type="text/javascript">
function validateEmail () {
 if (document.email.Email.value.length < 2) {
            window.alert("Please enter your email address.");
            document.email.Email.focus();
                return false;
        }
    else {
		if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(document.email.Email.value)){
		}
		else {
			alert("Invalid E-mail Address! Please re-enter.");
			document.email.Email.focus();
			return false;
		}
	}
return true;
}
</script>
<SCRIPT language="Javascript" type="text/javascript">
function footerEmailSignUp () {
	if (validateEmail(true)){
			document.email.action = "http://www.cohassetcolonials.com/catalog/emailList.php";
			document.email.submit();
	}
}
</SCRIPT>

<script type="text/javascript">

function kH(e) {
var pK = document.all? window.event.keyCode:e.which;
return pK != 13;
}
document.onkeypress = kH;
if (document.layers) document.captureEvents(Event.KEYPRESS);
</script>

<SCRIPT language="Javascript" type="text/javascript">
function validate() {
		
		
     if (document.search.search_term.value.length < 1) {

            window.alert("Please enter a search term.");

                return false;

        }
        return true;
}
</SCRIPT>

<script type="text/javascript">
function noenter() {
return !(window.event && window.event.keyCode == 13); 
}
</script>

<SCRIPT  language="Javascript" type="text/javascript">

function Search() {
	if (validate(true)){
		document.search.action = 'http://www.cohassetcolonials.com/catalog/search_results.php';
		document.search.submit();
		return true;
	}
}
</SCRIPT>
<script type="text/javascript">
	function EmailFriendWindow(URL, type) {
		window.open('http://www.cohassetcolonials.com/catalog/emailAFriend.php?pageURL=' + URL + '&type=', 'tellfriend', 'width=400,height=400,scrollbars=yes');
	}
</script>

