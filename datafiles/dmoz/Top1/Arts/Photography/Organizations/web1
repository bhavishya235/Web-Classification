//	Resolve background banding in long pages.
var tempBody = document.body,
tempHtml = document.documentElement;
var currentHeight = Math.max( tempBody.scrollHeight, tempBody.offsetHeight, tempHtml.clientHeight, tempHtml.scrollHeight, tempHtml.offsetHeight );
//alert (currentHeight);
document.write("<style type='text/css'>\nbody { height: "+currentHeight+"px }\n</style>");
</script>
