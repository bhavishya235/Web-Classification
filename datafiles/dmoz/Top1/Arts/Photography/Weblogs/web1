document.onkeyup = KeyCheck;       
 
	function KeyCheck(e)
		{
		   var KeyID = (window.event) ? event.keyCode : e.keyCode;
 
		   switch(KeyID)
		   {
 
		      case 37:
		      window.location = "http://www.absenter.org/2010/09/13/northward";
		      break;
 
		      case 38:
		      window.location = "/";
		      break;
 
		      case 39:
		      window.location = "";
		      break;
 
		      case 40:
		      window.location = "/archive/";
		      break;

		   }
		}
</script>

