      GROUPS.Y.use = function () {
            var arr = Array.prototype.slice.call(arguments),
                len = arguments.length,
                cb = arguments[len -1];

            if (Y.Lang.isFunction(cb)) {
                arr[len-1] = function () {
                    try{
                        return cb.apply(this, arguments);
                    } catch (err) {
                        try{
                            trackYGJSError(err.message, err.fileName, err.lineNumber);
                        }catch(e){}
                    }
                }; 
            }
            use.apply(this, arr);
      };
