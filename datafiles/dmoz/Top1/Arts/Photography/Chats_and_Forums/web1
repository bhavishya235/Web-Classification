<script type="text/javascript">//<![CDATA[
jQuery(document).ready(function(){
	// tooltips
    if (jQuery("[rel=tooltip]").length) {
		jQuery("[rel=tooltip]").tooltip();
	} //endif

	// popovers
	jQuery(".pop").each(function() {
		var popElem= jQuery(this);
		popElem.popover(
			{
				trigger: 'hover',
				placement:'right',
				delay: {show: 500, hide: 3000},
				title: getPopTitle(popElem.attr("id")),
			 	content: getPopContent(popElem.attr("id"))
			}
		);
	});
	function getPopTitle(target) {
		return jQuery("#" + target + "-content > .popTitle").html();
	}; //endfunc
	function getPopContent(target) {
		return jQuery("#" + target + "-content > .popContent").html();
	}; //endfunc

	// alerts
    if (jQuery(".alert").length) {
		jQuery(".alert").alert();
	} //endif

	// carousel
	jQuery(document).ready(function(){
		jQuery('.carousel').carousel({
			interval: 20000
		})
	})

	// autofocus support
	function hasAutofocus() {
		var elem = document.createElement("input");
		return 'autofocus' in elem;
	}; //endfunc
	if (! hasAutofocus()) {
		jQuery("input[autofocus=true]").focus();
	} //endif
	
	if (jQuery('.post').length > 1) {
		jQuery('<div id="ads_inline_place" class="indent-small"><\/div>').insertAfter(jQuery(".post:eq(0)"));
	} else if (jQuery('.thumbnail-photos-matrix').length > 3) {
		jQuery('<li class="span9"><div id="ads_inline_place" class="indent-small"><\/div></li>').insertAfter(jQuery(".thumbnails .span3:eq(2)"));
	} else if (jQuery('.thumbnail-users-matrix').length > 3) {
		jQuery('<li class="span9"><div id="ads_inline_place" class="indent-small"><\/div></li>').insertAfter(jQuery(".thumbnails .span3:eq(2)"));
	} else {
		jQuery("#ads_inline_block").hide();
	} //endif
	
});

function toggleDiv(divId) {
   jQuery("#"+divId).toggle('fast');
} //endfunc
//]]></script>

