	<script type="text/javascript">
		document.observe( 'dom:loaded', function() {
			$( 'button1' ).observe( 'click', function(){
				$( 'button1' ).addClassName( 'selected' );
				$( 'button2' ).removeClassName( 'selected' );
				$( 'button3' ).removeClassName( 'selected' );
				$( 'noutati_scroll' ).visualEffect( 'Move', { x: 0, mode: 'absolute', queue: 'end' } );
			});
			$( 'button2' ).observe( 'click', function(){
				$( 'button1' ).removeClassName( 'selected' );
				$( 'button2' ).addClassName( 'selected' );
				$( 'button3' ).removeClassName( 'selected' );
				$( 'noutati_scroll' ).visualEffect( 'Move', { x: -304, mode: 'absolute', queue: 'end' } );
			});
			$( 'button3' ).observe( 'click', function(){
				$( 'button1' ).removeClassName( 'selected' );
				$( 'button2' ).removeClassName( 'selected' );
				$( 'button3' ).addClassName( 'selected' );
				$( 'noutati_scroll' ).visualEffect( 'Move', { x: -608, mode: 'absolute', queue: 'end' } );
			});
		});
		
	</script>




