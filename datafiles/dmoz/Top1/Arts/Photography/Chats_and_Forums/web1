<!-- InstanceBeginEditable name="MainText" -->

		<script type="text/javascript">//<![CDATA[
        function setIndTop(strId)
        {
            if (strId == "pot") {
                jQuery("#indtop3").hide();
                jQuery("#indtop3_link").fadeIn();
                jQuery("#indtop3_text").hide();

                jQuery("#indpot3").fadeIn();
                jQuery("#indpot3_link").hide();
                jQuery("#indpot3_text").fadeIn();
				
                jQuery("#indtop2").hide();
                jQuery("#indtop2_link").fadeIn();
                jQuery("#indtop2_text").hide();
				
                jQuery("#indpot2").fadeIn();
                jQuery("#indpot2_link").hide();
                jQuery("#indpot2_text").fadeIn();
            } else {
                jQuery("#indtop3").fadeIn();
                jQuery("#indtop3_link").hide();
                jQuery("#indtop3_text").fadeIn();
				
                jQuery("#indpot3").hide();
                jQuery("#indpot3_link").fadeIn();
                jQuery("#indpot3_text").hide();

                jQuery("#indtop2").fadeIn();
                jQuery("#indtop2_link").hide();
                jQuery("#indtop2_text").fadeIn();
				
                jQuery("#indpot2").hide();
                jQuery("#indpot2_link").fadeIn();
                jQuery("#indpot2_text").hide();
            } //endif
            setMyCookie("indtop", strId, 60*60*24*30*6, "/");
        } //endfunc
        jQuery(document).ready(function(){
            if (getMyCookie("indtop") == "pot") {
                setIndTop("pot")
            } else {
                setIndTop("top")
            } //endif
			
			jQuery("#indtop3_link, #indtop2_link").click(function(event) {
				setIndTop('top');
			});
			jQuery("#indpot3_link, #indpot2_link").click(function(event) {
				setIndTop('pot');
			});
			
			var _hidethumb = getMyCookie('hidethumb');
			if (_hidethumb != '1') {
				jQuery(".hidethumb0").hide();
				jQuery(".hidethumb1").show();
			} else {
				jQuery(".hidethumb0").show();
				jQuery(".hidethumb1").hide();
			} //endif
			jQuery(".hidethumb0").click(function(event){
				setMyCookie('hidethumb', '0', 60*60*24*30*3, '/');
				window.location = location.href + (window.location.search ? '&' : '?') +'rnd=' + Math.round(Math.random() * 999);
			});
			jQuery(".hidethumb1").click(function(event){
				setMyCookie('hidethumb', '1', 60*60*24*30*3, '/');
				window.location = location.href + (window.location.search ? '&' : '?') +'rnd=' + Math.round(Math.random() * 999);
			});
			
        });
        //]]></script>

