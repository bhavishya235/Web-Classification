<!-- DO NOT REMOVE Start rules tab hack -->
<script type="text/javascript">

//colored borders for debugging
//document.getElementsByTagName('table')[0].style.cssText += 'border:solid red 1px; background-color:purple';
//document.getElementsByTagName('table')[0].setAttribute('cellspacing', 1);

//get the rows we are going to work with
rows = document.getElementsByTagName('table')[0].getElementsByTagName('tr');

//adjust top row to allow for new tab
rows[0].getElementsByTagName('td')[2].setAttribute('colSpan',17);

//create the cells for the top of the tab
top1 = document.createElement('td');
top1.setAttribute('rowSpan', 3);
top1.innerHTML = '<img src="/images/tab-unselected-unselected.gif" width="21" height="31">';

top2 = document.createElement('td');
top2.className = "unselectedtop";
top2.innerHTML = '<img src="/images/spacer.gif" width="40" height="1">';

//create the cell for the middle of the tab
mid = document.createElement('td');
mid.className = "unselectedtab";
mid.setAttribute('width', 40);
mid.setAttribute('height', 29);
mid.setAttribute('align', 'center');
mid.innerHTML = '<div class="tabtext" style="color:#ffffff;"><a href="http://www.photosig.com/go/main/help?name=rules">Rules</a></div>';

//create the shim cells
shim1 = document.createElement('td');
shim1.className = "black";
shim1.innerHTML = '<img src="/images/spacer.gif" height="1" width="1">';

shim2 = document.createElement('td');
shim2.className = "black";
shim2.innerHTML = '<img src="/images/spacer.gif" height="1" width="1">';


//add the cells for the top of the tab and move the spacer to the end
rows[1].appendChild(top1);
rows[1].appendChild(top2);
rows[1].appendChild(rows[1].getElementsByTagName('td')[14]);

//add the cell for the middle of the tab
rows[2].appendChild(mid);

//add the shim cell
rows[3].appendChild(shim1);
rows[3].appendChild(shim2);
</script>
<!-- end chat tab hack -->



