	document.observe( 'dom:loaded', function() {
		$( 'button1_1' ).observe( 'click', function(){
			$( 'button1_1' ).addClassName( 'selected' );
			$( 'button2_1' ).removeClassName( 'selected' );
			$( 'button3_1' ).removeClassName( 'selected' );
			$( 'discutii_scroll' ).visualEffect( 'Move', { x: 0, mode: 'absolute', queue: 'end' } );
		});
		$( 'button2_1' ).observe( 'click', function(){
			$( 'button1_1' ).removeClassName( 'selected' );
			$( 'button2_1' ).addClassName( 'selected' );
			$( 'button3_1' ).removeClassName( 'selected' );
			$( 'discutii_scroll' ).visualEffect( 'Move', { x: -304, mode: 'absolute', queue: 'end' } );
		});
		$( 'button3_1' ).observe( 'click', function(){
			$( 'button1_1' ).removeClassName( 'selected' );
			$( 'button2_1' ).removeClassName( 'selected' );
			$( 'button3_1' ).addClassName( 'selected' );
			$( 'discutii_scroll' ).visualEffect( 'Move', { x: -608, mode: 'absolute', queue: 'end' } );
		});
	});
	
</script>


