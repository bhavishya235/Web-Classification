	<script type="text/javascript">
		document.observe( "dom:loaded", function() { $( 'buttonMenuLogin' ).observe( 'click', login_function); });
		
		function login_function() {
		
				$('salveaza').hide();
    			$( 'divLoadingLogin' ).show();
    			$( 'divLoadForm' ).hide();
    			$( 'formMenuLogin' ).request({
    				onSuccess: function( transport ) {
    					if( transport.responseText.isJSON() )
    					{
    						response = transport.responseText.evalJSON();
    						if( 'ok' == response.status )
    						{
    							$( 'login' ).hide();
    							new Insertion.After( 'main_menu', '<div id="user_menu"></div>' );
    							new Ajax.Updater( 'user_menu', '/meniu-utilizator' ); 
    						}
    						else
    						{
    							$( 'divLoadingLogin' ).hide();
    							$( 'divLoadForm' ).show();
								$( 'username' ).setStyle({
									borderColor: '#ff0033',
									backgroundColor: '#ffcccc'
								});
								
								$( 'parola' ).setStyle({
									borderColor: '#ff0033',
									backgroundColor: '#ffcccc'
								});
    							//alert( response.errorMessage );
    						}
    					}
    					else
    					{
    						$( 'divLoadingLogin' ).hide();
    						$( 'divLoadForm' ).show();
    						alert( 'Raspuns: ' + transport.responseText );
    					}
    				},
    				onFailure: function() {
    					$( 'divLoadingLogin' ).hide();
    					$( 'divLoadForm' ).show();
    					alert( 'Nu s-a primit raspuns de la server!' );
    				},
    				evalScripts: true })
    			}
    </script>
</div>					</div>
										</div>
					
