			<div style="float:left;">Utilizator: <input type="text" id="username" name="username" value="" style="height:14px; margin-right:20px;width:80px" class="top"/></div>
			<div style="float:left;">Parola: <input type="password" id="parola" name="parola" value="" style="height:14px;margin-right:10px;width:80px" class="top" onKeyPress="return submitenter(this,event);" onFocus="$('salveaza').show();" /></div>
			<div style="float:left;"><input type="checkbox" id="retine" name="retine" value="retine" style="width:none; padding:0px; margin:0px; border-width:0px; background-color:red;width:12px; height:12px; margin-top:4px;"  /></div>
