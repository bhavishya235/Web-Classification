	<script type="text/javascript">
		document.observe( 'dom:loaded', function() {
			$( 'button1_2' ).observe( 'click', function(){
				$( 'button1_2' ).addClassName( 'selected' );
				$( 'button2_2' ).removeClassName( 'selected' );
				$( 'button3_2' ).removeClassName( 'selected' );
				$( 'blog_scroll' ).visualEffect( 'Move', { x: 0, mode: 'absolute', queue: 'end' } );
			});
			$( 'button2_2' ).observe( 'click', function(){
				$( 'button1_2' ).removeClassName( 'selected' );
				$( 'button2_2' ).addClassName( 'selected' );
				$( 'button3_2' ).removeClassName( 'selected' );
				$( 'blog_scroll' ).visualEffect( 'Move', { x: -304, mode: 'absolute', queue: 'end' } );
			});
			$( 'button3_2' ).observe( 'click', function(){
				$( 'button1_2' ).removeClassName( 'selected' );
				$( 'button2_2' ).removeClassName( 'selected' );
				$( 'button3_2' ).addClassName( 'selected' );
				$( 'blog_scroll' ).visualEffect( 'Move', { x: -608, mode: 'absolute', queue: 'end' } );
			});
		});
		
	</script>




