<!-- sayitpjs -->

<script type="text/javascript">

$(document).ready(function()
{
	//If user submits the form
	$("#message").submit(function(e)
	{	
		e.preventDefault();
		var fd = new FormData(document.forms.namedItem("message"));
		//var fn = document.getElementById('file');
		$.ajax({
			type: "POST",
			url: "/cgi-bin/pimgup?callback=?", 
			data: fd,
			processData: false,
			contentType: false,
			crossDomain: true,
			success: parseData
		});				
		//e.preventDefault();
		return false;
	});
	
});

function parseData(data)
{
	var text = '';
	if (typeof data !== "undefined")
	{
		$("#upimg").html('<img src="' + data.file + '" style="max-width:450px; max-height: 400px;" border=0><br>');
		document.getElementById('pimg').style.display = 'none';
		document.postform.imgurl.value = data.file;
	}
}

$(document).ready(function()
{
	$("#postform").submit(function(e)
	{	
		document.getElementById('postbox').style.display = 'none';
		document.getElementById('loadingbox').style.display = '';
		e.preventDefault();
		//var fd2 = new FormData(document.forms.namedItem("postform"));
		$.ajax({
			type: "POST",
			url: "/cgi-bin/postinline1?callback=?", 
			data: $("#postform").serialize(),
			processData: false,
			contentType: false,
			crossDomain: true,
			success: parseData2
		});				
		return false;
	});
	
});

function parseData2(data)
{
	document.getElementById('loadingbox').style.display = 'none';
	document.getElementById('messagebox').style.display = '';
}

$(document).ready(function()
{
	$("#ringsearch").click(function(e)
	{	
		document.getElementById('lookingup').style.display = '';
		e.preventDefault();
		$.ajax({
			type: "POST",
			url: "/cgi-bin/findrings?callback=?", 
			data: $("#postform").serialize(),
			//processData: false,
			//contentType: false,
			//crossDomain: true,
			//success: parseData3
			crossDomain: true,
			beforeSend: function(x) 
			{
				if (x && x.overrideMimeType) 
				{
					x.overrideMimeType("application/j-son;charset=UTF-8");
				}
			},
			dataType: "jsonp",
			success: function(data){parseData3(data);}
		});				
		return false;
	});
	
});

function parseData3(data)
{
	var text = '';
	var line = 'selected';
	document.getElementById('lookingup').style.display = 'none';
	if (typeof data !== "undefined")
	{
		text = 'Ring: <select name="ringid"><option value="">' + "\n";;
		for (var i = 0; i < data.rings.length; i++)
		{
			text = text + '<option value="' + data.rings[i].ring + '" ' + line + '>' + data.rings[i].title + "</option>\n";
			line = '';
		}
		text = text + "</select> WebRing to post to.\n";
		$("#ringselect").html(text);
	}
	//document.getElementById('ringoption').style.display = '';
	//	document.getElementById('ringselect').style.display = '';
}

</script>


