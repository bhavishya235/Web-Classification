			<td>
				<div style="padding:5px"><script type="text/javascript">

var Quotation=new Array() // do not change this!


Quotation[0] = "\"It's your attitude, not your aptitude, that determines your altitude.\" - Fortune Cookie";
Quotation[1] = "\"It’s a good time to finish up old tasks.\" - Fortune Cookie";
Quotation[2] = "\"You will enjoy good health and financial independence.\" - Fortune Cookie";
Quotation[3] = "\"You are not a person who can be ignored.\" - Fortune Cookie";
Quotation[4] = "\"Forget those things that aren't worth remembering.\" - Fortune Cookie";
Quotation[5] = "\"We learn by practice.\" - Fortune Cookie";
Quotation[6] = "\"Action is the basis of success.\" - Fortune Cookie";
Quotation[7] = "\"Advancement will come with hard work.\" - Fortune Cookie";
Quotation[8] = "\"Diligence and modesty can raise your social status.\" - Fortune Cookie";
Quotation[9] = "\"To the world you may be one person, but to one person you may be the world.\" - Fortune Cookie";
Quotation[10] = "\"Who is Fortune Cookie? I've never heard of her.\" - Don't ask.";



var Q = Quotation.length;
var whichQuotation=Math.round(Math.random()*(Q-1));
function showQuotation(){document.write(Quotation[whichQuotation]);}
showQuotation();
</script></div>			</td>
		</tr>
		</table>
						<!-- right Column  end -->
    </div>
    <div class="clear-columns"><!-- do not delete --></div>
  </div>
</div>
<!--body end-->
