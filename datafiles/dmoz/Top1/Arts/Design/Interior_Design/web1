								    function enrollServerName() {
								        var theForm = document.forms['aspnetForm']; // To make Firefox happy.
								        var tempHidSessionTokenString = '';

								        if (theForm.ctl00$CartItemsNumber1$hidSessionTokenString) {
								            tempHidSessionTokenString = theForm.ctl00$CartItemsNumber1$hidSessionTokenString.value;
								        }
								        if (theForm.ctl00$ctl00$CartItemsNumber1$hidSessionTokenString) {
								            tempHidSessionTokenString = theForm.ctl00$ctl00$CartItemsNumber1$hidSessionTokenString.value;
								        }
								        if (theForm.ctl00$ctl00$ctl00$CartItemsNumber1$hidSessionTokenString) {
								            tempHidSessionTokenString = theForm.ctl00$ctl00$ctl00$CartItemsNumber1$hidSessionTokenString.value;
								        }								       
								        
//										if (aspnetForm.ctl00$CartItemsNumber1$hidSessionTokenString) {
//										    tempHidSessionTokenString = aspnetForm.ctl00$CartItemsNumber1$hidSessionTokenString.value;
//										}
//										if (aspnetForm.ctl00$ctl00$CartItemsNumber1$hidSessionTokenString) {
//										    tempHidSessionTokenString = aspnetForm.ctl00$ctl00$CartItemsNumber1$hidSessionTokenString.value;
//										}
//										if (aspnetForm.ctl00$ctl00$ctl00$CartItemsNumber1$hidSessionTokenString) {
//										    tempHidSessionTokenString = aspnetForm.ctl00$ctl00$ctl00$CartItemsNumber1$hidSessionTokenString.value;
//										}
										switch (document.domain) {
											case 'www.uclaextension.edu':
												window.open('https://enrollhi.uclaextension.edu/Checkout/Default.aspx' + tempHidSessionTokenString, '_self');
												break;
								            case 'qa.uclaextension.edu':
								                window.open('https://enrollqa.uclaextension.edu:444/Checkout/Default.aspx' + tempHidSessionTokenString, '_self');
												break;
								            case 'wssdev.uclaextension.edu':
								                window.open('https://enrollwssdev.uclaextension.edu:9443/Checkout/Default.aspx' + tempHidSessionTokenString, '_self');
												break;
											case 'rrachman-wss01':
												window.open('https://enrollrrachman-wss01:9443/Checkout/Default.aspx' + tempHidSessionTokenString, '_self');
												break;
											case 'jsmithvmsvr-03':
												window.open('https://enrolljsmithvmsvr-03:9443/Checkout/Default.aspx' + tempHidSessionTokenString, '_self');
												break;
											case 'dev-sp2010-1':
												window.open('https://dev-sp2010-1.uclaextension.edu:9443/Checkout/Default.aspx' + tempHidSessionTokenString, '_self');
												break;
											case 'web-sp1.uclaextension.edu':
												window.open('https://enrollhi.uclaextension.edu/Checkout/Default.aspx' + tempHidSessionTokenString, '_self');
												break;
											case 'admin-sp1.uclaextension.edu':
