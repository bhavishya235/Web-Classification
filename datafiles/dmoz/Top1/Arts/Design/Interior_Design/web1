function noRightClick(buttonClicked) {
if (navigator.appName == 'Netscape' && 
(buttonClicked.which == 2 || buttonClicked.which == 3))
 return false;
else if (navigator.appName == 'Microsoft Internet Explorer' && 
(event.button == 3 || event.button == 2)) {
 alert("Copyright  FabJob.com. All rights reserved.\nReproduction of any portion of this website is strictly prohibited.");
return false;
}
return true;
}
document.onmousedown=noRightClick;
document.onmouseup=noRightClick;
if (document.layers)
 window.captureEvents(Event.MOUSEDOWN);
if (document.layers) window.captureEvents(Event.MOUSEUP);
 window.onmousedown=noRightClick;
 window.onmouseup=noRightClick;
</script>





