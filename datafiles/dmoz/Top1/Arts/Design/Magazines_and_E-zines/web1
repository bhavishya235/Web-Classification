		var cache = new Array(60); 		var unixtime = new Date; // Generic JS date object
		var time = unixtime.getTime();
		var waiting = false;
		var lastadload = unixtime.getTime();
		var addelay = 600; 				
		function gotoimage(imagenum){
			currentimage = imagenum;
			doReq();
			return false;
		}
		$('#nextlink').click(function() {
						if (waiting == true) {
				return false; 			} 
				
			currentimage++;
			doReq();
			return false;				  
		});
		
		$('#imagelink').click(function() {
						if (waiting == true) {
				return false; 			} 
				
			currentimage++;
			doReq();
			return false;
		});
		
		$('#prevlink').click(function() {
						if (waiting == true) {
				return false; 			} 
				
			currentimage--;
			doReq();					
			return false;
		});
		
				nextImage = (function(){
			if (waiting == true) {
				return false; 			}
				
			currentimage++;
			doReq();
			return false;
		});
		
		externalTimedReq = (function(){doTimedReq();});
					
				checkhash();	
				buildfirstCache();
				highlightCurrent();
		function checkhash(){
						var hash = window.location.hash.replace('#',"");
			if ((Number(hash) != 0)){
				currentimage = hash;
				doTimedReq();
			}
		}
		function doReq(){
			if (waiting == true) {
				return; 			}		
				
			highlightCurrent();
			var delay = 100; 			unixtime = new Date;
			var waittime = unixtime.getTime() - time; 			if (waittime > delay){
				doTimedReq(); 			}else{
								setTimeout("externalTimedReq()",delay - waittime );
								waiting = true;
			}
		}
		
		function doTimedReq(){
						waiting = false; 			unixtime = new Date; //Recreate the time..
			data = ajaxdata;
			if (currentimage > 60)
			{
				currentimage = 1;
			}				
			if (currentimage < 1) {
				currentimage = 60;
			}
			
			window.location.hash = currentimage;

			if (data[currentimage].main)
			{$('#mainslide').html(data[currentimage].main);}				
			if (data[currentimage].title) {$('#title').html(data[currentimage].title);}				
			if (data[currentimage].subtitle) {$('#subtitle').html(data[currentimage].subtitle);}				
			if (data[currentimage].picture_count && data[currentimage].entry_id)
			{makeImages();}				
			if (data[currentimage].full_link){
				$('#fullarticlelink').attr('href',data[currentimage].full_link);
				$('#titlelink').attr('href',data[currentimage].full_link);
				$('#subimages').attr('href',data[currentimage].full_link);
			}
//			if (data[currentimage].sharebutton){
//				$('#sharebutton').html(data[currentimage].sharebutton);
//			}
						if (unixtime.getTime() - lastadload > addelay){
				makeAds();
				lastadload = unixtime.getTime();
			}
			buildCache();
			//Hightlight the current tab
			highlightCurrent();
						time = unixtime.getTime();
		}
		function preloadImage(imgpath){
			var img = new Image();
			img.src = "http://cdn.trendhunterstatic.com/phpthumbnails/"+imgpath;
			$('#imagecache').append(img);
		}
		function loadImage(imgpath){
			$('#mainslide').html("<img onclick='nextImage()' class='mainImageBig' src='http://cdn.trendhunterstatic.com/phpthumbnails/"+imgpath +"'></img>");
		}
		function makeImages(){
			var x;
			$('#subimages').html('');
			if (data[currentimage].picture_count > 1){
			for (x=1;(x<=(data[currentimage].picture_count)) && (x <=6);x++)
				{
					var img = new Image();
					img.src = 'http://cdn.trendhunterstatic.com/phpthumbnails/'+parseInt(data[currentimage].entry_id/1000) + "/"+data[currentimage].entry_id + "/" + data[currentimage].entry_id + "_" + x + "_80.jpeg";
					$('#subimages').append('<a href="#" onMouseover="preloadImage(\''+parseInt(data[currentimage].entry_id/1000) + "/"+data[currentimage].entry_id + "/" +data[currentimage].entry_id + "_" + x + "_800.jpeg" +'\');" onclick="loadImage(\''+parseInt(data[currentimage].entry_id/1000) + "/"+data[currentimage].entry_id + "/"+data[currentimage].entry_id + "_" + x + "_800.jpeg" +'\');return false"><img src="' + img.src + '"></img></a>');
				}					
			}
		}
		var imgtracker = 0;
		function makeAds(){
									if (data[currentimage].entry_id){
				imgtracker++;
				var img = new Image();
				img.src = 'http://live.trendhunter.com/trackingpixel?entry_id='+data[currentimage].entry_id+'&img='+imgtracker;
				$('#trackingpixels').append(img);	
			}
			_gaq.push(['_trackPageview', '/frontpage-top/'+imgtracker]);
		}
		function highlightCurrent(){
			//Reset anything that might have been set before..
			$('.selected').removeClass('selected');
			$('.subselected').removeClass('subselected');
			$('.quicklink span').hide();
			$('#subtop'+currentimage).addClass('subselected');
			if (((currentimage) > 0) && ((currentimage) <= 20)){
				$('#TodayLink').addClass('selected');
				$('#TodaySub').show();
			}else if (((currentimage) >  20) && ((currentimage) <= 40)){
					$('#WeekLink').addClass('selected');
					$('#WeekSub').show();
			}else if (((currentimage) >  40) && ((currentimage) <= 60)){
					$('#MonthLink').addClass('selected');
					$('#MonthSub').show();
			}
		}
		
		function buildCache(){
						data = ajaxdata;
			var ahead = ((currentimage + 3 -1)%60)+1;
			var behind = ((currentimage + 60 - 3 -1)%60) +1;
			
			addCache(behind);
			addCache(ahead);
		}
		
		function buildfirstCache(){
			var tempimage = currentimage;
			tempimage = ((tempimage - 3 -1) % 60) +1;
			var i;
			for (i = 0; i < 2*3;i++){
				var thisimage = ((tempimage + 60+i -1)%60)+1
				addCache(thisimage);
			}
		}
		
		function addCache(image){
			data = ajaxdata;
			if (data[image] && ($.inArray(image,cache) == -1)){//Check if the image exists, and whether it has been cached before
				var main = data[image].main;
				//Check if the lead is an image
				if (main.indexOf('<img') != -1){$('#imagecache').append(main);}
				//For all the little images (up to 6) add them to the cache
				
				for (x=1;(x<=(data[image].picture_count)) && (x <=6);x++)
				{
					var img = new Image();
					img.src = 'http://cdn.trendhunterstatic.com/phpthumbnails/' +parseInt(data[image].entry_id/1000) + "/"+data[image].entry_id + "/"+ data[image].entry_id + "_" + x + "_80.jpeg";
					$('#imagecache').append(img);
				}				
				cache[image] = image;
			}
		}

		
					
	</script>





