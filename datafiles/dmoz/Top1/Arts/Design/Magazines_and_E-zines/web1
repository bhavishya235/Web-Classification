		$.getJSON('/embed-templates/header-ajax', function(data) {
			if (data.error){
				alert(data.error);
				return;
			}
			if(data.fresh){
				$.each(
					data.fresh,
					function( Index, html){$('#'+Index).html(html);}
					);
			}
		});
