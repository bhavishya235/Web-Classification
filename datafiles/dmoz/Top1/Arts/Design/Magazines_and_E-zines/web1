    <script>
		///*** WIDE OR NARROW  & SUBMENU ***///
			function responso() {
				var obj = $('ul.submenu li.active').parent().parent();
				obj.addClass('active');
				var response = $(window).width();
					if (response < 960) { 
						$('body').removeClass('wide');
						$('body').addClass('narrow');
						$('.submenu').css('left','158px');
						$('#nav > li').find('> a').css('width','110px');
						obj.find('> a').css('width','124px');
						//obj.find('ul.submenu').animate({ display:'block', opacity: 1 }, 350 );
					} else {
						$('body').removeClass('narrow');
						$('body').addClass('wide');
						//var dent = obj.width()-80+'px';
						//$('#nav > li').find('> a').css('width','auto');
					//	$('#nav > li.active').find('.submenu').css('left',dent);
					};
			};

			$(document).ready(function() { responso(); });
			$(window).resize(function() {	responso(); }).trigger("resize");
			

		</script>

    
