		<script>
var slideMegaMenu;
$(document).ready(function() 
{
	var onMegaMenu = false;
	var currentMenuItem = "";
	var $oe_menu		= $('#NavMegaMenu');
	var $oe_menu_items	= $oe_menu.children('li');
	var $oe_overlay		= $('#oe_overlay');

	/*$(function() 
	{
		var $oe_menu		= $('#NavMegaMenu');
		var $oe_menu_items	= $oe_menu.children('li');
		var $oe_overlay		= $('#oe_overlay');

		$oe_menu_items.bind('mouseenter',function(){
			var $this = $(this);
			$this.addClass('slided selected');
			$this.children('div').css('z-index','9999').stop(true,true).show();
		}).bind('mouseleave',function(){
			var $this = $(this);
			$this.removeClass('selected').children('div').css('z-index','1');
		});

		$oe_menu.bind('mouseenter',function(){
			var $this = $(this);
			$oe_overlay.show().stop(true,true).fadeTo(200, 0.6);
			$this.addClass('hovered');
		}).bind('mouseleave',function(){
			var $this = $(this);
			$this.removeClass('hovered');
			$oe_overlay.stop(true,true).fadeTo(200, 0).hide();
			$oe_menu_items.children('div').hide();
		})
    });*/

	slideMegaMenu = function(tab)
	{
	
		if(!onMegaMenu)
			return false;
		
		$tab = $("#"+tab);
		var menuItem = tab;

		if(menuItem != currentMenuItem)
			return false;

		$oe_overlay.show().stop(true,true).fadeTo(200, 0.6);			
		$tab.addClass('slided selected');
		$tab.children('div').css('z-index','9999').stop(true,true).show();
	}

    $(function() 
	{
		$oe_menu_items.bind('mouseenter',function() {
			var $this = $(this);
			currentMenuItem = $this.attr('id');				
			if(!onMegaMenu)
			{setTimeout("slideMegaMenu('"+currentMenuItem+"')",700);}
			else
			{
				//setTimeout("slideMegaMenu('"+currentMenuItem+"')",0)
				$tab = $(this);
				$oe_overlay.show().stop(true,true).fadeTo(200, 0.6);
				$tab.addClass('slided selected');
				$tab.children('div').css('z-index','9999').stop(true,true).show();
			}
		}).bind('mouseleave', function() {
			var $this = $(this);
			$(this).children('a').css('background-color','');
			$this.removeClass('selected').children('div').css('z-index','1');
			$oe_menu_items.children('div').hide();
		});

		$oe_menu.bind('mouseenter', function() {
			onMegaMenu = true;
			var $this = $(this);
		}).bind('mouseleave', function() {
			onMegaMenu = false;
			currentMenuItem = "";
			var $this = $(this);
			$this.removeClass('hovered');
			$oe_overlay.stop(true,true).fadeTo(200, 0).hide();
			$oe_menu_items.children('div').hide();
		})
	});
	
	


	$('.dropdown-toggle').dropdown();

	/* ==========================================================================
	Nav Drop on Scoll
	========================================================================== */
	$(window).scroll(function () { 
	var topbar = $(window).scrollTop();

	if (topbar > 150) { 
		if ($('.oe_wrapper').addClass('navi_scrolled').is(':animated')) {
	    	return false;
	  	} else {
	    	$('.oe_wrapper').addClass('navi_scrolled').animate( { 
	      	top : 0,
	      	opacity : 1 
	    	}, 0);
	  	}
	} else { 
	  	$('.oe_wrapper').removeClass('navi_scrolled').stop().attr( { style : '' } );
	}
	});

});
</script>

