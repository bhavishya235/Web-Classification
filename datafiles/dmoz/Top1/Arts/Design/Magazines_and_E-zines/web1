		$(window).scroll(function() 
		{
			if(isScrolledIntoView($('#showMoreSidebar')))
			{startSidebar();}
			
			if(isScrolledIntoView($('#showMoreButton')))
			{startLoad();}
			
		});
		
		function isScrolledIntoView($elem)
		{

			if($elem.length == 0 )
			{
				return false;
			}
			var docViewTop = $(window).scrollTop();
			var docViewBottom = docViewTop + $(window).height();
			var elemTop = $elem.offset().top;
			
			return docViewBottom >= (elemTop - 500);
		}
		
		$('body').on('click','#showMoreButton',function(){startLoad();});
		
		$('body').on('click','#showMoreSidebar',function(){startSidebar();});
	
		function startSidebar()
		{
			if(!loadingSidebar && $('#showMoreSidebar:visible').length > 0)
			{
				sidebarPage += 1;
				getSidebar();
			}
		}
	
		function startLoad()
		{
			if(!loadingContent && $('#showMoreButton:visible').length > 0)
			{
				$('#showMoreButton').hide();
				contentOptions.page += 1;
				loading2(true);
				getContent();
			}	
		}

		var showMoreSidebar = "<button type='button' id='showMoreSidebar'>More</button>";
		var $sidebarRequest = null;
		var loadingSidebar = false;
		
		function getSidebar()
		{
			if($('#showMoreSidebar').length > 0)
			{$('#showMoreSidebar').remove();}
			
			if($sidebarRequest != null)
			{$sidebarRequest.abort();}
			
			var paramObject = new Object();
			paramObject = $.extend(true, {}, contentOptions);//deep copy so page parameter doesnt interfere
			paramObject['ajax'] = true;
			paramObject['action'] = 'getSidebar';
			paramObject['page'] = sidebarPage;
			loadingSidebar = true;
			
			$sidebarRequest = $.ajax({
				url: ajaxPage,
				data: paramObject,
				success:
					function(response)
					{
						
						$sidebarRequest = null;
						loadingSidebar = false;
						if(!response.success)
						{
							if(response.error.indexOf('404') != -1)
							{
								// alert('No trends found');
							}
							return false;
						}
						$newSidebar = $(response.data);
						//always going to be adding no making new
						$('#trends #sidebarContent').append($newSidebar);
						bindHover($('#sidebarContent .trend'));
						shareHover($('#sidebarContent .social-share'));
						if($newSidebar.length > 0)
						{
							$('#trends #sidebarContent').append(showMoreSidebar);
						}
						displaySidebarAds();
					},
				dataType: 'json',
				timeout: 15000,
				type: 'GET',
				error:
					function(xhr,status,err)
					{
						$sidebarRequest = null;
						loadingSidebar = false;
						if(err == 'abort')
						{
							return false;
						}
					}	
			});
				
		}
		
		
		
		
		var showMoreButton = "<button type='button' id='showMoreButton'>Show More</button>";
		
		function getContent()
		{
			var posLeft = parseInt($("#slider-thumb").css('left'), 10); 

			if($('#topSection').length > 0 && posLeft < 1)
			{
				if(contentOptions.type != 'trends' || contentOptions.view != 'magazine')
				{$('#topSection').hide();}
				else
				{$('#topSection').show();}
			}
			
			if($('#sidebarContent').length > 0)
			{
				if(contentOptions.type != 'trends' || contentOptions.view != 'magazine')
				{$('#sidebarContent').hide();}
				else
				{$('#sidebarContent').show();}
			}
			
			if($('#showMoreButton').length > 0)
			{$('#showMoreButton').remove();}
			
			//loading(true);
			if($ajaxRequest != null)
			{$ajaxRequest.abort();}

			var paramObject = new Object();
			paramObject = contentOptions;
			paramObject['ajax'] = true;
			paramObject['action'] = 'getContent';

			loadingContent = true;
			$ajaxRequest = $.ajax({
				url: ajaxPage,
				data: paramObject,
				success:
					function(response)
					{
						$ajaxRequest = null;
						loadingContent = false;
						loading(false);
						loading2(false);
						if(!response.success)
						{
							if(response.error.indexOf('404') != -1)
							{
//								alert('No trends found');
							}
							return false;
						}
						$newTrends = $(response.data);
						//$newTrends.addClass(contentOptions.view);
						if(contentOptions.page != 1)
						{
							$('#trends #trendContent').append($newTrends);	
						}
						else
						{
							$('#trends #trendContent').html($newTrends);
						}
						bindHover( $('.trend') );
						shareHover( $('.social-share'));
						
						if($newTrends.length >= trendsPerPage)
						{
							$('#trends #trendContent').append(showMoreButton);
						}
						checkContent();
						
					},
				dataType: 'json',
				timeout: 15000,
				type: 'GET',
				error:
					function(xhr,status,err)
					{
						$ajaxRequest = null;
						loadingContent = false;
						if(err == 'abort')
						{
							return false;
						}
						loading(false);
						loading2(false);
					}	
			});			
		}
		
		function error(msg)
		{alert(msg);}

	
		
		function changeView(type)
		{
		
			if(type != 'magazine')
			{
				//remove sidebar, make trendContent 100%;
				$('#trends #sidebarContent').hide();
				$('#trends #trendContent').css('width', '100%');
				$(".ad-header").show();
			}
			else
			{

				$('#trends #trendContent').css('width','75%');
				$('#trends #sidebarContent').show();
				$(".ad-header").hide();

			}
		
			$(".wrapper").removeClass("regular small reading magazine");
			$(".wrapper").addClass(type);
			
			var posLeft = parseInt($("#slider-thumb").css('left'), 10); 
			if(posLeft < 1 && $('#topSection').length > 0 && type == 'magazine') {
				$("#topSection").show();
				$(".ad-header").show();
				//$("#sidebarContent").show();
			
			}
			else
			{
				$('#topSection').hide();
				$(".ad-header").hide();
				//$("#sidebarContent").hide();

			}
			contentOptions.view = type;	

			checkContent();

		}
		
		

		// bind the event when hovering over a trend block
		function bindHover($bindElement) {	
			$bindElement.hover(
				function () {$(this).find(".featuredStatScore").show();},
			  	function () {$(this).find(".featuredStatScore").hide();}
			);		
		}		
		
		/*infinite loading*/		
		function getDocHeight() {
			var D = document;
			return Math.max(
				Math.max(D.body.scrollHeight, D.documentElement.scrollHeight),
				Math.max(D.body.offsetHeight, D.documentElement.offsetHeight),
				Math.max(D.body.clientHeight, D.documentElement.clientHeight)
			);
		}

		function shareHover($bindElement) {
			$bindElement.hoverIntent(function() {
				shareIt($(this));
				$(this).find(".share-buttons").fadeIn(500);
			}, function() {$(this).find(".share-buttons").hide();});						
		}

		function shareIt($bindElement) {
			if($bindElement.find("#shareIt").length > 0)
			{			
				var data = $bindElement.find('#shareIt').data();
				var id = $bindElement.parent().data("id");
				var img = $bindElement.parent().find(".trendImg > img").attr("src");
				var wide = "false";
			
				if($bindElement.hasClass("popular") || $bindElement.hasClass("horizontal")) {
					wide = "true";
				}

				$ajaxRequest = $.ajax({
					url: "/embed-templates/sharrre",
					data: {ajax:true, wide:wide, url:data.url, via:data.via, text:data.text, related:data.related, img:img },
					dataType: 'json',
					type: 'POST',
					timeout: 10000,
					success: function(response) 
					{						
						$bindElement.find('.share-buttons').html(response.data);					
						twttr.widgets.load();
						FB.XFBML.parse(document.getElementById(id));
						gapi.plusone.go(".share-buttons");

					}				
				});
			}
		}
		
		$('.typeButton').live(
			'click',
			function()
			{
				if($(this).data('type') == "protrends") {
					showpopup(12);
					return false;
				}
				$('.typeButton').removeClass('active');
				$(this).addClass('active');
				contentOptions.type = $(this).data('type');
				$(".wrapper").removeClass("trends lists videos photos");
				$(".wrapper").addClass($(this).data('type'));

				contentOptions.page = 1; //reset page to wipe container
				updateBreadcrumb($(this).data('type'),$(this).data('medialink'),$(this).data('mediatitle'));
				loading(true);
				getContent();
			}
		);		
		
		function updateBreadcrumb(e,t,n){if($("#titleMedia").length==0){return false}if(e=="trends"){$("#titleMedia").hide();return}$("#titleMedia").show();var r=$("#titleMedia").children("a").lengt>0?" > <a href='http://www.trendhunter.com/"+t+"'>"+n+"</a>":" > "+n;$("#titleMedia").html(r);return}$(".filterButton").live("click",function(){$(".filterButton").removeClass("active");$(this).addClass("active")})
		
		
		function scoreFilter(e)
		{
			// 0	  			|  	30  			|  	60  							| 90
			// Show All 	  	|	hide threeWide 	|	hide threeWide & magazine		| Hide all except twoWide
		    if ($("#topSection").length > 0)
		    {$("#topSection").hide()}
		    contentOptions.score = e;		
			
			if(e == 0)
			{
				$('#trends #trendContent .trend:not(.sponsored)').css("display","inline-block");
			}
			else if(e == 30)
			{
				$('#trends #trendContent .trend:not(.threeWide,.sponsored)').css("display","inline-block");
				$('#trends #trendContent .trend.threeWide:not(.sponsored)').hide();
			}
			else if(e == 60)
			{
				$('#trends #trendContent .trend:not(.sponsored)').css("display","inline-block");
				$('#trends #trendContent .trend.threeWide:not(.sponsored),#trends #trendContent .ad-hide:not(.sponsored)').hide();
			}
			else if(e == 90)
			{
				$('#trends #trendContent .trend:not(.sponsored)').css("display","inline-block");	
				$('#trends #trendContent .trend:not(.twoWide,.sponsored)').hide();				
			}

		    if (contentOptions.view == "magazine")
		    {
		        if (e > 25 && $("#topSection").length > 0)
		        {$("#topSection").hide()}
		        else if ($("#topSection").length > 0)
		        {$("#topSection").show()}
		    }
			
			checkContent();
		}
		
		function checkContent()
		{
			if(isScrolledIntoView($('#showMoreButton')) && $('#showMoreButton:visible').length > 0)
			{$('#showMoreButton').trigger('click');}
			if(isScrolledIntoView($('#showMoreSidebar')) && $('#showMoreSidebar:visible').length > 0)
			{$('#showMoreSidebar').trigger('click');}
		}


		//show more loading gifs
		function loading(e){if(!e){$("#loadingDiv").hide();return false}var t=$(document).height();$("#loadingDiv").css("height",t);var n=$(window).scrollTop()+$(window).height()/2-150;$("#loadingDiv img").css("top",n);$("#loadingDiv").show()}
		function loading2(e){if(!e){$("#loadingDiv2").hide();return false}$("#loadingDiv2").show()}

