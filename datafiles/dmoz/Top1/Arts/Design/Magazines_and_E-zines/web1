<div id="errorsH"></div>
<input type="text" name="user_email" id="user_email" class="TextField"/>
<input type="submit" name="sub_newsletter" id="sub_newsletter" class="SubscribeButton" value="subscribe"/>
<input type="hidden" id="action" name="action" value="sub_newsletter"/>
