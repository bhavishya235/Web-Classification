      {{if confirm_btn}}
      <div class="form-item form-item-a" id="popup-confirm-container"><a class="form-button form-submit form-button-default form-button-default form-button-confirm" unselectable="on" id="popup-confirm">Confirm</a>
    </div>  {{/if}}
      {{if custom_btn}}
      <div class="form-item form-item-a" id="popup-custom-btn-container"><a class="form-button ${custom_btn.classname.join(' ')}" unselectable="on" id="popup-custom-btn">${custom_btn.value}</a>
    </div>  {{/if}}
      {{if done_btn}}
      <div class="form-item form-item-a" id="popup-done-container"><a class="form-button form-submit form-button-default" unselectable="on" id="popup-done">Done</a>
    </div>  {{/if}}
      {{if close_btn}}
      <div class="form-item form-item-a" id="popup-force-close-container"><a class="form-button form-button-dark form-button-dark form-button-close" unselectable="on" id="popup-force-close">Close</a>
