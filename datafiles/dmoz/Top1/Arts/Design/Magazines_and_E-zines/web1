<script type="text/javascript">
			
			$('#latest').find('.article_hp:nth-child(4)').addClass('tabux');
			$('#latest').find('.article_hp:nth-child(7)').addClass('tabux');
			
      var masonry = $('#latest').masonry({
				itemSelector: '.article_hp'
			});
      
			$('#latest .adv').css('display','block');
			
			function moreArticle() {
        var index = $('#idx').val();

        var request = $.ajax({
					url: "/more-news",
					type: "post",
					data: { 'index': index,
                            'section': 'index',
                            'tag_id': -1 }
				});

        request.done(function (response, textStatus, jqXHR){
          if (response.indexOf("__EMPTY__") !== -1) {
            // Ho finito i dati da inviare
            //alert('fine');
						$('#more').find('a').addClass('end').html('NO MORE NEWS');
          } else {
            $('#idx').val(parseInt($('#idx').val())+1);
						//$(response).appendTo('#latest'); // carico i dati  via ajax						
						$(response).insertBefore('#adv'); // carico i dati  via ajax						

						$('#latest').find('.article_hp:nth-child(10n+4)').addClass('tabux');
						$('#latest').find('.article_hp:nth-child(10n+7)').addClass('tabux');
						
						$('#latest').masonry('reload');
						//alert('fatto');

                         $('#latest .article_hp').hover(
	function () {
		$(this).find('.hover').stop().animate({ opacity: 1, left: '0', top:'0' }, 300, function() { 
		// Animation complete.
		});
	},
	function () {
		$(this).find('.hover').stop().animate({ opacity: 0, left: '-220', top:'-220' }, 300, function() {
		// Animation complete.
		});
	}
);

					}
    	});
}


    </script>
    
