<script type="text/javascript">
               function resizebox() {
                   if (jQuery(window).width() <= 480) return;
               
                   var left_width = parseInt(jQuery('#Wrapper').width() - (jQuery('#SidebarContainer').width()) - 38);

                   jQuery('#Slider').width(left_width);
                   sliderImageheight= jQuery('.FirstImage img:visible').height();
                   jQuery('#PreSlider').width(left_width);
                   jQuery('#PreSlider').height(sliderImageheight+50);
                   jQuery('#Slider').height(sliderImageheight+50);
                   jQuery('#page404').width(left_width);
                                jQuery('#Left').width(left_width);
               }

               function resizeboxforLoop(){
                   
                   if (jQuery(window).width() <= 480) return;
                   var left_width = parseInt(jQuery('#Wrapper').width() - (jQuery('#SidebarContainer').width()) - 38);
                   jQuery('#LeftLoop').width(left_width);
                   jQuery('.insideLoop').width(left_width);
                   jQuery('.ProjectMainTitle').width(left_width);
               }

               jQuery(document).ready(function(){
                   resizebox();
                   resizeboxforLoop();
                   var number_NL = '386,180';

                   jQuery("li.Newsletter").append('<b>&nbsp;('+number_NL+')</b>');

                   jQuery("#form_sub_newsletter_header").validate({
                    rules:{ user_email:{
                                        required: true,
                                        email: true
                                        }
                          },
                    errorLabelContainer: '#errorsH',
                    errorElement: 'p',
                    submitHandler: function(form)
                    {
                        jQuery.post('http://www.designboom.com/wp-admin/admin-ajax.php',jQuery("#form_sub_newsletter_header").serialize(), function(data)
                        {
                            jQuery('#Frequency p').text(data);
                            //setTimeout(function(){ jQuery("#NewsletterBox").slideToggle("medium"); }, 2000);
                        });
                    }
                    });

                    jQuery("#CommentsPolicy").click(function () {
                      jQuery("#CommentsPolicyText").slideToggle("slow");
                    });
               });

              var widthdevice = jQuery(window).width();
              jQuery(window).bind('resize', function(){
                   resizebox();
                   resizeboxforLoop();
                   if (jQuery(window).width()!=widthdevice){
                       if(jQuery(".SlidingMenuVoices").is(":visible") || jQuery(".SubSlidingMenuVoices").is(":visible")){
                         jQuery(".SlidingMenuVoices").slideUp("slow");
                         jQuery(".SubSlidingMenuVoices").slideUp("slow");
                     };
                   }
               });
               window.onorientationchange = function(){
                   resizebox();
                   resizeboxforLoop();
                    if(jQuery(".SlidingMenuVoices").is(":visible") || jQuery(".SubSlidingMenuVoices").is(":visible")){
                         jQuery(".SlidingMenuVoices").slideUp("slow");
                         jQuery(".SubSlidingMenuVoices").slideUp("slow");
                     };
               }

               jQuery(".SlidingMenuTitle").click(function () {
                   if(jQuery(window).width() > 730 && jQuery(window).width() < 1041){
                       jQuery(".SubSlidingMenuVoices").slideToggle("slow");
                   }else{
                       jQuery(".SlidingMenuVoices").slideToggle("slow");
                   }
               });

                jQuery("b.lens,b.icon-remove").click(function () {
                    jQuery("#Search").slideToggle("medium");
                 });

                jQuery(".Newsletter").click(function () {
                  jQuery("#NewsletterBox").slideToggle("medium");
                });

                jQuery("div#close_NLH").click(function () {
                  jQuery("#NewsletterBox").slideToggle("medium");
                  jQuery('#Frequency p').text('');
                });         

                jQuery("b.lens").hover(function() {
                    jQuery("b.lens img").attr("src","http://www.designboom.com/wp-content/themes/designboom/images/lens_over.gif");
                },function() {
                    jQuery("b.lens img").attr("src","http://www.designboom.com/wp-content/themes/designboom/images/lens.gif");
                });
            </script><div id="PreSlider" class="">
