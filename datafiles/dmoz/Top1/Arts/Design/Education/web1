                    jQuery(document).ready(function($) {
                        var sidebarID = '#SidebarContainer';
                        var sidebar = 'sidebar-home'; 
                                                var count = 2;
                        var check_loop = false;
                        var called = 0;
                                                                        var aaa =0;
                                                if ((($(document).height())-$(window).height()) > 1000) {
                                                    aaa= 1;
                                                }
                                                $(window).scroll(function() {
                                                    var scroll = (65*$(window).height())/100;
                                                    var sss = $(document).height()-scroll;
                                                    if (($(window).scrollTop() >= (($(document).height() - $(window).height()) - 1000)) && called == 0) {
                                                       called=1;
											                                if(count < 17){
				                                    $('.spinnerLoop').show();
				                                }                                                        loadArticle(count);
                                                        ga('send', 'pageview');
							if (count <=8 && $(window).width() > 480) {
                                                           loadSidebar(sidebar,count,sidebarID,0);
                                                       }
                                                       resizeboxforLoop();
                                                       count++;
                                                       aaa= 0;
                                                   }
                                                });                        function loadArticle(pageNumber) {

                            $.ajax({
                            url: "http://www.designboom.com/wp-admin/admin-ajax.php",
                                    type:'POST',
                                                            data: "action=infinite_scroll&pagina=" + pageNumber + '&loop=loop&category=',
                                                    }).done(function(html)
                                {
                                    
                                                                          // postscribe("#LeftLoop",html);
                                       jQuery("#LeftLoop").append(html);
                                        
                                                                    ga('send', 'pageview');
                                    called = 0;
                                })
                                .always(function(){
					resizeboxforLoop(); 
                                        called=0;
                                    	$('.spinnerLoop').hide();
                                });
                            // return false;
                        };

                    });
