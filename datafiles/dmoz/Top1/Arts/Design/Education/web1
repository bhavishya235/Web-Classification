      <div class="nid-calendar">
      	<h3>NID CALENDAR</h3>
      	<link href="http://www.nid.edu/css/calendar.css" rel="stylesheet" type="text/css">
<script type="text/javascript" language="javascript">
var current_month = 02;
var current_year = 2014;

function getPrevMonth()
{
	if(current_month == 1)
	{
		current_month = 12;
		current_year = current_year - 1;
	}
	else
	{
		current_month = current_month - 1;
	}
	
	$.post('http://www.nid.edu/calendar.php', { month: current_month, year: current_year }, 
		function(data) {
			$('#calendar_wrapper').html(data);
	});
}
function getNextMonth()
{
	if(current_month == 12)
	{
		current_month = 1;
		current_year = current_year + 1;
	}
	else
	{
		current_month = current_month + 1;
	}
	
	$.post('http://www.nid.edu/calendar.php', { month: current_month, year: current_year }, 
		function(data) {
			$('#calendar_wrapper').html(data);
	});
}
		
/*function loadTips() {
	$$("*").findAll(function(node){
	  return node.getAttribute('title');
	}).each(function(node){
	  new Tooltip(node,node.title);
	  node.removeAttribute("title");
	});
}

window.onload = loadTips;*/

</script>

<div id="calendar_wrapper">	<table id="calendar" cellspacing=1>
	<tr>
		<td colspan=2><a href="#" class="monthnav" onclick="getPrevMonth();return false;">&laquo; Prev</a></td>
		<td colspan=3 class="month">February 2014</td>
		<td colspan=2><a href="#" class="monthnav" onclick="getNextMonth();return false;">Next &raquo;</a></td>
	</tr>
	<tr class="daynames"> 
		<td>S</td><td>M</td><td>T</td><td>W</td><td>T</td><td>F</td><td>S</td>
	</tr><tr class="week"><td class="past"></td><td class="past"></td><td class="past"></td><td class="past"></td><td class="past"></td><td class="past"></td><td class="past">1</td></tr><tr class="week"><td class="past">2</td><td class="past">3</td><td class="past">4</td><td class="past">5</td><td class="past">6</td><td class="past">7</td><td class="past">8</td></tr><tr class="week"><td class="past">9</td><td class="past">10</td><td class="past">11</td><td class="past">12</td><td class="past">13</td><td class="past">14</td><td class="past">15</td></tr><tr class="week"><td class="past">16</td><td class="past">17</td><td class="past">18</td><td class="past">19</td><td class="past">20</td><td class="past">21</td><td class="past">22</td></tr><tr class="week"><td class="past">23</td><td class="past">24</td><td class="past">25</td><td class="past">26</td><td class="days">27</td><td class="days">28</td><td class="past"></td></tr></table></div>      </div>
