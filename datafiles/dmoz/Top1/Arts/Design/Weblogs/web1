/*scrollable*/
/*
root element for the scrollable.
when scrolling occurs this element stays still.
*/
.scrollable {
/* required settings */
position:relative;
overflow:hidden;
width: 700px;
height:320px;
/*
root element for scrollable items. Must be absolutely positioned
and it should have a extremely large width to accomodate scrollable
items.  it's enough that you set the width and height for the root
element and not for this element.
*/
.scrollable .items {
/* this cannot be too large */
width:20000em;
position:absolute;
clear:both;
}
.items div {
float:left;
width:700px;
}
/* single scrollable item */
.scrollable img {
float:left;
margin:20px 5px 20px 15px;
background-color:#fff;
width:700px;
//height:495px;
-moz-border-radius:4px;
-webkit-border-radius:4px;
}
/* active item */
.scrollable .active {
border:2px solid #000;
position:relative;
cursor:default;
}
/* this makes it possible to add next button beside scrollable */
.scrollable {
float:left;
}
.browse {
background: url("http://market.poppytalkhandmade.com/ads/blogimages/scrollable/navbar_grey_arrow.png") no-repeat scroll 0 0 transparent;
height: 320px;
left: -1px;
position: absolute;
//top: -1px;
width: 28px;
}
.browse.right {
background-position: -28px 0;
left: auto;
right: -1px;
}
/* disabled navigational button */
a.disabled {
visibility:hidden !important;
}
/* font */
a:link {
color: black;
text-decoration: none;
}
.comment-bubble {
float : left;
width : 30px;
height : 48px;
background : url("http://marketplace.poppytalk.com/js/images/speech_bubble_green.png");
background-repeat: no-repeat;
font-size : 18px;
margin-top : -10px;
margin-right : 20px;
text-align : center;
position : absolute;
left:-40px;
}
h3.post-title {
border-bottom: 1px solid #999999;
color: #949595;
font: 15px/160% 'Bebas','PT Sans Narrow';
letter-spacing: 1px;
margin-left: 40px;
text-rendering: optimizelegibility;
}
.footer-inner{
background-color: #5c5c5c;
color:#000;
}
.footer-outer .widget h2 {
color: #000;
}
.item-title a{
padding-bottom: 0.2em;
color: #000;
}
.item-title a:hover{
padding-bottom: 0.2em;
color: #000;
}
.item-title a:visited{
padding-bottom: 0.2em;
color: #000;
}/* navigation */
