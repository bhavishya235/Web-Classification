// padding:2px;
// border:1px solid #ccc;
width:700px;
//height:495px;
-moz-border-radius:4px;
-webkit-border-radius:4px;
}
/* active item */
.scrollable .active {
border:2px solid #000;
position:relative;
cursor:default;
}
