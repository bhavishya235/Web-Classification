/* try rollovers */
.highlight-thumb {
background-attachment: scroll;
background-clip: border-box;
background-color: transparent;
background-image: none;
background-origin: padding-box;
background-position: 0 0;
background-repeat: no-repeat;
background-size: cover;
border-right: 2px solid #21201E;
color: #FFFFFF;
float: left;
//   height: 150px;
margin: 0;
padding: 0;
text-decoration: none;
//   width: 150px;
}
.highlight-thumb {
padding: 0;
margin: 0;
height: 320px;
width: 700px;
color: #fff;
float: left;
background: 0 0 no-repeat;
background-size: cover;
//border-right: 2px solid #21201e;
//text-decoration: none;
}
.highlight-thumb h3 {
font-family:Arial,&#8203;Tahoma,&#8203;Helvetica,&#8203;FreeSans,&#8203;sans-serif;
font-size:13.5px;
font-weight:400;
display: none;
height:29px;
width:700px;
//	margin: 0 auto;
margin:290px 0 0 0px;
//	padding: 5px 7px;
padding:5px 5px 0 10px;
//	line-height: 48px;
overflow: hidden;
color: #dfdfd8;
}
.highlight-thumb:hover h3 {
//      font-size:60px;
display: inherit;
background-image:url("http://market.poppytalkhandmade.com/ads/blogimages/scrollable/rollover_bar.png");
}
/* Drop Down Menu *//* navigation */
#navwrapper {
width: 1200px;
}
.tabs-inner {
margin: .5em 1em;
padding: 0;
}
.tabs-inner .section {
margin: 5px;
}
.tabs-inner .widget ul {
padding: 0;
//background: #85898a none repeat scroll bottom;
background: #FFF none repeat scroll bottom;
border-top: 0 solid#000;
border-bottom: 0 solid#000;
-moz-border-radius: 0;
-webkit-border-radius: 0;
-goog-ms-border-radius: 0;
border-radius: 0;
}
.tabs-inner .widget li {
border: none;
}
.tabs-inner .widget li a {
display: inline-block;
//color: #FFF;
color:#000;
//padding: .5em 1em;
margin-right: 0px;
padding-top:4px;
//font: normal normal 12px Georgia, Utopia, 'Palatino Linotype', Palatino, serif;
font: normal normal 15px Arial,Tahoma,Helvetica,FreeSans,sans-serif;
font-family: 'Bebas', 'PT Sans Narrow';
line-height: 140%;
word-spacing: 4px;
letter-spacing: 0px;
text-rendering: optimizeLegibility;
-webkit-font-smoothing: antialiased;
-moz-border-radius: 0 0 0 0;
-webkit-border-top-left-radius: 0;
-webkit-border-top-right-radius: 0;
-goog-ms-border-radius: 0 0 0 0;
border-radius: 0 0 0 0;
background: transparent none no-repeat scroll top left;
border-right: 0px solid transparent;
}
.tabs-inner .widget li:first-child a {
//padding-left: 1.25em;
padding-left: 15px;
padding-right: 10px;
//  margin-left: 30px;
-moz-border-radius-topleft: 0;
-moz-border-radius-bottomleft: 0;
-webkit-border-top-left-radius: 0;
-webkit-border-bottom-left-radius: 0;
-goog-ms-border-top-left-radius: 0;
-goog-ms-border-bottom-left-radius: 0;
border-top-left-radius: 0;
border-bottom-left-radius: 0;
}
.tabs-inner .widget li.selected a,
.tabs-inner .widget li a:hover {
position: relative;
z-index: 1;
//background: #ddc008 none repeat scroll bottom;
background: #F10020 none repeat scroll bottom;
//color: #ffffff;
color:#FFF;
-moz-box-shadow: 0 0 0 rgba(0, 0, 0, .15);
-webkit-box-shadow: 0 0 0 rgba(0, 0, 0, .15);
-goog-ms-box-shadow: 0 0 0 rgba(0, 0, 0, .15);
box-shadow: 0 0 0 rgba(0, 0, 0, .15);
}
.tabs-inner {
margin: 0.5em 0em;
padding: 0;
}
.tabs-inner .section {
margin: 0px;
}
