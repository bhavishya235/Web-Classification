    function clearDefault(el, defaultValue) {
        //alert("comparing: '" + el.value + "' with: '" + defaultValue +"'");
        if (defaultValue==el.value){
            el.value = "";
        }
    }

    function restoreDefault(el, defaultValue){
        if (el.value == '') el.value = defaultValue
