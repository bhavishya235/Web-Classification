border: none;
}
.tabs-inner .widget li a {
display: inline-block;
//color: #FFF;
color:#000;
//padding: .5em 1em;
margin-right: 0px;
padding-top:4px;
//font: normal normal 12px Georgia, Utopia, 'Palatino Linotype', Palatino, serif;
